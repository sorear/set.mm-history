    $( Value of the arithmetic progression function. $)
    vdwapval $p |- ( ( K e. NN0 /\ A e. NN /\ D e. NN ) ->
      ( X e. ( A ( AP ` K ) D ) <->
        E. m e. ( 0 ... ( K - 1 ) ) X = ( A + ( m x. D ) ) ) ) $=
