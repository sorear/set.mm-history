  $( Unequal prime numbers are relatively prime.  (Contributed by Mario
     Carneiro, 23-Feb-2014.) $)
  prmrp $p |- ( ( P e. Prime /\ Q e. Prime ) ->
                ( ( P gcd Q ) = 1 <-> P =/= Q ) ) $=
    ( cprime wcel wa cdivides wbr wn cgcd co c1 wceq cz wb prmz coprm sylan2 c2
    wne cuz cfv prmuz2 dvdsprm sylan necon3bbid bitr3d ) ACDZBCDZEZABFGZHZABIJK
    LZABSUHUGBMDUKULNBOABPQUIUJABUGARTUADUHUJABLNAUBBAUCUDUEUF $.
    $( [23-Feb-2014] $)

  $( If an integer divides the product of two integers and is coprime to one of
     them, then it divides the other.  (Contributed by Paul Chapman,
     22-Jun-2011.) $)
  coprmdvds $p |- ( ( K e. ZZ /\ M e. ZZ /\ N e. ZZ ) ->
                    ( ( K || ( M x. N ) /\ ( K gcd M ) = 1 ) -> K || N ) ) $=
    ( cz wcel cmul co cdivides wbr cgcd c1 wceq wi wa zcn ancoms 3adant2 adantr
    cc cabs w3a mulcom syl2an 3adant1 breq2d simp1 zmulcl dvdsgcd syl3anc mpand
    dvdsmul2 sylbid cfv absmulgcd 3coml oveq2 mulid1 sylan9eqr fveq2d 3ad2antl3
