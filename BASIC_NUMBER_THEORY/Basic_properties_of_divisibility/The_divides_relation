$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                             The divides relation
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c || $.

  $( Extend the definition of a class to include the divides relation. $)
  cdivides $a class || $.

  ${
    $d n x y $.
    $( Define the divides relation. $)
    df-divides $a |- || = { <. x , y >. | ( ( x e. ZZ /\ y e. ZZ ) /\
                                            E. n e. ZZ ( n x. x ) = y ) } $.
  $}

  ${
    $d M n x y $.  $d N n x y $.
    $( Divides relation. ` M || N ` means ` M ` divides into ` N ` with no
       remainder.  (Contributed by Paul Chapman, 21-Mar-2011.) $)
    divides $p |- ( ( M e. ZZ /\ N e. ZZ ) ->
                    ( M || N <-> E. n e. ZZ ( n x. M ) = N ) ) $=
      ( vx vy cdivides wbr cop cv cz wcel wa cmul co wceq wrex copab df-divides
      df-br rexbidv eleq2i bitri oveq2 eqeq1d eqeq2 opelopab2 syl5bb ) BCFGZBCH
      ZDIZJKEIZJKLAIZUJMNZUKOZAJPZLDEQZKZBJKCJKLULBMNZCOZAJPZUHUIFKUQBCFSFUPUID
      EARUAUBUOURUKOZAJPUTDEBCJJUJBOZUNVAAJVBUMURUKUJBULMUCUDTUKCOVAUSAJUKCURUE
      TUFUG $.
      $( [21-Mar-2011] $)
  $}

  ${
    $d k M $.  $d k N $.
    $( One nonzero integer divides another integer if and only if their
       quotient is an integer.  (Contributed by Jeff Hankins, 29-Sep-2013.) $)
    divides2 $p |- ( ( M e. ZZ /\ M =/= 0 /\ N e. ZZ ) -> ( M || N <-> ( N / M
    ) e. ZZ ) ) $=
      ( vk cz wcel cc0 wne w3a cdivides wbr cv cmul co wceq wb wa cc zcn adantr
      wrex cdiv divides 3adant2 3ad2ant3 adantl 3ad2ant1 simpl2 syl112anc eqcom
      divmul3 syl6bb biimprd impr simprl eqeltrd exp32 rexlimdv divcan1 syl3anc
      simpr simp2 oveq1 eqeq1d rcla4ev syl2anc ex impbid bitrd ) ADEZAFGZBDEZHZ
      ABIJZCKZALMZBNZCDTZBAUAMZDEZVIVKVMVQOVJCABUBUCVLVQVSVLVPVSCDVLVNDEZVPVSVL
      VTVPPPVRVNDVLVTVPVRVNNZVLVTPZWAVPWBWABVONZVPWBBQEZVNQEZAQEZVJWAWCOVLWDVTV
      KVIWDVJBRUDZSVTWEVLVNRUEVLWFVTVIVJWFVKARUFZSVIVJVKVTUGBVNAUJUHBVOUIUKULUM
      VLVTVPUNUOUPUQVLVSVQVLVSPVSVRALMZBNZVQVLVSUTVLWJVSVLWDWFVJWJWGWHVIVJVKVAB
      AURUSSVPWJCVRDVNVRNVOWIBVNVRALVBVCVDVEVFVGVH $.
      $( [29-Sep-2013] $)
  $}

  ${
    $d k M $.  $d k N $.
    $( One nonzero integer divides another integer if and only if the remainder
       upon division is zero.  (Contributed by Mario Carneiro, 22-Feb-2014.) $)
    divides3 $p |- ( ( M e. NN /\ N e. ZZ ) ->
                     ( M || N <-> ( N mod M ) = 0 ) ) $=
      ( cn wcel cz wa cdivides wbr cdiv co cmo cc0 wceq wne wb nnz jca divides2
      nnne0 3expa sylan cr crp zre nnrp mod0 syl2anr bitr4d ) ACDZBEDZFABGHZBAI
      JEDZBAKJLMZUIAEDZALNZFUJUKULOZUIUNUOAPASQUNUOUJUPABRTUAUJBUBDAUCDUMULOUIB
      UDAUEBAUFUGUH $.
      $( [15-Jul-2014] $) $( [22-Feb-2014] $)
  $}

  $( Two ways to say ` A == B ( mod N ) ` . $)
  moddvds $p |- ( ( N e. NN /\ A e. ZZ /\ B e. ZZ ) ->
                  ( ( A mod N ) = ( B mod N ) <-> N || ( A - B ) ) ) $=
    ( wcel cz cmo co wceq wb wa cc0 syl caddc zre modadd1 3expia syl2anc oveq1d
    cr wi cn cmin cdivides wbr crp nnrp adantr eqeq2d ad2antrl ad2antll renegcl
    0mod cneg syl22anc recnd negsub negid eqeq12d sylibd resubcl 0re a1i addid2
    cc npcan impbid zsubcl divides3 sylan2 3bitr4d 3impb ) CUADZAEDZBEDZACFGZBC
    FGZHZCABUBGZUCUDZIVLVMVNJZJZVRCFGZKCFGZHZWBKHZVQVSWAWCKWBWACUEDZWCKHVLWFVTC
    UFUGZCULLUHWAVQWDWAVQABUMZMGZCFGZBWHMGZCFGZHZWDWAASDZBSDZWHSDZWFVQWMTVMWNVL
    VNANUIZVNWOVLVMBNUJZWAWOWPWRBUKLWGWNWOJWPWFJVQWMABWHCOPUNWAWJWBWLWCWAWIVRCF
    WAAVDDZBVDDZWIVRHWAAWQUOZWABWRUOZABUPQRWAWKKCFWAWTWKKHXBBUQLRURUSWAWDVRBMGZ
    CFGZKBMGZCFGZHZVQWAVRSDZKSDZWOWFWDXGTWAWNWOXHWQWRABUTQXIWAVAVBWRWGXHXIJWOWF
    JWDXGVRKBCOPUNWAXDVOXFVPWAXCACFWAWSWTXCAHXAXBABVEQRWAXEBCFWAWTXEBHXBBVCLRUR
    USVFVTVLVREDVSWEIABVGCVRVHVIVJVK $.
    $( [18-Feb-2014] $)

  ${
    $d K x $.  $d M x $.  $d N x $.
    $( A lemma to assist theorems of ` || ` with no antecedents.  (Contributed
       by Paul Chapman, 21-Mar-2011.) $)
    dvds0lem $p |- ( ( ( K e. ZZ /\ M e. ZZ /\ N e. ZZ ) /\ ( K x. M ) = N ) ->
                     M || N ) $=
      ( vx cz wcel w3a cmul co wceq cdivides wi wa cv wrex oveq1 eqeq1d rcla4ev
      wbr adantl wb divides adantr mpbird expr 3impa 3comr imp ) AEFZBEFZCEFZGA
      BHIZCJZBCKSZUJUKUIUMUNLZUJUKUIUOUJUKMZUIUMUNUPUIUMMZMUNDNZBHIZCJZDEOZUQVA
      UPUTUMDAEURAJUSULCURABHPQRTUPUNVAUAUQDBCUBUCUDUEUFUGUH $.
      $( [21-Mar-2011] $)
  $}

  ${
    $d J x $.  $d K x $.  $d M x z $.  $d N x z $.  $d Z z $.  $d ph x $.
    dvds1lem.1 $e |- ( ph -> ( J e. ZZ /\ K e. ZZ ) ) $.
    dvds1lem.2 $e |- ( ph -> ( M e. ZZ /\ N e. ZZ ) ) $.
    dvds1lem.3 $e |- ( ( ph /\ x e. ZZ ) -> Z e. ZZ ) $.
    dvds1lem.4 $e |- ( ( ph /\ x e. ZZ ) ->
                       ( ( x x. J ) = K -> ( Z x. M ) = N ) ) $.
    $( A lemma to assist theorems of ` || ` with one antecedent.  (Contributed
       by Paul Chapman, 21-Mar-2011.) $)
    dvds1lem $p |- ( ph -> ( J || K -> M || N ) ) $=
      ( vz cv cmul co wceq cz wrex wcel wa cdivides wbr oveq1 eqeq1d rcla4ev ex
      sylan syld rexlimdva wb divides syl 3imtr4d ) ABMZCNODPZBQRZLMZENOZFPZLQR
      ZCDUAUBZEFUAUBZAUOUTBQAUNQSTZUOGENOZFPZUTKVCVEUTVCGQSVEUTJUSVELGQUQGPURVD
      FUQGENUCUDUEUGUFUHUIACQSDQSTVAUPUJHBCDUKULAEQSFQSTVBUTUJILEFUKULUM $.
      $( [21-Mar-2011] $)
  $}

  ${
    $d I x y $.  $d J x y $.  $d K x y $.  $d L x y $.  $d M x y z $.
    $d N x y z $.  $d Z z $.  $d ph x y $.
    dvds2lem.1 $e |- ( ph -> ( I e. ZZ /\ J e. ZZ ) ) $.
    dvds2lem.2 $e |- ( ph -> ( K e. ZZ /\ L e. ZZ ) ) $.
    dvds2lem.3 $e |- ( ph -> ( M e. ZZ /\ N e. ZZ ) ) $.
    dvds2lem.4 $e |- ( ( ph /\ ( x e. ZZ /\ y e. ZZ ) ) -> Z e. ZZ ) $.
    dvds2lem.5 $e |- ( ( ph /\ ( x e. ZZ /\ y e. ZZ ) ) ->
               ( ( ( x x. I ) = J /\ ( y x. K ) = L ) -> ( Z x. M ) = N ) ) $.
    $( A lemma to assist theorems of ` || ` with two antecedents.  (Contributed
       by Paul Chapman, 21-Mar-2011.) $)
    dvds2lem $p |- ( ph -> ( ( I || J /\ K || L ) -> M || N ) ) $=
      ( wa cmul wceq cz wcel vz cdivides cv co wrex wb divides bi2anan9 syl2anc
      wbr biimpd reeanv syl6ibr oveq1 eqeq1d rcla4ev ex syld rexlimdvva sylibrd
      sylan syl ) ADEUBUJZFGUBUJZPZUAUCZHQUDZIRZUASUEZHIUBUJZAVEBUCZDQUDERZCUCZ
      FQUDGRZPZCSUEBSUEZVIAVEVLBSUEZVNCSUEZPZVPAVEVSADSTESTPZFSTGSTPZVEVSUFKLVT
      VCVQWAVDVRBDEUGCFGUGUHUIUKVLVNBCSSULUMAVOVIBCSSAVKSTVMSTPPZVOJHQUDZIRZVIO
      WBWDVIWBJSTWDVINVHWDUAJSVFJRVGWCIVFJHQUNUOUPVAUQURUSURAHSTISTPVJVIUFMUAHI
      UGVBUT $.
      $( [21-Mar-2011] $)
  $}

  $( An integer divides itself.  (Contributed by Paul Chapman, 21-Mar-2011.) $)
  iddvds $p |- ( N e. ZZ -> N || N ) $=
    ( cz wcel c1 cmul co wceq cdivides wbr cc zcn mulid2 syl 1z mp3anl1 anabsan
    dvds0lem mpdan ) ABCZDAEFAGZAAHIZSAJCTAKALMSTUADBCSSTUANDAAQOPR $.
    $( [21-Mar-2011] $)

  $( 1 divides any integer.  (Contributed by Paul Chapman, 21-Mar-2011.) $)
  1dvds $p |- ( N e. ZZ -> 1 || N ) $=
    ( cz wcel c1 cmul co wceq cdivides wbr cc zcn mulid1 syl 1z mp3anl2 anabsan
    dvds0lem mpdan ) ABCZADEFAGZDAHIZSAJCTAKALMSTUASDBCSTUANADAQOPR $.
    $( [21-Mar-2011] $)

  $( Any integer divides 0.  (Contributed by Paul Chapman, 21-Mar-2011.) $)
  dvds0 $p |- ( N e. ZZ -> N || 0 ) $=
    ( cz wcel cc0 cmul co wceq cdivides wbr cc zcn mul02 syl wi 0z w3a dvds0lem
    ex mp3an13 mpd ) ABCZDAEFDGZADHIZUAAJCUBAKALMDBCZUAUDUBUCNOOUDUAUDPUBUCDADQ
    RST $.
    $( [21-Mar-2011] $)

  ${
    $d M x $.  $d N x $.
    $( An integer divides another iff its negation does.  (Contributed by Paul
       Chapman, 21-Mar-2011.) $)
    negdvdsb $p |- ( ( M e. ZZ /\ N e. ZZ ) -> ( M || N <-> -u M || N ) ) $=
      ( vx cz wcel wa cdivides wbr cneg znegcl cmul co wceq zcn syl2anr adantlr
      cc eqeq1d biimprd dvds1lem cv id anim1i adantl mul2neg mulneg12 impbid )
      ADEZBDEZFZABGHAIZBGHUJCABUKBCUAZIZUJUBZUHUKDEUIAJUCZULDEZUMDEUJULJUDZUJUP
      FZUMUKKLZBMULAKLZBMURUSUTBUHUPUSUTMZUIUPULQEZAQEZVAUHULNZANZULAUEOPRSTUJC
      UKBABUMUOUNUQURUMAKLZBMULUKKLZBMURVFVGBUHUPVFVGMZUIUPVBVCVHUHVDVEULAUFOPR
      STUG $.
      $( [21-Mar-2011] $)

    $( An integer divides another iff it divides its negation.  (Contributed by
       Paul Chapman, 21-Mar-2011.) $)
    dvdsnegb $p |- ( ( M e. ZZ /\ N e. ZZ ) -> ( M || N <-> M || -u N ) ) $=
      ( vx cz wcel wa cdivides wbr cneg cv id znegcl cmul co wi cc zcn dvds1lem
      wceq negeq anim2i adantl mulneg1 eqeq2d syl5ibcom syl2anr negneg sylan9eq
      adantlr sylan9eqr expr 3impa syl3an 3coml 3expa impbid ) ADEZBDEZFZABGHAB
      IZGHUSCABAUTCJZIZUSKZURUTDEUQBLUAZVADEZVBDEUSVALUBZUQVEVAAMNZBSZVBAMNZUTS
      ZOZURVEVAPEZAPEZVKUQVAQZAQZVLVMFZVIVGIZSVHVJVAAUCZVHVQUTVIVGBTUDUEUFUIRUS
      CAUTABVBVDVCVFUQURVEVGUTSZVIBSZOZVEUQURWAVEVLUQVMURBPEZWAVNVOBQVLVMWBWAVP
      WBVSVTVPWBVSFVIVQBVRVSWBVQUTIBVGUTTBUGUJUHUKULUMUNUORUP $.
      $( [21-Mar-2011] $)
  $}

  $( An integer divides another iff its absolute value does.  (Contributed by
     Paul Chapman, 21-Mar-2011.) $)
  absdvdsb $p |- ( ( M e. ZZ /\ N e. ZZ ) ->
                   ( M || N <-> ( abs ` M ) || N ) ) $=
    ( cz wcel wa cabs cfv wceq wo cdivides wbr wb cr zre absor syl adantr breq1
    cneg bicomd wi a1i negdvdsb sylan9bb ex jaod mpd ) ACDZBCDZEZAFGZAHZUKASZHZ
    IZABJKZUKBJKZLZUHUOUIUHAMDUOANAOPQUJULURUNULURUAUJULUQUPUKABJRTUBUJUNURUJUP
    UMBJKZUNUQABUCUNUQUSUKUMBJRTUDUEUFUG $.
    $( [21-Mar-2011] $)

  $( An integer divides another iff it divides its absolute value.
     (Contributed by Paul Chapman, 21-Mar-2011.) $)
  dvdsabsb $p |- ( ( M e. ZZ /\ N e. ZZ ) ->
                   ( M || N <-> M || ( abs ` N ) ) ) $=
    ( cz wcel wa cabs cfv wceq wo cdivides wbr wb cr zre absor syl adantl breq2
    cneg bicomd wi a1i dvdsnegb sylan9bb ex jaod mpd ) ACDZBCDZEZBFGZBHZUKBSZHZ
    IZABJKZAUKJKZLZUIUOUHUIBMDUOBNBOPQUJULURUNULURUAUJULUQUPUKBAJRTUBUJUNURUJUP
    AUMJKZUNUQABUCUNUQUSUKUMAJRTUDUEUFUG $.
    $( [21-Mar-2011] $)

  ${
    $d N n $.
    $( Only 0 is divisible by 0 .  (Contributed by Paul Chapman,
       21-Mar-2011.) $)
    0dvds $p |- ( N e. ZZ -> ( 0 || N <-> N = 0 ) ) $=
      ( vn cz wcel cc0 cdivides wbr wceq cv cmul co wrex wb 0z divides mpan zcn
      cc mul01 syl eqtr2 sylan2 ancoms syl6bi dvds0 ax-mp breq2 mpbiri impbid1
      rexlimiva ) ACDZEAFGZAEHZUKULBIZEJKZAHZBCLZUMECDZUKULUQMNBEAOPUPUMBCUPUNC
      DZUMUSUPUOEHZUMUSUNRDUTUNQUNSTUOAEUAUBUCUJUDUMULEEFGZURVANEUEUFAEEFUGUHUI
      $.
      $( [21-Mar-2011] $)
  $}

  $( An integer divides a multiple of itself.  (Contributed by Paul Chapman,
     21-Mar-2011.) $)
  dvdsmul1 $p |- ( ( M e. ZZ /\ N e. ZZ ) -> M || ( M x. N ) ) $=
    ( cz wcel wa cmul co wceq cdivides wbr cc mulcom syl2anr wi zmulcl dvds0lem
    zcn w3a ex 3com12 mpd3an3 mpd ) ACDZBCDZEBAFGABFGZHZAUEIJZUDBKDAKDUFUCBQAQB
