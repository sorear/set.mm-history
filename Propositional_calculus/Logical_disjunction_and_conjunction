    $([24-Aug-95]$)

  $( A theorem similar to the standard definition of the biconditional. $)
  bi    $p |- ( ( ph <-> ps ) <-> ( ( ph -> ps ) /\ ( ps -> ph ) ) ) $=
    ( wb wi wn wa bii df-an bitr4 ) ABCABDZBADZEDEJKFABGJKHI $.
    $([5-Aug-93]$)

  ${
    impbid.1 $e |- ( ph -> ( ps -> ch ) ) $.
    impbid.2 $e |- ( ph -> ( ch -> ps ) ) $.
    $( Deduce an equivalence from two implications. $)
    impbid   $p |- ( ph -> ( ps <-> ch ) ) $=
      ( wi wa wb jca bi sylibr ) ABCFZCBFZGBCHALMDEIBCJK $.
      $([5-Aug-93]$)
  $}

  $( Commutative law for equivalence.  Theorem *4.21 of Whitehead and
     Russell. $)
  pm4.21 $p |- ( ( ph <-> ps ) <-> ( ps <-> ph ) ) $=
    ( wi wa wb ancom bi 3bitr4 ) ABCZBACZDJIDABEBAEIJFABGBAGH $.
    $([5-Aug-93]$)

  ${
    bicomd.1 $e |- ( ph -> ( ps <-> ch ) ) $.
    $( Commute two sides of a biconditional in a deduction. $)
    bicomd   $p |- ( ph -> ( ch <-> ps ) ) $=
      ( wb pm4.21 sylib ) ABCECBEDBCFG $.
      $([5-Aug-93]$)
  $}

  $( Contraposition.  Theorem *4.11 of Whitehead and Russell. $)
  pm4.11  $p |- ( ( ph <-> ps ) <-> ( -. ph <-> -. ps ) ) $=
    ( wb wn wi wa pm4.1 bi2an bi 3bitr4 pm4.21 bitr ) ABCZBDZADZCZONCABEZBAEZFN
    OEZONEZFMPQSRTABGBAGHABINOIJNOKL $.
    $([21-May-94]$)

  ${
    bicon4.1 $e |- ( -. ph <-> -. ps ) $.
