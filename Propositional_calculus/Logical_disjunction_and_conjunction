    $( [26-Jun-2011] $) $( [18-Oct-1995] $)

  $( A rearrangement of disjuncts. $)
  or23OLD $p |- ( ( ( ph \/ ps ) \/ ch ) <-> ( ( ph \/ ch ) \/ ps ) ) $=
