    CDHBCDHIJ $.
    $([12-Aug-94]$)

  $( Double distributive law for conjunction. $)
  anddi $p |- ( ( ( ph \/ ps ) /\ ( ch \/ th ) ) <->
              ( ( ( ph /\ ch ) \/ ( ph /\ th ) ) \/
              ( ( ps /\ ch ) \/ ( ps /\ th ) ) ) ) $=
    ( wo wa andir andi bi2or bitr ) ABECDEZFAKFZBKFZEACFADFEZBCFBDFEZEABKGLNMOA
    CDHBCDHIJ $.
    $([12-Aug-94]$)

  $( Prove formula-building rules for the biconditional connective. $)

  ${
    bibi.a   $e |- ( ph <-> ps ) $.

    $( Inference adding a biconditional to the left in an equivalence. $)
    bilbi    $p |- ( ( ch <-> ph ) <-> ( ch <-> ps ) ) $=
      ( wb wi wa bi birim bilan bilim biran bitr4 3bitr ) CAECAFZACFZGOBCFZGZCB
      EZCAHPQOABCDIJRCBFZQGSOTQABCDKLCBHMN $.
      $([5-Aug-93]$)

    $( Inference adding a biconditional to the right in an equivalence. $)
    birbi    $p |- ( ( ph <-> ch ) <-> ( ps <-> ch ) ) $=
      ( wb pm4.21 bilbi 3bitr ) ACECAECBEBCEACFABCDGCBFH $.
      $([5-Aug-93]$)

  $}

  ${
    bi2bi.1   $e |- ( ph <-> ps ) $.
    bi2bi.2   $e |- ( ch <-> th ) $.
    $( The equivalence of two equivalences. $)
    bi2bi     $p |- ( ( ph <-> ch ) <-> ( ps <-> th ) ) $=
      ( wb bilbi birbi bitr ) ACGADGBDGCDAFHABDEIJ $.
      $([5-Aug-93]$)
  $}

  ${
    bid.1    $e |- ( ph -> ( ps <-> ch ) ) $.

    $( Deduction negating both sides of a logical equivalence. $)
    binegd   $p |- ( ph -> ( -. ps <-> -. ch ) ) $=
      ( wb wn pm4.11 sylib ) ABCEBFCFEDBCGH $.
      $([21-May-94]$)

    $( Deduction adding an antecedent to both sides of a logical
       equivalence. $)
    bilimd  $p |- ( ph -> ( ( th -> ps ) <-> ( th -> ch ) ) ) $=
      ( wi pm5.74i bilim bi2.04 3bitr4 pm5.74ri ) ADBFZDCFZDABFZFDACFZFALFAMFNO
      DABCEGHADBIADCIJK $.
      $([5-Aug-93]$)

    $( Deduction adding a consequent to both sides of a logical equivalence. $)
    birimd  $p |- ( ph -> ( ( ps -> th ) <-> ( ch -> th ) ) ) $=
      ( wn wi binegd bilimd pm4.1 3bitr4g ) ADFZBFZGLCFZGBDGCDGAMNLABCEHIBDJCDJ
      K $.
      $([5-Aug-93]$)

    $( Deduction adding a left disjunct to both sides of a logical
       equivalence. $)
    bilord   $p |- ( ph -> ( ( th \/ ps ) <-> ( th \/ ch ) ) ) $=
      ( wn wi wo bilimd df-or 3bitr4g ) ADFZBGLCGDBHDCHABCLEIDBJDCJK $.
      $([5-Aug-93]$)

    $( Deduction adding a right disjunct to both sides of a logical
       equivalence. $)
    birord   $p |- ( ph -> ( ( ps \/ th ) <-> ( ch \/ th ) ) ) $=
      ( wo bilord orcom 3bitr4g ) ADBFDCFBDFCDFABCDEGBDHCDHI $.
      $([5-Aug-93]$)

    $( Deduction adding a left conjunct to both sides of a logical
       equivalence. $)
    biland   $p |- ( ph -> ( ( th /\ ps ) <-> ( th /\ ch ) ) ) $=
      ( wn wi wa binegd bilimd df-an 3bitr4g ) ADBFZGZFDCFZGZFDBHDCHANPAMODABCE
      IJIDBKDCKL $.
      $([5-Aug-93]$)

    $( Deduction adding a right conjunct to both sides of a logical
       equivalence. $)
    birand   $p |- ( ph -> ( ( ps /\ th ) <-> ( ch /\ th ) ) ) $=
      ( wa biland ancom 3bitr4g ) ADBFDCFBDFCDFABCDEGBDHCDHI $.
      $([5-Aug-93]$)

    $( Deduction adding a biconditional to the left in an equivalence. $)
    bilbid   $p |- ( ph -> ( ( th <-> ps ) <-> ( th <-> ch ) ) ) $=
      ( wi wa wb bilimd birand birimd biland bitrd bi 3bitr4g ) ADBFZBDFZGZDCFZ
      CDFZGZDBHDCHARSQGUAAPSQABCDEIJAQTSABCDEKLMDBNDCNO $.
      $([5-Aug-93]$)

    $( Deduction adding a biconditional to the right in an equivalence. $)
    birbid   $p |- ( ph -> ( ( ps <-> th ) <-> ( ch <-> th ) ) ) $=
      ( wb bilbid pm4.21 3bitr4g ) ADBFDCFBDFCDFABCDEGBDHCDHI $.
      $([5-Aug-93]$)

  $}

  ${
    bi2d.1   $e |- ( ph -> ( ps <-> ch ) ) $.
    bi2d.2   $e |- ( ph -> ( th <-> ta ) ) $.

    $( Deduction joining two equivalences to form equivalence of
       implications. $)
    bi2imd   $p |- ( ph -> ( ( ps -> th ) <-> ( ch -> ta ) ) ) $=
      ( wi birimd bilimd bitrd ) ABDHCDHCEHABCDFIADECGJK $.
      $([5-Aug-93]$)

    $( Deduction joining two equivalences to form equivalence of
       disjunctions. $)
    bi2ord     $p |- ( ph -> ( ( ps \/ th ) <-> ( ch \/ ta ) ) ) $=
      ( wo birord bilord bitrd ) ABDHCDHCEHABCDFIADECGJK $.
      $([5-Aug-93]$)

    $( Deduction joining two equivalences to form equivalence of
       conjunctions. $)
    bi2and     $p |- ( ph -> ( ( ps /\ th ) <-> ( ch /\ ta ) ) ) $=
      ( wa birand biland bitrd ) ABDHCDHCEHABCDFIADECGJK $.
      $([5-Aug-93]$)

    $( Deduction joining two equivalences to form equivalence of
       biconditionals. $)
    bi2bid   $p |- ( ph -> ( ( ps <-> th ) <-> ( ch <-> ta ) ) ) $=
      ( wb birbid bilbid bitrd ) ABDHCDHCEHABCDFIADECGJK $.
      $([5-Aug-93]$)

  $}

  ${
    $v et $. $( Greek eta $)
    bi2an9.wet $f wff et  $.
    bi2an9.1 $e |- ( ph -> ( ps <-> ch ) ) $.
    bi2an9.2 $e |- ( th -> ( ta <-> et ) ) $.

    $( Deduction joining two equivalences to form equivalence of
       conjunctions. $)
    bi2anan9 $p |- ( ( ph /\ th ) -> ( ( ps /\ ta ) <-> ( ch /\ et ) ) ) $=
      ( wa birand biland sylan9bb ) ABEICEIDCFIABCEGJDEFCHKL $.
      $([31-Jul-95]$)

    $( Deduction joining two equivalences to form equivalence of
       conjunctions. $)
    bi2anan9r $p |- ( ( th /\ ph ) -> ( ( ps /\ ta ) <-> ( ch /\ et ) ) ) $=
      ( wa wb bi2anan9 ancoms ) ADBEICFIJABCDEFGHKL $.
      $([19-Feb-96]$)

  $}

  $( Implication in terms of biconditional and conjunction.  Theorem *4.71 of
     Whitehead and Russell. $)
  pm4.71 $p |- ( ( ph -> ps ) <-> ( ph <-> ( ph /\ ps ) ) ) $=
    ( wi wa wb ancl pm3.26 a1i impbid bi1 pm3.27 syl6 impbi ) ABCZAABDZEZNAOABF
    OACNABGHIPAOBAOJABKLM $.
    $([5-Aug-93]$)

  $( Implication in terms of biconditional and conjunction.  Same as Theorem
     *4.71 of Whitehead and Russell with conjuncts swapped. $)
  pm4.71r $p |- ( ( ph -> ps ) <-> ( ph <-> ( ps /\ ph ) ) ) $=
    ( wi wa wb pm4.71 ancom bilbi bitr ) ABCAABDZEABADZEABFJKAABGHI $.
    $([25-Jul-99]$)

  $( Implication in terms of biconditional and disjunction.  Theorem *4.72 of
     Whitehead and Russell. $)
  pm4.72 $p |- ( ( ph -> ps ) <-> ( ps <-> ( ph \/ ps ) ) ) $=
    ( wi wo wb olc a1i wn pm2.61 df-or syl5ib impbid bi2 orc syl5 impbi ) ABCZB
    ABDZEZQBRBRCQBAFGQAHBCBRABIABJKLSRBABRMABNOP $.
    $([30-Aug-93]$)

  $( Introduction of antecedent as conjunct. $)
  iba $p |- ( ph -> ( ps <-> ( ps /\ ph ) ) ) $=
    ( wi wa wb ax-1 pm4.71 sylib ) ABACBBADEABFBAGH $.
    $([30-Mar-94]$)

  $( Introduction of antecedent as conjunct.
     From:  "J.P.  Ibar" <JP_IBAR@compuserve.com> I was looking at your proof
     of theorem Ibar.  What is it all about?  I am myself a physicist named
     Ibar and was educated at MIT. Who is this Ibar doing math out there?
     Can you put me on his tracks?  Thanks J.P.  Ibar
  $)
  ibar $p |- ( ph -> ( ps <-> ( ph /\ ps ) ) ) $=
    ( wa iba ancom syl6bb ) ABBACABCABDBAEF $.
    $([5-Dec-95]$)

  $( Distribution of implication over biconditional.  Theorem *5.32 of
     Whitehead and Russell. $)
  pm5.32 $p |- ( ( ph -> ( ps <-> ch ) ) <->
               ( ( ph /\ ps ) <-> ( ph /\ ch ) ) ) $=
    ( wb wi wn wa pm4.11 bilim pm5.74 3bitr df-an bi2bi bitr4 ) ABCDZEZABFZEZFZ
    ACFZEZFZDZABGZACGZDPAQTDZERUADUCOUFABCHIAQTJRUAHKUDSUEUBABLACLMN $.
    $([1-Aug-94]$)

  ${
    pm5.32i.1 $e |- ( ph -> ( ps <-> ch ) ) $.
