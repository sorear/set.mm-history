    pm5.32i   $p |- ( ( ph /\ ps ) <-> ( ph /\ ch ) ) $=
      ( wb wi wa pm5.32 mpbi ) ABCEFABGACGEABCHDI $.
      $([1-Aug-94]$)
  $}

