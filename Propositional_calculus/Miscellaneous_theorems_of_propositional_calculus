  $( Negation inferred from embedded conjunct. $)
  pclem6 $p |- ( ( ph <-> ( ps /\ -. ph ) ) -> -. ps ) $=
    ( wn wa wb bi1 simpr syl6 pm2.01d wi bi2 exp3a com23 con3 syli mpd ) ABACZD
