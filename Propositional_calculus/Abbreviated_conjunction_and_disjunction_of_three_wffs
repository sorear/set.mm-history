      syl3an2b $p |- ( ( ph /\ ta /\ ch ) -> th ) $=
        ( biimp syl3an2 ) ABCDEFEBGHI $.
        $( [22-Aug-1995] $)
    $}

    ${
      syl3an3b.2 $e |- ( ta <-> ch ) $.
