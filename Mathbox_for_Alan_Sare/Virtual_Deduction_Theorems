  ${
    a40.1 $e |- [. ph ->.. . . . ps , ch . , th . , ta . ]. $.
    a40.2 $e |- ( ps -> et ) $.
    $( Special case of ~ a1h . $)
    a40 $p |- [. ph ->.. . . . . ps , ch . , th . , ta . , et . ]. $=
