    e220 $p |- (. ph , ps |-. et ). $=
      ( vd02 e222 ) ABCDEFGHEABIKJL $.
      $( [24-Jun-2011] $)
  $}

  ${
    ee220.1 $e |- ( ph -> ( ps -> ch ) ) $.
