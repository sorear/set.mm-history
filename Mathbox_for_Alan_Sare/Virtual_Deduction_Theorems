      ( biimpri a40 ) ABCDEFGFBHIJ $.
      $( [19-Apr-2011] $)
  $}

  ${
    a41.1 $e |- [. ph ->.. . . . ps , ch . , th . , ta . ]. $.
    a41.2 $e |- ( ch -> et ) $.
    $( Special case of ~ a1h . $)
    a41 $p |- [. ph ->.. . . . . ps , ch . , th . , ta . , et . ]. $=
