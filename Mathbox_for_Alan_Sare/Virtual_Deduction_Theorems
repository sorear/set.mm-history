  ${
    a41bi.1 $e |- [. ph ->.. . . . ps , ch . , th . , ta . ]. $.
    a41bi.2 $e |- ( ch <-> et ) $.
    $( Biconditional form of ~ a41 . $)
    a41bi $p |- [. ph ->.. . . . . ps , ch . , th . , ta . , et . ]. $=
      ( biimpi a41 ) ABCDEFGCFHIJ $.
      $( [19-Apr-2011] $)
  $}

  ${
    a41bir.1 $e |- [. ph ->.. . . . ps , ch . , th . , ta . ]. $.
