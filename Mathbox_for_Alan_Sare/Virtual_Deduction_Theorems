    e123.3 $e |- (. ph ->... (. ch ->... [. ta ->.. et ]. ). ). $.
    e123.4 $e |- ( ps -> ( th -> ( et -> ze ) ) ) $.
    $( A virtual deduction elimination rule. $)
    e123 $p |- (. ph ->... (. ch ->... [. ta ->.. ze ]. ). ). $=
