      $( [21-Apr-2011] $)
  $}

  ${
    $( Definition of meta-implication for virtual hypotheses. $)
    df-wihmeta $a |- ( (. ph ->... ps ). <-> ( ph -> ps ) ) $.
      $( [21-Apr-2011] $)
  $}

  ${
