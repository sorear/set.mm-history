      $( [13-Jun-2011] $)
  $}

  ${
    e13an.1 $e |- [. ph ->.. ps ]. $.
    e13an.2 $e |- (. ph ->... (. ch ->... [. th ->.. ta ]. ). ). $.
    e13an.3 $e |- ( ( ps /\ ta ) -> et ) $.
    $( A virtual deduction elimination rule. $)
    e13an $p |- (. ph ->... (. ch ->... [. th ->.. et ]. ). ). $=
      ( ex e13 ) ABCDEFGHBEFIJK $.
      $( [24-Jun-2011] $)
  $}

