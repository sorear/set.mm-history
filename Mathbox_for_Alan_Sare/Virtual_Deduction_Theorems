$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
               Virtual Deduction Theorems
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ->.. $. $(  Symbol for meta-implication
      [10-Nov-2010] $)

  $c [. $. $( Left parenthesis for meta-implication.
      [6-Apr-2011] $)

  $c ]. $. $( Right parenthesis for meta-implication.
      [6-Apr-2011] $)

  $c . $. $( Right and left meta-parenthesis for conjunction.
      [10-Nov-2010] $)

  $c ->... $. $(  Symbol for meta-implication for virtual hypotheses.
      [21-Apr-2011] $)

  $c (. $. $( Left parenthesis for meta-implication for virtual hypotheses.
      [21-Apr-2011] $)

  $c ). $. $( Right parenthesis for meta-implication for virtual hypotheses.
      [21-Apr-2011] $)

  ${
    $( Syntax for virtual deduction.  The arrow is the virtual deduction
       symbol.  We also call it the symbol for "meta-implication" because it is
       a "meta-connective" connecting "individual" wffs.  The virtual deduction
       symbol is a second symbol denoting implication.  Our virtual deduction
       employs a variant of the natural deduction calculi for Intuitionistic
       and Classical logic due to Gentzen and Prawitz.  We call the "deduction"
       "virtual" because, although proofs using virtual deduction are
       constructed as if rules of natural deduction were being applied, each
       virtual deduction is actually an implication.  Using virtual deduction,
       Hilbert-style axiomatic proofs may be directly constructed using
       intuitionistic logic.  The deductions are virtual just as classes in set
       theory may be interpreted as virtual (merely defined terms).  Similarly,
       the individual wffs in the antecedent of a virtual deduction are
       "virtual hypotheses" and the individual wffs in the consequent of a
       virtual deduction are "virtual conclusions". $)
    wimeta $a wff [. ph ->.. ps ]. $.
      $( [23-Mar-2011] $)
  $}

  ${
    $( Definition of meta-implication. $)
    df-wimeta $a |- ( [. ph ->.. ps ]. <-> ( ph -> ps ) ) $.
      $( [10-Nov-2010] $)
  $}

  ${
    dfwimetai.1 $e |- [. ph ->.. ps ]. $.
    $( Inference form of ~ df-wimeta . $)
    dfwimetai $p |- ( ph -> ps ) $=
      ( wimeta wi df-wimeta mpbi ) ABDABECABFG $.
      $( [12-Nov-2010] $)
  $}

  ${
    dfwimetair.1 $e |- ( ph -> ps ) $.
    $( Inference form of ~ df-wimeta with the meta-implication as the
       assertion. $)
    dfwimetair $p |- [. ph ->.. ps ]. $=
      ( wimeta wi df-wimeta mpbir ) ABDABECABFG $.
      $( [12-Nov-2010] $)
  $}

  ${
    $( Syntax for meta-conjunction. $)
    wameta $a wff . ph , ps . $.
      $( [10-Nov-2010] $)
  $}

  ${
    $( Definition of meta-conjunction. $)
    df-wameta $a |- ( . ph , ps . <-> ( ph /\ ps ) ) $.
      $( [10-Nov-2010] $)
  $}

  ${
    dfwiwametai.1 $e |- [. . ph , ps . ->.. ch ]. $.
    $( Definition of the meta-form of an implication having an antecedent of
       two wffs conjoined. $)
    dfwiwametai $p |- ( ( ph /\ ps ) -> ch ) $=
      ( wa wameta df-wameta dfwimetai sylbir ) ABEABFZCABGJCDHI $.
      $( [26-Nov-2010] $)
  $}

  ${
    sylbimt.1 $e |- ( ph <-> ps ) $.
    sylbimt.2 $e |- ( ps -> ch ) $.
    $( ~ sylbi with a meta-implication assertion. $)
    sylbimt $p |- [. ph ->.. ch ]. $=
      ( sylbi dfwimetair ) ACABCDEFG $.
      $( [12-Nov-2010] $)
  $}

  ${
    sylibrmt.1 $e |- ( ph -> ps ) $.
    sylibrmt.2 $e |- ( ch <-> ps ) $.
    $( ~ sylibr with a meta-implication assertion. $)
    sylibrmt $p |- [. ph ->.. ch ]. $=
      ( sylibr dfwimetair ) ACABCDEFG $.
      $( [12-Nov-2010] $)
  $}

  ${
    adantrmt.1 $e |- ( ph -> ps ) $.
    $( ~ adantr with a meta-implication assertion. $)
    adantrmt $p |- [. . ph , ch . ->.. ps ]. $=
      ( wameta wa df-wameta adantr sylbimt ) ACEACFBACGABCDHI $.
      $( [12-Nov-2010] $)
  $}

  ${
    adantrmt2.1 $e |- [. ph ->.. ps ]. $.
    $( Virtual deduction form of ~ adantr . $)
    adantrmt2 $p |- [. . ph , ch . ->.. ps ]. $=
      ( dfwimetai adantrmt ) ABCABDEF $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( Virtual deduction identity rule which is ~ id with meta-connective
       symbols. $)
    idn1 $p |- [. ph ->.. ph ]. $=
      ( id dfwimetair ) AAABC $.
      $( [24-Jun-2011] $)
  $}

  ${
    $( A 2-element right-nested conjunction meta-implies the 0th element. $)
    s20 $p |- [. . ph , ps . ->.. ph ]. $=
      ( idn1 adantrmt2 ) AABACD $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 2-element right-nested conjunction meta-implies the 1st element. $)
    s21 $p |- [. . ph , ps . ->.. ps ]. $=
      ( wameta wa df-wameta id adantl sylbi dfwimetair ) ABCZBJABDBABEBBABFGHI
      $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 3-element right-nested conjunction meta-implies the 0th element. $)
    s30 $p |- [. . . ph , ps . , ch . ->.. ph ]. $=
      ( wameta s20 adantrmt2 ) ABDACABEF $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 3-element right-nested conjunction meta-implies the 1st element. $)
    s31 $p |- [. . . ph , ps . , ch . ->.. ps ]. $=
      ( wameta s21 adantrmt2 ) ABDBCABEF $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 4-element right-nested conjunction meta-implies the 0th element. $)
    s40 $p |- [. . . . ph , ps . , ch . , th . ->.. ph ]. $=
      ( wameta s30 adantrmt2 ) ABECEADABCFG $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 4-element right-nested conjunction meta-implies the 1st element. $)
    s41 $p |- [. . . . ph , ps . , ch . , th . ->.. ps ]. $=
      ( wameta s31 adantrmt2 ) ABECEBDABCFG $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 5-element right-nested conjunction meta-implies the 0th element. $)
    s50 $p |- [. . . . . ph , ps . , ch . , th . , ta . ->.. ph ]. $=
      ( wameta s40 adantrmt2 ) ABFCFDFAEABCDGH $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 5-element right-nested conjunction meta-implies the 1st element. $)
    s51 $p |- [. . . . . ph , ps . , ch . , th . , ta . ->.. ps ]. $=
      ( wameta s41 adantrmt2 ) ABFCFDFBEABCDGH $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 6-element right-nested conjunction meta-implies the 0th element. $)
    s60 $p |- [. . . . . . ph , ps . , ch . , th . , ta . , et . ->.. ph ]. $=
      ( wameta s50 adantrmt2 ) ABGCGDGEGAFABCDEHI $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 6-element right-nested conjunction meta-implies the 1st element. $)
    s61 $p |- [. . . . . . ph , ps . , ch . , th . , ta . , et . ->.. ps ]. $=
      ( wameta s51 adantrmt2 ) ABGCGDGEGBFABCDEHI $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 7-element right-nested conjunction meta-implies the 0th element. $)
    s70 $p |- [. . . . . . . ph , ps . , ch . , th . , ta . , et . , ze .
               ->.. ph ]. $=
      ( wameta s60 adantrmt2 ) ABHCHDHEHFHAGABCDEFIJ $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 7-element right-nested conjunction meta-implies the 1st element. $)
    s71 $p |- [. . . . . . . ph , ps . , ch . , th . , ta . , et . , ze .
               ->.. ps ]. $=
      ( wameta s61 adantrmt2 ) ABHCHDHEHFHBGABCDEFIJ $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 8-element right-nested conjunction meta-implies the 0th element. $)
    s80 $p |- [. . . . . . . . ph , ps . , ch . , th . , ta . , et . , ze . ,
               si . ->.. ph ]. $=
      ( wameta s70 adantrmt2 ) ABICIDIEIFIGIAHABCDEFGJK $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 8-element right-nested conjunction meta-implies the 1st element. $)
    s81 $p |- [. . . . . . . . ph , ps . , ch . , th . , ta . , et . , ze . ,
                si . ->.. ps ]. $=
      ( wameta s71 adantrmt2 ) ABICIDIEIFIGIBHABCDEFGJK $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 9-element right-nested conjunction meta-implies the 0th element. $)
    s90 $p |- [. . . . . . . . . ph , ps . , ch . , th . , ta . , et . , ze . ,
                si . , rh . ->.. ph ]. $=
      ( wameta s80 adantrmt2 ) ABJCJDJEJFJGJHJAIABCDEFGHKL $.
      $( [14-Mar-2011] $)
  $}

  ${
    $( A 9-element right-nested conjunction meta-implies the 1st element. $)
    s91 $p |- [. . . . . . . . . ph , ps . , ch . , th . , ta . , et . , ze . ,
               si . , rh . ->.. ps ]. $=
      ( wameta s81 adantrmt2 ) ABJCJDJEJFJGJHJBIABCDEFGHKL $.
      $( [14-Mar-2011] $)
  $}

  ${
    $v la $.
    st00.wla $f wff la $.
    $( A 10-element right-nested conjunction meta-implies the 0th element. $)
    st00 $p |- [. . . . . . . . . . ph , ps . , ch . , th . , ta . , et . ,
                ze . , si . , rh . , la . ->.. ph ]. $=
      ( wameta s90 adantrmt2 ) ABKCKDKEKFKGKHKIKAJABCDEFGHILM $.
      $( [14-Mar-2011] $)
  $}

  ${

    $v la $.
    st01.wla $f wff la $.
    $( A 10-element right-nested conjunction meta-implies the 1st element. $)
    st01 $p |- [. . . . . . . . . . ph , ps . , ch . , th . , ta . , et . ,
                ze . , si . , rh . , la . ->.. ps ]. $=
      ( wameta s91 adantrmt2 ) ABKCKDKEKFKGKHKIKBJABCDEFGHILM $.
      $( [14-Mar-2011] $)
  $}

  ${
    ah.1 $e |- [. ph ->.. ps ]. $.
    $( Add a virtual hypothesis to a virtual deduction.  Duplicate this virtual
       hypothesis as the last virtual conclusion.  "ah" is an abbreviation for
       "add (virtual) hypothesis". $)
    ah $p |- [. . ph , ch . ->.. . ps , ch . ]. $=
      ( wameta wa dfwimetai anim1i df-wameta 3imtr4i dfwimetair ) ACEZBCEZACFBC
      FLMABCABDGHACIBCIJK $.
      $( [18-Mar-2011] $)
  $}

  ${
    aah.1 $e |- ph $.
    $( Add a virtual hypthesis to a theorem, converting the theorem into a
       virtual deduction.  Duplicate this virtual hypothesis as the last
       virtual conclusion. $)
    aah $p |- [. ps ->.. . ph , ps . ]. $=
      ( wameta wa jctl df-wameta sylibr dfwimetair ) BABDZBABEJBACFABGHI $.
      $( [18-Mar-2011] $)
  $}

  ${
    a5h.1 $e |- [. ph ->.. ps ]. $.
    a5h.2 $e |- ( ch -> ( th -> ( ta -> ( et -> ( ze -> si ) ) ) ) ) $.
    a5h.3 $e |- [. ps ->.. ch ]. $.
    a5h.4 $e |- [. ps ->.. th ]. $.
    a5h.5 $e |- [. ps ->.. ta ]. $.
    a5h.6 $e |- [. ps ->.. et ]. $.
    a5h.7 $e |- [. ps ->.. ze ]. $.
    $( Meta-conjoin to the consequent of a meta-implication a wff which is the
       end wff of a five-antecedent nested implication. $)
    a5h $p |- [. ph ->.. . ps , si . ]. $=
      ( wa wameta dfwimetai wi syl syl5 pm2.43i jca df-wameta sylibrmt ) ABHPBH
      QABHABIRZABHUFBHBGHBBGHSZBFUGBBFUGSZBEUHBBEUHSZBDUIBBCDUISBCKRJTBDLRUAUBB
      EMRUAUBBFNRUAUBBGORUAUBTUCBHUDUE $.
      $( [17-Mar-2011] $)
  $}

  ${
    a4h.1 $e |- [. ph ->.. ps ]. $.
    a4h.2 $e |- ( ch -> ( th -> ( ta -> ( et -> ze ) ) ) ) $.
    a4h.3 $e |- [. ps ->.. ch ]. $.
    a4h.4 $e |- [. ps ->.. th ]. $.
    a4h.5 $e |- [. ps ->.. ta ]. $.
    a4h.6 $e |- [. ps ->.. et ]. $.
    $( Meta-conjoin to the consequent of a meta-implication a wff which is the
       end wff of a four-antecedent nested implication. $)
    a4h $p |- [. ph ->.. . ps , ze . ]. $=
      ( wi a1i a5h ) ABCCDEFGHCDEFGNNNNCIOJJKLMP $.
      $( [17-Mar-2011] $)
  $}

  ${
    a3h.1 $e |- [. ph ->.. ps ]. $.
    a3h.2 $e |- ( ch -> ( th -> ( ta -> et ) ) ) $.
    a3h.3 $e |- [. ps ->.. ch ]. $.
    a3h.4 $e |- [. ps ->.. th ]. $.
    a3h.5 $e |- [. ps ->.. ta ]. $.
    $( Meta-conjoin to the consequent of a meta-implication a wff which is the
       end wff of a three-antecedent nested implication. $)
    a3h $p |- [. ph ->.. . ps , et . ]. $=
      ( wi a1i a4h ) ABCCDEFGCDEFLLLCHMIIJKN $.
      $( [17-Mar-2011] $)
  $}

  ${
    a2h.1 $e |- [. ph ->.. ps ]. $.
    a2h.2 $e |- ( ch -> ( th -> ta ) ) $.
    a2h.3 $e |- [. ps ->.. ch ]. $.
    a2h.4 $e |- [. ps ->.. th ]. $.
    $( Meta-conjoin to the consequent of a meta-implication a wff which is the
       end wff of a two-antecedent nested implication. $)
    a2h $p |- [. ph ->.. . ps , ta . ]. $=
      ( wi a1i a3h ) ABCCDEFCDEJJCGKHHIL $.
      $( [17-Mar-2011] $)
  $}

  ${
    a1h.1 $e |- [. ph ->.. ps ]. $.
    a1h.2 $e |- ( ch -> th ) $.
    a1h.3 $e |- [. ps ->.. ch ]. $.
    $( Meta-conjoin to the consequent of a meta-implication a wff which is the
       consequent of an implication. $)
    a1h $p |- [. ph ->.. . ps , th . ]. $=
      ( wi a1i a2h ) ABCCDECDHCFIGGJ $.
      $( [17-Mar-2011] $)
  $}

  ${
    a0h.1 $e |- [. ph ->.. ps ]. $.
    a0h.2 $e |- ch $.
    $( Meta-conjoin to the consequent of a meta-implication a theorem. $)
    a0h $p |- [. ph ->.. . ps , ch . ]. $=
      ( id a1i dfwimetair a1h ) ABCCDCFBCCBEGHI $.
      $( [8-Apr-2011] $)
  $}

  ${
    a10.1 $e |- [. ph ->.. ps ]. $.
    a10.2 $e |- ( ps -> ch ) $.
    $( Special case of ~ a1h .  The "1" in "a10" indicates that the original
       virtual deduction has 1 virtual conclusion.  The "0" in "a10" indicates
       that the antecedent of the implication is the 0th virtual conclusion of
       the original virtual deduction. $)
    a10 $p |- [. ph ->.. . ps , ch . ]. $=
      ( idn1 a1h ) ABBCDEBFG $.
      $( [18-Mar-2011] $)
  $}

  ${
    a10bi.1 $e |- [. ph ->.. ps ]. $.
    a10bi.2 $e |- ( ps <-> ch ) $.
    $( Biconditional form of ~ a10 . $)
    a10bi $p |- [. ph ->.. . ps , ch . ]. $=
      ( biimpi a10 ) ABCDBCEFG $.
      $( [19-Mar-2011] $)
  $}

  ${
    a10bir.1 $e |- [. ph ->.. ps ]. $.
    a10bir.2 $e |- ( ch <-> ps ) $.
    $( Right biconditional form of ~ a10 . $)
    a10bir $p |- [. ph ->.. . ps , ch . ]. $=
      ( biimpri a10 ) ABCDCBEFG $.
      $( [19-Mar-2011] $)
  $}

  ${
    a20.1 $e |- [. ph ->.. . ps , ch . ]. $.
    a20.2 $e |- ( ps -> th ) $.
    $( Special case of ~ a1h .  The "2" in "a20" indicates that the original
       virtual deduction has 2 virtual conclusions.  The "0" in "a20" indicates
       that the antecedent of the implication is the 0th virtual conclusion of
       the original virtual deduction. $)
    a20 $p |- [. ph ->.. . . ps , ch . , th . ]. $=
      ( wameta s20 a1h ) ABCGBDEFBCHI $.
      $( [18-Mar-2011] $)
  $}

  ${
    a20bi.1 $e |- [. ph ->.. . ps , ch . ]. $.
