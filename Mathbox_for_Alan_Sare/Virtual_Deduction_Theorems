  ${
    e000.1 $e |- ph $.
    e000.2 $e |- ps $.
    e000.3 $e |- ch $.
    e000.4 $e |- ( ph -> ( ps -> ( ch -> th ) ) ) $.
    $( A virtual deduction elimination rule. $)
    e000 $p |- th $=
      ( wi mp2 ax-mp ) CDGABCDIEFHJK $.
      $( [14-Jun-2011] $)
  $}

  ${
    e00.1 $e |- ph $.
    e00.2 $e |- ps $.
    e00.3 $e |- ( ph -> ( ps -> ch ) ) $.
    $( A virtual deduction elimination rule. $)
    e00 $p |- ch $=
      ( mp2 ) ABCDEFG $.
      $( [14-Jun-2011] $)
  $}

  ${
    e00an.1 $e |- ph $.
    e00an.2 $e |- ps $.
    e00an.3 $e |- ( ( ph /\ ps ) -> ch ) $.
    $( Conjunction form of ~ e00 . $)
    e00an $p |- ch $=
      ( ex e00 ) ABCDEABCFGH $.
      $( [15-Jun-2011] $)
  $}

  ${
    e0_.1 $e |- ph $.
    e0_.2 $e |- ( ph -> ps ) $.
    $( A virtual deduction elimination rule. $)
    e0_ $p |- ps $=
      ( ax-mp ) ABCDE $.
      $( [14-Jun-2011] $)
  $}

  ${
    e0bi.1 $e |- ph $.
    e0bi.2 $e |- ( ph <-> ps ) $.
    $( Biconditional form of ~ e0_ . $)
    e0bi $p |- ps $=
      ( biimpi e0_ ) ABCABDEF $.
      $( [15-Jun-2011] $)
  $}

  ${
    e0bir.1 $e |- ph $.
    e0bir.2 $e |- ( ps <-> ph ) $.
    $( Right biconditional form of ~ e0_ . $)
    e0bir $p |- ps $=
      ( biimpri e0_ ) ABCBADEF $.
      $( [15-Jun-2011] $)
  $}

