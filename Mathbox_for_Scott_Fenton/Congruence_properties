  $( Identity law for congruence.  (Contributed by Scott Fenton,
     12-Jun-2013.) $)
  cgrid2 $p |- ( ( N e. NN /\
    ( A e. ( EE ` N ) /\ B e. ( EE ` N ) /\ C e. ( EE ` N ) ) ) ->
    ( <. A , A >. Cgr <. B , C >. -> B = C ) ) $=
    ( cn wcel cee cfv w3a wa cop ccgr wbr wceq wb simpl simpr1 simpr2 simpr3
    cgrcom syl122anc wi 3anrot axcgrid sylan2b sylbid ) DEFZADGHZFZBUHFZCUHFZIZ
    JZAAKZBCKZLMZUOUNLMZBCNZUMUGUIUIUJUKUPUQOUGULPUGUIUJUKQZUSUGUIUJUKRUGUIUJUK
    SAABCDTUAULUGUJUKUIIUQURUBUIUJUKUCBCADUDUEUF $.
    $( [16-Sep-2013] $) $( [12-Jun-2013] $)

  $( Two congruent segments are either both degenrate or both non-degenerate.
     (Contributed by Scott Fenton, 12-Jun-2013.) $)
  cgrdegen $p |- ( ( N e. NN /\ ( A e. ( EE ` N ) /\ B e. ( EE ` N ) ) /\
    ( C e. ( EE ` N ) /\ D e. ( EE ` N ) ) ) ->
      ( <. A , B >. Cgr <. C , D >. -> ( A = B <-> C = D ) ) ) $=
    ( cn wcel cee cfv wa w3a cop ccgr wbr wceq wb opeq1 breq1d biimpac wi
    simp1 simp2r simp3l simp3r cgrid2 syl13anc syl5 expdimp breq2d simp2l
    axcgrid impbid ex ) EFGZAEHIZGZBUOGZJZCUOGZDUOGZJZKZABLZCDLZMNZABOZCDOZPVBV
    EJVFVGVBVEVFVGVEVFJBBLZVDMNZVBVGVFVEVIVFVCVHVDMABBQRSVBUNUQUSUTVIVGTUNURVAU
    AZUNUPUQVAUBZUNURUSUTUCUNURUSUTUDZBCDEUEUFUGUHVBVEVGVFVEVGJVCDDLZMNZVBVFVGV
    EVNVGVDVMVCMCDDQUISVBUNUPUQUTVNVFTVJUNUPUQVAUJVKVLABDEUKUFUGUHULUM $.
    $( [16-Sep-2013] $) $( [12-Jun-2013] $)

  $( Link congruence over a pair of line segments.  (Contributed by Scott
     Fenton, 12-Jun-2013.) $)
  cgrextend $p |- ( ( N e. NN /\
    ( A e. ( EE ` N ) /\ B e. ( EE ` N ) /\ C e. ( EE ` N ) ) /\
    ( D e. ( EE ` N ) /\ E e. ( EE ` N ) /\ F e. ( EE ` N ) ) ) ->
    ( ( ( B Btwn <. A , C >. /\ E Btwn <. D , F >. ) /\
        ( <. A , B >. Cgr <. D , E >. /\
          <. B , C >. Cgr <. E , F >. ) ) ->
      <. A , C >. Cgr <. D , F >. ) ) $=
    ( cn wcel cee cfv w3a cop cbtwn wbr wa ccgr wi wceq simp1 simp22 simp31
    simp32 3jca jca simprl cgrid2 imp syl2an simprrr opeq1 breq2d biimpar
    syl11anc ex anbi1d breq1d anbi12d imbi12d imbi2d mpbiri wne simprll adantl
    simprlr simp21 cgrtriv syl111anc adantr simprrl wb cgrcomlr syl122anc
    mpbid simpl1 simpl21 simpl22 simpl23 simpl31 simpl32 simpl33 ax5seg
    syl333anc mpd simp23 simp33 mpbird exp3a com12 pm2.61ine ) GHIZAGJKZIZBWLIZ
    CWLIZLZDWLIZEWLIZFWLIZLZLZBACMZNOZEDFMZNOZPZABMZDEMZQOZBCMZEFMZQOZPZPZXBXDQ
