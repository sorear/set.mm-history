  $( Two points that satsify the conclusion of ~ axsegcon are identical.
     (Contributed by Scott Fenton, 12-Jun-2013.) $)
  segconeq $p |- ( ( N e. NN /\
   ( A e. ( EE ` N ) /\ B e. ( EE ` N ) /\ C e. ( EE ` N ) ) /\
   ( Q e. ( EE ` N ) /\ X e. ( EE ` N ) /\ Y e. ( EE ` N ) ) ) ->
   ( ( Q =/= A /\
     ( A Btwn <. Q , X >. /\ <. A , X >. Cgr <. B , C >. ) /\
     ( A Btwn <. Q , Y >. /\ <. A , Y >. Cgr <. B , C >. ) )
   -> X = Y ) ) $=
    ( cn wcel cee cfv w3a wne cop cbtwn wbr ccgr wa wceq simpr1 simpr2l 3jca
    simp1 simp31 simp21 cgrrflx syl111anc simp32 jca adantr simpr3l wb simp33
    simp22 simp23 cgrcom syl122anc anbi12d simpr3r simpr2r mpbi2and wi axcgrtr
    syl133anc mpd cgrextend mp2and ax5seg syl333anc axcgrid syl13anc ex ) EHIZA
    EJKZIZBVNIZCVNIZLZDVNIZFVNIZGVNIZLZLZDAMZADFNZOPZAFNZBCNZQPZRZADGNZOPZAGNZW
    HQPZRZLZFGSZWCWPRZFGNFFNQPZWQWRWDWFWFLZDANZXAQPZWGWGQPZRZWKWEQPZWMWGQPZRZLZ
