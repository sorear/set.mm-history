  $( Reflexivity law for congruence.  (Contributed by Scott Fenton,
     12-Jun-2013.) $)
  cgrrflx $p |- ( ( N e. NN /\ A e. ( EE ` N ) /\ B e. ( EE ` N ) ) ->
    <. A , B >. Cgr <. A , B >. ) $=
    ( cn wcel cee cfv w3a cop ccgr wbr axcgrrflx 3com23 wa wi simp1 simp3
    simp2 axcgrtr syl133anc mp2and ) CDEZACFGZEZBUCEZHZBAIABIZJKZUHUGUGJKZUBUEU
    DUHBACLMZUJUFUBUEUDUDUEUDUEUHUHNUIOUBUDUEPUBUDUEQZUBUDUERZULUKULUKBAABABCST
    UA $.
    $( [16-Sep-2013] $) $( [12-Jun-2013] $)

  $( Congruence commutes on the two sides.  Implication version.  (Contributed
     by Scott Fenton, 12-Jun-2013.) $)
  cgrcomim $p |- ( ( N e. NN /\ ( A e. ( EE ` N ) /\ B e. ( EE ` N ) ) /\
    ( C e. ( EE ` N ) /\ D e. ( EE ` N ) ) ) ->
    ( <. A , B >. Cgr <. C , D >. ->
      <. C , D >. Cgr <. A , B >. ) ) $=
    ( cn wcel cee cfv wa w3a cop ccgr wbr cgrrflx 3expb 3adant3 wi simp1
    simp2l simp2r simp3l simp3r axcgrtr syl133anc mpan2d ) EFGZAEHIZGZBUHGZJZCU
    HGZDUHGZJZKZABLZCDLZMNZUPUPMNZUQUPMNZUGUKUSUNUGUIUJUSABEOPQUOUGUIUJULUMUIUJ
    URUSJUTRUGUKUNSUGUIUJUNTZUGUIUJUNUAZUGUKULUMUBUGUKULUMUCVAVBABCDABEUDUEUF
    $.
    $( [16-Sep-2013] $) $( [12-Jun-2013] $)

  $( Congruence commutes between the two sides.  (Contributed by Scott Fenton,
     12-Jun-2013.) $)
  cgrcom $p |- ( ( N e. NN /\ ( A e. ( EE ` N ) /\ B e. ( EE ` N ) ) /\
    ( C e. ( EE ` N ) /\ D e. ( EE ` N ) ) ) ->
    ( <. A , B >. Cgr <. C , D >. <->
      <. C , D >. Cgr <. A , B >. ) ) $=
    ( cn wcel cee cfv wa w3a cop ccgr wbr cgrcomim wi 3com23 impbid ) EFGZAEHIZ
    GBTGJZCTGDTGJZKABLZCDLZMNZUDUCMNZABCDEOSUBUAUFUEPCDABEOQR $.
    $( [16-Sep-2013] $) $( [12-Jun-2013] $)

  $( Congruence commutes on the left.  (Contributed by Scott Fenton,
     12-Jun-2013.) $)
  cgrcoml $p |- ( ( N e. NN /\ ( A e. ( EE ` N ) /\ B e. ( EE ` N ) ) /\
    ( C e. ( EE ` N ) /\ D e. ( EE ` N ) ) ) ->
    ( <. A , B >. Cgr <. C , D >. <->
      <. B , A >. Cgr <. C , D >. ) ) $=
    ( cn wcel cee cfv wa w3a cop ccgr wbr simp1 simp2l simp2r axcgrrflx
    syl111anc wi simp3l simp3r axcgrtr syl133anc mpand impbid ) EFGZAEHIZGZBUHG
