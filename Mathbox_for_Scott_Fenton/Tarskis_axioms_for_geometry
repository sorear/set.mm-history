    $( Lemma for ~ ax5seg .  Rexpress another congruence sum given
       betweenness. $)
    ax5seglem2 $p |- ( ( N e. NN /\ ( A e. ( EE ` N ) /\ C e. ( EE ` N ) ) /\
       ( T e. ( 0 [,] 1 ) /\
         A. i e. ( 1 ... N ) ( B ` i ) =
          ( ( ( 1 - T ) x. ( A ` i ) ) + ( T x. ( C ` i ) ) ) ) ) ->
       sum_ j e. ( 1 ... N ) ( ( ( B ` j ) - ( C ` j ) ) ^ 2 ) =
       ( ( ( 1 - T ) ^ 2 ) x.
