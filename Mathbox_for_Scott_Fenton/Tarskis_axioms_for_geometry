    $( Lemma for ~ ax5seg .  Rexpress a one congruence sum given
       betweenness. $)
    ax5seglem1 $p |- ( ( N e. NN /\ ( A e. ( EE ` N ) /\ C e. ( EE ` N ) ) /\
       ( T e. ( 0 [,] 1 ) /\
