$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Tarski's axioms for geometry
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d N i $.  $d A i $.  $d B i $.
    $( ` A ` is as far from ` B ` as ` B ` is from ` A ` .  Tarski's axiom 1
       for Euclidean geometry.  (Contributed by Scott Fenton, 3-Jun-2013.) $)
    axcgrrflx $p |- ( ( N e. NN /\ A e. ( EE ` N ) /\ B e. ( EE ` N ) ) ->
       <. A , B >. Cgr <. B , A >. ) $=
      ( vi cee cfv wcel cop ccgr wbr cn wa c1 cfz co cv cmin c2 cexp csu wceq
      cc fveecn sqsubswap syl2an anandirs sumeq2dv wb id simpr simpl brcgr
      syl12anc mpbird 3adant1 ) ACEFZGZBUPGZABHBAHIJZCKGUQURLZUSMCNOZDPZAFZVBBF
      ZQORSOZDTVAVDVCQORSOZDTUAZUTVAVEVFDUQURVBVAGZVEVFUAZUQVHLVCUBGVDUBGVIURVH
      LAVBCUCBVBCUCVCVDUDUEUFUGUTUTURUQUSVGUHUTUIUQURUJUQURUKABBADCULUMUNUO $.
      $( [16-Sep-2013] $) $( [3-Jun-2013] $)
  $}

  ${
    $d N i $.  $d A i $.  $d B i $.  $d C i $.  $d D i $.  $d E i $.  $d F i $.
    $( Congruence is transitive.  Tarski's axiom 2 for Euclidean geometry.
       (Contributed by Scott Fenton, 3-Jun-2013.) $)
    axcgrtr $p |- ( ( N e. NN /\
       ( A e. ( EE ` N ) /\ B e. ( EE ` N ) /\ C e. ( EE ` N ) ) /\
       ( D e. ( EE ` N ) /\ E e. ( EE ` N ) /\ F e. ( EE ` N ) ) ) ->
       ( ( <. A , B >. Cgr <. C , D >. /\
           <. A , B >. Cgr <. E , F >. ) ->
         <. C , D >. Cgr <. E , F >. ) ) $=
      ( vi cee cfv wcel w3a cop ccgr wbr wa wi cn c1 cfz co cv cmin c2 cexp
      csu wceq eqtr2 wb simpl1 simpl2 simpl3 simpr1 brcgr syl22anc simpr2
      simpr3 anbi12d imbi12d mpbiri 3adant1 ) AGIJZKZBVBKZCVBKZLZDVBKZEVBKZFVBK
