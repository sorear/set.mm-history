  $( Lemma for ~ ax5seg .  Use the weak deduction theorem to eliminate the
     hypotheses from ~ ax5seglem7 . $)
  ax5seglem8 $p |- ( ( ( A e. CC /\ T e. CC ) /\ ( C e. CC /\ D e. CC ) ) ->
