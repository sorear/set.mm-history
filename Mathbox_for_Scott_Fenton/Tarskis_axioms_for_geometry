    axlowdim2 $p |- ( N e. ( ZZ>= ` 2 ) ->
       E. x e. ( EE ` N ) E. y e. ( EE ` N ) E. z e. ( EE ` N )
        -. ( x Btwn <. y , z >. \/ y Btwn <. z , x >. \/
             z Btwn <. x , y >. ) ) $=
