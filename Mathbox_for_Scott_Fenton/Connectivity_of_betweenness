  $( Lemma for ~ btwnconn1 .  Now, we show that two of the hypotheticals we
     introduced in the first lemma are identical. $)
  btwnconn1lem2 $p |- (
     ( ( ( N e. NN /\ A e. ( EE ` N ) /\ B e. ( EE ` N ) ) /\
         ( C e. ( EE ` N ) /\ D e. ( EE ` N ) /\ c e. ( EE ` N ) ) /\
         ( d e. ( EE ` N ) /\ b e. ( EE ` N ) /\ X e. ( EE ` N ) ) ) /\
         ( ( ( A =/= B /\ B =/= C ) /\
             ( B Btwn <. A , C >. /\ B Btwn <. A , D >. ) ) /\
           ( ( D Btwn <. A , c >. /\ <. D , c >. Cgr <. C , D >. ) /\
             ( C Btwn <. A , d >. /\ <. C , d >. Cgr <. C , D >. ) ) /\
           ( ( c Btwn <. A , b >. /\ <. c , b >. Cgr <. C , B >. ) /\
             ( d Btwn <. A , X >. /\ <. d , X >. Cgr <. D , B >. ) ) ) ) ->
     X = b ) $=
    ( cn wcel cee cfv w3a cv wne wa cop cbtwn wbr ccgr wceq simp1ll adantl
    simp11 simp12 simp13 simp21 simp33 simp1rl simp2rl simp3rl jca wi simp31
    btwnexch syl122anc adantr mpd btwnexchand axcgrrflx syl111anc simp23
    simp32 simp1rr simp2ll simp22 simp3ll btwnexch3and wb btwncom syl13anc
    mpbid btwnconn1lem1 simp3lr cgrextendand 3jca segconeq syl133anc ) EJKZAELM
    ZKZBWAKZNZCWAKZDWAKZHOZWAKZNZIOZWAKZGOZWAKZFWAKZNZNZABPZBCPZQZBACRSTZBADRST
