  $( If ` A ` , ` B ` , and ` C ` fall in order on a line, and ` A B ` and
     ` A C ` are congruent, then ` C = B ` .  (Contributed by Scott Fenton,
     7-Oct-2013.) $)
  endofsegid $p |- ( ( N e. NN /\
    ( A e. ( EE ` N ) /\ B e. ( EE ` N ) /\ C e. ( EE ` N ) ) ) ->
    ( ( B Btwn <. A , C >. /\ <. A , C >. Cgr <. A , B >. ) ->
      C = B ) ) $=
    ( cn wcel cee cfv w3a wa cop cbtwn wbr ccgr wceq ccgr3 wb simpl simpr1
    simpr3 simpr2 cgrcom syl122anc biimpd idd axcgrrflx 3adant3r1 a1d 3jcad
    3ancomb brcgr3 syl3an3br 3anidm23 sylibrd wi btwnxfr sylan2d a1i jcad
    3anrot btwnswapid2 sylan2br syld ) DEFZADGHZFZBVEFZCVEFZIZJZBACKZLMZVKABKZN
    MZJZCVMLMZVLJZCBOZVJVOVPVLVJVNABCKZKACBKZKPMZVLVPVJVNVMVKNMZVNVSVTNMZIZWAVJ
    VNWBVNWCVJVNWBVJVDVFVHVFVGVNWBQVDVIRVDVFVGVHSZVDVFVGVHTWEVDVFVGVHUAACABDUBU
    CUDVJVNUEVJWCVNVDVGVHWCVFBCDUFUGUHUIVDVIWAWDQZVIVDVIVFVHVGIZWFVFVHVGUJZABCA
    CBDUKULUMUNVDVIVLWAJVPUOZVIVDVIWGWIWHABCACBDUPULUMUQVOVLUOVJVLVNRURUSVIVDVH
    VFVGIVQVRUOVHVFVGUTCABDVAVBVC $.
    $( [14-Oct-2013] $) $( [7-Oct-2013] $)

  $( Lemma for ~ btwnconn1 .  The next several lemmas introduce various
     properties of hypothetical points that end up eliminating alternatives to
     connectivity.  We begin by showing a congruence property of those
     hypothetical points. $)
  btwnconn1lem1 $p |- (
     ( ( ( N e. NN /\ A e. ( EE ` N ) /\ B e. ( EE ` N ) ) /\
         ( C e. ( EE ` N ) /\ D e. ( EE ` N ) /\ c e. ( EE ` N ) ) /\
         ( d e. ( EE ` N ) /\ b e. ( EE ` N ) /\ X e. ( EE ` N ) ) ) /\
         ( ( ( A =/= B /\ B =/= C ) /\
             ( B Btwn <. A , C >. /\ B Btwn <. A , D >. ) ) /\
           ( ( D Btwn <. A , c >. /\ <. D , c >. Cgr <. C , D >. ) /\
             ( C Btwn <. A , d >. /\ <. C , d >. Cgr <. C , D >. ) ) /\
           ( ( c Btwn <. A , b >. /\ <. c , b >. Cgr <. C , B >. ) /\
             ( d Btwn <. A , X >. /\ <. d , X >. Cgr <. D , B >. ) ) ) ) ->
     <. B , c >. Cgr <. X , C >. ) $=
    ( cn wcel cee cfv w3a cv wne wa cop cbtwn wbr ccgr simp11 simp13 simp22
    simp23 simp33 simp31 simp21 simp12 simp1rr adantl simp2ll btwnexch3and
    simp2rl simp3rl btwncomand simp3rr cgrcomand cgrcomlrand simp2lr simp2rr
    cgrcomland cgrtr3and cgrextendand ) EJKZAELMZKZBVFKZNZCVFKZDVFKZHOZVFKZNZIO
    ZVFKZGOZVFKZFVFKZNZNZABPBCPQZBACRSTZBADRSTZQQZDAVLRSTZDVLRCDRZUATZQZCAVORST
    ZCVORWGUATZQZQZVLAVQRSTVLVQRCBRUATQZVOAFRSTZVOFRDBRUATZQQZNZBDVLFVOCEVEVGVH
    VNVTUBZVEVGVHVNVTUCZVIVJVKVMVTUDZVIVJVKVMVTUEZVIVNVPVRVSUFZVIVNVPVRVSUGZVIV
    JVKVMVTUHZWAWRABDVLEWSVEVGVHVNVTUIZWTXAXBWRWDWAWCWDWBWMWQUJUKWRWFWAWFWHWLWE
    WQULUKUMWAWRVOCFEWSXDXEXCWAWRACVOFEWSXFXEXDXCWRWJWAWJWKWIWEWQUNUKWRWOWAWOWP
    WNWEWMUOUKUMUPWAWRDBVOFEWSXAWTXDXCWAWRVOFDBEWSXDXCXAWTWRWPWAWOWPWNWEWMUQUKU
    RUSWAWRDVLVOCCDEWSXAXBXDXEXEXAWRWHWAWFWHWLWEWQUTUKWAWRCVOCDEWSXEXDXEXAWRWKW
