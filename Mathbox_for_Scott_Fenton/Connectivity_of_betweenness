  $( If two points fall in the same place in the middle of a segment, then they
     are identical.  (Contributed by Scott Fenton, 16-Oct-2013.) $)
  midofsegid $p |- ( ( N e. NN /\ ( A e. ( EE ` N ) /\ B e. ( EE ` N ) ) /\
   ( D e. ( EE ` N ) /\ E e. ( EE ` N ) ) ) ->
   ( ( D Btwn <. A , B >. /\ E Btwn <. A , B >. /\
       <. A , D >. Cgr <. A , E >. ) -> D = E ) ) $=
    ( cn wcel cee cfv wa w3a cop wbr ccgr mpd simprr simprl3 endofsegidand expr
    cbtwn wceq wo 3simpa adantl simp2l simp3l simp3r simp2r btwnconn3 syl122anc
    wi simp1 adantr cgrcomand eqcomd jaod ex ) EFGZAEHIZGZBUSGZJZCUSGZDUSGZJZKZ
    CABLZTMZDVGTMZACLZADLZNMZKZCDUAZVFVMJZCVKTMZDVJTMZUBZVNVOVHVIJZVRVMVSVFVHVI
