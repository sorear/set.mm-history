  $( Removing identical parts from the end of a line segment preserves
     congruence.  Theorem 4.3 of [Schwabhauser], p. 35.  (Contributed by Scott
     Fenton, 4-Oct-2013.) $)
  cgrsub $p |- ( ( N e. NN /\
         ( A e. ( EE ` N ) /\ B e. ( EE ` N ) /\ C e. ( EE ` N ) ) /\
         ( D e. ( EE ` N ) /\ E e. ( EE ` N ) /\ F e. ( EE ` N ) ) ) ->
         ( ( ( B Btwn <. A , C >. /\ E Btwn <. D , F >. ) /\
             ( <. A , C >. Cgr <. D , F >. /\ <. B , C >. Cgr <. E , F >. ) )
           -> <. A , B >. Cgr <. D , E >. ) ) $=
    ( cn wcel cee cfv w3a cop cbtwn wbr wa ccgr simprl simprr simpl1 simpl21
    simpl31 cgrtriv syl111anc simprrl wb simpl23 simpl33 cgrcomlr syl122anc
    mpbid jca 3jca wi simpl22 simpl32 cifs brifs ifscgr sylbird syl333anc mpd
    ex ) GHIZAGJKZIZBVEIZCVEIZLZDVEIZEVEIZFVEIZLZLZBACMZNOEDFMZNOPZVOVPQOZBCMEF
    MQOZPZPZABMZDEMZQOZVNWAPZBAMEDMQOZWDWEVQVTAAMDDMQOZCAMZFDMZQOZPZLZWFWEVQVTW
    KVNVQVTRVNVQVTSWEWGWJWEVDVFVJWGVDVIVMWATZVFVGVHVDVMWAUAZVJVKVLVDVIWAUBZADGU
    CUDWEVRWJVNVQVRVSUEWEVDVFVHVJVLVRWJUFWMWNVFVGVHVDVMWAUGZWOVJVKVLVDVIWAUHZAC
    DFGUIUJUKULUMWEVDVFVGVHVFVJVKVLVJWLWFUNWMWNVFVGVHVDVMWAUOZWPWNWOVJVKVLVDVIW
    AUPZWQWOVDVFVGLVHVFVJLVKVLVJLLWLWBWHMWCWIMUQOWFABCADEFDGURABCADEFDGUSUTVAVB
    WEVDVGVFVKVJWFWDUFWMWRWNWSWOBAEDGUIUJUKVC $.
