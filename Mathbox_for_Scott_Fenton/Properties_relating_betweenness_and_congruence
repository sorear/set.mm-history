  $( Permutation law for three-place congruence.  (Contributed by Scott Fenton,
     5-Oct-2013.) $)
  cgr3permute3 $p |- ( ( N e. NN /\
       ( A e. ( EE ` N ) /\ B e. ( EE ` N ) /\ C e. ( EE ` N ) ) /\
       ( D e. ( EE ` N ) /\ E e. ( EE ` N ) /\ F e. ( EE ` N ) ) ) ->
       ( <. A , <. B , C >. >. Cgr3 <. D , <. E , F >. >. <->
         <. B , <. C , A >. >. Cgr3 <. E , <. F , D >. >. ) ) $=
    ( cn wcel cee cfv w3a cop ccgr wbr ccgr3 wa wb id 3simpa cgrcomlr syl3an
    3simpb 3anbi12d 3anrot syl6bbr brcgr3 biid syl3anb 3bitr4d ) GHIZAGJKZIZBUL
