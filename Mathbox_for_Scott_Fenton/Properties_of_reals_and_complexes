  $( Swap the order of subtraction in a square.  (Contributed by Scott Fenton,
     10-Jun-2013.) $)
  sqsubswap $p |- ( ( A e. CC /\ B e. CC ) ->
             ( ( A - B ) ^ 2 ) = ( ( B - A ) ^ 2 ) ) $=
    ( cc wcel wa cmin co cneg c2 cexp wceq subcl sqneg negsubdi2 opreq1d eqtr3d
    syl ) ACDBCDEZABFGZHZIJGZSIJGZBAFGZIJGRSCDUAUBKABLSMQRTUCIJABNOP $.
