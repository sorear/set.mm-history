  $( The square root function is one to one.  (Contributed by Scott Fenton,
     11-Jun-2013.) $)
  sqr11 $p |- ( ( ( A e. RR /\ 0 <_ A ) /\ ( B e. RR /\ 0 <_ B ) ) ->
    ( ( sqr ` A ) = ( sqr ` B ) <-> A = B ) ) $=
    ( cr wcel cc0 cle wbr csqr cfv wb wa wi cif breq2 fveq2 bibi12d imbi12d 0re
    wceq elimel anbi1d eqeq1d eqeq1 anbi2d eqeq2d eqeq2 sqr11i dedth2h imp an4s
