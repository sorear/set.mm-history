  $( Lemma for ~ axfe .  The empty set is a surreal. $)
  axfelem1 $p |- (/) e. No $=
    ( vx c0 csur wcel cv c1o c2o cpr wf con0 wrex 0elon f0 feq2 rcla4ev mp2an
    elno mpbir ) BCDAEZFGHZBIZAJKZBJDBTBIZUBLTMUAUCABJSBTBNOPABQR $.
    $( [10-Jul-2011] $) $( [16-Jun-2011] $)

  $( Lemma for ~ axfe .  Calculate the birthday of the empty set. $)
  axfelem2 $p |- ( bday ` (/) ) = (/) $=
    ( c0 cbday cfv cdm csur wcel wceq axfelem1 bdayval ax-mp dm0 eqtri ) ABCZAD
    ZAAEFMNGHAIJKL $.
    $( [10-Jul-2011] $) $( [21-Jun-2011] $)

  ${
    $d F a b n $.  $d X a b $.
    axfelem3.1 $e |- X e. { 1o , 2o } $.
