    $( Lemma for ~ axfe .  Calculate the birthday of ` ( C X. { X } ) ` . $)
    axfelem4 $p |- ( ( F e. _V /\ F C_ No ) ->
                     ( bday ` ( C X. { X } ) ) = C ) $=
      ( cvv wcel csur wss wa csn cxp cbday cfv cdm con0 wceq axfelem3 noxpsgn
      bdayval 3syl c0 wne c1o c2o cpr elisseti snnz dmxp ax-mp syl6eq ) CIJCKLM
