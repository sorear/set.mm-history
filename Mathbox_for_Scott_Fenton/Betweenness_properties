    $( Outer transitivity law for betweenness.  (Contributed by Scott Fenton,
       12-Jun-2013.) $)
    btwnouttr2 $p |- ( ( N e. NN /\ ( A e. ( EE ` N ) /\ B e. ( EE ` N ) ) /\
       ( C e. ( EE ` N ) /\ D e. ( EE ` N ) ) ) ->
       ( ( B =/= C /\
           B Btwn <. A , C >. /\ C Btwn <. B , D >. ) ->
         C Btwn <. A , D >. ) ) $=
      ( vx cn wcel cee cfv wa w3a wne cop cbtwn wbr cv ccgr wrex simp1 simp2l
      simp3l simp3r axsegcon syl122anc adantr wi simprrl wceq simprl1 simpl2
