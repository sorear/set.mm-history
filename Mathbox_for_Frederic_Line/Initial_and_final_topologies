      CVFDAUTFVBUKULUMUNUODCFABUPUQCFURUS $.
      $( [22-Dec-2008] $) $( [22-Dec-2008] $)
  $}

  $( The subspace topology induced by the topology ` J ` on the empty set. $)
  subspemp2 $p |- ( J e. Top -> ( subSp ` <. (/) , J >. ) = { (/) } ) $=
    ( ctop wcel c0 cop csubsp cfv csn wceq cif opeq2 fveq2d eqeq1d sn0top
    elimel subspemp dedth ) ABCZDAEZFGZDHZIDRAUAJZEZFGZUAIAUAAUBIZTUDUAUESUCFAU
    BDKLMUBAUABNOPQ $.
