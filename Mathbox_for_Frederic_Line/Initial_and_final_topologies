  ${
    $d A x $.  $d J u v x z $.  $d K u v x z $.
    eltpt.1 $e |- B = { z | E. u e. J E. v e. K z = ( u X. v ) } $.
    $( Condition for a set to belong to a product topology.. $)
    eltpt $p |- ( ( J e. Top /\ K e. Top ) -> ( A e. ( J tX K ) <-> E. x
       ( x C_ B /\ A = U. x ) ) ) $=
      ( ctop wcel wa ctx co cv cxp wceq wrex cab ctg cfv wss cuni wex txval
      eleq2d ctb wb txbas eltg3 syl sseq2i bicomi anbi1i exbii a1i 3bitrd ) GJK
      HJKLZEGHMNZKEBODOCOPQCHRDGRBSZTUAZKZAOZUTUBZEVCUCQZLZAUDZVCFUBZVELZAUDZUR
      USVAEBCDGHUEUFURUTUGKVBVGUHBCDGHUIAEUTUJUKVGVJUHURVFVIAVDVHVEVHVDFUTVCIUL
      UMUNUOUPUQ $.
      $( [18-Apr-2010] $) $( [18-Apr-2010] $)
  $}

  ${
    topgele.1 $e |- X = U. J $.
    $( The topologies over the same set have a greatest element (the discrete
       topology) and a least element (the undiscrete topology). $)
    topgele $p |- ( J e. Top -> ( { (/) , X } C_ J /\ J C_ ~P X ) ) $=
      ( ctop wcel cuni cpw wss wa c0 cpr cop ctps istps2 eltopsp bitr3i wi
      wceq wb id eqcoms eleq1d anbi2d ax-mp prssg ibi sylbi pweq eqcomi sseq2i
      biimpi anim12i expcom a1i imp31 sylbir ) ADEZUQAAFZGZHZIJAEZURAEZIZIZJBKA
      HZABGZHZIZVDURALMEUQURANAOPUQUTVCVHUTVCVHQQUQVCUTVHVCVEUTVGVCVABAEZIZVEBU
      RRZVCVJSCVKVBVIVAVKURBAURBRZURBVLTUAUBUCUDVJVEJBAAAUEUFUGUTVGUSVFAVFUSVKV
      FUSRCBURUHUDUIUJUKULUMUNUOUP $.
      $( [18-Apr-2010] $) $( [18-Apr-2010] $)

    $( The topologies over the same set have a greatest element (the discrete
       topology). $)
    topge $p |- ( J e. Top -> J C_ ~P X ) $=
      ( ctop wcel c0 cpr wss cpw topgele pm3.27d ) ADEFBGAHABIHABCJK $.
      $( [1-Jan-2011] $) $( [1-Jan-2011] $)
  $}

  ${
    ptincpw.1 $e |- X = U. J $.
    ptincpw.2 $e |- Y = U. K $.
    $( A product topology is included in the powerset of the cross product of
       the underlying sets.  The cross product and the empty set are elements
       of a product topology. $)
    ptincpw $p |- ( ( J e. Top /\ K e. Top )
      -> ( { (/) , ( X X. Y ) } C_ ( J tX K )
           /\ ( J tX K ) C_ ~P ( X X. Y ) ) ) $=
      ( ctx co cuni cxp wceq ctop wcel wa c0 cpr wss cpw eqid txuni preq2
      eqcoms sseq1d wb pweq sseq2d anbi12d txtop topindis syl syl5bir mpcom )
      ABGHZIZCDJZKZALMBLMNZOUOPZUMQZUMUORZQZNZABUMCDUMSZEFTUPVBOUNPZUMQZUMUNRZQ
      ZNZUQUPUSVEVAVGUPURVDUMURVDKUOUNUOUNOUAUBUCVAVGUDUOUNUOUNKUTVFUMUOUNUEUFU
      BUGUQUMLMVHABUMVCUHUMUIUJUKUL $.
      $( [18-Apr-2010] $) $( [18-Apr-2010] $)

  $}

  ${
