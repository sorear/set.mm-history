      VOUOUPVQVTVEVGVHUQURUSVIVEUTUNVAABEDVBVPVRVKVJUPVQVTVGVHVCURVD $.
      $( [31-May-2014] $) $( [17-Aug-2008] $)
  $}

  $( ` { (/) } ` is the only topology with one element.  (Contributed by FL,
     18-Aug-2008.) $)
  top1 $p |- ( J e. Top -> ( J ~~ 1o <-> J = { (/) } ) ) $=
    ( ctop wcel c1o cen wbr c0 csn wceq wi 0opn en1eqsn ex syl 0ex ensn1 mpbiri
    breq1 impbid1 ) ABCZADEFZAGHZIZTGACZUAUCJAKUDUAUCGALMNUCUAUBDEFGOPAUBDERQS
