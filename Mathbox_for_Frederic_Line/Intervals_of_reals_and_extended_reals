  ${
    $d A x $.  $d B x $. $( ` x ` is dummy. $)
    $( An open interval is included in ` RR ` if ` A ` and ` B ` belong to
       ` RR* ` . $)
    iooirrsa $p |- ( ( A e. RR* /\ B e. RR* ) -> ( A (,) B ) C_ RR ) $=
      ( cxr wcel wa cioo co cr cpw wss ioof foprcl elpwi syl ) ACDBCDEABFGZHIZD
      OHJABPCCFKLOHMN $.
      $( [3-Mar-2007] $)
  $}


  ${
