  ${
    $d A x $.  $d B x $. $( ` x ` is dummy. $)
    $( An open interval is included in ` RR ` if ` A ` and ` B ` belong to
       ` RR* ` .  (Contributed by FL, 3-Mar-2007.) $)
    iooirrsa $p |- ( ( A e. RR* /\ B e. RR* ) -> ( A (,) B ) C_ RR ) $=
      ( cxr wcel wa cioo co cr cpw wss ioof foprcl elpwi syl ) ACDBCDEABFGZHIZD
      OHJABPCCFKLOHMN $.
      $( [3-Mar-2007] $)
  $}


  ${
    $d A x $.  $d B x $.
    $( A closed interval is a set of extended reals.  (Contributed by FL,
       28-Jul-2008.) $)
    clsrebb $p |- ( B e. _V -> ( A [,] B ) C_ RR* ) $=
      ( vx cvv wcel cxr wa cicc co wss wn c0 cdm cxp wceq cpw iccf fdmi cle wbr
      ndmoprg mp3an1 0ss a1i eqsstrd ex w3a elicc1 simp1 syl6bi ssrdv pm2.61d2
      cv ) BDEZAFEBFEGZABHIZFJZUNUOKZUQUNURGZUPLFHMFFNZOUNURUPLOUTFPHQRABDFFHUA
      UBLFJUSFUCUDUEUFUOCUPFUOCUMZUPEVAFEZAVASTZVABSTZUGVBABVAUHVBVCVDUIUJUKUL
      $.
      $( [28-Jul-2008] $)
  $}

  ${
    $d A x y z $.  $d w x y z $.
    $( Membership to the set of open intervals implied the existence of two
       bounds in the set of the extended reals.  (Revised by Mario Carneiro,
       3-Nov-2013.) $)
    bsi $p |- ( A e. ran (,) <->
      E. x e. RR* E. y e. RR* A = ( x (,) y ) ) $=
      ( cxr cxp cr cpw cioo wf wfn crn wcel cv co wceq wrex ioof ffn oprvelrn
      wb mp2b ) DDEZFGZHIHUBJCHKLCAMBMHNOBDPADPTQUBUCHRABDDCHSUA $.
      $( [3-Nov-2013] $) $( [31-Oct-2007] $)
  $}

  $( If an open interval has an element, then ` A < B ` .  (Contributed by FL,
     14-Aug-2007.) $)
  elioo1t3 $p |- ( C e. ( A (,) B ) -> A < B ) $=
    ( cxr wcel wa cioo co clt wi elioo1 xrlttr exp3a 3com23 3expia 3impd sylbid
    wbr w3a c0 wn wceq ndmioo eleq2 noel pm2.21i syl6bi syl pm2.61i ) ADEZBDEZF
    ZCABGHZEZABIRZJZULUNCDEZACIRZCBIRZSUOABCKULUQURUSUOUJUKUQURUSUOJJZUJUQUKUTU
    JUQUKSURUSUOACBLMNOPQULUAUMTUBZUPABUCVAUNCTEZUOUMTCUDVBUOCUEUFUGUHUI $.
    $( [14-Sep-2007] $) $( [14-Aug-2007] $)

  $( An open interval with its upper bound equal to ` -oo ` is empty.
     (Contributed by FL, 12-Sep-2007.) $)
  oisbmi $p |- ( A (,) -oo ) = (/) $=
    ( cxr wcel cmnf wa cioo co c0 wceq cle wbr mnfle adantr ioo0 mpbird pm2.61i
    ndmioo ) ABCZDBCZEZADFGHIZTUADAJKZRUBSALMADNOADQP $.
    $( [14-Sep-2007] $) $( [12-Sep-2007] $)

  $( An open interval with its lower bound equal to ` +oo ` is empty.
     (Contributed by FL, 12-Sep-2007.) $)
  oisbmj $p |- ( +oo (,) A ) = (/) $=
    ( cpnf cxr wcel wa cioo co c0 wceq cle wbr pnfge adantl ioo0 mpbird pm2.61i
    ndmioo ) BCDZACDZEZBAFGHIZTUAABJKZSUBRALMBANOBAQP $.
    $( [14-Sep-2007] $) $( [12-Sep-2007] $)

  $( Closure of translation in a half-infinite interval.  (Contributed by FL,
     11-Sep-2007.) $)
