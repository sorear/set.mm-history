$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              The "maps to" notation
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)


  ${
    fnoprab2ga.1 $e |- F = ( x e. A , y e. B |-> C ) $.
    ${
      $d A x y z $.  $d B x y z $.  $d C z $.
      $( Functionality and domain of a class given by the "maps to" notation.
         Equivalent to ~ fnoprab2g . $)
      fnoprab2ga $p |- ( A. x e. A A. y e. B C e. _V <-> F Fn ( A X. B ) ) $=
        ( vz cmpt2 cv wcel wa wceq copab2 df-mpt2 eqtri fnoprab2g ) ABHCDEFFABC
        DEIAJCKBJDKLHJEMLABHNGABHCDEOPQ $.
        $( [18-May-2010] $) $( [17-May-2010] $)
    $}

    ${
      fnoprab2g.2 $e |- C e. _V $.
      ${
        $d A x y z $.  $d B x y z $.  $d C z $.
        $( Functionality and domain of a class given by the "maps to"
           notation.  Equivalent to ~ fnoprab2 . $)
        fnoprab2a $p |- F Fn ( A X. B ) $=
          ( vz cmpt2 cv wcel wa wceq copab2 df-mpt2 eqtri fnoprab2 ) ABICDEFHFA
          BCDEJAKCLBKDLMIKENMABIOGABICDEPQR $.
          $( [18-May-2010] $) $( [17-May-2010] $)

        $( Domain of a class given by the "maps to" notation.  Equivalent to
           ~ dmoprab2 . $)
        dmoprab2a $p |- dom F = ( A X. B ) $=
          ( vz cmpt2 cv wcel wa wceq copab2 df-mpt2 eqtri dmoprab2 ) ABICDEFHFA
          BCDEJAKCLBKDLMIKENMABIOGABICDEPQR $.
          $( [18-May-2010] $) $( [17-May-2010] $)
      $}

      ${
        $d A x y z $.  $d B x y z $.  $d C z $.  $d D x y z $.
        $( Functionality, domain and range of a class given by the "maps to"
           notation.  Equivalent to ~ foprab2 . $)
        foprab2a $p |- ( A. x e. A A. y e. B C e. D
          <-> F : ( A X. B ) --> D ) $=
          ( vz cmpt2 cv wcel wa wceq copab2 df-mpt2 eqtri foprab2 ) ABJCDEFGGAB
          CDEKALCMBLDMNJLEONABJPHABJCDEQRS $.
          $( [18-May-2010] $) $( [17-May-2010] $)
      $}

    $}

    ${
      $d A x y z $.  $d B x y z $.  $d C z $.  $d D x $.  $d X x y z $.
      $d Y x y z $.  $d Z y z $.
      oprabval2ga.1 $e |- ( x = X -> C = D ) $.
      oprabval2ga.2 $e |- ( y = Y -> D = Z ) $.
      $( Todo: change the variables. $)
      $( The value of an operation class abstraction.  Special case.
         Equivalent to ~ oprabval2g . $)
      oprabval2ga $p |- ( ( X e. A /\ Y e. B /\ Z e. N )
          -> ( X F Y ) = Z ) $=
        ( vz cmpt2 cv wcel wa wceq copab2 df-mpt2 eqtri oprabval2g ) ABOIJCDEKG
        FHMNGABCDEPAQCRBQDRSOQETSABOUALABOCDEUBUCUD $.
        $( [18-May-2010] $) $( [17-May-2010] $)

    $}

    ${
      $d A x y z $.  $d B x y z $.  $d C z $.
      $( Existence of an operation class abstraction (special case). $)
      oprabex2ga $p |- ( ( A e. R /\ B e. S ) -> F e. _V ) $=
        ( vz cmpt2 cv wcel wa wceq copab2 df-mpt2 eqtri oprabex2g ) ABJCDEFGHHA
        BCDEKALCMBLDMNJLEONABJPIABJCDEQRS $.
        $( [18-May-2010] $) $( [17-May-2010] $)
    $}

  $}

  ${
    $d A z $.  $d B z $.  $d x y $.  $d x z $.
    $( ` x ` is bound in the "maps to" notation. $)
    hbcmpt $p |- ( y e. ( x e. A |-> B ) -> A. x y e. ( x e. A |-> B ) ) $=
      ( vz cmpt cv wcel wceq wa copab df-mpt hbopab1 hbxfr ) ABACDFAGCHEGDIJZAE
      KAECDLOAEBMN $.
      $( [17-Feb-2008] $) $( [17-Feb-2008] $)
  $}

  ${
    $d A y $.  $d B y $.  $d x y $.
    fvopab2b.1 $e |- F = ( x e. A |-> B ) $.
    $( Value of a function given by the "maps to" notation.  Equivalent to
       ~ fvopab2 . $)
    fvopab2b $p |- ( ( x e. A /\ B e. C ) -> ( F ` x ) = B ) $=
      ( vy cv wcel wa wceq copab cfv fvopab2 cmpt df-mpt eqtri fveq1i syl5eq )
      AHZBIZCDIJTUAGHCKJAGLZMCTEMAGBCDNTEUBEABCOUBFAGBCPQRS $.
      $( [21-Jun-2010] $) $( [21-Jun-2010] $)
  $}

  ${
    $d A w x y z $.  $d B w y z $.  $d C w x z $.
    cmpbvb.1 $e |- F = ( x e. A |-> B ) $.
    cmpbvb.2 $e |- ( x = y -> B = C ) $.
    $( Rule to change a bound variable in the "maps to" notation. $)
    cmpbvb $p |- F = ( y e. A |-> C ) $=
      ( vz vw cmpt cv wcel wceq wa copab df-mpt pm3.26 eleq1d pm3.27 adantr
      eqeq12d anbi12d cbvopabv eqtr4i 3eqtri ) FACDKALZCMZILZDNZOZAIPZBCEKZGAIC
      DQULBLZCMZJLZENZOZBJPUMUKURAIBJUGUNNZUIUPNZOZUHUOUJUQVAUGUNCUSUTRSVAUIUPD
      EUSUTTUSDENUTHUAUBUCUDBJCEQUEUF $.
      $( [21-Jun-2010] $) $( [21-Jun-2010] $)
  $}

  ${
    $d A x y $.  $d C y $.
    fopab2ga.1 $e |- F = ( x e. A |-> C ) $.
    $( Functionality and domain of a class given by the "maps to" notation. $)
    fopab2ga $p |- ( A. x e. A C e. _V <-> F Fn A ) $=
      ( vy cmpt cv wcel wceq wa copab df-mpt eqtri fnopab2g ) AFBCDDABCGAHBIFHC
      JKAFLEAFBCMNO $.
      $( [17-Feb-2008] $) $( [17-Feb-2008] $)
  $}

  ${
    $d A x y $.  $d B x y $.  $d C y $.
    fopab2a.1 $e |- F = ( x e. A |-> C ) $.
    $( Functionality of a class given by the "maps to" notation. $)
    fopab2a $p |- ( A. x e. A C e. B <-> F : A --> B ) $=
      ( vy cmpt cv wcel wceq wa copab df-mpt eqtri fopab2 ) AGBCDEEABDHAIBJGIDK
      LAGMFAGBDNOP $.
      $( [17-Feb-2008] $) $( [17-Feb-2008] $)
  $}

  ${
    $d A y $.  $d B y $.  $d x y $.
    cmp.1 $e |- F = ( x e. A |-> B ) $.
    $( Functionality of a class given by a "maps to" notation. $)
    cmpfun $p |- Fun F $=
      ( vy cmpt wfun cv wcel wceq wa copab wss pm3.27 ssopab2i funopabeq funss
      mp2 wb df-mpt funeq ax-mp mpbir bicomd mpbi ) ABCGZHZDHZUHAIBJZFICKZLZAFM
      ZHZUMUKAFMZNUOHUNULUKAFUJUKOPAFCQUMUORSUGUMKUHUNTAFBCUAUGUMUBUCUDDUGKZUHU
      ITEUPUIUHDUGUBUEUCUF $.
