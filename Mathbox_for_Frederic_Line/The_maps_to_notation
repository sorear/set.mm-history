  $}

  ${
    $d A x y $.  $d B y $.
    cmpdom.1 $e |- F = ( x e. A |-> B ) $.
    $( Domain of a class given by the "maps to" notation. $)
    cmpdom $p |- ( A. x e. A B e. _V <-> dom F = A ) $=
      ( cvv wcel wral cdm wceq wfun wfn wa df-fn fopab2ga ancom 3bitr4i cmpfun
      mpbiran2 ) CFGABHZDIBJZDKZDBLUBUAMTUAUBMDBNABCDEOUAUBPQABCDERS $.
      $( [17-Feb-2008] $) $( [17-Feb-2008] $)
  $}

  ${
    $d A x $.
    cmpdom2.1 $e |- F = ( x e. A |-> ( B G C ) ) $.
    $( Domain of a class given by the "maps to" notation. $)
    cmpdom2 $p |- dom F = A $=
      ( co cvv wcel wral cdm wceq cv oprex a1i rgen cmpdom mpbi ) CDFHZIJZABKEL
      BMUAABUAANBJCDFOPQABTEGRS $.
      $( [21-Jun-2010] $) $( [21-Jun-2010] $)
  $}

  ${
    $d A y $.  $d B y $.  $d x y $.
    cmpran.1 $e |- F = ( x e. A |-> B ) $.
    $( Range of a class given by the "maps to" notation. $)
    cmpran $p |- ran F = { y | E. x e. A y = B } $=
      ( crn cmpt cv wcel wceq wa copab wrex cab rneqi df-mpt rnopab2 3eqtri )
      EGACDHZGAICJBIDKZLABMZGUAACNBOETFPTUBABCDQPABCDRS $.
      $( [17-Feb-2008] $) $( [17-Feb-2008] $)
  $}

  ${
    $d A y z $.  $d B z $.  $d C z $.  $d x y z $.
    rngop.1 $e |- F = ( x e. A , y e. B |-> C ) $.
    $( The range of an operation given by the "maps to" notation. $)
    rngop $p |- ran F = { z | E. x e. A E. y e. B z = C } $=
      ( crn cv wcel wa wceq copab2 wex cab wrex cmpt2 df-mpt2 eqtri rneqi
      rnoprab r2ex bicomi abbii 3eqtri ) GIAJDKBJEKLCJFMZLZABCNZIUHBOAOZCPUGBEQ
      ADQZCPGUIGABDEFRUIHABCDEFSTUAUHABCUBUJUKCUKUJUGABDEUCUDUEUF $.
      $( [20-Jun-2011] $) $( [20-Jun-2011] $)
  $}

  ${
    $d A x y $.  $d C y $.
    $( Necessary condition so that the function given by the "maps to" notation
       is a set.  (Moved to ~ mptexg in main set.mm --NM.) $)
    mpsetOLD $p |- ( A e. B -> ( x e. A |-> C ) e. _V ) $=
      ( vy wcel cv wceq wa copab cvv cmpt opabex2g df-mpt syl5eqel ) BCFAGBFEGD
      HIAEJKABDLAEBDCMAEBDNO $.
      $( [6-Jun-2011] $) $( [6-Jun-2011] $)
  $}

  ${
