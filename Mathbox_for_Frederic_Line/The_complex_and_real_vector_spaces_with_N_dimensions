  $c .cv $. $( Multiplication of complex vectors by a scalar. $)

  $( Extends class notation with scalar multiplication of complex vectors. $)
  csmcv $a class .cv $.

  ${
    $d n s u x $.
    $( Multiplication of complex vectors by a scalar in a space of dimension
       ` n ` .  Experimental. $)
    df-mulcv $a |- .cv = ( n e. NN |->
      ( s e. CC , u e. ( CC ^m ( 1 ... n ) ) |->
        ( x e. ( 1 ... n ) |-> ( s x. ( u ` x ) ) ) ) ) $.
  $}

  ${
    $d x u S s $.  $d x u U $.  $d x u n N s $.
    ismulcv.1 $e |- .t = ( .cv ` N ) $.
    $( Multiplication of complex vectors by a scalar in a space of dimension
       ` n ` .  (Contributed by FL, 15-Sep-2013.) $)
    ismulcv $p |- ( ( N e. NN /\ S e. CC /\ U e. ( CC ^m ( 1 ... N ) ) )
       -> ( S .t U ) = ( x e. ( 1 ... N ) |-> ( S x. ( U ` x ) ) ) ) $=
