  ${
    $d A x $.  $d ps x $.
    r19.2zr.1 $e |- ( ph -> ps ) $.
    $( Quantifying a hypothesis with a universal restricted quantifier. $)
    r19.2zr $p |- ( ( A =/= (/) /\ A. x e. A ph ) -> ps ) $=
      ( c0 wne wral wa wrex r19.2z r19.23aivr syl ) DFGACDHIACDJBACDKABCDELM $.
      $( [19-Sep-2011] $)
  $}

  ${
    $d A x $.  $d ph x $.
    $( Removing a universal restricted quantifier when the variable doesn't
       occur in the proposition. $)
    r19.2zrr $p |- ( ( A =/= (/) /\ A. x e. A ph ) -> ph ) $=
      ( id r19.2zr ) AABCADE $.
      $( [19-Sep-2011] $)
  $}

  ${
    $d A x $.
    r19.23aib.1 $e |- ( ps -> A. x ps ) $.
    r19.23aib.2 $e |- ( x e. A -> ( ph -> ps ) ) $.
    $( Removal of an universal restricted quantifier in an antecedent.  See
       also ~ reximdva0 . $)
    r19.23aib $p |- ( ( A =/= (/) /\ A. x e. A ph ) -> ps ) $=
      ( c0 wne wral wrex wi r19.2zb r19.23ai imim2i sylbi imp ) DGHZACDIZBQRACD
      JZKRBKACDLSBRABCDEFMNOP $.
      $( [19-Apr-2012] $)
  $}

  ${
    $d A x $.  $d ps x $.
    eqint.1 $e |- ( x = A -> ( ph <-> ps ) ) $.
    eqint.2 $e |- ps $.
    eqint.3 $e |- ( ph -> A C_ x ) $.
    $( To prove that a set ` A ` is the finest one that has the property
       ` ph ` , prove that ` A ` is a part of all sets that has this property
       and that ` A ` has also that property. $)
    eqint $p |- ( A e. B -> A = |^| { x | ph } ) $=
      ( wcel cab cint wss cv wi ssintab mpgbir a1i intmin3 eqssd ) DEIZDACJKZDU
      ALZTUBADCMLNCACDOHPQABCDEFGRS $.
      $( [21-Apr-2012] $)
  $}

  ${
    $d A x $.  $d ch x $.  $d ph x $.
    eqintg.1 $e |- ( x = A -> ( ch <-> ps ) ) $.
    eqintg.2 $e |- ( ph -> ch ) $.
    eqintg.3 $e |- ( ( ph /\ ps ) -> A C_ x ) $.
    $( To prove that a set ` A ` is the finest one that has the property ` ph `
       prove that ` A ` is a part of all sets that has this property and that
       ` A ` has also that property. $)
    eqintg $p |- ( ( ph /\ A e. B ) -> A = |^| { x | ps } ) $=
      ( wcel wa cab cint cv wss wi wal ex 19.21aiv adantr ssintab sylibr wceq
      wb elabgt simpr bicomd ax-gen sylancl mpbird intss1 syl eqssd ) AEFJZKZEB
      DLZMZUOBEDNZOZPZDQZEUQOAVAUNAUTDABUSIRSTBDEUAUBUOEUPJZUQEOUOVBCACUNHTUNUR
      EUCZBCUDPZDQVBCUDUOBCDEFUEAUNUFVDDVCCBGUGUHUIUJEUPUKULUM $.
      $( [15-Oct-2012] $)
  $}

  ${
    r19.37.1 $e |- ( ph -> A. x ph ) $.
    $( Restricted version of one direction of Theorem 19.37 of [Margaris]
       p. 90.  (The other direction doesn't hold when ` A ` is empty.) $)
    r19.37 $p |- ( E. x e. A ( ph -> ps ) -> ( ph -> E. x e. A ps ) ) $=
      ( wi wrex wral r19.35 cv wcel ax-1 r19.21ai imim1i sylbi ) ABFCDGACDHZBCD
      GZFAQFABCDIAPQAACDEACJDKLMNO $.
      $( [13-May-2012] $)
  $}

  $( Restricted specialization. $)
  ra42e $p |- ( ( x e. A /\ y e. B /\ ph ) -> E. x e. A E. y e. B ph ) $=
    ( cv wcel w3a wrex wa wex simp1 ra4e 3adant1 19.8a syl11anc df-rex sylibr
    ) BFDGZCFEGZAHZSACEIZJZBKZUBBDIUASUBUDSTALTAUBSACEMNUCBOPUBBDQR $.
    $( [4-Jun-2012] $)

  ${
    $d A x $.
    $( Two ways to express substitution of ` A ` for ` x ` in ` ph ` . $)
    alexeqd $p |- ( A e. B
      -> ( A. x ( x = A -> ph ) <-> E. x ( x = A /\ ph ) ) ) $=
      ( wcel cvv cv wceq wi wal wa wex wb elisset c0 cif eqeq2 imbi1d albidv
      anbi1d exbidv bibi12d 0ex elimel alexeq dedth syl ) CDECFEZBGZCHZAIZBJZUJ
      AKZBLZMZCDNUHUOUIUHCOPZHZAIZBJZUQAKZBLZMCOCUPHZULUSUNVAVBUKURBVBUJUQACUPU
      IQZRSVBUMUTBVBUJUQAVCTUAUBABUPCOFUCUDUEUFUG $.
      $( [4-Jun-2012] $)
  $}


  ${
    $d A x $.  $d A z $.  $d B w $.  $d B x y $.  $d B z y $.  $d ch w $.
    $d ch x $.  $d ph z $.  $d ps y $.
    cbvrex2v.1 $e |- ( x = z -> ( ph <-> ch ) ) $.
    cbvrex2v.2 $e |- ( y = w -> ( ch <-> ps ) ) $.
    $( Change bound variables of double restricted universal quantification,
       using implicit substitution. $)
    cbvrex2v $p |- ( E. x e. A E. y e. B ph <-> E. z e. A E. w e. B ps ) $=
      ( wrex weq rexbidv cbvrexv rexbii bitri ) AEILZDHLCEILZFHLBGILZFHLRSDFHDF
      MACEIJNOSTFHCBEGIKOPQ $.
      $( [2-Jul-2012] $)
  $}


  ${
    $d A x y $.  $d B y $.  $d C x $.  $d D x y $.  $d ch y $.  $d ph x y $.
    $d th x $.
    rcla42edv.1 $e |- ( x = A -> ( ps <-> th ) ) $.
    rcla42edv.2 $e |- ( y = B -> ( th <-> ch ) ) $.
    $( 2-variable restricted existential specialization, using implicit
       substitution.  ( ~ rcla42ev with an antecedent.) $)
    rcla42edv $p |- ( ( ph /\ A e. C /\ B e. D )
        -> ( ch -> E. x e. C E. y e. D ps ) ) $=
      ( wcel wrex wi wa w3a cv wceq anbi2d rcla42ev simpr reximi syl 3expia
      exp4b com3r 3imp ) AGIMZHJMZCBFJNZEINZOZUIUJAUMUIUJACULUIUJACPZULUIUJUNQA
      BPZFJNZEINULUOUNADPEFGHIJERGSBDAKTFRHSDCALTUAUPUKEIUOBFJABUBUCUCUDUEUFUGU
      H $.
      $( [2-Jul-2012] $)
