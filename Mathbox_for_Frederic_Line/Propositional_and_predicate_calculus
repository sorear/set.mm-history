  ${
    $d ps x $.
    r19.23aivr.1 $e |- ( ph -> ps ) $.
    $( Quantifying a hypothesis with an existential restricted quantifier. $)
    r19.23aivr $p |- ( E. x e. A ph -> ps ) $=
      ( wi cv wcel a1i r19.23aiv ) ABCDABFCGDHEIJ $.
      $( [19-Sep-2011] $)
