  ${
    $d A a $.  $d J a $.
    $( The intersection of a non-empty finite family of open sets is open.
       (Contributed by FL, 20-Apr-2012.) $)
    neffopo $p |- ( J e. Top -> ( ( A C_ J /\ A =/= (/) /\ A e. Fin )
        -> |^| A e. J ) ) $=
      ( va wss c0 wne cfn wcel w3a ctop cint wi elpwg cv wceq sseq1 neeq1 com12
      cpw wal eleq1 3anbi123d inteq eleq1d imbi2d imbi12d wa istop2g ibi adantl
      cuni ax4 syl vtoclg 3exp com3r com4r syl6bir pm2.43a com4l pm2.43i 3imp )
      ABDZAEFZAGHZIZBJHZAKZBHZVCVDVEVGVILZVCVDVEVJLLVEVCVCVDVJVCVEVCVDVJLLZVEVC
      ABSZHZVEVKLABGMVEVCVDVMVJVCVDVEVMVJLZVCVDVEVNVMVFVJCNZBDZVOEFZVOGHZIZVGVO
      KZBHZLZLVFVJLCAVLVOAOZVSVFWBVJWCVPVCVQVDVRVEVOABPVOAEQVOAGUAUBWCWAVIVGWCV
      TVHBVOAUCUDUEUFVGVSWAVGVPVOUKBHLCTZVSWALZCTZUGZWEVGWGCJBUHUIWFWEWDWECULUJ
      UMRUNRUOUPUQURUSUTVAVBR $.
      $( [20-Apr-2012] $)
  $}

  ${
    $( If a subbase is included into a topology, so is the generated topology.
       (Contributed by FL, 20-Apr-2012.) $)
    sbincgt $p |- ( ( J e. Top /\ A C_ J ) -> ( topGen ` ( fi ` A ) ) C_ J ) $=
      ( ctop wcel wss cfi cfv ctg wceq wi tgtop ctb w3a ex sseq2 imbi2d syl5ibr
      eqcoms mpcom imp wa fibas a1i topbas adantr fitop fiss 3jca tgss syl6 wb
      ) BCDZABEZAFGZHGZBEZBHGZBIZULUMUPJZBKULUSURUMUOUQEZJULUMUNLDZBLDZUNBEZMZU
      TULUMVDULUMUAZVAVBVCVAVEAUBUCULVBUMBUDUEULUMVCBFGZBIULUMVCJZBUFULVGJBVFUL
      VGBVFIZUMUNVFEZJULUMVIABCUGNVHVCVIUMBVFUNOPQRSTUHNUNBUIUJURUPUTUMUPUTUKBU
      QBUQUOORPQST $.
      $( [25-Nov-2013] $) $( [20-Apr-2012] $)
  $}

  ${
    $d A v $.  $d J v $.  $d X v $.
    apnei.1 $e |- X = U. J $.
    $( Any point has a neighborhood.  (Contributed by FL, 15-Oct-2012.) $)
    apnei $p |- ( ( J e. Top /\ A e. X ) -> E. v v e. ( ( nei ` J ) ` { A } ) )
        $=
      ( ctop wcel wa cv csn cnei cfv wex wss snssi tpnei syl5ibcom impcom eleq1
      wrex rcla4ev syl2anc df-rex anidm exbii bitri sylib ) CFGZBDGZHZAIZBJZCKL
      LZGZAUMTZUNAMZUJDUMGZUQUOUIUHUQUIULDNUHUQBDOULCDEPQRZURUNUQADUMUKDUMSUAUB
      UOUNUNHZAMUPUNAUMUCUSUNAUNUDUEUFUG $.
      $( [15-Oct-2012] $)
  $}

  ${
    $( A neighborhood of a point can't be empty.  (Contributed by FL,
       15-Oct-2012.) $)
    npmp $p |- ( ( J e. Top /\ A e. X /\ N e. ( ( nei ` J ) ` { A } ) )
     -> N =/= (/) ) $=
      ( ctop wcel csn cnei cfv w3a c0 wne wa wn simp1 snnzg 3ad2ant2 jca 0nnei
      wi nelne2 ex 3ad2ant3 com12 3syl pm2.43i ) BEFZADFZCAGZBHIIZFZJZCKLZULUGU
      IKLZMKUJFNZULUMTULUGUNUGUHUKOUHUGUNUKADPQRUIBSULUOUMUKUGUOUMTUHUKUOUMCKUJ
      UAUBUCUDUEUF $.
      $( [15-Oct-2012] $)

  $}

