  ${
    cldifemp.1 $e |- X = U. J $.
    $( The closure of a class ` S ` is empty iff ` S ` is empty.  (Contributed
       by FL, 15-Sep-2013.) $)
    cldifemp $p |- ( ( J e. Top /\ S C_ X ) ->
      ( ( ( cls ` J ) ` S ) = (/) <-> S = (/) ) ) $=
      ( ctop wcel wss wa ccl cfv c0 wceq wi sscls sseq0 ex syl cls0 fveq2
      eqeq1d syl5ibrcom adantr impbid ) BEFZACGZHZABIJZJZKLZAKLZUFAUHGZUIUJMABC
      DNUKUIUJAUHOPQUDUJUIMUEUDUIUJKUGJZKLBRUJUHULKAKUGSTUAUBUC $.
      $( [15-Sep-2013] $)
  $}

  ${
    $d P n o $.  $d S n o $.  $d n o J $.  $d n o X $.
    neindisj2.1 $e |- X = U. J $.
    $( A point ` P ` belongs to the closure of a set ` S ` iff every
       neighborhood of ` P ` meets ` S ` .  (Contributed by FL,
       15-Sep-2013.) $)
    neindisj2 $p |- ( ( J e. Top /\ S C_ X /\ P e. X ) ->
        ( P e. ( ( cls ` J ) ` S ) <->
