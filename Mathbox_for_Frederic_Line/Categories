$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Categories
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c Cat $. $( The class of all categories. $)

  $( Extend class notation with the class of categories. $)
  ccat $a class Cat $.

  ${
    $d x d c j r f g h a $.
    $( A category is a deductive system where composition is associative, and
       identities are neutral elements.  A category is an abstraction of a
       structure (a group, a topology, an order...)  Category theory consists
       in finding new formulation of the concepts associated to those
       structures (product, substructure...) using morphisms instead of the
       belonging relation.  That trick has the interesting property that
       heterogeneous structures like topologies or groups for instance become
       comparable.  (Note: in category theory morphisms are also called
       arrows.) $)
    df-cat $a |- Cat = { x | E. d E. c E. j E. r
    ( x = <. <. d , c >. , <. j , r >. >. /\
           ( ( <. <. d , c >. , <. j , r >. >. e. Ded /\
           A. f e. dom d A. g e. dom d A. h e. dom d
              (
                ( ( d ` h ) = ( c ` g ) /\ ( d ` g ) = ( c ` f ) )
              ->
                ( h r ( g r f ) ) = ( ( h r g ) r f )
               ) ) /\
          ( A. a e. dom j A. f e. dom d ( ( c ` f ) = a ->
             ( ( j ` a ) r f ) = f ) /\
          A. a e. dom j A. f e. dom d ( ( d ` f ) = a ->
             ( f r ( j ` a ) ) = f ) ) ) ) } $.
  $}

  ${
    $d C a c d f g h j r $.  $d D a c d f g h j r $.  $d J a c d f j r $.
    $d M c d j r f $.  $d O c d j r $.  $d R a c d f g h j r $.
    $d a c d f g h j r x $.
    iscat.1 $e |- M = dom D $.
    iscat.2 $e |- O = dom J $.
    $( The predicate "is a category".  (Contributed by FL, 24-Oct-2007.) $)
    iscat $p |- ( ( ( D e. A /\ C e. B /\ J e. F ) /\ R e. G )
      -> ( <. <. D , C >. , <. J , R >. >. e. Cat
        <-> ( ( <. <. D , C >. , <. J , R >. >. e. Ded /\
           A. f e. M A. g e. M A. h e. M
              (
                ( ( D ` h ) = ( C ` g ) /\ ( D ` g ) = ( C ` f ) )
              ->
                ( h R ( g R f ) ) = ( ( h R g ) R f )
               ) ) /\
          ( A. a e. O A. f e. M ( ( C ` f ) = a ->
             ( ( J ` a ) R f ) = f ) /\
          A. a e. O A. f e. M ( ( D ` f ) = a ->
             ( f R ( J ` a ) ) = f ) ) ) ) ) $=
      ( vx vd vc vj vr cop ccat wcel cv wceq cded cfv wa co wi cdm wral wex
      cab w3a df-cat eleq2i opeq1 opeq1d eleq1d dmeq syl6eqr eleq2d fveq1
      eqeq1d anbi12d imbi1d imbi12d ralbidv2 raleqdv ralbidv opeq2 eqeq2d
      2ralbidv anbi1d opeq2d opreq1d imbi2d opreq2d opreq eqtrd eqeq12d elo
      syl5bb ) DCUBZKEUBZUBZUCUDWHQUERUEZSUEZUBZTUEZUAUEZUBZUBZUFWOUGUDZHUEZWIU
      HZGUEZWJUHZUFZWSWIUHZFUEZWJUHZUFZUIZWQWSXCWMUJZWMUJZWQWSWMUJZXCWMUJZUFZUK
