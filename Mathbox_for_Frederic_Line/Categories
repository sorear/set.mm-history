  ${
    $d f g t u v w x y z $.
    $( Structure of a category. $)
    strcat $p |- Cat C_ ( ( _V X. _V ) X. ( _V X. _V ) ) $=
      ( vx vg vf vv vu vw vz vy vt ccat cv cop wceq cded wcel cfv wa co wi cdm
      wral wex cab cvv cxp df-cat stcat eqsstri ) JAKBKZCKZLDKZEKZLLZMUMNOFKZUI
      PGKZUJPMUOUIPHKZUJPZMQUNUOUPULRULRUNUOULRUPULRMSFUITZUAGURUAHURUAQUQIKZMU
      SUKPZUPULRUPMSHURUAIUKTZUAUPUIPUSMUPUTULRUPMSHURUAIVAUAQQZQEUBDUBCUBBUBAU
      CUDUDUEZVCUEAHGFDEICBUFVBABCEDUGUH $.
      $( [26-Oct-2007] $)
  $}

  $( A category is a relation. $)
  relcat $p |- Rel Cat $=
    ( ccat wrel cvv cxp wss strcat xpss sstri df-rel mpbir ) ABACCDZEAKKDKFKKGH
    AIJ $.
    $( [26-Oct-2007] $)

  $( The domain of a category is a relation. $)
  reldcat $p |- Rel dom Cat $=
    ( ccat cdm wrel cvv cxp wss strcat dmss ax-mp dmxpid sseqtri df-rel mpbir
    ) ABZCNDDEZFNOOEZBZOAPFNQFGAPHIOJKNLM $.
    $( [26-Oct-2007] $)

  $( The range of a category is a relation. $)
  relrcat $p |- Rel ran Cat $=
    ( ccat crn wrel cvv cxp wss strcat rnss ax-mp c0 wne wceq wa vn0 pm3.2i
    xpnz mpbi rnxp sseqtri df-rel mpbir ) ABZCUBDDEZFUBUCUCEZBZUCAUDFUBUEFGAUDH
    IUCJKZUEUCLDJKZUGMUFUGUGNNODDPQUCUCRISUBTUA $.
    $( [26-Oct-2007] $)

  ${
    $d T w x y z $.
    $( A category is a deductive system. $)
    catded $p |- ( T e. Cat -> T e. Ded ) $=
      ( vz vy vx vw ccat wcel cdom_ cfv ccod_ cop cid_ co_ cded c1st c2nd wrel
      cdm crn w3a wceq relcat reldcat relrcat 3pm3.2i 11st22nd mpan eqid
      domval codval opeq12i idval cmpval syl6eqr cv wa co wi wral cati pm3.26d
      eqeltrd ) AFGZAAHIZAJIZKZALIZAMIZKZKZNVCAAOIZOIZVKPIZKZAPIZOIZVOPIZKZKZVJ
      FQZFRQZFSQZTVCAVSUAVTWAWBUBUCUDUEAFUFUGVFVNVIVRVDVLVEVMVDAVDUHZUIVEAVEUHZ
      UJUKVGVPVHVQAVGVGUHZULAVHVHUHZUMUKUKUNVCVJNGZBUOZVDICUOZVEIUAWIVDIDUOZVEI
      ZUAUPWHWIWJVHUQVHUQWHWIVHUQWJVHUQUAURBVDRZUSCWLUSDWLUSZVCWGWMUPWKEUOZUAWN
      VGIZWJVHUQWJUAURDWLUSEVGRZUSWJVDIWNUAWJWOVHUQWJUAURDWLUSEWPUSUPVEVDVHADCB
      VGWLWPEWCWDWEWFWLUHWPUHUTVAVAVB $.
      $( [26-Oct-2007] $)
  $}

  ${
    cat.1 $e |- M = dom D $.
    cat.3 $e |- D = ( dom_ ` T ) $.
    ${
      cat.2 $e |- O = dom J $.
      cat.5 $e |- J = ( id_ ` T ) $.
      ${
        $( The 1st "axiom" of a category: ` ( dom_ `` T ) ` is a mapping from
           the morphisms of ` T ` to the objects of ` T ` . $)
        domc $p |- ( T e. Cat -> D : M --> O ) $=
          ( ccat wcel cded calg wf catded dedalg doma 3syl ) BJKBLKBMKDEANBOBPA
          BCDEFGHIQR $.
          $( [2-Jan-2008] $)
      $}

      ${
        cat.4 $e |- C = ( cod_ ` T ) $.
        $( The 2nd "axiom" of a category ` ( cod_ `` T ) ` is a mapping from
           the morphisms of ` T ` to the objects of ` T ` . $)
        codc $p |- ( T e. Cat -> C : M --> O ) $=
          ( ccat wcel cded calg wf catded dedalg coda 3syl ) CLMCNMCOMEFAPCQCRA
          BCDEFGHIJKST $.
          $( [2-Jan-2008] $)
      $}

      ${
        $( The 3rd "axiom" of a category ` ( id_ `` T ) ` is a mapping from the
           objects of ` T ` to the morphisms of ` T ` . $)
        idc $p |- ( T e. Cat -> J : O --> M ) $=
          ( ccat wcel cded calg wf catded dedalg ida 3syl ) BJKBLKBMKEDCNBOBPAB
          CDEFGHIQR $.
          $( [5-Dec-2007] $)
