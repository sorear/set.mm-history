  $( Lemma for ~ 0ded . $)
  0alg $p |- <. <. (/) , (/) >. , <. (/) , (/) >. >. e. Alg $=
    ( c0 cop calg wcel wf w3a wfun cdm cxp wss crn cvv wa wb 0ex 3pm3.2i dm0
    eqcomi isalg mp2an f0 fun0 ssid xp0r 3sstr4i rn0 eqimssi mpbir2an ) AABZUIB
