  ${
    $d f g t u v w x y z $.
    $( Structure of a category.  (Contributed by FL, 26-Oct-2007.) $)
    strcat $p |- Cat C_ ( ( _V X. _V ) X. ( _V X. _V ) ) $=
      ( vx vg vf vv vu vw vz vy vt cv cop wceq cfv wa co wi cdm wral wex cvv
      ccat cded wcel cab cxp df-cat stcat eqsstri ) UAAJBJZCJZKDJZEJZKKZLUMUBUC
      FJZUIMGJZUJMLUOUIMHJZUJMZLNUNUOUPULOULOUNUOULOUPULOLPFUIQZRGURRHURRNUQIJZ
      LUSUKMZUPULOUPLPHURRIUKQZRUPUIMUSLUPUTULOUPLPHURRIVARNNZNESDSCSBSAUDTTUEZ
      VCUEAHGFDEICBUFVBABCEDUGUH $.
      $( [26-Oct-2007] $)
  $}

  $( A category is a relation.  (Contributed by FL, 26-Oct-2007.) $)
  relcat $p |- Rel Cat $=
    ( ccat wrel cvv cxp wss strcat xpss sstri df-rel mpbir ) ABACCDZEAKKDKFKKGH
    AIJ $.
    $( [26-Oct-2007] $)

  $( The domain of a category is a relation.  (Contributed by FL,
     26-Oct-2007.) $)
  reldcat $p |- Rel dom Cat $=
    ( ccat cdm wrel cvv cxp wss strcat dmss ax-mp dmxpid sseqtri df-rel mpbir )
    ABZCNDDEZFNOOEZBZOAPFNQFGAPHIOJKNLM $.
    $( [26-Oct-2007] $)

  $( The range of a category is a relation.  (Contributed by FL,
     26-Oct-2007.) $)
  relrcat $p |- Rel ran Cat $=
    ( ccat crn wrel cvv cxp wss strcat rnss ax-mp wne wceq vn0 pm3.2i xpnz mpbi
    c0 wa rnxp sseqtri df-rel mpbir ) ABZCUBDDEZFUBUCUCEZBZUCAUDFUBUEFGAUDHIUCP
    JZUEUCKDPJZUGQUFUGUGLLMDDNOUCUCRISUBTUA $.
    $( [26-Oct-2007] $)

  ${
    $d T w x y z $.
    $( A category is a deductive system.  (Contributed by FL, 26-Oct-2007.) $)
    catded $p |- ( T e. Cat -> T e. Ded ) $=
      ( vz vy vx vw ccat cfv cop c1st c2nd wrel wceq eqid opeq12i cv wa co wral
      cdm wi wcel cdom_ ccod_ cid_ co_ cded crn relcat reldcat relrcat 11st22nd
      w3a 3pm3.2i mpan domval codval idval cmpval syl6eqr cati simpld eqeltrd )
      AFUAZAAUBGZAUCGZHZAUDGZAUEGZHZHZUFVCAAIGZIGZVKJGZHZAJGZIGZVOJGZHZHZVJFKZF
