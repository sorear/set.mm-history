    $( The predicate "is a monomorphism". $)
    ismonb1 $p |- ( ( T e. Cat /\ F e. M ) -> ( F e. ( Monic ` T ) <->
      A. g e. M A. h e. M
        ( ( ( D ` g ) = ( D ` h ) /\
            ( C ` g ) = ( D ` F ) /\
            ( C ` h ) = ( D ` F ) ) ->
              ( ( F R g ) = ( F R h ) -> g = h ) ) ) ) $=
      ( ccat wcel wa cmon cfv cv wceq w3a co wi wral ismonb wb ibar adantl
      bitr4d ) DMNZGHNZOGDPQNUJERZBQFRZBQSUKAQGBQZSULAQUMSTGUKCUAGULCUASUKULSUB
      UBFHUCEHUCZOZUNHABCDEFGHIJKLUDUJUNUOUEUIUJUNUFUGUH $.
