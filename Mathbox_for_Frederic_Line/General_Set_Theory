  $( Image of a pre-image.  (Contributed by FL, 1-Jun-2008.) $)
  f2imacnv $p |- ( ( F : A -1-1-onto-> B /\ C C_ B ) ->
                 ( F " ( `' F " C ) ) = C ) $=
    ( wf1o wss ccnv cima wceq wf1 wi f1ocnv f1of1 wa imacnvcnv f1imacnv
    syl5eqr ex 3syl imp ) ABDEZCBFZDDGZCHZHZCIZUABAUCEBAUCJZUBUFKABDLBAUCMUGUBU
    FUGUBNUEUCGUDHCDUDOBACUCPQRST $.
    $( [1-Jun-2008] $)


  $( Symmetrical equality of the images and of their antecedents when the
     mapping is one to one.  (Contributed by FL, 1-Jun-2008.) $)
  oooeqim2 $p |- ( ( F : A -1-1-> B /\ X C_ A /\ Y C_ A )
     -> ( ( F " X ) = ( F " Y ) <-> X = Y ) ) $=
    ( wf1 wss w3a cima wceq ccnv wa f1imacnv ex anim12d 3impib imaeq2 wi eqtr
