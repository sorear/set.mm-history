    ${
      $d R x $.
      $( The range of a preorder equals its field. $)
      preoran $p |- ( R e. Preset -> ran R = X ) $=
        ( vx cpreset wcel cdm crn wrel ccom wss cid cuni cres w3a wceq isprs
        ibi cv wbr wral domrngref ref3w sylan2br 3adant2 syl preodom eqtr3d )
