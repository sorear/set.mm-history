  $c OrHom $.

  $( Extend class notation with the the class of all the order
     homomorphisms. $)
  cincfun $a class OrHom $.

  $c DecFun $.

  $( Extend class notation with the the class of all decreasing functions. $)
  cdecfun $a class DecFun $.

  $c OrIso $.

  $( Extend class notation with the the class of all the order isomorphisms. $)
  coriso $a class OrIso $.

  ${
    $d r s y f a b $.
    $( Order homomorphism.  Increasing function.  Also called isotone mapping,
       order preserving mapping.  Experimental. $)
    orHom-df $a |- OrHom = { <. <. r , s >. , y >. | y = { f | ( f : U. U. r
      --> U. U. s /\ A. a e. U. U. r
      A. b e. U. U. r ( a r b -> ( f ` a ) s ( f ` b ) ) ) } } $.

    $( Order isomorphism.  Experimental. $)
    orIso-df $a |- OrIso = { <. <. r , s >. , y >. | y = { f | ( f : U. U. r
      -onto-> U. U. s /\ A. a e. U. U. r
      A. b e. U. U. r ( a r b <-> ( f ` a ) s ( f ` b ) ) ) } } $.

    $( Decreasing function.  Experimental. $)
    decfun-df $a |- DecFun = { <. <. r , s >. , y >. | y = { f | ( f : U. U. r
      --> U. U. s /\ A. a e. U. U. r A. b e. U. U. r
      ( a r b -> ( f ` b ) r ( f ` a ) ) ) } } $.
  $}

  $c BndLat $.
  $( Extend class notation to include bound lattice. $)
  clbl $a class BndLat $.

  $( A bound lattice is a lattice that has a greatest and a least element. $)
  df-bnlat $a |- BndLat =
    { r e. LatRel | ( ( se ` r ) e. dom r /\ ( ge ` r ) e. dom r ) } $.


