    ${
      $d A x $.  $d R x $.
      $( A preset is reflexive. $)
      preoref22 $p |- ( ( R e. Preset /\ A e. X ) -> A R A ) $=
        ( vx cpreset wcel wbr cdm cuni wceq cid cres wss wi preodom2 preoref12
