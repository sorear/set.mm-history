    $( If ` A <_ B ` then ` A ` belongs to the field of the preset. $)
    pre1befi2 $p |- ( ( R e. Preset /\ A R B ) -> A e. X ) $=
      ( cpreset wcel wbr wa cdm wrel preorel releldm sylan syl6eleqr ) CFGZABCH
      ZIACJZDPCKQARGCLABCMNEO $.
