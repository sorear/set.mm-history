$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Order theory
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c mxl $.
  $c ge $.
  $c ub $.
  $c Preset $.

  $( Extend class notation with the class of all the preorder. $)
  cpreset $a class Preset $.

  $( Extend class notation with a function that returns the maximal elements of
     a preset. $)
  cmxl $a class mxl $.

  $( Extend class notation with a function that returns the greatest element of
     a poset. $)
  cge $a class ge $.

  $( Extend class notation with a function that returns the upper bounds of a
     part of a preset. $)
  cub $a class ub $.

  $( Define the class of all preorders.  A preorder is a transitive and
     reflexive relation. $)
  df-prs $a |- Preset = { r | ( Rel r /\ ( r o. r ) C_ r /\
              ( _I |` U. U. r ) C_ r ) } $.

  ${
    $d R r $.
    $( The predicate "is a preorder". $)
    isprs $p |- ( R e. A -> ( R e. Preset <->
       ( Rel R /\ ( R o. R ) C_ R /\ ( _I |` U. U. R ) C_ R ) ) ) $=
      ( vr cv wrel ccom wss cid cuni cres w3a cpreset wceq releq wb coeq1
      coeq2 eqtrd sseq12 mpancom unieq unieqd reseq2 syl 3anbi123d df-prs
      elab2g ) CDZEZUHUHFZUHGZHUHIZIZJZUHGZKBEZBBFZBGZHBIZIZJZBGZKCBLAUHBMZUIUP
      UKURUOVBUHBNUJUQMVCUKUROVCUJBUHFUQUHBUHPUHBBQRUJUQUHBSTUNVAMZVCUOVBOVCUMU
      TMVDVCULUSUHBUAUBUMUTHUCUDUNVAUHBSTUECUFUG $.
      $( [1-May-2011] $) $( [1-May-2011] $)
  $}

  $( A preorder is a relation. $)
  preorel $p |- ( R e. Preset -> Rel R ) $=
    ( cpreset wcel wrel ccom wss cid cuni cres w3a isprs ibi 3simp1d ) ABCZADZA
    AEAFZGAHHIAFZNOPQJBAKLM $.
    $( [18-May-2011] $) $( [18-May-2011] $)

  ${
    preoref.1 $e |- X = U. U. R $.
    $( A preorder is reflexive. $)
    preoref1 $p |- ( R e. Preset -> ( _I |` X ) C_ R ) $=
      ( cpreset wcel cid cuni cres wrel ccom wss w3a isprs ibi 3simp3d wceq
      reseq2 ax-mp syl5ss ) ADEZFAGGZHZAFBHZTAIZAAJAKZUBAKZTUDUEUFLDAMNOBUAPUCU
      BPCBUAFQRS $.
      $( [18-May-2011] $) $( [18-May-2011] $)

    ${
      $d A x $.  $d R x $.
      $( A preorder is reflexive. $)
      preoref2 $p |- ( ( R e. Preset /\ A e. X ) -> A R A ) $=
        ( vx cpreset wcel wbr cid cres wss wi preoref1 cv cuni wral ref3w wceq
        eqcomi reseq2 ax-mp sseq1i bitri eleq2i wb breq12 anidms rcla4va ex
        sylbi com12 sylbir syl imp ) BFGZACGZAABHZUOICJZBKZUPUQLZBCDMUSENZVABHZ
