        UODAUBDAUCUDUEUFABCUGUH $.
        $( [22-May-2011] $) $( [22-May-2011] $)
    $}

    $( If ` <_ ` is a preorder and ` A <_ B ` then ` A ` belongs to the field
       of the preorder. $)
    pre1befi $p |- ( ( R e. Preset /\ A R B ) -> A e. X ) $=
      ( cpreset wcel wbr cdm wceq wi preodom eqcom biimpi eleq2d imbi2d wrel
