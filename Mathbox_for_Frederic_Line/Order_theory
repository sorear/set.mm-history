    ${
      $d R x $.
      $( The range of a preset equals its field. $)
      preoran2 $p |- ( R e. Preset -> ran R = X ) $=
        ( vx cpreset wcel crn wceq wrel ccom wss cid cuni cres w3a isprs cv
        wbr wral wa cdm domrngref syl5req ref3w sylan2br 3adant2 syl6bi
        pm2.43i ) AEFZAGZBHZUIUIAIZAAJAKZLAMMZNAKZOUKEAPULUOUKUMULDQZUPARDUNSZU
        KUOULUQTAUAUJBDAUBCUCDAUDUEUFUGUH $.
        $( [22-May-2011] $)
    $}

    $( If ` A <_ B ` then ` A ` belongs to the field of the preset. $)
    pre1befi2 $p |- ( ( R e. Preset /\ A R B ) -> A e. X ) $=
      ( cpreset wcel wbr wa cdm wrel releldm preorel sylan syl6eleqr ) CFGZABCH
      ZIACJZDCKQARGPABCLCMNEO $.
      $( [23-May-2011] $)

    $( If ` A <_ B ` then ` B ` belongs to the field of the preset. $)
    pre2befi2 $p |- ( ( R e. Preset /\ B e. C /\ A R B ) -> B e. X ) $=
      ( cpreset wcel wbr w3a crn wrel wi relelrng 3exp preorel syl5com 3imp wb
      preoran2 eqcomd eleq2d 3ad2ant1 mpbird ) DGHZBCHZABDIZJBEHZBDKZHZUEUFUGUJ
      UFDLZUGUJMUEUFUKUGUJABCDNODPQRUEUFUHUJSUGUEEUIBUEUIEDEFTUAUBUCUD $.
      $( [23-May-2011] $)
  $}

  ${
    domcnvpre.1 $e |- X = dom R $.
    $( If ` R ` is a preset, its domain and the domain of its converse are
       equal. $)
    domcnvpre $p |- ( R e. Preset -> X = dom `' R ) $=
      ( cpreset wcel crn ccnv cdm preoran2 df-rn syl5reqr ) ADEAFBAGHABCIAJK $.
      $( [19-Sep-2011] $)
