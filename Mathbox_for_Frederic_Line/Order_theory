    $( If ` A <_ B ` then ` B ` belongs to the field of the preset. $)
    pre2befi2 $p |- ( ( R e. Preset /\ B e. C /\ A R B ) -> B e. X ) $=
      ( cpreset wcel wbr w3a crn wrel wi preorel relelrng 3exp syl5com 3imp wb
