    ${
      $d R x $.
      $( The domain of a preorder equals its field. $)
      preodom $p |- ( R e. Preset -> dom R = X ) $=
        ( vx cpreset wcel cdm wceq wrel ccom wss cid cuni cres w3a isprs cv
        wbr wral wa domfldref syl6eqr ref3w sylan2br 3adant2 syl6bi pm2.43i )
