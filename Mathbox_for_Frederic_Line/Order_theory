    $( If ` <_ ` is a preorder and ` A <_ B ` then ` B ` belongs to the field
       of the preorder. $)
    pre2befi $p |- ( ( R e. Preset /\ B e. C /\ A R B ) -> B e. X ) $=
      ( cpreset wcel wbr crn wceq wi preoran eqcom biimpi eleq2d imbi2d wrel
