$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Order theory
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c mxl $.
  $c mnl $.
  $c ub $.
  $c lb $.
  $c Preset $.
  $c ge $.
  $c se $.
  $c AntiDir $.

  $( Extend class notation with the class of all the presets. $)
  cpreset $a class Preset $.

  $( Extend class notation with a function that returns the maximal elements of
     a preset. $)
  cmxl $a class mxl $.

  $( Extend class notation with a function that returns the minimal elements of
     a preset. $)
  cmnl $a class mnl $.

  $( Extend class notation with a function that returns the upper bounds of a
     part of a preset. $)
  cub $a class ub $.

  $( Extend class notation with a function that returns the lower bounds of a
     part of a preset. $)
  clb $a class lb $.

  $( Extend class notation with a function that returns the greatest element of
     a poset. $)
  cge $a class ge $.

  $( Extend class notation with a function that returns the smallest element of
     a poset. $)
  cse $a class se $.

  $( Extend class notation with the class of all the anti-directions. $)
  cantidir $a class AntiDir $.

  $( Define the class of all presets.  A preset is a transitive and reflexive
     relation.  ("preset" is the short for preordered set.) $)
  df-prs $a |- Preset = { r | ( Rel r /\ ( r o. r ) C_ r /\
              ( _I |` U. U. r ) C_ r ) } $.

  ${
    $d r x y z $.
    $( An antidirected set (also called a set filtering on the left by
       Bourbaki) is a preset whose every pair of elements has a lower bound. $)
    df-antidir $a |- AntiDir = ( Preset i^i
     { r | A. x e. U. U. r A. y e. U. U. r
          E. z e. U. U. r z e. ( r lb { x , y } ) } ) $.
  $}

  ${
    $d R r $.
    $( The predicate "is a preset". $)
    isprs $p |- ( R e. A -> ( R e. Preset <->
       ( Rel R /\ ( R o. R ) C_ R /\ ( _I |` U. U. R ) C_ R ) ) ) $=
      ( vr cv wrel ccom wss cid cuni cres w3a cpreset wceq releq wb coeq1
      coeq2 eqtrd sseq12 mpancom unieq unieqd reseq2 syl 3anbi123d df-prs
      elab2g ) CDZEZUHUHFZUHGZHUHIZIZJZUHGZKBEZBBFZBGZHBIZIZJZBGZKCBLAUHBMZUIUP
      UKURUOVBUHBNUJUQMVCUKUROVCUJBUHFUQUHBUHPUHBBQRUJUQUHBSTUNVAMZVCUOVBOVCUMU
      TMVDVCULUSUHBUAUBUMUTHUCUDUNVAUHBSTUECUFUG $.
      $( [1-May-2011] $)
  $}

  $( A preset is a relation. $)
  preorel $p |- ( R e. Preset -> Rel R ) $=
    ( cpreset wcel wrel ccom wss cid cuni cres w3a isprs ibi 3simp1d ) ABCZADZA
    AEAFZGAHHIAFZNOPQJBAKLM $.
    $( [18-May-2011] $)

  ${
    $d R x $.
    $( The domain of a preset equals its field. $)
    preodom2 $p |- ( R e. Preset -> dom R = U. U. R ) $=
      ( vx cpreset wcel cdm cuni wceq wrel ccom wss cid cres w3a isprs cv wbr
      wral domfldref ref3w sylan2br 3adant2 syl6bi pm2.43i ) ACDZAEAFFZGZUDUDAH
      ZAAIAJZKUELAJZMUFCANUGUIUFUHUGBOZUJAPBUEQUFUIBARBASTUAUBUC $.
      $( [22-May-2011] $)
  $}

  ${
    preoref12.1 $e |- X = dom R $.
    $( The field of a preset is a set. $)
    ppldrels $p |- ( R e. Preset -> X e. _V ) $=
      ( cpreset wcel cdm cvv dmexg syl5eqel ) ADEAFGBADHCI $.
      $( [19-Sep-2011] $)

    $( A preset is reflexive. $)
    preoref12 $p |- ( R e. Preset -> ( _I |` X ) C_ R ) $=
      ( cpreset wcel cid cres cuni wceq cdm preodom2 syl5eq reseq2 syl wrel
      ccom wss w3a isprs ibi 3simp3d eqsstrd ) ADEZFBGZFAHHZGZAUCBUEIUDUFIUCAJU
      EBAKCLBUEFMNUCAOZAAPAQZUFAQZUCUGUHUIRDASTUAUB $.
      $( [18-May-2011] $)

    ${
      $d A x $.  $d R x $.
      $( A preset is reflexive. $)
      preoref22 $p |- ( ( R e. Preset /\ A e. X ) -> A R A ) $=
        ( vx cpreset wcel wbr cdm cuni wceq cid cres wss wi wa eqtr cv wral
        ref3w wb breq12 anidms rcla4va expcom sylbir reseq2 sseq1d eleq2
        imbi1d imbi12d mpbiri syl mpan preodom2 preoref12 sylc imp ) BFGZACGZAA
        BHZBIZBJJZKZLCMZBNZUTVAOZUSCVBKZVDVFVGOZDVHVDPCVCKZVICVBVCQVJVILVCMZBNZ
        AVCGZVAOZOVLERZVOBHZEVCSZVNEBTVMVQVAVPVAEAVCVOAKVPVAUAVOAVOABUBUCUDUEUF
