  $( The relation induced by a preset on a part of its field is a preset. $)
  indpre $p |- ( ( R e. Preset /\ A e. B )
        -> ( R i^i ( A X. A ) ) e. Preset ) $=
    ( wcel cpreset cxp cin sqpre int2pre sylan2 ) ABDCEDAAFZEDCKGEDABHCKIJ $.
    $( [28-Dec-2011] $)

  $( A partial order is a preset. $)
  posprs $p |- PosetRel C_ Preset $=
    ( vr cv wrel ccom wss ccnv cin cid cuni cres wceq w3a cab cps cpreset
    inss1 sseq1 mpbiri eqcoms 3anim3i ss2abi df-ps df-prs 3sstr4i ) ABZCZUEUEDU
    EEZUEUEFZGZHUEIIJZKZLZAMUFUGUJUEEZLZAMNOULUNAUKUMUFUGUMUJUIUJUIKUMUIUEEUEUH
    PUJUIUEQRSTUAAUBAUCUD $.
    $( [1-May-2011] $)

  $( A poset is a preset. $)
  posispre $p |- ( A e. PosetRel -> A e. Preset ) $=
    ( cps cpreset posprs sseli ) BCADE $.
    $( [19-Sep-2011] $)

  $( The empty set is a poset. $)
  empos $p |- (/) e. PosetRel $=
    ( c0 cps wcel wrel ccom wss ccnv cin cid cuni cres wceq cvv w3a wb 0ex
    isps ax-mp rel0 co01 eqimssi cnv0 ineq2i inidm uni0 unieqi eqtri reseq2i
