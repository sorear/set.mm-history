  $( A category belongs to the set of its subcategories. $)
  catsbc $p |- ( T e. Cat -> T e. ( SubCat ` T ) ) $=
    ( ccat wcel csubc cfv cid_ wss cdom_ ccod_ wa co_ w3a ssid pm3.2i 3pm3.2i
    wb eqid issubcatb anidms mpbiri ) ABCZAADECZAFEZUCGZAHEZUEGZAIEZUGGZJZAKEZU
    JGZLZUDUIUKUCMUFUHUEMUGMNUJMOUAUBULPUGUEUJAAUCUEQUGQUJQUCQRST $.
    $( [6-Oct-2009] $) $( [17-Sep-2009] $)

  ${
    obsubc2.1 $e |- O1 = dom ( id_ ` T ) $.
    obsubc2.2 $e |- O2 = dom ( id_ ` U ) $.
    $( The objects of a subcategory are a subset of those of the
       supercategory.  JFM CAT_2 th. 11 . $)
    obsubc2 $p |- ( U e. ( SubCat ` T ) -> O2 C_ O1 ) $=
      ( ccat wcel csubc cfv wss besubbeca wa cid_ cdm cdom_ ccod_ co_ w3a eqid
      issubcata dmss 3ad2ant1 adantl syl6bi imp 3sstr4g mpancom ) AGHZBAIJHZDCK
      ABLUIUJMBNJZOZANJZOZDCUIUJULUNKZUIUJBGHZUKUMKZBPJAPJZKBQJAQJZKMZBRJARJZKZ
      SZMUOUSURVAABUMURTUSTVATUMTUAVCUOUPUQUTUOVBUKUMUBUCUDUEUFFEUGUH $.
      $( [6-Oct-2009] $) $( [17-Sep-2009] $)
  $}

  ${
    idsubc.1 $e |- I1 = ( id_ ` T ) $.
    idsubc.2 $e |- I2 = ( id_ ` U ) $.
    $( The identity function of a subcategory is a subset of the identity
       function of the supercategory. $)
    idsubc $p |- ( U e. ( SubCat ` T ) -> I2 C_ I1 ) $=
      ( ccat wcel csubc cfv wss besubbeca wa cid_ cdom_ ccod_ co_ w3a eqid
      issubcata sseq2i biimpi 3ad2ant1 adantl syl6bi imp 3sstr4g mpancom ) AGHZ
      BAIJHZDCKABLUIUJMBNJZANJZDCUIUJUKULKZUIUJBGHZUKCKZBOJAOJZKBPJAPJZKMZBQJAQ
      JZKZRZMUMUQUPUSABCUPSUQSUSSETVAUMUNUOURUMUTUOUMCULUKEUAUBUCUDUEUFFEUGUH
      $.
      $( [6-Oct-2009] $) $( [17-Sep-2009] $)
  $}

  ${
    domsubc.1 $e |- D1 = ( dom_ ` T ) $.
    domsubc.2 $e |- D2 = ( dom_ ` U ) $.
    $( The domain function of a subcategory is a subset of the domain function
       of the supercategory. $)
    domsubc $p |- ( U e. ( SubCat ` T ) -> D2 C_ D1 ) $=
      ( ccat wcel csubc cfv wss besubbeca wa cdom_ cid_ ccod_ co_ w3a eqid
      issubcata simpr2l syl6bi imp 3sstr4g mpancom ) AGHZBAIJHZDCKABLUFUGMBNJZA
      NJZDCUFUGUHUIKZUFUGBGHZBOJAOJZKZUJBPJAPJZKZMBQJAQJZKZRMUJUNUIUPABULUISUNS
      UPSULSTUJUOUMUQUKUAUBUCFEUDUE $.
