  $( The closed sets of the topology ` { (/) } ` .  (Contributed by FL,
     5-Jan-2009.) $)
  clsingemp $p |- ( Clsd ` { (/) } ) = { (/) } $=
    ( c0 cpw csn wceq ccld cfv pw0 0ex dtopcl eqcomi fveq2 3eqtr4a eqcoms ax-mp
    id ) ABZACZDQEFZQDZGSQPQPDZPEFZPRQPUAAHIJQPEKTOLMN $.
    $( [5-Jan-2009] $)

