      UHUIUJUKULVLVNUMUNVLWDMWEVLWDWEVLVQVTWEWCVOWEVPBKUOUPUQULVLWDUMUNWFVMFUDZ
      KHZGUDZLHZMZWKWMWMNOZWMPOZHZWKQRZWMWMPOZHZSZFGUEZHWDVNXBVOWNMZBWQHZWAWTHZ
      SWDFGBCTAWKBURZWOXDWRXEXAXFXGWLVOWNWKBKUSUTWKBWQUSXGWSWAWTWKBQVAVBVCWMCUR
      ZXDVQXEVTXFWCXHWNVPVOWMCLUSVDXHWQVSBXHWPVRWMCPXHWMCWMCNXHVEZXIVFXIVFVGXHW
      TWBWAXHWMCWMCPXIXIVFVGVCVHJXCVMFGUGVIVJVK $.
      $( [18-Apr-2010] $)
  $}

  ${
    topgrpi.1 $e |- G = ( 1st ` K ) $.
    ${
      topgrpi.2 $e |- J = ( 2nd ` K ) $.
      ${
        $d G g j $.  $d J g j $.  $d K g j $.  $d N g j $.
        topgrpi.3 $e |- N = ( inv ` G ) $.
        $( "Axioms" of a topological group. $)
        topgrpi $p |- ( K e. TopGrp -> ( ( G e. Grp /\ J e. Top ) /\
          G e. ( ( J tX J ) Cn J ) /\ N e. ( J Cn J ) ) ) $=
