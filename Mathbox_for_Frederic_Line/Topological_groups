      QURUSUTVAVBABGCDIHKJNVCVDABCDEFGHJKLMVEVF $.
      $( [1-Jan-2011] $) $( [30-Dec-2010] $)
    $( In a topological group, the product of an open set by another set is
       open .  (Contributed by FL, 1-Jan-2011.)  (Revised by Mario Carneiro,
       15-Dec-2013.) $)
    tpgprop2 $p |- ( ( K e. TopGrp /\ A e. ~P X /\ O e. J )
        -> ( O .t A ) e. J ) $=
      ( vy wcel cpw co wceq 3ad2ant1 ctopgrp w3a csn ciun cgr cdm topgrpgrp wss
      cuni pwuni a1i crn topgrpbs syl5eq pweq syl sseqtr4d ssel2 sylan grporndm
