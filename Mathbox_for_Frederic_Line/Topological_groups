$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Topological groups
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c TopGrp $.

  $( Extend class notation with the class of all topological groups. $)
  ctopgrp $a class TopGrp $.

  ${
    $d g j $.
    $( Define the class of all topological groups.  A topological group is a
       group whose operation and inverse function are continuous. $)
    df-topgrp $a |- TopGrp = { <. g , j >. | ( ( g e. GrpOp /\ j e. Top ) /\
         g e. ( ( j tX j ) Cn j ) /\ ( inv ` g ) e. ( j Cn j ) ) } $.
  $}

  ${
    $d G g j $.  $d J g j $.  $d x y $.
    $( The predicate "is a topological group".  Bourbaki TG III.1 (Contributed
       by FL, 18-Apr-2010.) $)
    istopgrp $p |- ( J e. A -> ( <. G , J >. e. TopGrp <->
      ( ( G e. GrpOp /\ J e. Top ) /\ G e. ( ( J tX J ) Cn J )
        /\ ( inv ` G ) e. ( J Cn J ) ) ) ) $=
      ( vx vy vg vj wcel ctopgrp cgr ctop wa ctx co ccn cgn cfv w3a cv eleq1 id
      cop cvv wbr df-br wrel copab relopab df-topgrp releqi mpbir sylbir adantl
      brrelexi simpl jca elex adantr 3ad2ant1 eleq2i anbi1d fveq2 eleq1d anbi2d
      wceq 3anbi123d oveq12d eleq2d opelopabg syl5bb pm5.21nd ) CAHZBCUBZIHZBJH
      ZCKHZLZBCCMNZCONZHZBPQZCCONZHZRZBUCHZVLLZVLVNLWEVLVNWEVLVNBCIUDWEBCIUEBCI
      IUFDSZJHESZKHLWGWHWHMNWHONHWGPQWHWHONHRZDEUGZUFWIDEUHIWJDEUIUJUKUNULUMVLV
      NUOUPVLWDLWEVLWDWEVLVQVTWEWCVOWEVPBJUQURUSUMVLWDUOUPVNVMFSZJHZGSZKHZLZWKW
      MWMMNZWMONZHZWKPQZWMWMONZHZRZFGUGZHWFWDIXCVMFGUIUTXBVOWNLZBWQHZWAWTHZRWDF
      GBCUCAWKBVEZWOXDWRXEXAXFXGWLVOWNWKBJTVAWKBWQTXGWSWAWTWKBPVBVCVFWMCVEZXDVQ
      XEVTXFWCXHWNVPVOWMCKTVDXHWQVSBXHWPVRWMCOXHWMCWMCMXHUAZXIVGXIVGVHXHWTWBWAX
      HWMCWMCOXIXIVGVHVFVIVJVK $.
      $( [18-Apr-2010] $)
  $}

  ${
    topgrpi.1 $e |- G = ( 1st ` K ) $.
    ${
      topgrpi.2 $e |- J = ( 2nd ` K ) $.
      ${
        $d G g j $.  $d J g j $.  $d K g j $.  $d N g j $.
        topgrpi.3 $e |- N = ( inv ` G ) $.
        $( "Axioms" of a topological group.  (Contributed by FL,
           18-Apr-2010.) $)
        topgrpi $p |- ( K e. TopGrp -> ( ( G e. GrpOp /\ J e. Top ) /\
          G e. ( ( J tX J ) Cn J ) /\ N e. ( J Cn J ) ) ) $=
          ( vg vj wcel cgr ctop wa ctx co ccn cgn cfv w3a wceq ctopgrp cv copab
          df-topgrp eleq2i wb eqtr3 eqcomd eleq1 anbi1d fveq2 syl6eqr 3anbi123d
          c1st eleq1d syl mpan c2nd anbi2d oveq12 anidms mpancom eleq2d elopabi
          sylbi ) CUAJCHUBZKJZIUBZLJZMZVFVHVHNOZVHPOZJZVFQRZVHVHPOZJZSZHIUCZJAK
          JZBLJZMZABBNOZBPOZJZDBBPOZJZSZUAVRCHIUDUEVQVSVIMZAVLJZDVOJZSZWGHICACU
          NRZTZVFWLTZVQWKUFZEWMWNMZVFATZWOWPAVFAVFWLUGUHWQVJWHVMWIVPWJWQVGVSVIV
          FAKUIUJVFAVLUIWQVNDVOWQVNAQRDVFAQUKGULUOUMUPUQBCURRZTZVHWRTZWKWGUFZFW
          SWTMZVHBTZXAXBBVHBVHWRUGUHXCWHWAWIWDWJWFXCVIVTVSVHBLUIUSXCVLWCAVKWBTZ
          XCVLWCTXCXDVHBVHBNUTVAVKWBVHBPUTVBVCXCVOWEDXCVOWETVHBVHBPUTVAVCUMUPUQ
          VDVE $.
          $( [18-Apr-2010] $)

        $( In a topological group, the inverse function is continuous.
           (Contributed by FL, 21-Jun-2010.) $)
        topgrpinv $p |- ( K e. TopGrp -> N e. ( J Cn J ) ) $=
          ( ctopgrp wcel cgr ctop wa ctx co ccn topgrpi simp3d ) CHIAJIBKILABBM
          NBONIDBBONIABCDEFGPQ $.
          $( [21-Jun-2010] $)
      $}

      $( In a topological group, the base set of the topology and the base set
         of the group are equal.  (Contributed by FL, 21-Jun-2010.) $)
      topgrpbs $p |- ( K e. TopGrp -> ran G = U. J ) $=
        ( ctopgrp wcel cgr ctop wa ctx co ccn w3a wceq eqid wi wf fdm syl eqtr2
        cgn cfv crn cuni topgrpi wf1o grpoinvf cnf f1of cdm syl2im syl5com 3exp
        ex pm2.43i com3r imp31 3adant2 ) CFGAHGZBIGZJZABBKLBMLGZAUBUCZBBMLGZNAU
        DZBUEZOZABCVDDEVDPZUFVBVEVHVCUTVAVEVHUTVFVFVDUGZVAVEVHQQAVDVFVFPVIUHVAV
        EVJVHVAVEVJVHQZQVAVAVEVKVAVAVENVGVGVDRZVJVHVDBBVGVGVGPZVMUIVJVFVFVDRZVL
        VHQVFVFVDUJVNVDUKZVFOZVLVOVGOZVHVFVFVDSVGVGVDSVPVQVHVOVFVGUAUOULTUMUNUP
        UQTURUST $.
        $( [21-Jun-2010] $)

      $( In a topological group, the operation ` G ` is continuous.
         (Contributed by FL, 21-Jun-2010.) $)
      topgrpcn $p |- ( K e. TopGrp -> G e. ( ( J tX J ) Cn J ) ) $=
        ( ctopgrp wcel cgr ctop wa ctx co ccn cgn cfv eqid topgrpi simp2d ) CFG
        AHGBIGJABBKLBMLGANOZBBMLGABCSDESPQR $.
        $( [21-Jun-2010] $)
    $}

    $( In a topological group, the operation ` G ` is a group.  (Contributed by
       FL, 21-Jun-2010.) $)
    topgrpgrp $p |- ( K e. TopGrp -> G e. GrpOp ) $=
      ( ctopgrp wcel cgr c2nd cfv ctop wa ctx co ccn eqid topgrpi simp1d simpld
      cgn ) BDEZAFEZBGHZIEZSTUBJAUAUAKLUAMLEARHZUAUAMLEAUABUCCUANUCNOPQ $.
      $( [21-Jun-2010] $)
  $}

  ${
    topgrptop.1 $e |- J = ( 2nd ` K ) $.
    $( In a topological group, ` J ` is a topology.  (Contributed by FL,
       21-Jun-2010.) $)
    topgrptop $p |- ( K e. TopGrp -> J e. Top ) $=
      ( ctopgrp wcel c1st cfv cgr ctop wa ctx co ccn eqid topgrpi simp1d simprd
      cgn ) BDEZBFGZHEZAIEZSUAUBJTAAKLAMLETRGZAAMLETABUCTNCUCNOPQ $.
      $( [21-Jun-2010] $)
  $}

  ${
    extopgrp.1 $e |- A e. _V $.
    $( The trivial topological group.  (Contributed by FL, 21-Jun-2010.)
       (Revised by Mario Carneiro, 15-Dec-2013.) $)
    extopgrp $p |- <. { <. <. A , A >. , A >. } , { (/) , { A } } >.
       e. TopGrp $=
      ( cop csn c0 cpr ctopgrp wcel cgr wa ctx co ccn cgn cfv cvv w3a ax-mp cun
      ctop wb prex istopgrp elexi grposn indistop pm3.2i cxp wf txtopi wfo opex
      crn rnsnop eqcomi grpofo fof syl cuni 0ex snex unipr uncom 3eqtrri txunii
      un0 mapudiscn mp2an cid cres ginvsn idcn eqeltri mpbir3an ) AACZACDZEADZF
      ZCGHZVPIHZVRTHZJZVPVRVRKLZVRMLHZVPNOZVRVRMLZHZVRPHVSWBWDWGQUAEVQUBPVPVRUC
      RVTWAAAPBUDZUEZVQUFZUGWCTHVQVQUHZVQVPUIZWDVRVRWJWJUJVTWLWIVTWKVQVPUKWLVPV
      QVPUMVQVOAAAULWHUNUOUPWKVQVPUQURRWKVQVPWCVRVRVQVQWJWJVRUSEVQSVQESVQEVQUTA
      VAZVBEVQVCVQVFVDZWNVEWMVGVHWEVIVQVJZWFABVKWAWOWFHWJVRVQWNVLRVMVN $.
      $( [15-Dec-2013] $) $( [21-Jun-2010] $)
  $}

  ${
    $d x y z G $.  $d x y z J $.  $d x y z K $.
    topgrpsubcn.1 $e |- G = ( 1st ` K ) $.
    topgrpsubcn.2 $e |- J = ( 2nd ` K ) $.
    topgrpsubcn.3 $e |- D = ( /g ` G ) $.
    $( In a topological group, the "subtraction" (or "division") is
       continuous.  Bourbaki TG III.1 axiom GT' (Contributed by FL,
       21-Jun-2010.)  (The proof was shortened by Mario Carneiro,
       30-May-2014.) $)
    topgrpsubcn $p |- ( K e. TopGrp -> D e. ( ( J tX J ) Cn J ) ) $=
      ( vx vy ctopgrp wcel cuni cv cgn cfv co cmpt2 ctx wceq eqid ccn topgrpgrp
      crn cgr grpodivfval syl topgrpbs mpt2eq12 syl2anc eqtrd cnmpt1st cnmpt2nd
      topgrptop topgrpinv cnmpt21f topgrpcn cnmpt22f eqeltrd ) DJKZAHICLZUTHMZI
      MZBNOZOZBPZQZCCRPCUAPUSAHIBUCZVGVEQZVFUSBUDKAVHSBDEUBHIABVCVGVGTVCTZGUEUF
      USVGUTSZVJVHVFSBCDEFUGZVKHIVGVGUTUTVEUHUIUJUSHIVAVDBCCCCCUTUTUTUTUTTZVLCD
      FUMZVMVMVMUSHICCUTUTVLVLVMVMUKUSHIVBVCCCCCUTUTVLVLVMVMVMVMUSHICCUTUTVLVLV
      MVMULBCDVCEFVIUNUOVLVLVMBCDEFUPUQUR $.
      $( [23-Dec-2013] $) $( [21-Jun-2010] $)
  $}

  ${
    $d x y A $.  $d y F $.  $d x y G $.  $d x y J $.  $d x y K $.  $d x y X $.
    $d y O $.
    trhom.1 $e |- X = ran G $.
    trhom.2 $e |- F = ( x e. X |-> ( x G A ) ) $.
    trhom.3 $e |- G = ( 1st ` K ) $.
    trhom.4 $e |- J = ( 2nd ` K ) $.
    $( In a topological group a right translation is a homeomorphism.  Bourbaki
       TG III.2 (Contributed by FL, 21-Jun-2010.)  (The proof was shortened by
       Mario Carneiro, 30-May-2014.) $)
    trhom $p |- ( ( K e. TopGrp /\ A e. X ) -> F e. ( J Homeo J ) ) $=
      ( wcel wa co ccn adantr cmpt wceq syl5eq mpteq1 ctopgrp ccnv wb topgrptop
      chomeosm ctop hmeobc syl2anc cuni crn topgrpbs eqid cnmptid simpr eleqtrd
      cv syl cnmptc ctx topgrpcn cnmpt12f eqeltrd cgs cfv topgrpgrp trinv sylan
      cgr eqtrd topgrpsubcn mpbir2and ) FUALZBGLZMZCEEUENLZCEEONZLZCUBZVPLZVNEU
      FLZVTVOVQVSMUCVLVTVMEFKUDPZWACEEUGUHVNCAEUIZAUPZBDNZQZVPVNCAGWDQZWEIVNGWB
      RZWFWERVNGDUJZWBHVLWHWBRVMDEFJKUKPSZAGWBWDTUQSVNAWCBDEEEEWBWBWBULZWAWJWAW
      AVNAEWBWJWAUMZVNABEEWBWBWJWAWJWAVNBGWBVLVMUNWIUOURZWAVLDEEUSNEONZLVMDEFJK
      UTPVAVBVNVRAWBWCBDVCVDZNZQZVPVNVRAGWOQZWPVLDVHLVMVRWQRDFJVEABWNCDGIHWNULZ
      VFVGVNWGWQWPRWIAGWBWOTUQVIVNAWCBWNEEEEWBWBWJWAWJWAWAWKWLWAVLWNWMLVMWNDEFJ
      KWRVJPVAVBVK $.
      $( [21-Jun-2010] $)
    $( In a topological group, a converse of a right translation is a
       homeomorphism.  Bourbaki TG III.2 (Contributed by FL, 30-Dec-2010.) $)
    cnvtrhom $p |- ( ( K e. TopGrp /\ A e. X )
        -> `' F e. ( J Homeo J ) ) $=
      ( ctopgrp wcel wa ctop chomeosm co ccnv topgrptop jca trhom cnvhmpha sylc
      adantr ) FLMZBGMZNEOMZUGNZCEEPQZMCRUIMUEUHUFUEUGUGEFKSZUJTUDABCDEFGHIJKUA
      CEEUBUC $.
      $( [30-Dec-2010] $)
    $( In a topological group the image of an open set by a right translation
       is an open set.  Bourbaki TG III.2 (Contributed by FL, 30-Dec-2010.) $)
    trhom2 $p |- ( ( K e. TopGrp /\ A e. X /\ O e. J )
       -> ( F " O ) e. J ) $=
      ( vy ctopgrp wcel cima chomeosm co wa wi trhom cv wral ctop topgrptop jca
      adantr hmeocnb syl wceq imaeq2 eleq1d rcla4cv syl6com mpcom 3impia ) FNOZ
      BHOZGEOZCGPZEOZCEEQROZUQURSZUSVATZABCDEFHIJKLUAVCVBCMUBZPZEOZMEUCZVDVCEUD
