$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Topological groups
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c TopGrp $.

  $( Extend class notation with the class of all topological groups. $)
  ctopgrp $a class TopGrp $.

  ${
    $d g j $.
    $( Define the class of all topological groups.  A topological group is a
       group whose operation and inverse function are continuous. $)
    df-topgrp $a |- TopGrp = { <. g , j >. | ( ( g e. GrpOp /\ j e. Top ) /\
         g e. ( ( j tX j ) Cn j ) /\ ( inv ` g ) e. ( j Cn j ) ) } $.
  $}

  ${
    $d G g j $.  $d J g j $.  $d x y $.
    $( The predicate "is a topological group".  Bourbaki TG III.1 (Contributed
       by FL, 18-Apr-2010.) $)
    istopgrp $p |- ( J e. A -> ( <. G , J >. e. TopGrp <->
      ( ( G e. GrpOp /\ J e. Top ) /\ G e. ( ( J tX J ) Cn J )
        /\ ( inv ` G ) e. ( J Cn J ) ) ) ) $=
      ( vx vy vg vj wcel ctopgrp cgr ctop wa ctx co ccn cgn cfv w3a cv eleq1 id
      cop cvv wbr df-br wrel copab relopab df-topgrp releqi mpbir sylbir adantl
      brrelexi simpl elisset adantr 3ad2ant1 eleq2i wceq anbi1d fveq2 3anbi123d
      jca eleq1d anbi2d opreq12d eleq2d opelopabg syl5bb pm5.21nd ) CAHZBCUBZIH
      ZBJHZCKHZLZBCCMNZCONZHZBPQZCCONZHZRZBUCHZVLLZVLVNLWEVLVNWEVLVNBCIUDWEBCIU
      EBCIIUFDSZJHESZKHLWGWHWHMNWHONHWGPQWHWHONHRZDEUGZUFWIDEUHIWJDEUIUJUKUNULU
      MVLVNUOVDVLWDLWEVLWDWEVLVQVTWEWCVOWEVPBJUPUQURUMVLWDUOVDVNVMFSZJHZGSZKHZL
      ZWKWMWMMNZWMONZHZWKPQZWMWMONZHZRZFGUGZHWFWDIXCVMFGUIUSXBVOWNLZBWQHZWAWTHZ
      RWDFGBCUCAWKBUTZWOXDWRXEXAXFXGWLVOWNWKBJTVAWKBWQTXGWSWAWTWKBPVBVEVCWMCUTZ
      XDVQXEVTXFWCXHWNVPVOWMCKTVFXHWQVSBXHWPVRWMCOXHWMCWMCMXHUAZXIVGXIVGVHXHWTW
      BWAXHWMCWMCOXIXIVGVHVCVIVJVK $.
      $( [18-Apr-2010] $)
  $}

  ${
    topgrpi.1 $e |- G = ( 1st ` K ) $.
    ${
      topgrpi.2 $e |- J = ( 2nd ` K ) $.
      ${
        $d G g j $.  $d J g j $.  $d K g j $.  $d N g j $.
        topgrpi.3 $e |- N = ( inv ` G ) $.
        $( "Axioms" of a topological group.  (Contributed by FL,
           18-Apr-2010.) $)
        topgrpi $p |- ( K e. TopGrp -> ( ( G e. GrpOp /\ J e. Top ) /\
          G e. ( ( J tX J ) Cn J ) /\ N e. ( J Cn J ) ) ) $=
          ( vg vj wcel cgr ctop wa ctx co ccn cgn cfv w3a wceq ctopgrp cv copab
          df-topgrp eleq2i wb eqtr3 eqcomd eleq1 anbi1d fveq2 syl6eqr 3anbi123d
          c1st eleq1d syl mpan c2nd anbi2d opreq12 anidms mpancom elopabi sylbi
          eleq2d ) CUAJCHUBZKJZIUBZLJZMZVFVHVHNOZVHPOZJZVFQRZVHVHPOZJZSZHIUCZJA
          KJZBLJZMZABBNOZBPOZJZDBBPOZJZSZUAVRCHIUDUEVQVSVIMZAVLJZDVOJZSZWGHICAC
          UNRZTZVFWLTZVQWKUFZEWMWNMZVFATZWOWPAVFAVFWLUGUHWQVJWHVMWIVPWJWQVGVSVI
          VFAKUIUJVFAVLUIWQVNDVOWQVNAQRDVFAQUKGULUOUMUPUQBCURRZTZVHWRTZWKWGUFZF
          WSWTMZVHBTZXAXBBVHBVHWRUGUHXCWHWAWIWDWJWFXCVIVTVSVHBLUIUSXCVLWCAVKWBT
          ZXCVLWCTXCXDVHBVHBNUTVAVKWBVHBPUTVBVEXCVOWEDXCVOWETVHBVHBPUTVAVEUMUPU
          QVCVD $.
          $( [18-Apr-2010] $)

        $( In a topological group, the inverse function is continuous.
           (Contributed by FL, 21-Jun-2010.) $)
        topgrpinv $p |- ( K e. TopGrp -> N e. ( J Cn J ) ) $=
          ( ctopgrp wcel cgr ctop wa ctx co ccn topgrpi simp3d ) CHIAJIBKILABBM
          NBONIDBBONIABCDEFGPQ $.
          $( [21-Jun-2010] $)
      $}

      $( In a topological group, the base set of the topology and the base set
         of the group are equal.  (Contributed by FL, 21-Jun-2010.) $)
      topgrpbs $p |- ( K e. TopGrp -> ran G = U. J ) $=
        ( ctopgrp wcel cgr ctop wa ctx co ccn w3a wceq eqid wi wf fdm syl eqtr2
        cgn cfv crn cuni topgrpi wf1o grpinvf cnf cdm ex syl2im syl5com pm2.43i
        f1of 3exp com3r imp31 3adant2 ) CFGAHGZBIGZJZABBKLBMLGZAUBUCZBBMLGZNAUD
        ZBUEZOZABCVDDEVDPZUFVBVEVHVCUTVAVEVHUTVFVFVDUGZVAVEVHQQAVDVFVFPVIUHVAVE
        VJVHVAVEVJVHQZQVAVAVEVKVAVAVENVGVGVDRZVJVHVDBBVGVGVGPZVMUIVJVFVFVDRZVLV
        HQVFVFVDUOVNVDUJZVFOZVLVOVGOZVHVFVFVDSVGVGVDSVPVQVHVOVFVGUAUKULTUMUPUNU
        QTURUST $.
        $( [21-Jun-2010] $)

      $( In a topological group, the operation ` G ` is continuous.
         (Contributed by FL, 21-Jun-2010.) $)
      topgrpcn $p |- ( K e. TopGrp -> G e. ( ( J tX J ) Cn J ) ) $=
        ( ctopgrp wcel cgr ctop wa ctx co ccn cgn cfv eqid topgrpi simp2d ) CFG
        AHGBIGJABBKLBMLGANOZBBMLGABCSDESPQR $.
        $( [21-Jun-2010] $)
    $}

    $( In a topological group, the operation ` G ` is a group.  (Contributed by
       FL, 21-Jun-2010.) $)
    topgrpgrp $p |- ( K e. TopGrp -> G e. GrpOp ) $=
      ( ctopgrp wcel cgr c2nd cfv ctop wa ctx co ccn eqid topgrpi simp1d simpld
      cgn ) BDEZAFEZBGHZIEZSTUBJAUAUAKLUAMLEARHZUAUAMLEAUABUCCUANUCNOPQ $.
      $( [21-Jun-2010] $)
  $}

  ${
    topgrptop.1 $e |- J = ( 2nd ` K ) $.
    $( In a topological group, ` J ` is a topology.  (Contributed by FL,
       21-Jun-2010.) $)
    topgrptop $p |- ( K e. TopGrp -> J e. Top ) $=
      ( ctopgrp wcel c1st cfv cgr ctop wa ctx co ccn eqid topgrpi simp1d simprd
      cgn ) BDEZBFGZHEZAIEZSUAUBJTAAKLAMLETRGZAAMLETABUCTNCUCNOPQ $.
      $( [21-Jun-2010] $)
  $}

  ${
    extopgrp.1 $e |- A e. _V $.
    $( The trivial topological group.  (Contributed by FL, 21-Jun-2010.)
       (Revised by Mario Carneiro, 15-Dec-2013.) $)
    extopgrp $p |- <. { <. <. A , A >. , A >. } , { (/) , { A } } >.
       e. TopGrp $=
      ( cop csn c0 cpr ctopgrp wcel cgr ctop wa ctx co ccn cvv ax-mp mp2an cuni
      eqcomi cun cgn cfv w3a wb prex istopgrp elisseti grpsn indistop pm3.2i wf
      cxp eqid txtop wfo crn opex rnsnop grpfo fof syl wceq 0ex unipr uncom un0
      snex 3eqtrri txuni mapudiscn cid cres ginvsn idcn eqeltri mpbir3an ) AACZ
      ACDZEADZFZCGHZVRIHZVTJHZKZVRVTVTLMZVTNMHZVRUAUBZVTVTNMZHZVTOHWAWDWFWIUCUD
      EVSUEOVRVTUFPWBWCAAOBUGZUHZVSUIZUJWEJHZVSVSULZVSVRUKZWFWCWCWMWLWLVTVTWEWE
      UMZUNQWBWOWKWBWNVSVRUOWOVRVSVRUPVSVQAAAUQWJURSUSWNVSVRUTVAPWNVSVRWEWERZWN
      WCWCWQWNVBWLWLVTVTWEVSVSWPVTREVSTVSETVSEVSVCAVGZVDEVSVEVSVFVHZWSVIQSWRVJQ
      WGVKVSVLZWHABVMWCWTWHHWLVTVSWSVNPVOVP $.
      $( [15-Dec-2013] $) $( [21-Jun-2010] $)
  $}

  ${
    topgrpsubcn.1 $e |- G = ( 1st ` K ) $.
    topgrpsubcn.2 $e |- J = ( 2nd ` K ) $.
    topgrpsubcn.3 $e |- D = ( /g ` G ) $.
    ${
      $d G u x y z $.  $d J u x y $.
      topgrpsubcn.4 $e |- K e. TopGrp $.
      $( Lemma for ~ topgrpsubcn . $)
      topgrpsubcnlem $p |- D e. ( ( J tX J ) Cn J ) $=
        ( vx vy vz vu cv wcel wa co wceq eqid syl ax-mp crn cgn cfv ctx ctopgrp
