$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Topological groups
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c TopGrp $.

  $( Extend class notation with the class of all topological groups. $)
  ctopgrp $a class TopGrp $.

  ${
    $d g j $.
    $( Define the class of all topological groups.  A topological group is a
       group whose operation and inverse function are continuous. $)
    df-topgrp $a |- TopGrp = { <. g , j >. | ( ( g e. GrpOp /\ j e. Top ) /\
         g e. ( ( j tX j ) Cn j ) /\ ( inv ` g ) e. ( j Cn j ) ) } $.
  $}

  ${
    $d G g j $.  $d J g j $.  $d x y $.
    $( The predicate "is a topological group".  Bourbaki TG III.1 (Contributed
       by FL, 18-Apr-2010.) $)
    istopgrp $p |- ( J e. A -> ( <. G , J >. e. TopGrp <->
      ( ( G e. GrpOp /\ J e. Top ) /\ G e. ( ( J tX J ) Cn J )
        /\ ( inv ` G ) e. ( J Cn J ) ) ) ) $=
      ( vx vy vg vj wcel cop ctopgrp cgr ctop wa ctx co ccn cgn cfv w3a cvv
      wbr df-br wrel cv copab relopab df-topgrp releqi mpbir brrelexi sylbir
      adantl simpl jca elisset adantr 3ad2ant1 eleq2i wceq eleq1 anbi1d fveq2
      eleq1d 3anbi123d anbi2d id opreq12d eleq2d opelopabg syl5bb pm5.21nd ) CA
      HZBCIZJHZBKHZCLHZMZBCCNOZCPOZHZBQRZCCPOZHZSZBTHZVLMZVLVNMWEVLVNWEVLVNBCJU
      AWEBCJUBBCJJUCDUDZKHEUDZLHMWGWHWHNOWHPOHWGQRWHWHPOHSZDEUEZUCWIDEUFJWJDEUG
      UHUIUJUKULVLVNUMUNVLWDMWEVLWDWEVLVQVTWEWCVOWEVPBKUOUPUQULVLWDUMUNVNVMFUDZ
      KHZGUDZLHZMZWKWMWMNOZWMPOZHZWKQRZWMWMPOZHZSZFGUEZHWFWDJXCVMFGUGURXBVOWNMZ
      BWQHZWAWTHZSWDFGBCTAWKBUSZWOXDWRXEXAXFXGWLVOWNWKBKUTVAWKBWQUTXGWSWAWTWKBQ
      VBVCVDWMCUSZXDVQXEVTXFWCXHWNVPVOWMCLUTVEXHWQVSBXHWPVRWMCPXHWMCWMCNXHVFZXI
      VGXIVGVHXHWTWBWAXHWMCWMCPXIXIVGVHVDVIVJVK $.
      $( [18-Apr-2010] $)
  $}

  ${
    topgrpi.1 $e |- G = ( 1st ` K ) $.
    ${
      topgrpi.2 $e |- J = ( 2nd ` K ) $.
      ${
        $d G g j $.  $d J g j $.  $d K g j $.  $d N g j $.
        topgrpi.3 $e |- N = ( inv ` G ) $.
        $( "Axioms" of a topological group.  (Contributed by FL,
           18-Apr-2010.) $)
        topgrpi $p |- ( K e. TopGrp -> ( ( G e. GrpOp /\ J e. Top ) /\
          G e. ( ( J tX J ) Cn J ) /\ N e. ( J Cn J ) ) ) $=
          ( vg vj ctopgrp wcel cv cgr ctop wa ctx co ccn cgn cfv w3a copab
          df-topgrp eleq2i c1st wceq wb eqtr3 eqcomd eleq1 anbi1d fveq2
          syl6eqr eleq1d 3anbi123d syl mpan c2nd anbi2d opreq12 anidms mpancom
          eleq2d elopabi sylbi ) CJKCHLZMKZILZNKZOZVFVHVHPQZVHRQZKZVFSTZVHVHRQZ
          KZUAZHIUBZKAMKZBNKZOZABBPQZBRQZKZDBBRQZKZUAZJVRCHIUCUDVQVSVIOZAVLKZDV
          OKZUAZWGHICACUETZUFZVFWLUFZVQWKUGZEWMWNOZVFAUFZWOWPAVFAVFWLUHUIWQVJWH
          VMWIVPWJWQVGVSVIVFAMUJUKVFAVLUJWQVNDVOWQVNASTDVFASULGUMUNUOUPUQBCURTZ
          UFZVHWRUFZWKWGUGZFWSWTOZVHBUFZXAXBBVHBVHWRUHUIXCWHWAWIWDWJWFXCVIVTVSV
          HBNUJUSXCVLWCAVKWBUFZXCVLWCUFXCXDVHBVHBPUTVAVKWBVHBRUTVBVCXCVOWEDXCVO
          WEUFVHBVHBRUTVAVCUOUPUQVDVE $.
          $( [18-Apr-2010] $)

        $( In a topological group, the inverse function is continuous.
           (Contributed by FL, 21-Jun-2010.) $)
        topgrpinv $p |- ( K e. TopGrp -> N e. ( J Cn J ) ) $=
          ( ctopgrp wcel cgr ctop wa ctx co ccn topgrpi simp3d ) CHIAJIBKILABBM
          NBONIDBBONIABCDEFGPQ $.
          $( [21-Jun-2010] $)
      $}

      $( In a topological group, the base set of the topology and the base set
         of the group are equal.  (Contributed by FL, 21-Jun-2010.) $)
      topgrpbs $p |- ( K e. TopGrp -> ran G = U. J ) $=
        ( ctopgrp wcel cgr ctop wa ctx co ccn cgn cfv w3a crn cuni wceq eqid
        topgrpi wf1o wi grpinvf wf cnf f1of cdm fdm eqtr2 ex syl2im syl
        syl5com 3exp pm2.43i com3r imp31 3adant2 ) CFGAHGZBIGZJZABBKLBMLGZANOZB
        BMLGZPAQZBRZSZABCVDDEVDTZUAVBVEVHVCUTVAVEVHUTVFVFVDUBZVAVEVHUCUCAVDVFVF
        TVIUDVAVEVJVHVAVEVJVHUCZUCVAVAVEVKVAVAVEPVGVGVDUEZVJVHVDBBVGVGVGTZVMUFV
        JVFVFVDUEZVLVHUCVFVFVDUGVNVDUHZVFSZVLVOVGSZVHVFVFVDUIVGVGVDUIVPVQVHVOVF
        VGUJUKULUMUNUOUPUQUMURUSUM $.
        $( [21-Jun-2010] $)

      $( In a topological group, the operation ` G ` is continuous.
         (Contributed by FL, 21-Jun-2010.) $)
      topgrpcn $p |- ( K e. TopGrp -> G e. ( ( J tX J ) Cn J ) ) $=
        ( ctopgrp wcel cgr ctop wa ctx co ccn cgn cfv eqid topgrpi simp2d ) CFG
        AHGBIGJABBKLBMLGANOZBBMLGABCSDESPQR $.
        $( [21-Jun-2010] $)
    $}

    $( In a topological group, the operation ` G ` is a group.  (Contributed by
       FL, 21-Jun-2010.) $)
    topgrpgrp $p |- ( K e. TopGrp -> G e. GrpOp ) $=
      ( ctopgrp wcel cgr c2nd cfv ctop wa ctx co ccn cgn eqid topgrpi simp1d
      simpld ) BDEZAFEZBGHZIEZSTUBJAUAUAKLUAMLEANHZUAUAMLEAUABUCCUAOUCOPQR $.
      $( [21-Jun-2010] $)
  $}

  ${
    topgrptop.1 $e |- J = ( 2nd ` K ) $.
    $( In a topological group, ` J ` is a topology.  (Contributed by FL,
       21-Jun-2010.) $)
    topgrptop $p |- ( K e. TopGrp -> J e. Top ) $=
      ( ctopgrp wcel c1st cfv cgr ctop wa ctx co ccn cgn eqid topgrpi simp1d
      simprd ) BDEZBFGZHEZAIEZSUAUBJTAAKLAMLETNGZAAMLETABUCTOCUCOPQR $.
      $( [21-Jun-2010] $)
  $}

  ${
    extopgrp.1 $e |- A e. B $.
    ${
      $d A x y $.
      $( The identity element of the trivial group.  (Contributed by FL,
         21-Jun-2010.) $)
      idtrgrp $p |- ( Id ` { <. <. A , A >. , A >. } ) = A $=
