  ${
    $d x y l p q r $.
    $( If ` p ` , ` q ` , ` r ` are three distinct colinear points then ` p `
       is between ` q ` and ` r ` or ` q ` is between ` p ` and ` r ` or ` r `
       is between ` p ` and ` q ` .  Aitken's axiom B-3.  Experimental. $)
    df-plibg3 $a |- Plibg3 = { <. x , y >. |
        A. l e. x A. p e. U. x A. q e. U. x A. r e. U. x
           ( ( ( p e. l /\ q e. l /\ r e. l ) /\
             ( p =/= q /\ q =/= r /\ p =/= r ) ) ->
             ( ( <. <. q , p >. , r >. e. y /\
                 -. <. <. p , q >. , r >. e. y /\
                 -. <. <. p , r >. , q >. e. y ) \/
               ( -. <. <. q , p >. , r >. e. y /\
                    <. <. p , q >. , r >. e. y /\
                 -. <. <. p , r >. , q >. e. y ) \/
               ( -. <. <. q , p >. , r >. e. y /\
                 -. <. <. p , q >. , r >. e. y /\
                    <. <. p , r >. , q >. e. y ) ) ) } $.
  $}

  ${
    $d B l p q r x y $.  $d L l p q r x y $.  $d P p q r x y $.
    isplibg3.1 $e |- P = U. L $.
    $( The predicate "repects Aitken's axiom B-3 of a linear
       incidence-betweenness geometry ".  See ~ df-plibg3 . $)
    isplibg3 $p |- ( ( L e. A /\ B e. C ) -> ( <. L , B >. e. Plibg3 <->
