  $( A planar incidence betweeness geometry respects the axioms ~ df-plibg0 to
     ~ df-plibg4b .  Experimental. $)
  df-plibg $a |- Plibg = ( Plibg0 i^i ( Plibg1 i^i ( Plibg2 i^i ( Plibg3
                           i^i ( Plibg4a i^i Plibg4b ) ) ) ) ) $.

  ${
    $d B a b c d e $.  $d B a l p q r $.  $d L a b c d e $.  $d L a l p q r $.
    $d P a b c d e $.  $d P a p q r $.
    isplibg.1 $e |- P = U. L $.
    isplibg.2 $e |- ( ph <-> ( ( L e. Plig /\ Rel B /\ Rel dom B ) /\
       ( dom dom B = P /\ ran dom B = P /\ ran B = P ) ) ) $.
    isplibg.3 $e |- ( ps <-> A. p e. P A. q e. P A. r e. P
       ( <. <. p , q >. , r >. e. B ->
          ( <. <. r , q >. , p >. e. B
            /\ E. l e. L ( p e. l /\ q e. l /\ r e. l )
            /\ ( p =/= q /\ p =/= r /\ q =/= r ) ) ) ) $.
    isplibg.4 $e |- ( ch <-> A. b e. P A. d e. P ( b =/= d ->
      E. a e. P E. c e. P E. e e. P
             ( <. <. a , b >. , d >. e. B
               /\ <. <. b , c >. , d >. e. B
               /\ <. <. b , d >. , e >. e. B ) ) ) $.
    isplibg.5 $e |- ( th <-> A. l e. L A. p e. P A. q e. P A. r e. P
