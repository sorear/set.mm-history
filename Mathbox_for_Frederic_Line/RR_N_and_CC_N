        $( [15-Sep-2013] $)
    $}

    ${
      $d x N $.
      $( The null vector is a complex vector.  (Contributed by FL,
         15-Sep-2013.) $)
      zernpl $p |- ( N e. NN -> 0w e. ( CC ^m ( 1 ... N ) ) ) $=
        ( vx cn wcel c1 cfz co cc0 cmpt cc cmap isnullcv wf cv a1i12 eqid fmptd
        0cn cnex oprex elmap sylibr eqeltrd ) AEFZBDGAHIZJKZLUGMIZDABCNUFUGLUHO
        UHUIFUFDUGJLUHUFDPUGFJLFTQUHRSLUGUHUAGAHUBUCUDUE $.
        $( [15-Sep-2013] $)
    $}

    ${
