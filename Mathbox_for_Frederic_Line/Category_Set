$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Category Set
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Class variables used as operators, relations, etc. I use the singular
     form because it was Cantor's use (who invented the .e sign). "A e. B"
     in Cantor's mind meant "A is a B" . Thus "A e. Morphism" means "A is
     a Morphism". $)

  $v .Morphism $.
  $v .Object $.
  $v .graph $.
  $v .dom $.
  $v .cod $.
  $v .id $.

  $( Let variable ` .Morphism ` be a class. $)
  cmorp $f class .Morphism $.
  $( Let variable ` .Object ` be a class. $)
  cobj $f class .Object $.
  $( Let variable ` .graph ` be a class. $)
  cgra $f class .graph $.
  $( Let variable ` .dom ` be a class. $)
  cdomc $f class .dom $.
  $( Let variable ` .cod ` be a class. $)
  ccod $f class .cod $.
  $( Let variable ` .id ` be a class. $)
  cidc $f class .id $.

  $c MorphismSetCat $.
  $( Extend class notation to include the morphisms of the category Set. $)
  ccmrcase $a class MorphismSetCat $.

  ${
    $d x a b c $.
    $( The morphisms of the category Set.  ( ` a ` is redundant and could be
       retrieved from ` c ` .)  Experimental. $)
    df-morcatset $a |- MorphismSetCat = ( x e. Univ |->
     { <. <. a , b >. , c >. | ( a e. x /\ b e. x /\ c e. ( b ^m a ) ) } ) $.
  $}

