$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Generic modules and vector spaces
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c Vec $.

  $( Extend class notation with the class of all generic vector spaces and
     modules. $)
  cvec $a class Vec $.

  ${
    $d g h s a b u x v y $.
    $( Definition of a vector space ( ` <. g , h >. ` is a field ), or of a
       module ( ` <. g , h >. ` is a ring ). $)
    df-vec $a |- Vec = { z | E. g E. h E. a E. b
     ( z = <. <. g , h >. , <. a , b >. >. /\
        ( a e. AbelOp
         /\ b : ( ran g X. ran a ) --> ran a
         /\ A. u e. ran a ( ( ( Id ` h ) b u ) = u /\ A. x e. ran g
           ( A. v e. ran a ( x b ( u a v ) ) = ( ( x b u ) a ( x b v ) )
              /\ A. y e. ran g ( ( ( x g y ) b u ) = ( ( x b u ) a ( y b u ) )
              /\ ( ( x h y ) b u ) = ( x b ( y b u ) ) ) ) ) ) ) } $.
  $}


  ${
    $d A a b g h u v x y $.  $d B a b g h u v x y $.  $d G a b g h u x y $.
    $d H a b g h u x y $.  $d a b g h u v x y z $.
    vecval1b.1 $e |- X = ran G $.
    vecval1b.4 $e |- W = ran A $.
    $( The predicate "is a vector space" or "is a module".  (Contributed by FL,
       12-Jul-2010.) $)
    vecval1b $p |- ( ( ( G e. M /\ H e. N /\ A e. O ) /\ B e. P )
      -> ( <. G , H >. Vec <. A , B >. <->
      ( A e. AbelOp /\ B : ( X X. W ) --> W
       /\ A. u e. W ( ( ( Id ` H ) B u ) = u
          /\ A. x e. X (
             A. v e. W
                 ( x B ( u A v ) ) = ( ( x B u ) A ( x B v ) )
             /\ A. y e. X ( ( ( x G y ) B u ) = ( ( x B u ) A ( y B u ) )
             /\ ( ( x H y ) B u ) = ( x B ( y B u ) ) ) ) ) ) ) ) $=
      ( wa co wceq wral vz vg vh va vb wcel w3a cop cvec wbr crn cxp wf cgi cfv
      cabl cv df-br wex cab df-vec eleq2i wb rneq xpeq1 feq2 3syl opreq opreq1d
      eqeq1d anbi1d raleqbidv anbi2d ralbidv 3anbi23d fveq2 anbi12d eleq1 xpeq2
      3anbi3d syl feq23d opreq2d eqeq12d eqeq2d 3anbi123d opreq12d id opreq123d
      feq1 elo syl5bb biid eqcomi xpeq12i feq23i imbi1i ralbii2 anbi12i imbi12i
      eqidd anbi2i 3anbi123i syl6bb ) HJUFIKUFELUFUGFGUFQZHIUHZEFUHZUIUJZEUPUFZ
      HUKZEUKZULZXKFUMZIUNUOZDUQZFRZXOSZAUQZXOCUQZERZFRZXRXOFRZXRXSFRZERZSZCXKT
