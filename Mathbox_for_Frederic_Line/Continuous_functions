  ${
    elsubops.1 $e |- K = ( topGen ` ( fi ` S ) ) $.
    $( The elements of a subbase are open sets. $)
    elsubops $p |- ( S e. A -> S C_ K ) $=
      ( wcel cfi cfv wss abfi2 ctg ctb wi fibas bastg sstr2 syl5com syl eqcomi
      sseq2i syl6ib mpd ) BAEZBBFGZHZBCHZBAIUBUDBUCJGZHZUEUBUCKEZUDUGLBAMUHUCUF
