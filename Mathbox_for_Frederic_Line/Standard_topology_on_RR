      GUTTZDFEFGHVAKELUADLUAZQVFDELLVAGUBVGVFVHVGVCVDVEVGVCVDUCUPURKUQUSLLUNUOG
      UDUQUPCURUQUPCICUPUEUFUGUHUIUJUKULUM $.
      $( [28-Jan-2009] $) $( [5-Jan-2009] $)
  $}

  ${
    $d A x y $.
    $( An open interval has two extended reals bounds. $)
    bsi3 $p |- ( A e. ran (,) <->
      E. x e. RR* E. y e. RR* A = ( x (,) y ) ) $=
      ( cioo crn wcel cv co wceq cxr wrex bsi bsi2 impbii ) CDEFCAGBGDHIBJKAJKA
      BCLABCMN $.
      $( [28-Jan-2009] $) $( [5-Jan-2009] $)
  $}

  $( Condition for an interval to belong to the range of ` (,) ` $)
  intrn $p |- ( ( A e. RR* /\ B e. RR* ) -> ( A (,) B ) e. ran (,) ) $=
    ( cxr cxp cr cpw cioo wf wcel wa co crn wi ioof wfn ffn fnoprvrn 3expib
    syl ax-mp ) CCDZEFZGHZACIZBCIZJABGKGLIZMZNUCGUAOZUGUAUBGPUHUDUEUFCCABGQRST
    $.
    $( [28-Jan-2009] $) $( [5-Jan-2009] $)


  ${
    $d A y z $.
    $( Lemma for ~ altretop . $)
    altretoplem1 $p |- ( A e. ( topGen ` ran (,) ) <-> ( A C_ RR
              /\ A. y e. A E. z e. ran (,)
                    ( y e. z /\ z C_ A ) ) ) $=
      ( cioo crn ctb wcel ctg cfv cr wss cv wa wrex wral wb retopbas cuni
      eltg2 unirnioo sseq2i anbi1i syl6bb ax-mp ) DEZFGZCUEHIGZCJKZALBLZGUICKMB
      UENACOZMZPQUFUGCUERZKZUJMUKABCUESUMUHUJULJCTUAUBUCUD $.
      $( [28-Jan-2009] $) $( [5-Jan-2009] $)
  $}


  ${
    $d A a b y z $.
    $( Lemma for ~ altretop . $)
    altretoplem2 $p |- ( A e. ( topGen ` ran (,) ) <-> ( A C_ RR
              /\ A. y e. A E. a e. RR* E. b e. RR*
        ( y e. ( a (,) b ) /\ ( a (,) b ) C_ A ) ) ) $=
      ( vz cioo crn ctg cfv wcel cr wss cv wa wrex wral co cxr altretoplem1
      wceq bsi eleq2 sseq1 anbi12d biimpcd reximdv syl5com r19.23aiv wi intrn
      rcla4ev ex syl r19.23aivv impbii ralbii anbi2i bitri ) BFGZHIJBKLZAMZEMZJ
      ZVBBLZNZEUSOZABPZNUTVACMZDMZFQZJZVJBLZNZDROZCROZABPZNAEBSVGVPUTVFVOABVFVO
