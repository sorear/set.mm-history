$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Standard topology on RR
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d A x y $.  $d w z $.
    $( An open interval has two extended reals bounds.  (Contributed by FL,
       5-Jan-2009.) $)
    bsi2 $p |- ( E. x e. RR* E. y e. RR* A = ( x (,) y )
       -> A e. ran (,) ) $=
      ( vz vw cv cioo co wceq crn wcel cxr cxp cr cpw wf wa wi ioof wral biimpi
      wfn ffnoprv w3a fnoprvrn eqcom eleq1d syl5ibcom 3expib adantr sylbi ax-mp
      r19.23aivv ) CAFZBFZGHZIZCGJZKZABLLLLMZNOZGPZUNLKZUOLKZQUQUSRZRZSVBGUTUBZ
      DFEFGHVAKELTDLTZQVFDELLVAGUCVGVFVHVGVCVDVEVGVCVDUDUPURKUQUSLLUNUOGUEUQUPC
      URUQUPCICUPUFUAUGUHUIUJUKULUM $.
      $( [5-Jan-2009] $)
  $}

  ${
    $d A x y $.
    $( An open interval has two extended reals bounds.  (Contributed by FL,
       5-Jan-2009.) $)
    bsi3 $p |- ( A e. ran (,) <->
      E. x e. RR* E. y e. RR* A = ( x (,) y ) ) $=
      ( cioo crn wcel cv co wceq cxr wrex bsi bsi2 impbii ) CDEFCAGBGDHIBJKAJKA
      BCLABCMN $.
      $( [5-Jan-2009] $)
  $}

  $( Condition for an interval to belong to the range of ` (,) ` (Contributed
     by FL, 5-Jan-2009.) $)
  intrn $p |- ( ( A e. RR* /\ B e. RR* ) -> ( A (,) B ) e. ran (,) ) $=
    ( cxr cxp cr cpw cioo wf wcel wa co crn wi ioof wfn ffn fnoprvrn 3expib syl
    ax-mp ) CCDZEFZGHZACIZBCIZJABGKGLIZMZNUCGUAOZUGUAUBGPUHUDUEUFCCABGQRST $.
    $( [5-Jan-2009] $)


  ${
    $d A y z $.
    $( Lemma for ~ altretop . $)
    altretoplem1 $p |- ( A e. ( topGen ` ran (,) ) <-> ( A C_ RR
              /\ A. y e. A E. z e. ran (,)
                    ( y e. z /\ z C_ A ) ) ) $=
      ( cioo crn ctb wcel ctg cfv cr wss cv wa wrex wral wb retopbas cuni eltg2
      unirnioo sseq2i anbi1i syl6bb ax-mp ) DEZFGZCUEHIGZCJKZALBLZGUICKMBUENACO
      ZMZPQUFUGCUERZKZUJMUKABCUESUMUHUJULJCTUAUBUCUD $.
      $( [5-Jan-2009] $)
  $}


  ${
    $d A a b y z $.
    $( Lemma for ~ altretop . $)
    altretoplem2 $p |- ( A e. ( topGen ` ran (,) ) <-> ( A C_ RR
              /\ A. y e. A E. a e. RR* E. b e. RR*
        ( y e. ( a (,) b ) /\ ( a (,) b ) C_ A ) ) ) $=
      ( vz cioo crn ctg cfv wcel cr wss cv wa wrex wral co altretoplem1 reximdv
      cxr bsi eleq2 sseq1 anbi12d biimpcd syl5com r19.23aiv wi intrn rcla4ev ex
      wceq syl r19.23aivv impbii ralbii anbi2i bitri ) BFGZHIJBKLZAMZEMZJZVBBLZ
      NZEUSOZABPZNUTVACMZDMZFQZJZVJBLZNZDTOZCTOZABPZNAEBRVGVPUTVFVOABVFVOVEVOEU
      SVBUSJVBVJULZDTOZCTOVEVOCDVBUAVEVRVNCTVEVQVMDTVQVEVMVQVCVKVDVLVBVJVAUBVBV
      JBUCUDZUESSUFUGVMVFCDTTVHTJVITJNVJUSJZVMVFUHVHVIUIVTVMVFVEVMEVJUSVSUJUKUM
      UNUOUPUQUR $.
      $( [5-Jan-2009] $)
  $}

