  $c SemiGrp $.

  $( Extend class notation with the class of all semi-groups. $)
  csem $a class SemiGrp $.

  $( A semi-group is an associative magma. $)
  df-sgr $a |- SemiGrp = ( Magma i^i Ass ) $.

  $( A semi-group is a magma. $)
  smgrpismgm $p |- ( G e. SemiGrp -> G e. Magma ) $=
    ( csem wcel cmagm cass cin df-sgr eleq2i elin pm3.26bi sylbi ) ABCADEFZCZAD
    CZBLAGHMNAECADEIJK $.
    $( [3-Nov-2009] $) $( [2-Nov-2009] $)

  $( A semi-group is associative. $)
  smgrpisass $p |- ( G e. SemiGrp -> G e. Ass ) $=
    ( csem wcel cmagm cass cin df-sgr eleq2i elin pm3.27bi sylbi ) ABCADEFZCZAE
    CZBLAGHMADCNADEIJK $.
    $( [3-Nov-2009] $) $( [2-Nov-2009] $)


