  $( ` RR ` is a group for addition. $)
  rrisgrp $p |- ( + |` ( RR X. RR ) ) e. Grp $=
    ( caddc cgr wcel cr cxp cres wss csubg cfv w3a readdsubg issubg mpbi
    simp2i ) ABCZADDEFZBCZPAGZPAHICOQRJKAPLMN $.
    $( [22-Dec-2008] $)

  $( A way to express the domain of a group. $)
  dmrngrp $p |- ( G e. Grp -> dom G = ( ran G X. ran G ) ) $=
    ( cgr wcel crn cxp wfo cdm wceq eqid grpfo wf fof fdm syl ) ABCADZOEZOAFZAG
    PHZAOOIJQPOAKRPOALPOAMNN $.
    $( [9-Jan-2011] $)

  ${
    $d G x y $.  $d X x y $.
    bsmgrli.1 $e |- X = ran G $.
    $( The base set of an operation with a right and left identity element is
       not empty. $)
    bsmgrli $p |- ( G e. ( Magma i^i ExId ) -> X =/= (/) ) $=
      ( vx vy cmagm cexid cin wcel cv co wceq wa wral wrex c0 wne isexid2
      rexn0 syl ) AFGHIDJZEJZAKUALUBUAAKUALMDBNZEBOBPQDEABCRUCEBST $.
