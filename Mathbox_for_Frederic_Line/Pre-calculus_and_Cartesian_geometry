  $( The members of a 'less than or equal' relationship are extended reals. $)
  mlteqer $p |- ( B e. C -> ( A <_ B -> ( A e. RR* /\ B e. RR* ) ) ) $=
    ( cle wbr wcel cxr wa cop wi df-br cxp clt ccnv cdif df-le eleq2i wn eldif
    opelxpg biimpcd adantr sylbi com12 ) ABDEZBCFZAGFBGFHZUEABIZDFZUFUGJZABDKUI
