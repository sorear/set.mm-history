  $( The members of a 'less than or equal' relationship are extended reals.
     (Contributed by FL, 31-Jul-2009.) $)
  mlteqer $p |- ( B e. C -> ( A <_ B -> ( A e. RR* /\ B e. RR* ) ) ) $=
    ( cle wbr wcel cxr wa cop wi df-br cxp clt ccnv df-le eleq2i wn eldif sylbi
    cdif opelxpg biimpcd adantr com12 ) ABDEZBCFZAGFBGFHZUEABIZDFZUFUGJZABDKUIU
    HGGLZMNZTZFZUJDUMUHOPUNUHUKFZUHULFQZHUJUHUKULRUOUJUPUFUOUGABGGCUAUBUCSSSUD
    $.
    $( [31-Jul-2009] $)

  $( Transitive law for ordering on extended reals ( compare ~ xrletr ).
     (Contributed by FL, 2-Aug-2009.) $)
  xrletr2 $p |- ( C e. D ->
             ( ( A <_ B /\ B <_ C ) -> A <_ C ) ) $=
