  $( Trichotomy law.  (Contributed by FL, 2-Aug-2009.) $)
  letri31 $p |- ( ( A e. RR* /\ B e. RR* ) ->
                ( A = B <-> ( A <_ B /\ B <_ A ) ) ) $=
