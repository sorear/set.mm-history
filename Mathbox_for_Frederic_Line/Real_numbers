$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Real numbers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c -e $. $( the negative of an extended real number. $)
  $c +e $. $( Addition of extended real numbers. $)
  $c xe $. $( Multiplication of extended real numbers. $)

  $( Extend class notation to include the negative of an extended number. $)
  clminex $a class -e A $.

  $( Extend class notation to include the negative of an extended number. $)
  claddex $a class +e $.


  $( Define the negative of an extended real number. $)
  df-nege $a |- -e A = if ( A = +oo , -oo , if ( A = -oo , +oo , -u A ) ) $.

  $( Define addition over extended real numbers. $)
  df-adde $a |- +e = { <. <. x , y >. , z >. | ( x e. RR* /\ y e. RR*
   /\ z = if ( A = +oo ,
          if ( B = -oo , ~P RR* , +oo ) ,
       if ( A = -oo ,
          if ( B = +oo , ~P RR* , -oo ) ,
             if ( B = +oo , +oo ,
               if ( B = -oo , -oo , ( A + B ) ) ) ) ) ) } $.

  $( Minus ` +oo ` .  Bourbaki TG IV.15. $)
  mpinf $p |- -e +oo = -oo $=
    ( cpnf clminex wceq cmnf cneg cif df-nege eqid iftrue ax-mp eqtri ) ABAACZD
    ADCAAEFZFZDAGLNDCAHLDMIJK $.
    $( [26-Dec-2011] $)

  $( Minus ` -oo ` .  Bourbaki TG IV.15. $)
  miminf $p |- -e -oo = +oo $=
    ( cmnf clminex cpnf wceq cneg cif df-nege wn wne pnfnemnf df-ne necom
    bitr3i mpbir iffalse eqid iftrue ax-mp syl6eq eqtri ) ABACDZAAADZCAEZFZFZCA
    GUAHZUECDUFCAIZJUFACIUGACKACLMNUFUEUDCUAAUDOUBUDCDAPUBCUCQRSRT $.
    $( [26-Dec-2011] $)


  $( Minus a real number.  Bourbaki TG IV.15. $)
  mreal $p |- ( A e. RR -> -e A = -u A ) $=
    ( cr wcel cmnf wne cpnf clminex cneg wceq renemnf renepnf wa cif df-nege
    a1i wn df-ne iffalse sylbi adantl adantr 3eqtrd syl11anc ) ABCADEZAFEZAGZAH
    ZIAJAKUDUELZUFAFIZDADIZFUGMZMZUKUGUFULIUHANOUEULUKIZUDUEUIPUMAFQUIDUKRSTUDU
    KUGIZUEUDUJPUNADQUJFUGRSUAUBUC $.
    $( [26-Dec-2011] $)


