  ${
    $d x y K $.  $d x y R $.  $d x y ph $.
    $( Ring base $)
    issrngd.k $e |- ( ph -> K = ( Base ` R ) ) $.
    $( Ring plus $)
    issrngd.p $e |- ( ph -> P = ( +g ` R ) ) $.
    $( Ring times $)
    issrngd.t $e |- ( ph -> T = ( .r ` R ) ) $.
    $( Involution (e.g. complex conjugate) $)
    issrngd.c $e |- ( ph -> C = ( *r ` R ) ) $.
    issrngd.r $e |- ( ph -> R e. Ring ) $.
    $( Closure $)
    issrngd.cl $e |- ( ( ph /\ x e. K ) -> ( C ` x ) e. K ) $.
    $( Distributive law over addition $)
    issrngd.dp $e |- ( ( ph /\ x e. K /\ y e. K )
           -> ( C ` ( x P y ) ) = ( ( C ` x ) P ( C ` y ) ) ) $.
    $( Distributive law over multiplication $)
    issrngd.dt $e |- ( ( ph /\ x e. K /\ y e. K )
           -> ( C ` ( x T y ) ) = ( ( C ` y ) T ( C ` x ) ) ) $.
    $( Idempotency $)
    issrngd.id $e |- ( ( ph /\ x e. K ) -> ( C ` ( C ` x ) ) = x ) $.
    $( Properties that determine a star ring. $)
    issrngd $p |- ( ph -> R e. *Ring ) $=
      ( crg wcel cv cstv cfv cbs cplusg co wceq cmulr w3a wa wral csr simpl
      simprl syl11anc simprr syl111anc jca syl 3jca r19.21aivva fveq1d eleq12d
      opreqd fveq12d opreq123d eqeq12d eqeq1d 3anbi123d anbi12d raleqbidv
      anbi2d mpbid eqid issrng sylibr ) AFRSZBTZFUAUBZUBZFUCUBZSZVQCTZFUDUBZUEZ
      VRUBZVSWBVRUBZWCUEZUFZVQWBFUGUBZUEZVRUBZWFVSWIUEZUFZVSVRUBZVQUFZUHZUIZCVT
