  $c <oNEW $.
  $c AtomsNEW $.
  $c AtLat $.
  $(
  $c AtsLat $.
  $c CvLat $.
  $)
  $c HL $.

  $( Extend class notation with covering relation. $)
  ccvr $a class <oNEW $.

  $( Extend class notation with atoms. $)
  catm $a class AtomsNEW $.

  $( Extend class notation with atomic lattices. $)
  cal $a class AtLat $.

  $( Extend class notation with atomistic lattices. $)
  $(
  cas $a class AtsLat $.
  $)

  $( Extend class notation with lattices with the covering property. $)
  $(
  clc $a class CvLat $.
  $)

  $( Extend class notation with Hilbert lattices. $)
  chlt $a class HL $.

  ${
    $d p l c a b $.
    $( Def. 8.4.13 p. 209. $)
    $( Define the covering relation ("is covered by") for posets. $)
    df-covers $a |- <oNEW = ( p e. _V |-> { <. a , b >. | ( ( a e. ( base ` p )
               /\ b e. ( base ` p ) )
           /\ a ( lt ` p ) b /\ -. E. z e. ( base ` p )
              ( a ( lt ` p ) z /\ z ( lt ` p ) b ) ) } ) $.

    $( Def. 8.4.14 p. 210. $)
    $( Define the class of poset atoms. $)
    df-atoms $a |- AtomsNEW = ( p e. _V |-> { a e. ( base ` p ) |
                      ( 0. ` p ) ( <oNEW ` p ) a } ) $.

    $( Def. 8.4.15 p. 210. $)
    $( Define the class of atomic lattices. $)
    df-atlat $a |- AtLat = { l e. LatNEW | A. a e. ( base ` l )
       ( a =/= ( 0. ` l ) ->
          E. b e. ( AtomsNEW ` l ) b ( le ` l ) a ) } $.

    $( Def. 8.4.16 p. 210. $)
    $( Define the class of atomistic lattices.  Every non-zero element
       is the join of all atoms less than it.  If an
        orthomodular lattice is atomic, then it is also atomistic
      (Beltrametti_Cassinelli p. 98). $)
    $(
    df-atslat $a |- AtsLat = { l e. LatNEW | A. a e. ( base ` l )
         a = ( ( lub ` l ) ` { p e. ( AtomsNEW ` l ) | p ( le ` l ) a } ) } $.
    $)

    $( Def. 8.4.17 p. 210. $)
    $( Define the class of lattices with the covering property. $)
    $(
    df-cvlat $a |- CvLat = { l e. AtLat | A. a e. ( base ` l )
       A. p e. ( AtomsNEW ` l ) ( ( a ( meet ` l ) p ) = ( 0. ` l )
          -> a ( <oNEW ` l ) ( a ( join ` l ) p ) ) } $.
    $)

    $( Def. 8.4.18 p. 211. $)
    $( Define the class of Hilbert lattices, which are complete, atomic
       lattices satisfying the superposition principle and minimum height. $)
    df-hlat $a |- HL = { l e. ( ( OML i^i ComplLat ) i^i AtLat ) |
      ( A. a e. ( AtomsNEW ` l ) A. b e. ( AtomsNEW ` l ) ( a =/= b
       -> ( E. c e. ( AtomsNEW ` l )
             ( c =/= a /\ c =/= b /\ c ( le ` l ) ( a ( join ` l ) b ) )
         /\ A. c e. ( AtomsNEW ` l )
            ( ( c =/= a /\ c =/= b /\ c ( le ` l ) ( a ( join ` l ) b ) )
          -> a ( le ` l ) ( b ( join ` l ) c ) ) ) )
      /\ E. a e. ( base ` l ) E. b e. ( base ` l ) E. c e. ( base ` l )
          ( ( ( 0. ` l ) ( lt ` l ) a /\ a ( lt ` l ) b )
        /\ ( b ( lt ` l ) c /\ c ( lt ` l ) ( 1. ` l ) ) ) ) } $.
  $}

