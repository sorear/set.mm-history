  $}

  ${
    $d p s t u x y z $.
    $( Def. 8.4.4 p. 205. $)
    $( Define poset least upper bound.  If it doesn't exist, an undefined value
       not in the base set is returned. $)
    df-lub $a |- lub = ( p e. _V |-> ( s e. ~P ( base ` p ) |->
        ( iota_ x e. ( base ` p ) ( A. y e. s y ( le ` p ) x
                          /\ A. z e. ( base ` p )
                  ( A. y e. s y ( le ` p ) z -> x ( le ` p ) z ) ) ) ) ) $.

    $( Def. 8.4.4 p. 205. $)
    $( Define poset least upper bound.  If it doesn't exist, an undefined value
       not in the base set is returned. $)
    $(
    df-lubOLD $a |- lub = ( p e. _V |-> ( s e. ~P ( base ` p ) |->
     { t | E. u ( u = { x e. ( base ` p ) | ( A. y e. s y ( le ` p ) x
                          /\ A. z e. ( base ` p )
                             ( A. y e. s y ( le ` p ) z -> x ( le ` p ) z ) ) }
      /\ t = if ( E. v u = { v } , U. u , ( Undef ` ( base ` p ) ) ) ) } ) ) $.
    $)

    $( Def. 8.4.4 p. 205. $)
    $( Define poset greatest lower bound. $)
    df-glb $a |- glb = ( p e. _V |-> ( s e. ~P ( base ` p ) |->
        ( iota_ x e. ( base ` p ) ( A. y e. s y ( geNEW ` p ) x
                          /\ A. z e. ( base ` p )
                  ( A. y e. s y ( geNEW ` p ) z -> x ( geNEW ` p ) z ) ) ) ) )
        $.

    $( Def. 8.4.4 p. 205. $)
    $( Define poset greatest lower bound. $)
    $(
    df-glbOLD $a |- glb = ( p e. _V |-> ( s e. ~P ( base ` p ) |->
          { t | E. u ( u = { x e. ( base ` p ) | ( A. y e. s y ( geNEW ` p ) x
        /\
       A. z e. ( base ` p ) ( A. y e. s y ( geNEW ` p ) z -> x ( geNEW ` p ) z
        ) ) }
      /\ t = if ( E. v u = { v } , U. u , ( Undef ` ( base ` p ) ) ) ) } ) ) $.
    $)

    $( Def. 8.4.4 p. 205. $)
    $( Define poset join. $)
    df-join $a |- join = ( p e. _V |-> ( x e. ( base ` p ) , y e. ( base ` p )
        |-> ( ( lub ` p ) ` { x , y } ) ) ) $.

    $( Def. 8.4.4 p. 205. $)
    $( Define poset meet. $)
    df-meet $a |- meet = ( p e. _V |-> ( x e. ( base ` p ) , y e. ( base ` p )
        |-> ( ( glb ` p ) ` { x , y } ) ) ) $.
  $}

  ${
    $d p s x z B $.  $d p s x y z K $.  $d p L $.
    lubfval.1 $e |- B = ( base ` K ) $.
    lubfval.2 $e |- L = ( le ` K ) $.
    lubfval.3 $e |- U = ( lub ` K ) $.
    $( Value of least upper bound function of a poset. $)
