  $( A Hilbert lattice is orthomodular. $)
  hloml $p |- ( K e. HL -> K e. OML ) $=
    ( chlt wcel coml ccla cal hllem1 simp1d ) ABCADCAECAFCAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is complete. $)
  hlclat $p |- ( K e. HL -> K e. CLat ) $=
    ( chlt wcel coml ccla cal hllem1 simp2d ) ABCADCAECAFCAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is atomic. $)
  hlatl $p |- ( K e. HL -> K e. AtLat ) $=
    ( chlt wcel coml ccla cal hllem1 simp3d ) ABCADCAECAFCAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is an ortholattice. $)
  hlol $p |- ( K e. HL -> K e. OL ) $=
    ( chlt wcel coml col hloml omlol syl ) ABCADCAECAFAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is an orthoposet. $)
  hlop $p |- ( K e. HL -> K e. OP ) $=
    ( chlt wcel col cops hlol olop syl ) ABCADCAECAFAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is a lattice. $)
  hllat $p |- ( K e. HL -> K e. LatNEW ) $=
    ( chlt wcel cal clat hlatl atlatlat syl ) ABCADCAECAFAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is a poset. $)
  hlpos $p |- ( K e. HL -> K e. PosetNEW ) $=
    ( chlt wcel clat cpo hllat latpos syl ) ABCADCAECAFAGH $.
    $( [20-Oct-2011] $)

