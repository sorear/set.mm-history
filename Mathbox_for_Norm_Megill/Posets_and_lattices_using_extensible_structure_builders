  ${
    $d p s x y $.
    $( Def. 8.4.5 p. 207. $)
    $( Define the class of all lattices. $)
    df-lat $a |- LatNEW = { p e. PosetNEW | A. x e. ( base ` p )
       A. y e. ( base ` p ) ( ( x ( join ` p ) y ) e. ( base ` p )
          /\ ( x ( meet ` p ) y ) e. ( base ` p ) ) } $.

    $( Def. 8.4.5 p. 207. $)
    $( Define the class of all complete lattices. $)
    df-clat $a |- ComplLat = { p e. PosetNEW | A. s ( s C_ ( base ` p )
      -> ( ( ( lub ` p ) ` s ) e. ( base ` p ) /\
           ( ( glb ` p ) ` s ) e. ( base ` p ) ) ) } $.
  $}

