  $( An ortholattice is a lattice. $)
  ollat $p |- ( K e. OL -> K e. LatNEW ) $=
    ( col wcel clat cops isolat simplbi ) ABCADCAECAFG $.
    $( [18-Sep-2011] $)

  $( An ortholattice is an orthoposet. $)
  olop $p |- ( K e. OL -> K e. OP ) $=
    ( col wcel clat cops isolat simprbi ) ABCADCAECAFG $.
    $( [18-Sep-2011] $)

  $( An ortholattice is a poset. $)
  olpos $p |- ( K e. OL -> K e. PosetNEW ) $=
    ( col wcel cops cpo olop opposet syl ) ABCADCAECAFAGH $.
    $( [16-Oct-2011] $)

