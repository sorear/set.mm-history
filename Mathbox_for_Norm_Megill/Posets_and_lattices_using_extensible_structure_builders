  $}

  ${
    $d p x y z B $.  $d p K $.  $d p x y z L $.
    isposi.0NEW $e |- K e. Struct ( 2 , p , T. ) $.
    isposi.1NEW $e |- B = ( base ` K ) $.
    isposi.2NEW $e |- L = ( le ` K ) $.
    isposi.3NEW $e |- B =/= (/) $.
    isposi.4NEW $e |- ( x e. B -> x L x ) $.
    isposi.5NEW $e |- ( ( x e. B /\ y e. B ) ->
                   ( ( x L y /\ y L x ) -> x = y ) ) $.
    isposi.6NEW $e |- ( ( x e. B /\ y e. B /\ z e. B ) ->
                   ( ( x L y /\ y L z ) -> x L z ) ) $.
    $( Properties that determine a poset (constructed structure version). $)
    isposiNEW $p |- K e. PosetNEW $=
      ( cpo wcel wtru c2 cstru c0 wne cv wbr wa wceq wi w3a wral eqid isposNEW
      3ad2ant1 3adant3 3jca rgen3 mpbir3an ) EOPEQGRSZPDTUAAUBZUQFUCZUQBUBZFUCZ
      USUQFUCUDUQUSUEUFZUTUSCUBZFUCUDUQVBFUCUFZUGZCDUHBDUHADUHABCDUPEFGUPUIIJUJ
      HKVDABCDDDUQDPZUSDPZVBDPZUGURVAVCVEVFURVGLUKVEVFVAVGMULNUMUNUO $.
      $( [11-Sep-2011] $)
  $}

  ${
    $d p x y z B $.  $d p K $.  $d p x y z L $.
    isposix.aNEW $e |- B e. _V $.
    isposix.bNEW $e |- L e. _V $.
    isposix.1NEW $e |- K = { <. 1 , B >. , <. 2 , L >. } $.
    isposix.2NEW $e |- B =/= (/) $.
    isposix.3NEW $e |- ( x e. B -> x L x ) $.
    isposix.4NEW $e |- ( ( x e. B /\ y e. B ) ->
                   ( ( x L y /\ y L x ) -> x = y ) ) $.
    isposix.5NEW $e |- ( ( x e. B /\ y e. B /\ z e. B ) ->
                   ( ( x L y /\ y L z ) -> x L z ) ) $.
    $( Properties that determine a poset (explicit structure version). $)
    isposixNEW $p |- K e. PosetNEW $=
      ( vp stb2strx cbs df-base stb2v1x cple df-ple stb2v2x isposiNEW ) ABCDEFN
      NEDFGHIOPNEDFGHINQRSNEDFGHINTUAJKLMUB $.
      $( [11-Sep-2011] $)
  $}

  $( Def. 8.4.3 p. 204. $)
  $( Define less-than ordering for posets and related structures. $)
  df-plt $a |- lt = ( p e. _V |-> ( ( le ` p ) \ _I ) ) $.

  ${
    $d p K $.
    pltval.1 $e |- L = ( le ` K ) $.
    pltval.2 $e |- S = ( lt ` K ) $.
    $( Less-than relation. $)
    pltval $p |- ( ( K e. A /\ X e. B /\ Y e. C )
        -> ( X S Y <-> ( X L Y /\ X =/= Y ) ) ) $=
      ( vp wcel wbr wne wa wb cple cfv cid cdif cplt cvv wceq elisset cv fveq2
      difeq1d df-plt fvex difexg ax-mp fvmpt syl syl5eq breqd wn ideqg
      necon3bbid adantl anbi2d difeq1i breqi brdif bitr3i syl5bb sylan9bb
      3impb ) EALZGBLZHCLZGHDMZGHFMZGHNZOZPVHVKGHEQRZSTZMZVIVJOZVNVHDVPGHVHEUAR
      ZVPDVHEUBLVSVPUCEAUDKEKUEZQRZSTVPUBUAVTEUCWAVOSVTEQUFUGKUHVOUBLVPUBLEQUIV
      OSUBUJUKULUMJUNUOVRVLGHSMZUPZOZVNVQVRWCVMVLVJWCVMPVIVJWBGHGHCUQURUSUTVQGH
      FSTZMWDGHWEVPFVOSIVAVBGHFSVCVDVEVFVG $.
      $( [12-Oct-2011] $)

    $( Less-than implies less-than-or-equal. $)
    pltle $p |- ( ( ( K e. A /\ X e. B /\ Y e. C ) /\ X S Y ) -> X L Y ) $=
      ( wcel w3a wbr wne pltval pm3.26bda ) EAKGBKHCKLGHDMGHFMGHNABCDEFGHIJOP
      $.
      $( [18-Oct-2011] $)
  $}

  ${
    pleval2.1 $e |- B = ( base ` K ) $.
    pleval2.2 $e |- L = ( le ` K ) $.
    pleval2.3 $e |- S = ( lt ` K ) $.
    $( Less-than-or-equal in terms of less-than. $)
    pleval2 $p |- ( ( K e. PosetNEW /\ X e. B /\ Y e. B )
        -> ( X L Y <-> ( X S Y \/ X = Y ) ) ) $=
      ( cpo wcel w3a wbr wceq wo wne wa wi wb breq2 posref 3adant3 syl5cbi
      pm4.72 sylib ordi wn exmid df-ne orbi2i mpbir mpbiran2 syl6bbr pltval
      orbi2d bitr4d orcom syl6bb ) CJKZEAKZFAKZLZEFDMZEFNZEFBMZOZVEVDOVBVCVDVCE
      FPZQZOZVFVBVCVDVCOZVIVBVDVCRVCVJSVDEEDMZVCVBEFEDTUSUTVKVAACDEGHUAUBUCVDVC
      UDUEVIVJVDVGOZVDVCVGUFVLVDVDUGZOVDUHVGVMVDEFUIUJUKULUMVBVEVHVDJAABCDEFHIU
      NUOUPVDVEUQUR $.
      $( [17-Oct-2011] $)

    $( Less-than imples not inverse less-than-or-equal. $)
    pltnle $p |- ( ( ( K e. PosetNEW /\ X e. B /\ Y e. B ) /\ X S Y )
                    -> -. Y L X ) $=
      ( cpo wcel w3a wbr wn wne wa pltval wceq posasymb biimpd expdimp
      necon3ad expimpd sylbid imp ) CJKEAKFAKLZEFBMZFEDMZNZUFUGEFDMZEFOZPUIJAAB
      CDEFHIQUFUJUKUIUFUJPUHEFUFUJUHEFRZUFUJUHPULACDEFGHSTUAUBUCUDUE $.
      $( [18-Oct-2011] $)

    $( Compare ~ dfpss3 $)
    $( Alternate expression for less-than relation. $)
    pltval2 $p |- ( ( K e. PosetNEW /\ X e. B /\ Y e. B )
        -> ( X S Y <-> ( X L Y /\ -. Y L X ) ) ) $=
      ( cpo wcel w3a wbr wne wa wn pltval wceq wi breq1 posref 3adant3 syl5cbi
      adantr posasymb biimpd expdimp impbid necon3abid pm5.32da bitrd ) CJKZEAK
      ZFAKZLZEFBMEFDMZEFNZOUPFEDMZPZOJAABCDEFHIQUOUPUQUSUOUPOZUREFUTEFRZURUOVAU
