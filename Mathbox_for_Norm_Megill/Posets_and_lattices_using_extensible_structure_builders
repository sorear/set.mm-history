    $( A poset ordering is asymetric, using greater-than-or-equal ordering. $)
    posgeasymb $p |- ( ( K e. PosetNEW /\ X e. B /\ Y e. B )
                -> ( ( X S Y /\ Y S X ) <-> X = Y ) ) $=
      ( cpo wcel w3a wbr wa wceq wb wi posgelem 3simp1 3simp2 3simp3 syl3an2c
