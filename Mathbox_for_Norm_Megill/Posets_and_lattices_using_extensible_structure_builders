    $( A poset ordering is transitive, using greater-than-or-equal ordering. $)
    posgetrNEW $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X S Y /\ Y S Z ) -> X S Z ) ) $=
      ( cpo wcel w3a wa wbr wceq wb wi posgelem 3simp3d ) CIJDAJEAJFAJKLDDBMDEB
      MZEDBMLDENOSEFBMLDFBMPABCDEFGHQR $.
