      MDENZCLUCAOZGPADQBOZUDGPADQUEUCGPRBCQSCDEFHJTABKCDEFGHIJUAUB $.
      $( [5-Dec-2011] $)

    $d y z X $.
    $( A greatest lower bound is a least element. $)
    clatglble $p |- ( ( K e. CLat /\ S C_ B /\ X e. S )
          -> ( G ` S ) L X ) $=
      ( ccla wcel wss cfv wbr wa wi clatglbcl glble exp32 mpd 3impia ) DJKZBALZ
      FBKZBCMZFENZUBUCOZUEAKZUDUFPABCDGIQUGUHUDUFJABCDEFGHIRSTUA $.
      $( [5-Dec-2011] $)

    $( Two ways of expressing "less than or equal to the greatest lower
       bound." $)
    clatleglb $p |- ( ( K e. CLat /\ X e. B /\ S C_ B )
     -> ( X L ( G ` S ) <-> A. y e. S X L y ) ) $=
      ( vz ccla wcel wss w3a cfv wbr cv wral wa clatglble 3expa 3adantl2 clat
      wi simpl1 clatlat syl simpl2 clatglbcl 3adant2 adantr ssel 3ad2ant3 imp
