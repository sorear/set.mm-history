      UJ $.
      $( [8-Nov-2011] $)

    $( Compare ~ inindi . $)
    $( Lattice meet distributes over itself. $)
    latmmdi $p |- ( ( K e. OL /\ ( X e. B /\ Y e. B /\ Z e. B ) ) ->
             ( X M ( Y M Z ) ) = ( ( X M Y ) M ( X M Z ) ) ) $=
