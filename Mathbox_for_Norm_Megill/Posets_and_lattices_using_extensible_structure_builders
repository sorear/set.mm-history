  ${
    $d x y B $.  $d x y K $.
    islati.1 $e |- K e. PosetNEW $.
    islati.b $e |- B = ( base ` K ) $.
    islati.j $e |- J = ( join ` K ) $.
    islati.m $e |- M = ( meet ` K ) $.
    islati.5 $e |- ( ( x e. B /\ y e. B ) -> ( x J y ) e. B ) $.
    islati.6 $e |- ( ( x e. B /\ y e. B ) -> ( x M y ) e. B ) $.
    $( Properties that determine a lattice. $)
    islati $p |- K e. LatNEW $=
      ( clat wcel cpo cv co wa wral islat jca rgen2a mpbir2an ) EMNEONAPZBPZDQC
      NZUDUEFQCNZRZBCSACSABCDEFHIJTGUHABCUDCNUECNRUFUGKLUAUBUC $.
      $( [12-Sep-2011] $)
  $}

  ${
    latref.b $e |- B = ( base ` K ) $.
    latref.l $e |- L = ( le ` K ) $.
    $( A lattice ordering is reflexive.  (Th. ~ ssid analog.) $)
    latref $p |- ( ( K e. LatNEW /\ X e. B ) -> X L X ) $=
      ( cpo wcel wbr clat posref latpos sylan ) BGHDAHDDCIBJHABCDEFKBLM $.
      $( [8-Oct-2011] $)

    $( A lattice ordering is asymetric.  (Th. ~ eqss analog.) $)
    latasymb $p |- ( ( K e. LatNEW /\ X e. B /\ Y e. B )
                -> ( ( X L Y /\ Y L X ) <-> X = Y ) ) $=
      ( cpo wcel wbr wa wceq wb clat posasymb latpos syl3an1 ) BHIDAIEAIDECJEDC
      JKDELMBNIABCDEFGOBPQ $.
      $( [22-Oct-2011] $)

    $( A lattice ordering is asymetric.  (Th. ~ eqss analog.) $)
    latasym $p |- ( ( K e. LatNEW /\ X e. B /\ Y e. B )
                -> ( ( X L Y /\ Y L X ) -> X = Y ) ) $=
      ( clat wcel w3a wbr wa wceq latasymb biimpd ) BHIDAIEAIJDECKEDCKLDEMABCDE
      FGNO $.
      $( [8-Oct-2011] $)

    $( A lattice ordering is transitive.  (Th. ~ sstr analog.) $)
    lattr $p |- ( ( K e. LatNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X L Y /\ Y L Z ) -> X L Z ) ) $=
      ( cpo wcel w3a wbr wa wi clat postrNEW latpos sylan ) BIJDAJEAJFAJKDECLEF
      CLMDFCLNBOJABCDEFGHPBQR $.
      $( [17-Nov-2011] $)
  $}

  ${
    latasymd.b $e |- B = ( base ` K ) $.
    latasymd.l $e |- L = ( le ` K ) $.
    latasymd.3 $e |- ( ph -> K e. LatNEW ) $.
    latasymd.4 $e |- ( ph -> X e. B ) $.
    latasymd.5 $e |- ( ph -> Y e. B ) $.
    latasymd.6 $e |- ( ph -> X L Y ) $.
    latasymd.7 $e |- ( ph -> Y L X ) $.
    $( Deduce equality from lattice ordering.  (Th. ~ eqssd analog.) $)
    latasymd $p |- ( ph -> X = Y ) $=
      ( wbr wceq clat wcel wa wb latasymb syl111anc mpbi2and ) AEFDNZFEDNZEFOZA
      CPQEBQFBQUCUDRUESIJKBCDEFGHTUALMUB $.
