    $( A poset ordering is reflexive. $)
    posref $p |- ( ( P e. PosetNEW /\ X e. B ) -> X R X ) $=
      ( cpo wcel wa wbr wceq wi w3a poslem id 3jca sylan2 3simp1 syl ) BGHZDAHZ
      IDDCJZUBUBIZDDKLZUCUBLZMZUBTUAUAUAMUFUAABCDDDEFNUAUAUAUAUAOZUGUGPQUBUDUER
