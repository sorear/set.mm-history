    $( A poset ordering is transitive. $)
    postrNEW $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X L Y /\ Y L Z ) -> X L Z ) ) $=
      ( cpo wcel w3a wa wbr wceq wi poslem 3simp3d ) BIJDAJEAJFAJKLDDCMDECMZEDC
      MLDENOREFCMLDFCMOABCDEFGHPQ $.
