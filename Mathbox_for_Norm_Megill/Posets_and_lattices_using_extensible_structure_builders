      BUCPUAUBUCQUAUBUCRZUJSUEUGUHQT $.
      $( [11-Sep-2011] $)

    $( A poset ordering is transitive. $)
    postrNEW $p |- ( ( P e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X R Y /\ Y R Z ) -> X R Z ) ) $=
      ( cpo wcel w3a wa wbr wceq wi poslem 3simp3d ) BIJDAJEAJFAJKLDDCMDECMZEDC
      MLDENOREFCMLDFCMOABCDEFGHPQ $.
      $( [11-Sep-2011] $)
  $}

  ${
    $d p x y z B $.  $d p P $.  $d p x y z R $.
    isposi.0NEW $e |- P e. Struct ( 2 , p , T. ) $.
    isposi.1NEW $e |- B = ( base ` P ) $.
    isposi.2NEW $e |- R = ( le ` P ) $.
    isposi.3NEW $e |- B =/= (/) $.
    isposi.4NEW $e |- ( x e. B -> x R x ) $.
    isposi.5NEW $e |- ( ( x e. B /\ y e. B ) ->
                   ( ( x R y /\ y R x ) -> x = y ) ) $.
    isposi.6NEW $e |- ( ( x e. B /\ y e. B /\ z e. B ) ->
                   ( ( x R y /\ y R z ) -> x R z ) ) $.
    $( Properties that determine a poset (constructed structure version). $)
    isposiNEW $p |- P e. PosetNEW $=
      ( cpo wcel wtru c2 cstru c0 wne cv wbr wa wceq wi w3a wral eqid isposNEW
      3ad2ant1 3adant3 3jca rgen3 mpbir3an ) EOPEQGRSZPDTUAAUBZUQFUCZUQBUBZFUCZ
      USUQFUCUDUQUSUEUFZUTUSCUBZFUCUDUQVBFUCUFZUGZCDUHBDUHADUHABCDEFUPGUPUIIJUJ
      HKVDABCDDDUQDPZUSDPZVBDPZUGURVAVCVEVFURVGLUKVEVFVAVGMULNUMUNUO $.
      $( [11-Sep-2011] $)
  $}

  ${
    $d p x y z B $.  $d p P $.  $d p x y z R $.
    isposix.aNEW $e |- B e. _V $.
    isposix.bNEW $e |- R e. _V $.
    isposix.1NEW $e |- P = { <. 1 , B >. , <. 2 , R >. } $.
    isposix.2NEW $e |- B =/= (/) $.
    isposix.3NEW $e |- ( x e. B -> x R x ) $.
    isposix.4NEW $e |- ( ( x e. B /\ y e. B ) ->
                   ( ( x R y /\ y R x ) -> x = y ) ) $.
    isposix.5NEW $e |- ( ( x e. B /\ y e. B /\ z e. B ) ->
                   ( ( x R y /\ y R z ) -> x R z ) ) $.
    $( Properties that determine a poset (explicit structure version). $)
    isposixNEW $p |- P e. PosetNEW $=
      ( vp stb2strx cbs df-base stb2v1x cple df-ple stb2v2x isposiNEW ) ABCDEFN
      NEDFGHIOPNEDFGHINQRSNEDFGHINTUAJKLMUB $.
      $( [11-Sep-2011] $)
  $}

  $( Def. 8.4.3 p. 204. $)
  $( Define less-than ordering for posets and related structures. $)
  df-plt $a |- lt = ( p e. _V |-> ( ( le ` p ) \ _I ) ) $.

  ${
    $d p P $.
    pltval.1 $e |- R = ( le ` P ) $.
    pltval.2 $e |- S = ( lt ` P ) $.
    $( Less-than relation. $)
    pltval $p |- ( ( P e. A /\ X e. B /\ Y e. C )
        -> ( X S Y <-> ( X R Y /\ X =/= Y ) ) ) $=
      ( vp wcel wbr wne wa wb cple cfv cid cdif wceq cplt cvv elisset cv fveq2
      difeq1d df-plt fvex difexg ax-mp fvmpt syl syl5eq breq wn ideqg
      necon3bbid adantl anbi2d difeq1i breqi brdif bitr3i syl5bb sylan9bb
      3impb ) DALZGBLZHCLZGHFMZGHEMZGHNZOZPVHVKGHDQRZSTZMZVIVJOZVNVHFVPUAVKVQPV
