    $( A poset ordering is reflexive, using greater-than-or-equal ordering. $)
    posgeref $p |- ( ( K e. PosetNEW /\ X e. B ) -> X S X ) $=
      ( cpo wcel wa wbr wceq wb wi w3a posgelem id 3jca sylan2 3simp1 syl ) CGH
      ZDAHZIDDBJZUCUCIZDDKLZUDUCMZNZUCUAUBUBUBNUGUBABCDDDEFOUBUBUBUBUBPZUHUHQRU
      CUEUFST $.
