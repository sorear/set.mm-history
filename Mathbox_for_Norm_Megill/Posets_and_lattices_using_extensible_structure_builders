$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
               Posets and lattices using extensible structure builders
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c base $.
  $c le $.
  $c PosetNEW $.
  $c lt $.
  $c geNEW $.
  $c gt $.
  $c lub $.
  $c glb $.
  $c join $.
  $c meet $.

  $( Extend class notation with the class of all base set extractors. $)
  cbs $a class base $.

  $( Extend class notation with less-than-or-equal for posets. $)
  cple $a class le $.

  $( Extend class notation with the class of posets. $)
  cpo $a class PosetNEW $.

  $( Extend class notation with less-than for posets. $)
  cplt $a class lt $.

  $( Extend class notation with greater-than-or-equal for posets. $)
  cpge $a class geNEW $.

  $( Extend class notation with greater-than for posets. $)
  cpgt $a class gt $.

  $( Extend class notation with poset least upper bound. $)
  club $a class lub $.

  $( Extend class notation with poset greatest lower bound. $)
  cglb $a class glb $.

  $( Extend class notation with poset join. $)
  cjn $a class join $.

  $( Extend class notation with poset meet. $)
  cmee $a class meet $.

  $( Def. 8.4.2 p. 204 $)
  $( Define the class of base set extractors for posets and related
     structures. $)
  df-base $a |- base = ( p e. _V |-> ( p ` 1 ) ) $.

  ${
    $d p K $.
    baseval.1 $e |- K e. _V $.
    $( Value of the base set extractor. $)
    baseval $p |- ( base ` K ) = ( K ` 1 ) $=
      ( vp cbs c1 df-base strcval ) DCAEBCFG $.
      $( [4-Sep-2011] $)
  $}

  $( Def. 8.4.2 p. 204. $)
  $( Define less-than-or-equal ordering for posets and related structures. $)
  df-ple $a |- le = ( p e. _V |-> ( p ` 2 ) ) $.

  ${
    $d p K $.
    pleval.1 $e |- K e. _V $.
    $( Value of the poset ordering extractor. $)
    pleval $p |- ( le ` K ) = ( K ` 2 ) $=
      ( vp cple c2 df-ple strcval ) DCAEBCFG $.
      $( [9-Sep-2011] $)
  $}

  ${
    $d f b r x y z $.
    $( Def. 8.4.1 p. 203. $)
    $( Define the class of posets. $)
    df-poset $a |- PosetNEW = Struct ( 2 , f , E. b E. r
              ( b = ( base ` f )
               /\ r = ( le ` f )
              /\ ( b =/= (/)
               /\ A. x e. b A. y e. b A. z e. b ( x r x
                 /\ ( ( x r y /\ y r x ) -> x = y )
                 /\ ( ( x r y /\ y r z ) -> x r z ) ) ) ) ) $.
  $}

  ${
    $d b p r x y z B $.  $d b p r K $.  $d b p r x y z L $.
    ispos.0NEW $e |- S = Struct ( 2 , p , T. ) $.
    ispos.1NEW $e |- B = ( base ` K ) $.
    ispos.2NEW $e |- L = ( le ` K ) $.
    $( The predicate "is a poset." $)
    isposNEW $p |- ( K e. PosetNEW <-> ( K e. S /\ B =/= (/)
            /\ A. x e. B A. y e. B A. z e. B ( x L x
                 /\ ( ( x L y /\ y L x ) -> x = y )
                 /\ ( ( x L y /\ y L z ) -> x L z ) ) ) ) $=
      ( vb vr cpo wcel cv cbs cfv wceq cple c0 wne wbr wa wi w3a wral wex c2
      cstru df-poset eleq2i fveq2 syl6eqr eqeq2d 3anbi12d 2exbidv elstr2 cvv
      fvex eqeltri neeq1 raleq raleqbi1dv anbi12d breq imbi1d imbi12d
      3anbi123d ralbidv 2ralbidv anbi2d ceqsex2v anbi2i 3anass bitr4i 3bitri )
