    $( A poset ordering is transitive, using greater-than-or-equal ordering. $)
    posgetrNEW $p |- ( ( P e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X S Y /\ Y S Z ) -> X S Z ) ) $=
