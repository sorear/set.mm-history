      $( [11-Sep-2011] $)

    $( A poset ordering is asymetric. $)
    posasym $p |- ( ( P e. PosetNEW /\ X e. B /\ Y e. B )
                -> ( ( X R Y /\ Y R X ) -> X = Y ) ) $=
      ( cpo wcel w3a wbr wa wceq wi poslem 3simp1 3simp2 3simp3 syl3an2c syl )
