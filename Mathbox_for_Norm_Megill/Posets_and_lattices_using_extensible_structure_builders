$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
               Posets and lattices using extensible structure builders
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c base $.
  $c le $.
  $c PosetNEW $.
  $c lt $.
  $c geNEW $.
  $c gt $.
  $c lub $.
  $c glb $.
  $c join $.
  $c meet $.

  $( Extend class notation with the class of all base set extractors. $)
  cbs $a class base $.

  $( Extend class notation with less-than-or-equal for posets. $)
  cple $a class le $.

  $( Extend class notation with the class of posets. $)
  cpo $a class PosetNEW $.

  $( Extend class notation with less-than for posets. $)
  cplt $a class lt $.

  $( Extend class notation with greater-than-or-equal for posets. $)
  cpge $a class geNEW $.

  $( Extend class notation with greater-than for posets. $)
  cpgt $a class gt $.

  $( Extend class notation with poset least upper bound. $)
  club $a class lub $.

  $( Extend class notation with poset greatest lower bound. $)
  cglb $a class glb $.

  $( Extend class notation with poset join. $)
  cjn $a class join $.

  $( Extend class notation with poset meet. $)
  cmee $a class meet $.

  $( Def. 8.4.2 p. 204 $)
  $( Define the class of base set extractors for posets and related
     structures. $)
  df-base $a |- base = ( p e. _V |-> ( p ` 1 ) ) $.

  ${
    $d p K $.
    baseval.1 $e |- K e. _V $.
    $( Value of the base set extractor. $)
    baseval $p |- ( base ` K ) = ( K ` 1 ) $=
      ( vp cbs c1 df-base strcval ) DCAEBCFG $.
      $( [4-Sep-2011] $)
  $}

  $( Def. 8.4.2 p. 204. $)
  $( Define less-than-or-equal ordering for posets and related structures. $)
  df-ple $a |- le = ( p e. _V |-> ( p ` 2 ) ) $.

  ${
    $d p K $.
    pleval.1 $e |- K e. _V $.
    $( Value of the poset ordering extractor. $)
    pleval $p |- ( le ` K ) = ( K ` 2 ) $=
      ( vp cple c2 df-ple strcval ) DCAEBCFG $.
      $( [9-Sep-2011] $)
  $}

  ${
    $d f b r x y z $.
    $( Def. 8.4.1 p. 203. $)
    $( Define the class of posets. $)
    df-poset $a |- PosetNEW = Struct ( 2 , f , E. b E. r
              ( b = ( base ` f )
               /\ r = ( le ` f )
              /\ ( b =/= (/)
               /\ A. x e. b A. y e. b A. z e. b ( x r x
                 /\ ( ( x r y /\ y r x ) -> x = y )
                 /\ ( ( x r y /\ y r z ) -> x r z ) ) ) ) ) $.
  $}

  ${
    $d b p r x y z B $.  $d b p r K $.  $d b p r x y z L $.
    ispos.0NEW $e |- S = Struct ( 2 , p , T. ) $.
    ispos.1NEW $e |- B = ( base ` K ) $.
    ispos.2NEW $e |- L = ( le ` K ) $.
    $( The predicate "is a poset." $)
    isposNEW $p |- ( K e. PosetNEW <-> ( K e. S /\ B =/= (/)
            /\ A. x e. B A. y e. B A. z e. B ( x L x
                 /\ ( ( x L y /\ y L x ) -> x = y )
                 /\ ( ( x L y /\ y L z ) -> x L z ) ) ) ) $=
      ( vb vr cpo wcel cv cbs cfv wceq cple c0 wne wbr wa wi w3a wral wex c2
      cstru df-poset eleq2i fveq2 syl6eqr eqeq2d 3anbi12d 2exbidv elstr2 cvv
      fvex eqeltri neeq1 raleq raleqbi1dv anbi12d breq imbi1d imbi12d
      3anbi123d ralbidv 2ralbidv anbi2d ceqsex2v anbi2i 3anass bitr4i 3bitri )
      FNOFLPZHPZQRZSZMPZVSTRZSZVRUAUBZAPZWFWBUCZWFBPZWBUCZWHWFWBUCZUDZWFWHSZUEZ
      WIWHCPZWBUCZUDZWFWNWBUCZUEZUFZCVRUGZBVRUGZAVRUGZUDZUFZMUHLUHZHUIUJZOFEOZV
      RDSZWBGSZXCUFZMUHLUHZUDZXGDUAUBZWFWFGUCZWFWHGUCZWHWFGUCZUDZWLUEZXOWHWNGUC
      ZUDZWFWNGUCZUEZUFZCDUGZBDUGADUGZUFZNXFFABCHMLUKULXEXKEHFUIIVSFSZXDXJLMYGW
      AXHWDXIXCYGVTDVRYGVTFQRZDVSFQUMJUNUOYGWCGWBYGWCFTRZGVSFTUMKUNUOUPUQURXLXG
      XMYEUDZUDYFXKYJXGXCXMWSCDUGZBDUGZADUGZUDYJLMDGDYHUSJFQUTVAGYIUSKFTUTVAXHW
      EXMXBYMVRDUAVBXAYLAVRDWTYKBVRDWSCVRDVCVDVDVEXIYMYEXMXIYKYDABDDXIWSYCCDXIW
      GXNWMXRWRYBWFWFWBGVFXIWKXQWLXIWIXOWJXPWFWHWBGVFZWHWFWBGVFVEVGXIWPXTWQYAXI
      WIXOWOXSYNWHWNWBGVFVEWFWNWBGVFVHVIVJVKVLVMVNXGXMYEVOVPVQ $.
      $( [9-Sep-2011] $)
  $}

  ${
    $d p x y z B $.  $d p K $.  $d p x y z L $.  $d x y z X $.  $d y z Y $.
    $d z Z $.
    poslem.b $e |- B = ( base ` K ) $.
    poslem.l $e |- L = ( le ` K ) $.
    $( Lemma for poset properties. $)
    poslem $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( X L X
                 /\ ( ( X L Y /\ Y L X ) -> X = Y )
                 /\ ( ( X L Y /\ Y L Z ) -> X L Z ) ) ) $=
      ( vx vy vz vp cpo wcel cv wbr wa wceq wi w3a wral wtru c2 cstru c0 wne
      eqid isposNEW simp3bi breq1 breq2 bitrd anbi12d eqeq1 imbi12d biidd
      3anbi123d eqeq2 anbi2d rcla43v mpan9 ) BMNZIOZVCCPZVCJOZCPZVEVCCPZQZVCVER
      ZSZVFVEKOZCPZQZVCVKCPZSZTZKAUAJAUAIAUAZDANEANFANTDDCPZDECPZEDCPZQZDERZSZV
      SEFCPZQZDFCPZSZTZVBBUBLUCUDZNAUEUFVQIJKAWIBCLWIUGGHUHUIVPWHVRDVECPZVEDCPZ
      QZDVERZSZWJVLQZDVKCPZSZTVRWCVSEVKCPZQZWPSZTIJKDEFAAAVCDRZVDVRVJWNVOWQXAVD
      DVCCPVRVCDVCCUJVCDDCUKULXAVHWLVIWMXAVFWJVGWKVCDVECUJZVCDVECUKUMVCDVEUNUOX
      AVMWOVNWPXAVFWJVLVLXBXAVLUPUMVCDVKCUJUOUQVEERZVRVRWNWCWQWTXCVRUPXCWLWAWMW
      BXCWJVSWKVTVEEDCUKZVEEDCUJUMVEEDURUOXCWOWSWPWPXCWJVSVLWRXDVEEVKCUJUMXCWPU
      PUOUQVKFRZVRVRWCWCWTWGXEVRUPXEWCUPXEWSWEWPWFXEWRWDVSVKFECUKUSVKFDCUKUOUQU
      TVA $.
      $( [11-Sep-2011] $)

    $( A poset ordering is reflexive. $)
    posref $p |- ( ( K e. PosetNEW /\ X e. B ) -> X L X ) $=
      ( cpo wcel wa wbr wceq wi w3a poslem id 3jca sylan2 simp1 syl ) BGHZDAHZI
      DDCJZUBUBIZDDKLZUCUBLZMZUBTUAUAUAMUFUAABCDDDEFNUAUAUAUAUAOZUGUGPQUBUDUERS
      $.
      $( [11-Sep-2011] $)

    $( A poset ordering is asymetric. $)
    posasymb $p |- ( ( K e. PosetNEW /\ X e. B /\ Y e. B )
                -> ( ( X L Y /\ Y L X ) <-> X = Y ) ) $=
      ( cpo wcel w3a wbr wa wceq wi simp1 simp2 simp3 poslem syl13anc syl
      breq2 posref syl5cbi breq1 jcad 3adant3 impbid ) BHIZDAIZEAIZJZDECKZEDCKZ
      LZDEMZUKDDCKZUNUONZULEECKLULNZJZUQUKUHUIUJUJUSUHUIUJOUHUIUJPUHUIUJQZUTABC
      DEEFGRSUPUQURPTUHUIUOUNNUJUHUILZUOULUMUOUPULVADEDCUAABCDFGUBZUCUOUPUMVADE
      DCUDVBUCUEUFUG $.
      $( [21-Oct-2011] $)

    $( A poset ordering is transitive. $)
    postrNEW $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X L Y /\ Y L Z ) -> X L Z ) ) $=
      ( cpo wcel w3a wa wbr wceq wi poslem simp3d ) BIJDAJEAJFAJKLDDCMDECMZEDCM
      LDENOREFCMLDFCMOABCDEFGHPQ $.
      $( [11-Sep-2011] $)
  $}

  ${
    $d p x y z B $.  $d p K $.  $d p x y z L $.
    isposi.0NEW $e |- K e. Struct ( 2 , p , T. ) $.
    isposi.1NEW $e |- B = ( base ` K ) $.
    isposi.2NEW $e |- L = ( le ` K ) $.
    isposi.3NEW $e |- B =/= (/) $.
    isposi.4NEW $e |- ( x e. B -> x L x ) $.
    isposi.5NEW $e |- ( ( x e. B /\ y e. B ) ->
                   ( ( x L y /\ y L x ) -> x = y ) ) $.
    isposi.6NEW $e |- ( ( x e. B /\ y e. B /\ z e. B ) ->
                   ( ( x L y /\ y L z ) -> x L z ) ) $.
    $( Properties that determine a poset (constructed structure version). $)
    isposiNEW $p |- K e. PosetNEW $=
      ( cpo wcel wtru c2 cstru c0 wne cv wbr wa wceq wi w3a wral eqid isposNEW
      3ad2ant1 3adant3 3jca rgen3 mpbir3an ) EOPEQGRSZPDTUAAUBZUQFUCZUQBUBZFUCZ
      USUQFUCUDUQUSUEUFZUTUSCUBZFUCUDUQVBFUCUFZUGZCDUHBDUHADUHABCDUPEFGUPUIIJUJ
      HKVDABCDDDUQDPZUSDPZVBDPZUGURVAVCVEVFURVGLUKVEVFVAVGMULNUMUNUO $.
      $( [11-Sep-2011] $)
  $}

  ${
    $d p x y z B $.  $d p K $.  $d p x y z L $.
    isposix.aNEW $e |- B e. _V $.
    isposix.bNEW $e |- L e. _V $.
    isposix.1NEW $e |- K = { <. 1 , B >. , <. 2 , L >. } $.
    isposix.2NEW $e |- B =/= (/) $.
    isposix.3NEW $e |- ( x e. B -> x L x ) $.
    isposix.4NEW $e |- ( ( x e. B /\ y e. B ) ->
                   ( ( x L y /\ y L x ) -> x = y ) ) $.
    isposix.5NEW $e |- ( ( x e. B /\ y e. B /\ z e. B ) ->
                   ( ( x L y /\ y L z ) -> x L z ) ) $.
    $( Properties that determine a poset (explicit structure version). $)
    isposixNEW $p |- K e. PosetNEW $=
      ( vp stb2strx cbs df-base stb2v1x cple df-ple stb2v2x isposiNEW ) ABCDEFN
      NEDFGHIOPNEDFGHINQRSNEDFGHINTUAJKLMUB $.
      $( [11-Sep-2011] $)
  $}

  $( Def. 8.4.3 p. 204. $)
  $( Define less-than ordering for posets and related structures. $)
  df-plt $a |- lt = ( p e. _V |-> ( ( le ` p ) \ _I ) ) $.

  ${
    $d p K $.
    pltval.l $e |- L = ( le ` K ) $.
    pltval.s $e |- S = ( lt ` K ) $.
    $( Less-than relation.  (Def. ~ df-pss analog.) $)
    pltval $p |- ( ( K e. A /\ X e. B /\ Y e. C )
        -> ( X S Y <-> ( X L Y /\ X =/= Y ) ) ) $=
      ( vp wcel wbr wne wa wb cple cfv cid cdif cplt cvv wceq elisset cv fveq2
      difeq1d df-plt fvex difexg ax-mp fvmpt syl syl5eq breqd wn ideqg
      necon3bbid adantl anbi2d difeq1i breqi brdif bitr3i syl5bb sylan9bb
      3impb ) EALZGBLZHCLZGHDMZGHFMZGHNZOZPVHVKGHEQRZSTZMZVIVJOZVNVHDVPGHVHEUAR
      ZVPDVHEUBLVSVPUCEAUDKEKUEZQRZSTVPUBUAVTEUCWAVOSVTEQUFUGKUHVOUBLVPUBLEQUIV
      OSUBUJUKULUMJUNUOVRVLGHSMZUPZOZVNVQVRWCVMVLVJWCVMPVIVJWBGHGHCUQURUSUTVQGH
      FSTZMWDGHWEVPFVOSIVAVBGHFSVCVDVEVFVG $.
      $( [12-Oct-2011] $)

    $( Less-than implies less-than-or-equal.  (Th. ~ pssss analog.) $)
    pltle $p |- ( ( K e. A /\ X e. B /\ Y e. C ) -> ( X S Y -> X L Y ) ) $=
      ( wcel w3a wbr wne pltval simprbda ex ) EAKGBKHCKLZGHDMZGHFMZRSTGHNABCDEF
      GHIJOPQ $.
      $( [4-Dec-2011] $)
  $}

  ${
    pltne.s $e |- S = ( lt ` K ) $.
    $( Less-than relation.  (Def. ~ df-pss analog.) $)
    pltne $p |- ( ( K e. A /\ X e. B /\ Y e. C )
        -> ( X S Y -> X =/= Y ) ) $=
      ( wcel w3a wbr wne cple cfv eqid pltval simplbda ex ) EAIFBIGCIJZFGDKZFGL
      ZSTFGEMNZKUAABCDEUBFGUBOHPQR $.
      $( [2-Dec-2011] $)

    $( The less-than relation is not reflexive.  (Th. ~ pssirr analog.) $)
    pltirr $p |- ( ( K e. A /\ X e. B ) -> -. X S X ) $=
      ( wcel wa wceq wbr wn eqid wne wi pltne 3anidm23 necon2bd mpi ) DAGZEBGZH
      ZEEIEECJZKELUAUBEESTUBEEMNABBCDEEFOPQR $.
      $( [7-Feb-2012] $)
  $}

  ${
    pleval2.b $e |- B = ( base ` K ) $.
    pleval2.l $e |- L = ( le ` K ) $.
    pleval2.s $e |- S = ( lt ` K ) $.
    $( Less-than-or-equal in terms of less-than.  (Th. ~ sspss analog.) $)
    pleval2 $p |- ( ( K e. PosetNEW /\ X e. B /\ Y e. B )
        -> ( X L Y <-> ( X S Y \/ X = Y ) ) ) $=
      ( cpo wcel w3a wbr wceq wo wne wa wi wb breq2 posref 3adant3 syl5cbi
      pm4.72 sylib ordi wn exmid df-ne orbi2i mpbir mpbiran2 syl6bbr pltval
      orbi2d bitr4d orcom syl6bb ) CJKZEAKZFAKZLZEFDMZEFNZEFBMZOZVEVDOVBVCVDVCE
      FPZQZOZVFVBVCVDVCOZVIVBVDVCRVCVJSVDEEDMZVCVBEFEDTUSUTVKVAACDEGHUAUBUCVDVC
      UDUEVIVJVDVGOZVDVCVGUFVLVDVDUGZOVDUHVGVMVDEFUIUJUKULUMVBVEVHVDJAABCDEFHIU
      NUOUPVDVEUQUR $.
      $( [17-Oct-2011] $)

    $( Less-than imples not inverse less-than-or-equal. $)
    pltnle $p |- ( ( ( K e. PosetNEW /\ X e. B /\ Y e. B ) /\ X S Y )
                    -> -. Y L X ) $=
      ( cpo wcel w3a wbr wn wne wa pltval wceq posasymb biimpd expdimp
      necon3ad expimpd sylbid imp ) CJKEAKFAKLZEFBMZFEDMZNZUFUGEFDMZEFOZPUIJAAB
      CDEFHIQUFUJUKUIUFUJPUHEFUFUJUHEFRZUFUJUHPULACDEFGHSTUAUBUCUDUE $.
      $( [18-Oct-2011] $)

    $( Alternate expression for less-than relation.  (Th. ~ dfpss3 analog.) $)
    pltval3 $p |- ( ( K e. PosetNEW /\ X e. B /\ Y e. B )
        -> ( X S Y <-> ( X L Y /\ -. Y L X ) ) ) $=
      ( cpo wcel w3a wbr wne wa wn pltval wceq wi breq1 posref 3adant3 syl5cbi
      adantr posasymb biimpd expdimp impbid necon3abid pm5.32da bitrd ) CJKZEAK
      ZFAKZLZEFBMEFDMZEFNZOUPFEDMZPZOJAABCDEFHIQUOUPUQUSUOUPOZUREFUTEFRZURUOVAU
      RSUPVAEEDMZURUOEFEDTULUMVBUNACDEGHUAUBUCUDUOUPURVAUOUPUROVAACDEFGHUEUFUGU
      HUIUJUK $.
      $( [4-Nov-2011] $)
  $}

  ${
    pltnlt.b $e |- B = ( base ` K ) $.
    pltnlt.s $e |- S = ( lt ` K ) $.
    $( The less-than relation implies the negation of its inverse. $)
    pltnlt $p |- ( ( ( K e. PosetNEW /\ X e. B /\ Y e. B ) /\ X S Y )
                  -> -. Y S X ) $=
      ( cpo wcel w3a wbr wa cple cfv eqid pltnle wi pltle 3com23 adantr mtod )
      CHIZDAIZEAIZJZDEBKZLEDBKZEDCMNZKZABCUHDEFUHOZGPUEUGUIQZUFUBUDUCUKHAABCUHE
      DUJGRSTUA $.
      $( [18-Oct-2011] $)

    $( The less-than relation has no 2-cycle loops.  (Th. ~ pssn2lp analog.) $)
    pltn2lp $p |- ( ( K e. PosetNEW /\ X e. B /\ Y e. B )
        -> -. ( X S Y /\ Y S X ) ) $=
      ( cpo wcel w3a wbr wn wi wa cple cfv eqid pltnle ex pltle 3com23 nsyld
      imnan sylib ) CHIZDAIZEAIZJZDEBKZEDBKZLMUIUJNLUHUIEDCOPZKZUJUHUIULLABCUKD
      EFUKQZGRSUEUGUFUJULMHAABCUKEDUMGTUAUBUIUJUCUD $.
      $( [2-Dec-2011] $)

    $( The less-than relation is transitive.  (Th. ~ psstr analog.) $)
    plttr $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
        -> ( ( X S Y /\ Y S Z ) -> X S Z ) ) $=
      ( cpo wcel w3a wa wbr cple cfv wne eqid postrNEW wi pltle 3adant3r3
      3adant3r1 syl2and wceq wn breq2 anbi2d notbid pltn2lp syl5cbi necon2ad
      jcad wb pltval 3adant3r2 sylibrd ) CIJZDAJZEAJZFAJZKLZDEBMZEFBMZLZDFCNOZM
      ZDFPZLZDFBMZVAVDVFVGVADEVEMZEFVEMZVFVBVCACVEDEFGVEQZRUQURUSVBVJSUTIAABCVE
      DEVLHTUAUQUSUTVCVKSURIAABCVEEFVLHTUBUCVAVDDFDFUDZVBEDBMZLZUEZVDUEVAVMVOVD
      VMVNVCVBDFEBUFUGUHUQURUSVPUTABCDEGHUIUAUJUKULUQURUTVIVHUMUSIAABCVEDFVLHUN
      UOUP $.
      $( [2-Dec-2011] $)
  $}

  ${
    pltletr.b $e |- B = ( base ` K ) $.
    pltletr.l $e |- L = ( le ` K ) $.
    pltletr.s $e |- S = ( lt ` K ) $.
    $( Transitive law for chained less-than and less-than-or-equal.  (Th.
       ~ psssstr analog.) $)
    pltletr $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
        -> ( ( X S Y /\ Y L Z ) -> X S Z ) ) $=
      ( cpo wcel w3a wa wbr wceq wo wb pleval2 3adant3r1 adantr plttr expdimp
      wi breq2 biimpcd adantl jaod sylbid expimpd ) CKLZEALZFALZGALZMNZEFBOZFGD
      OZEGBOZUOUPNZUQFGBOZFGPZQZURUOUQVBRZUPUKUMUNVCULABCDFGHIJSTUAUSUTURVAUOUP
      UTURABCEFGHJUBUCUPVAURUDUOVAUPURFGEBUEUFUGUHUIUJ $.
      $( [2-Dec-2011] $)

    $( Transitive law for chained less-than-or-equal and less-than.  (Th.
       ~ sspsstr analog.) $)
    plelttr $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
        -> ( ( X L Y /\ Y S Z ) -> X S Z ) ) $=
      ( cpo wcel w3a wa wbr wceq wo wi wb pleval2 3adant3r3 plttr exp3a breq1
      biimprd a1i jaod sylbid imp3a ) CKLZEALZFALZGALZMNZEFDOZFGBOZEGBOZUNUOEFB
      OZEFPZQZUPUQRZUJUKULUOUTSUMABCDEFHIJTUAUNURVAUSUNURUPUQABCEFGHJUBUCUSVARU
      NUSUQUPEFGBUDUEUFUGUHUI $.
      $( [2-May-2012] $)
  $}

  $( Def. 8.4.3 p. 204. $)
  $( Define the class of greater-than-or-equal ordering for posets and related
     structures. $)
  df-pge $a |- geNEW = ( p e. _V |-> `' ( le ` p ) ) $.

  $( Def. 8.4.3 p. 204. $)
  $( Define the class of greater-than ordering for posets and related
     structures. $)
  df-pgt $a |- gt = ( p e. _V |-> `' ( lt ` p ) ) $.

  ${
    $d p K $.  $d p L $.
    pgeval.l $e |- L = ( le ` K ) $.
    pgeval.s $e |- S = ( geNEW ` K ) $.
    $( Value of greater-than-or-equal ordering for a poset. $)
    pgeval $p |- ( ( K e. A /\ X e. B /\ Y e. C ) -> ( X S Y <-> Y L X ) ) $=
      ( vp wcel wbr wb ccnv wa cpge cfv cvv wceq elisset cv cple fveq2 syl6eqr
      cnveq syl df-pge fvex eqeltri cnvex fvmpt syl5eq breqd brcnvg sylan9bb
      3impb ) EALZGBLZHCLZGHDMZHGFMZNURVAGHFOZMUSUTPVBURDVCGHUREQRZVCDURESLVDVC
      TEAUAKEKUBZUCRZOZVCSQVEETZVFFTVGVCTVHVFEUCRZFVEEUCUDIUEVFFUFUGKUHFFVISIEU
      CUIUJUKULUGJUMUNGHBCFUOUPUQ $.
      $( [17-Sep-2011] $)
  $}

  ${
    $d p x y z B $.  $d p K $.  $d p x y z S $.  $d x y z X $.  $d y z Y $.
    $d z Z $.
    posgelem.b $e |- B = ( base ` K ) $.
    posgelem.s $e |- S = ( geNEW ` K ) $.
    $( Lemma for poset properties, using greater-than-or-equal ordering. $)
    posgelem $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( X S X
                 /\ ( ( X S Y /\ Y S X ) <-> X = Y )
                 /\ ( ( X S Y /\ Y S Z ) -> X S Z ) ) ) $=
      ( cpo wcel w3a wa wbr wceq wb wi cple cfv eqid posref pgeval 3anidm23
      mpbird 3ad2antr1 3com23 anbi12d posasymb bitrd ancom syl5bb 3adant3r3
      postrNEW 3anrev sylan2b 3adant3r1 3adant3r2 3imtr4d ancomsd 3jca ) CIJZDA
      JZEAJZFAJZKZLZDDBMZDEBMZEDBMZLZDENZOZVGEFBMZLDFBMZPUTVBVAVFVCUTVALVFDDCQR
      ZMZACVNDGVNSZTUTVAVFVOOIAABCVNDDVPHUAUBUCUDUTVAVBVKVCUTVAVBKZVHVGLZVJVIVQ
      VRDEVNMZEDVNMZLVJVQVHVSVGVTUTVBVAVHVSOIAABCVNEDVPHUAUEIAABCVNDEVPHUAZUFAC
      VNDEGVPUGUHVGVHUIUJUKVEVLVGVMVEFEVNMZVTLZFDVNMZVLVGLVMUTVCVBVAKWCWDPVDACV
      NFEDGVPULVAVBVCUMUNVEVLWBVGVTUTVBVCVLWBOVAIAABCVNEFVPHUAUOUTVAVBVGVTOVCWA
      UKUFUTVAVCVMWDOVBIAABCVNDFVPHUAUPUQURUS $.
      $( [22-Oct-2011] $)

    $( A poset ordering is reflexive, using greater-than-or-equal ordering. $)
    posgeref $p |- ( ( K e. PosetNEW /\ X e. B ) -> X S X ) $=
      ( cpo wcel wa wbr wceq wb wi w3a posgelem id 3jca sylan2 simp1 syl ) CGHZ
      DAHZIDDBJZUCUCIZDDKLZUDUCMZNZUCUAUBUBUBNUGUBABCDDDEFOUBUBUBUBUBPZUHUHQRUC
      UEUFST $.
      $( [17-Sep-2011] $)

    $( A poset ordering is asymetric, using greater-than-or-equal ordering. $)
    posgeasymb $p |- ( ( K e. PosetNEW /\ X e. B /\ Y e. B )
                -> ( ( X S Y /\ Y S X ) <-> X = Y ) ) $=
      ( cpo wcel w3a wbr wa wceq wb wi simp1 simp2 simp3 posgelem syl13anc syl
      ) CHIZDAIZEAIZJZDDBKZDEBKZEDBKLDEMNZUGEEBKLUGOZJZUHUEUBUCUDUDUJUBUCUDPUBU
      CUDQUBUCUDRZUKABCDEEFGSTUFUHUIQUA $.
      $( [17-Sep-2011] $)

    $( A poset ordering is transitive, using greater-than-or-equal ordering. $)
    posgetrNEW $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X S Y /\ Y S Z ) -> X S Z ) ) $=
      ( cpo wcel w3a wa wbr wceq wb wi posgelem simp3d ) CIJDAJEAJFAJKLDDBMDEBM
      ZEDBMLDENOSEFBMLDFBMPABCDEFGHQR $.
      $( [17-Sep-2011] $)
  $}

  ${
    $d p s t u x y z $.
    $( Def. 8.4.4 p. 205. $)
    $( Define poset least upper bound.  If it doesn't exist, an undefined value
       not in the base set is returned. $)
    df-lub $a |- lub = ( p e. _V |-> ( s e. ~P ( base ` p ) |->
        ( iota_ x e. ( base ` p ) ( A. y e. s y ( le ` p ) x
                          /\ A. z e. ( base ` p )
                  ( A. y e. s y ( le ` p ) z -> x ( le ` p ) z ) ) ) ) ) $.

    $( Def. 8.4.4 p. 205. $)
    $( Define poset least upper bound.  If it doesn't exist, an undefined value
       not in the base set is returned. $)
    $(
    df-lubOLD $a |- lub = ( p e. _V |-> ( s e. ~P ( base ` p ) |->
     { t | E. u ( u = { x e. ( base ` p ) | ( A. y e. s y ( le ` p ) x
                          /\ A. z e. ( base ` p )
                             ( A. y e. s y ( le ` p ) z -> x ( le ` p ) z ) ) }
      /\ t = if ( E. v u = { v } , U. u , ( Undef ` ( base ` p ) ) ) ) } ) ) $.
    $)

    $( Def. 8.4.4 p. 205. $)
    $( Define poset greatest lower bound. $)
    df-glb $a |- glb = ( p e. _V |-> ( s e. ~P ( base ` p ) |->
        ( iota_ x e. ( base ` p ) ( A. y e. s y ( geNEW ` p ) x
                          /\ A. z e. ( base ` p )
                  ( A. y e. s y ( geNEW ` p ) z -> x ( geNEW ` p ) z ) ) ) ) )
        $.

    $( Def. 8.4.4 p. 205. $)
    $( Define poset greatest lower bound. $)
    $(
    df-glbOLD $a |- glb = ( p e. _V |-> ( s e. ~P ( base ` p ) |->
          { t | E. u ( u = { x e. ( base ` p ) | ( A. y e. s y ( geNEW ` p ) x
        /\
       A. z e. ( base ` p ) ( A. y e. s y ( geNEW ` p ) z -> x ( geNEW ` p ) z
        ) ) }
      /\ t = if ( E. v u = { v } , U. u , ( Undef ` ( base ` p ) ) ) ) } ) ) $.
    $)

    $( Def. 8.4.4 p. 205. $)
    $( Define poset join. $)
    df-join $a |- join = ( p e. _V |-> ( x e. ( base ` p ) , y e. ( base ` p )
        |-> ( ( lub ` p ) ` { x , y } ) ) ) $.

    $( Def. 8.4.4 p. 205. $)
    $( Define poset meet. $)
    df-meet $a |- meet = ( p e. _V |-> ( x e. ( base ` p ) , y e. ( base ` p )
        |-> ( ( glb ` p ) ` { x , y } ) ) ) $.
  $}

