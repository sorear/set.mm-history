    latm4 $p |- ( ( K e. OL /\ ( X e. B /\ Y e. B ) /\ ( Z e. B /\ W e. B ) )
        -> ( ( X M Y ) M ( Z M W ) ) = ( ( X M Z ) M ( Y M W ) ) ) $=
      ( col wcel wa w3a co wceq latm12 3simp1 3simp2r 3simp3l 3simp3r syl3an2c
      opreq2d olmass 3simp2l clat latmcl ollat 3ad2ant1 syl3anc 3eqtr4d ) BJKZ
