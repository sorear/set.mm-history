    $( Part of proof of Lemma E in [Crawley] p. 113. $)
    cdleme23a $p |- ( ( ( K e. HL /\ W e. H
         /\ ( S e. A /\ -. S .<_ W ) )
     /\ ( ( T e. A /\ -. T .<_ W ) /\ X e. B /\ -. X .<_ W )
     /\ ( S =/= T /\ ( S .\/ ( X ./\ W ) ) = X /\ ( T .\/ ( X ./\ W ) ) = X ) )
            -> V .<_ W ) $=
