    $( Define isomorphism H. $)
    df-dih $a |- DIsoH = ( k e. _V |-> ( w e. ( LHyp ` k ) |->
  ( x e. ( Base ` k ) |-> if ( x ( le ` k ) w , ( ( DIsoB ` k ) ` w ) ,
    ( iota_ u e. ( LSubSp ` ( ( DVecH ` k ) ` w ) )
