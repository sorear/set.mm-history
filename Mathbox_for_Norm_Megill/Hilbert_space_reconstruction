    cdlemefs50.e $e |- E = ( ( P J Q ) M ( D J ( ( s J t ) M W ) ) ) $.
    cdlemef50.f $e |- F = ( x e. B |->
         if ( ( P =/= Q /\ -. x L W ) ,
           $( Value when p ` =/= ` q and ` -. ` x ` <_ ` w. $)
           ( iota_ z e. B A. s e. A
                ( ( -. s L W /\ ( s J ( x M W ) ) = x ) -> z = (
                    if ( s L ( P J Q ) ,
                       $( Unique f_t(s) for all t, s ` <_ ` p ` \/ ` q $)
                       ( iota_ y e. B A. t e. A
                             ( ( -. t L W /\ -. t L ( P J Q ) ) -> y =
                          $( f_t(s) $)
                          E )
                       ) ,
                       $( f(s), ` -. ` s ` <_ ` p ` \/ ` q. $)
                       [_ s / t ]_ D
                    )
                J ( x M W ) ) ) ) ,
           $( Value when p = q or x ` <_ ` w is just x. $)
           x
         ) ) $.
