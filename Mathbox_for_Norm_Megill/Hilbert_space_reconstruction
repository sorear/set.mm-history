  ${
    $( TODO fix $d's $)
    $d s t x y z A $.  $d s t x y z B $.  $d s x y z D $.  $d x y z E $.
    $d s t x y z H $.  $d s t x y z .\/ $.  $d s t x y z K $.
    $d s t x y z .<_ $.  $d s t x y z ./\ $.  $d s t x y z P $.
    $d s t x y z Q $.  $d s t x y z R $.  $d s t x y z U $.  $d s t x y z W $.
    $d s t x y z A $.  $d s t x y z B $.  $d x y z D $.  $d x y z E $.
    $d s t x y z H $.  $d s t x y z .\/ $.  $d s t x y z K $.
    $d s t x y z .<_ $.  $d s t x y z ./\ $.  $d s t x y z P $.
    $d s t x y z Q $.  $d s t y R $.  $d s t x y z S $.  $d s t x y z U $.
    $d s t x y z W $.
    cdlemef46.b $e |- B = ( Base ` K ) $.
    cdlemef46.l $e |- .<_ = ( le ` K ) $.
    cdlemef46.j $e |- .\/ = ( join ` K ) $.
    cdlemef46.m $e |- ./\ = ( meet ` K ) $.
    cdlemef46.a $e |- A = ( Atoms ` K ) $.
    cdlemef46.h $e |- H = ( LHyp ` K ) $.
    cdlemef46.u $e |- U = ( ( P .\/ Q ) ./\ W ) $.
    $( f(t), ` -. ` t ` <_ ` p ` \/ ` q. $)
    cdlemef46.d $e |- D
        = ( ( t .\/ U ) ./\ ( Q .\/ ( ( P .\/ t ) ./\ W ) ) ) $.
    $( f_t(s), s ` <_ ` p ` \/ ` q $)
    cdlemefs46.e $e |- E
        = ( ( P .\/ Q ) ./\ ( D .\/ ( ( s .\/ t ) ./\ W ) ) ) $.
    cdlemef46.f $e |- F = ( x e. B |->
         if ( ( P =/= Q /\ -. x .<_ W ) ,
           $( Value when p ` =/= ` q and ` -. ` x ` <_ ` w. $)
           ( iota_ z e. B A. s e. A
                ( ( -. s .<_ W /\ ( s .\/ ( x ./\ W ) ) = x ) -> z = (
                    if ( s .<_ ( P .\/ Q ) ,
                       $( Unique f_t(s) for all t, s ` <_ ` p ` \/ ` q $)
                       ( iota_ y e. B A. t e. A
                             ( ( -. t .<_ W /\ -. t .<_ ( P .\/ Q ) ) -> y =
                          $( f_t(s) $)
                          E )
                       ) ,
                       $( f(s), ` -. ` s ` <_ ` p ` \/ ` q. $)
                       [_ s / t ]_ D
                    )
                .\/ ( x ./\ W ) ) ) ) ,
           $( Value when p = q or x ` <_ ` w is just x. $)
