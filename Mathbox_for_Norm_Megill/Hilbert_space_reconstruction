    cdlemef46.f $e |- F = ( x e. B |->
         if ( ( P =/= Q /\ -. x .<_ W ) ,
           $( Value when p ` =/= ` q and ` -. ` x ` <_ ` w. $)
           ( iota_ z e. B A. s e. A
                ( ( -. s .<_ W /\ ( s .\/ ( x ./\ W ) ) = x ) -> z = (
                    if ( s .<_ ( P .\/ Q ) ,
                       $( Unique f_t(s) for all t, s ` <_ ` p ` \/ ` q $)
                       ( iota_ y e. B A. t e. A
                             ( ( -. t .<_ W /\ -. t .<_ ( P .\/ Q ) ) -> y =
                          $( f_t(s) $)
                          E )
                       ) ,
                       $( f(s), ` -. ` s ` <_ ` p ` \/ ` q. $)
                       [_ s / t ]_ D
                    )
                .\/ ( x ./\ W ) ) ) ) ,
           $( Value when p = q or x ` <_ ` w is just x. $)
           x
         ) ) $.
