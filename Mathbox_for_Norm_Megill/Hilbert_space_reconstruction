    ${
      cdlemg2.u $e |- U = ( ( P .\/ Q ) ./\ W ) $.
      $( f(t), ` -. ` t ` <_ ` p ` \/ ` q. $)
      cdlemg2.d $e |- D
          = ( ( t .\/ U ) ./\ ( Q .\/ ( ( P .\/ t ) ./\ W ) ) ) $.
      $( f_t(s), s ` <_ ` p ` \/ ` q $)
      cdlemg2.e $e |- E
          = ( ( P .\/ Q ) ./\ ( D .\/ ( ( s .\/ t ) ./\ W ) ) ) $.
      cdlemg2.g $e |- G = ( x e. B |->
         if ( ( P =/= Q /\ -. x .<_ W ) ,
           $( Value when p ` =/= ` q and ` -. ` x ` <_ ` w. $)
           ( iota_ z e. B A. s e. A
                ( ( -. s .<_ W /\ ( s .\/ ( x ./\ W ) ) = x ) -> z = (
                    if ( s .<_ ( P .\/ Q ) ,
                       $( Unique f_t(s) for all t, s ` <_ ` p ` \/ ` q $)
                       ( iota_ y e. B A. t e. A
                             ( ( -. t .<_ W /\ -. t .<_ ( P .\/ Q ) ) -> y =
                          $( f_t(s) $)
                          E )
                       ) ,
                       $( f(s), ` -. ` s ` <_ ` p ` \/ ` q. $)
                       [_ s / t ]_ D
                    )
                .\/ ( x ./\ W ) ) ) ) ,
           $( Value when p = q or x ` <_ ` w is just x. $)
           x
         ) ) $.
      $( Any translation belongs to the set of functions constructed for
         ~ cdleme .  TODO:  Fix comment. $)
      cdlemg2c $p |- ( ( ( ( K e. HL /\ W e. H ) /\ ( P e. A /\ -. P .<_ W )
         /\ ( Q e. A /\ -. Q .<_ W ) ) /\ ( F ` P ) = Q )
         -> ( F e. T
               <-> F = G ) ) $=
        ( vf chlt wcel wa wbr wn w3a cfv wceq cv crio cdlemg1c cdlemg1b2 adantr
        eqid eqeq2d bitrd ) QUNUOTOUOUPHEUOHTRUQURUPIEUOITRUQURUPUSZHMUTIVAZUPZ
