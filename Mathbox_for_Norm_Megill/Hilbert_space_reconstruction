    ${
      dalawlem.o $e |- O = ( LPlanes ` K ) $.
      $( Lemma for ~ dalaw .  Special case of ~ dath2 , where ` C ` is replaced
         by ` ( ( P .\/ S ) ./\ ( Q .\/ T ) ) ` .  The remaining lemmas will
         eliminate the conditions on the atoms imposed by ~ dath2 . $)
      dalawlem1 $p |- ( ( ( K e. HL
           /\ ( P e. A /\ Q e. A /\ R e. A )
         /\ ( S e. A /\ T e. A /\ U e. A ) )
      /\ ( ( ( P .\/ Q ) .\/ R ) e. O /\ ( ( S .\/ T ) .\/ U ) e. O )
      /\ ( ( -. ( ( P .\/ S ) ./\ ( Q .\/ T ) ) .<_ ( P .\/ Q )
             /\ -. ( ( P .\/ S ) ./\ ( Q .\/ T ) ) .<_ ( Q .\/ R )
             /\ -. ( ( P .\/ S ) ./\ ( Q .\/ T ) ) .<_ ( R .\/ P ) )
         /\ ( -. ( ( P .\/ S ) ./\ ( Q .\/ T ) ) .<_ ( S .\/ T )
             /\ -. ( ( P .\/ S ) ./\ ( Q .\/ T ) ) .<_ ( T .\/ U )
             /\ -. ( ( P .\/ S ) ./\ ( Q .\/ T ) ) .<_ ( U .\/ S ) )
         /\ ( ( P .\/ S ) ./\ ( Q .\/ T ) ) .<_ ( R .\/ U ) ) )
           -> ( ( P .\/ Q ) ./\ ( S .\/ T ) )
               .<_ ( ( ( Q .\/ R ) ./\ ( T .\/ U ) )
                     .\/ ( ( R .\/ P ) ./\ ( U .\/ S ) ) ) ) $=
        ( chlt wcel w3a co wa wbr wn cbs cfv simp11 clat hllat syl simp121
        simp131 eqid hlatjcl syl111anc simp122 simp132 latmcl jca simp12
        simp13 simp2l simp2r simp31 simp32 latmle1 latmle2 simp33 3jca dath2
        syl323anc ) IRSZBASZCASZDASZTZEASZFASZGASZTZTZBCHUAZDHUALSZEFHUAZGHUALS
        ZUBZBEHUAZCFHUAZKUAZWBJUCUDWICDHUAZJUCUDWIDBHUAZJUCUDTZWIWDJUCUDWIFGHUA
        ZJUCUDWIGEHUAZJUCUDTZWIDGHUAJUCZTZTZVLWIIUEUFZSZUBVPVTWCWEWLWOWIWGJUCZW
        IWHJUCZWPTWBWDKUAZWJWMKUAZWKWNKUAZHUAJUCWRVLWTVLVPVTWFWQUGZWRIUHSZWGWSS
