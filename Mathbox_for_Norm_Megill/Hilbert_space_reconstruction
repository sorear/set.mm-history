    $( Part of proof of Lemma D in [Crawley] p. 113. $)
    cdlemd7 $p |- ( ( ( ( K e. HL /\ W e. H /\ R e. A )
                         /\ ( F e. ( T ` W ) /\ G e. ( T ` W ) ) )
          /\ ( ( P e. A /\ -. P L W ) /\ ( Q e. A /\ -. Q L W ) )
          /\ ( ( F ` P ) = ( G ` P ) /\ -. Q L ( P J ( F ` P ) ) ) )
        -> ( F ` R ) = ( G ` R ) ) $=
