    $( Lemma for ~ pl42 . $)
    pl42lem4 $p |- ( ( ( K e. HL /\ X e. B /\ Y e. B )
           /\ ( Z e. B /\ W e. B /\ V e. B ) )
     -> ( ( X L ( O ` Y ) /\ Z L ( O ` W ) )
        -> ( F ` ( ( ( ( X J Y ) M Z ) J W ) M V ) )
           C_ ( F ` ( ( X J Y ) J ( ( X J W ) M ( Y J V ) ) ) ) ) ) $=
      ( chlt wcel w3a cfv wbr wa co wss cin wceq pl42lem1 3impia pl42lem3
