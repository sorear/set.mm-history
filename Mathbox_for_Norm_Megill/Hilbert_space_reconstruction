    4thatleme.l $e |- .<_ = ( le ` K ) $.
    4thatleme.j $e |- .\/ = ( join ` K ) $.
    4thatleme.m $e |- ./\ = ( meet ` K ) $.
    4thatleme.a $e |- A = ( Atoms ` K ) $.
    4thatleme.h $e |- H = ( LHyp ` K ) $.
    $( Lemma for ~ 4atexlem7 . $)
    4atexlemex6 $p |- ( ( ( ( K e. HL /\ W e. H )
                /\ ( P e. A /\ -. P .<_ W ) /\ ( Q e. A /\ -. Q .<_ W ) )
            /\ ( S e. A
                /\ ( R e. A /\ -. R .<_ W /\ ( P .\/ R ) = ( Q .\/ R ) )
                /\ ( P =/= Q /\ -. S .<_ ( P .\/ Q ) ) ) )
        -> E. z e. A ( -. z .<_ W /\ ( P .\/ z ) = ( S .\/ z ) ) ) $=
