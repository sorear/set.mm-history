    $d s t x y z Q $.  $d s t x y z R $.  $d s t x y z U $.  $d s t x y z W $.
    $d s t x y z A $.  $d s t x y z B $.  $d x y z D $.  $d x y z E $.
    $d s t x y z H $.  $d s t x y z .\/ $.  $d s t x y z K $.
    $d s t x y z .<_ $.  $d s t x y z ./\ $.  $d s t x y z P $.
    $d s t x y z Q $.  $d s t y R $.  $d s t x y z S $.  $d s t x y z U $.
    $d s t x y z W $.  $d a b c u v A $.  $d a b c u v B $.  $d a b c v D $.
    $d a b c E $.  $d a b c u v F $.  $d a b c u v H $.  $d a b c u v .\/ $.
    $d a b c u v K $.  $d a b c u v .<_ $.  $d a b c u v ./\ $.
    $d a b c u v P $.  $d a b c u v Q $.  $d a b c v U $.  $d a b c u v W $.
    $d a b c s t u v x y z X $.  $d a b c s t u v x y z Y $.
    cdlemef50.b $e |- B = ( Base ` K ) $.
    cdlemef50.l $e |- .<_ = ( le ` K ) $.
    cdlemef50.j $e |- .\/ = ( join ` K ) $.
    cdlemef50.m $e |- ./\ = ( meet ` K ) $.
    cdlemef50.a $e |- A = ( Atoms ` K ) $.
    cdlemef50.h $e |- H = ( LHyp ` K ) $.
    cdlemef50.u $e |- U = ( ( P .\/ Q ) ./\ W ) $.
    $( f(t), ` -. ` t ` <_ ` p ` \/ ` q. $)
    cdlemef50.d $e |- D
        = ( ( t .\/ U ) ./\ ( Q .\/ ( ( P .\/ t ) ./\ W ) ) ) $.
    $( f_t(s), s ` <_ ` p ` \/ ` q $)
    cdlemefs50.e $e |- E
        = ( ( P .\/ Q ) ./\ ( D .\/ ( ( s .\/ t ) ./\ W ) ) ) $.
    cdlemef50.f $e |- F = ( x e. B |->
         if ( ( P =/= Q /\ -. x .<_ W ) ,
           $( Value when p ` =/= ` q and ` -. ` x ` <_ ` w. $)
           ( iota_ z e. B A. s e. A
                ( ( -. s .<_ W /\ ( s .\/ ( x ./\ W ) ) = x ) -> z = (
                    if ( s .<_ ( P .\/ Q ) ,
                       $( Unique f_t(s) for all t, s ` <_ ` p ` \/ ` q $)
