  ${
    $d a b c u v w A $.  $d a b c u v B $.  $d a b c u v H $.
    $d a b c u v w .\/ $.  $d a b c u v K $.  $d a b c u v .<_ $.
    $d a b c u v w ./\ $.  $d a b c u w N $.  $d a b c O $.
    $d a b c u v w P $.  $d a b c u v w Q $.  $d a b c u v w R $.
    $d a b c u v w S $.  $d a b c u v V $.  $d a b c u v w W $.
    cdlemef47.b $e |- B = ( Base ` K ) $.
    cdlemef47.l $e |- .<_ = ( le ` K ) $.
    cdlemef47.j $e |- .\/ = ( join ` K ) $.
    cdlemef47.m $e |- ./\ = ( meet ` K ) $.
    cdlemef47.a $e |- A = ( Atoms ` K ) $.
    cdlemef47.h $e |- H = ( LHyp ` K ) $.
    cdlemef47.v $e |- V = ( ( Q .\/ P ) ./\ W ) $.
    $( g(t), ` -. ` v ` <_ ` p ` \/ ` q. $)
    cdlemef47.n $e |- N
        = ( ( v .\/ V ) ./\ ( P .\/ ( ( Q .\/ v ) ./\ W ) ) ) $.
    $( g_t(s), u ` <_ ` p ` \/ ` q $)
    cdlemefs47.o $e |- O
        = ( ( Q .\/ P ) ./\ ( N .\/ ( ( u .\/ v ) ./\ W ) ) ) $.
    cdlemef47.g $e |- G = ( a e. B |->
         if ( ( Q =/= P /\ -. a .<_ W ) ,
           $( g(x) when p ` =/= ` q and ` -. ` a ` <_ ` w. $)
           ( iota_ c e. B A. u e. A
                ( ( -. u .<_ W /\ ( u .\/ ( a ./\ W ) ) = a ) -> c = (
                    $( g_t(s), u ` <_ ` p ` \/ ` q $)
                    if ( u .<_ ( Q .\/ P ) ,
                       ( iota_ b e. B A. v e. A
                             ( ( -. v .<_ W /\ -. v .<_ ( Q .\/ P ) )
                             -> b = O ) ) ,
                    $( g(s), ` -. ` u ` <_ ` p ` \/ ` q. $)
                       [_ u / v ]_ N )
                .\/ ( a ./\ W ) ) ) ) ,
           $( g(x) when p = q or a ` <_ ` w is just x. $)
           a ) ) $.
    $d a c u v X $.
