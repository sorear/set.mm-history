  $c LHyp $.
  $c LAut $.
  $c WAtoms $.
  $c PAut $.

  $( Extend class notation with set of all lattice hyperplanes. $)
  clh $a class LHyp $.

  $( Extend class notation with set of all lattice automorphisms. $)
  claut $a class LAut $.

  $( Extend class notation with W points. $)
  cwpoints $a class WAtoms $.

  $( Extend class notation with set of all projective automorphisms. $)
  cpaut $a class PAut $.

  ${
    $d k f d x y $.
    $( Define the set of lattice hyperplanes, which are all lattice elements
       covered by 1 (i.e. all co-atoms).  We call them "hyperplanes" instead of
       "co-atoms" in analogy with projective geometry hyperplanes. $)
    df-lhyp $a |- LHyp = ( k e. _V
         |-> { x e. ( base ` k ) | x ( <oNEW ` k ) ( 1. ` k ) } ) $.

    $( Def. 8.6.15, p. 233. $)
    $( Define set of lattice autoisomorphisms. $)
    df-laut $a |- LAut = ( k e. _V |-> { f | ( f : ( base ` k ) -1-1-onto->
      ( base ` k ) /\ A. x e. ( base ` k ) A. y e. ( base ` k )
        ( x ( le ` k ) y <-> ( f ` x ) ( le ` k ) ( f ` y ) ) ) } ) $.

    $( Define W atoms corresponding to a "designated atom" ` d ` .  These are
       all atoms not in the polarity of ` { d } ) ` , which is the hyperplane
       determined by ` d ` .  Definition of set W in [Crawley] p. 111. $)
    df-watoms $a |- WAtoms = ( k e. _V |-> ( d e. ( AtomsNEW ` k )
         |-> ( ( AtomsNEW ` k ) \ ( ( _|_P ` k ) ` { d } ) ) ) ) $.

    $( Def. 8.6.16, p. 234. $)
    $( Define set of all projective automorphisms.  This is the intended
       definition of automorphism in [Crawley] p. 112. $)
    df-paut $a |- PAut = ( k e. _V |-> { f | ( f : ( PSubSp ` k ) -1-1-onto->
      ( PSubSp ` k ) /\ A. x e. ( PSubSp ` k ) A. y e. ( PSubSp ` k )
        ( x C_ y <-> ( f ` x ) C_ ( f ` y ) ) ) } ) $.
  $}

  ${
    $d d k A $.  $d d D $.  $d d k K $.
    watomfval.a $e |- A = ( AtomsNEW ` K ) $.
    watomfval.p $e |- P = ( _|_P ` K ) $.
    watomfval.w $e |- W = ( WAtoms ` K ) $.
    $( The W atoms function. $)
    watomfval $p |- ( K e. B -> W = ( d e. A
         |-> ( A \ ( ( _|_P ` K ) ` { d } ) ) ) ) $=
      ( vk wcel cvv cv csn cpol cfv cdif cmpt wceq elisset cwpoints catm fveq2
      syl6eqr difeq1d fveq1d difeq2d eqtrd mpteq12dv df-watoms fvex eqeltri
      mptexg ax-mp fvmpt syl5eq syl ) DBKDLKZEFAAFMNZDOPZPZQZRZSDBTURDUAPVCEJDF
      JMZUBPZVEUSVDOPZPZQZRVCLUAVDDSZFVEVHAVBVIVEDUBPZAVDDUBUCGUDZVIVHAVGQVBVIV
      EAVGVKUEVIVGVAAVIUSVFUTVDDOUCUFUGUHUIJFUJALKVCLKAVJLGDUBUKULFAVBLUMUNUOIU
      PUQ $.
      $( [26-Jan-2012] $)

    $( Value of the W atoms function. $)
    watomval $p |- ( ( K e. B /\ D e. A )
       -> ( W ` D ) = ( A \ ( ( _|_P ` K ) ` { D } ) ) ) $=
      ( vd wcel cfv cv csn cpol cdif cmpt watomfval fveq1d wceq sneq fveq2d
      difeq2d eqid cvv catm fvex eqeltri difexg ax-mp fvmpt sylan9eq ) EBKZCAKC
      FLCJAAJMZNZEOLZLZPZQZLACNZUPLZPZUMCFUSABDEFJGHIRSJCURVBAUSUNCTZUQVAAVCUOU
      TUPUNCUAUBUCUSUDAUEKVBUEKAEUFLUEGEUFUGUHAVAUEUIUJUKUL $.
      $( [26-Jan-2012] $)

    $( The predicate "is a W atom" (corresponding to designated atom
       ` D ` ). $)
    iswatom $p |- ( ( K e. B /\ D e. A ) -> ( P e. ( W ` D )
       <-> ( P e. A /\ -. P e. ( ( _|_P ` K ) ` { D } ) ) ) ) $=
      ( wcel wa cfv csn cpol cdif wn watomval eleq2d eldif syl6bb ) EBJCAJKZDCF
      LZJDACMENLLZOZJDAJDUCJPKUAUBUDDABCDEFGHIQRDAUCST $.
      $( [26-Jan-2012] $)
