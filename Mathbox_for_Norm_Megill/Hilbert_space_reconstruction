    ldillaut.d $e |- D = ( ( LDil ` K ) ` W ) $.
    $( A lattice dilation is an automorphism. $)
    ldillaut $p |- ( ( K e. C /\ W e. H /\ F e. D ) -> F e. I ) $=
      ( vx wcel wa cv cple cfv wbr wceq wi cbs wral eqid isldil simpl syl6bi
      3impia ) FALZGDLZCBLZCELZUGUHMUIUJKNZGFOPZQUKCPUKRSKFTPZUAZMUJKUMABCDEFUL
