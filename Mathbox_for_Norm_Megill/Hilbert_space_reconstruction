$(
    dihval.t @e |- T = ( ( LTrn ` K ) ` W ) @.
    dihval.r @e |- R = ( ( trL ` K ) ` W ) @.
    dihval.i @e |- I = ( ( DIsoH ` K ) ` W ) @.
    @( The  isomorphism H for a lattice ` K ` . @)
    dihfval @p |- ( ( K e. V /\ W e. H ) -> I = ( x e. { y e. B | y .<_ W } |->
             { f e. T | ( R ` f ) .<_ x } ) ) @=
      ? @.
      @( [15-Oct-1013] @)

    @( The  isomorphism H for a lattice ` K ` .  Definition of
       isomorphism map in [Crawley] p. 122 line 3. @)
    dihval @p |- ( ( ( K e. V /\ W e. H ) /\ ( X e. B /\ X .<_ W ) )
          -> ( I ` X ) = { f e. T | ( R ` f ) .<_ X } ) @=
      ? @.
      @( [15-Oct-1013] @)
  @}

  @{
    @d x y .<_ @.  @d x y B @.  @d f x y K @.  @d f x y W @.
    dihfn.b @e |- B = ( Base ` K ) @.
    dihfn.l @e |- .<_ = ( le ` K ) @.
    dihfn.h @e |- H = ( LHyp ` K ) @.
    dihfn.i @e |- I = ( ( DIsoH ` K ) ` W ) @.
    @( Functionality and domain of the  isomorphism H. @)
    dihfn @p |- ( ( K e. HL /\ W e. H ) -> I Fn { x e. B | x .<_ W } ) @=
      ? @.
      @( [26-Nov-1013] @)
  @}

  @{
    @d s .<_ @.  @d s E @.  @d g s F @.  @d g s H @.  @d g s K @.  @d g s R @.
    @d g s T @.  @d g s W @.
    dih1dim.l @e |- .<_ = ( le ` K ) @.
    dih1dim.h @e |- H = ( LHyp ` K ) @.
    dih1dim.t @e |- T = ( ( LTrn ` K ) ` W ) @.
    dih1dim.r @e |- R = ( ( trL ` K ) ` W ) @.
    dih1dim.e @e |- E = ( ( TEndo ` K ) ` W ) @.
    dih1dim.i @e |- I = ( ( DIsoH ` K ) ` W ) @.
    @( Two expressions for the 1-dimensional subspaces of  vector space
       A (when ` F ` is a nonzero vector i.e. non-identity translation).
       Remark after Lemma L in [Crawley] p. 120 line 21. @)
    dih1dim @p |- ( ( ( K e. HL /\ W e. H ) /\ F e. T )
      -> ( I ` ( R ` F ) ) = { g e. T | E. s e. E g = ( s ` F ) } ) @=
      ? @.
      @( [15-Oct-1013] @)
  @}

  @{
    @d f B @.  @d f H @.  @d f K @.  @d f W @.  @d f Z @.
    dih0.b @e |- B = ( Base ` K ) @.
    dih0.z @e |- Z = ( 0. ` K ) @.
    dih0.h @e |- H = ( LHyp ` K ) @.
    dih0.i @e |- I = ( ( DIsoH ` K ) ` W ) @.
    @( The value of the  isomorphism H at the lattice zero is the
       singleton of the identity translation i.e. the zero subspace. @)
    dih0 @p |- ( ( K e. HL /\ W e. H ) -> ( I ` Z ) = { ( _I |` B ) } ) @=
      ? @.
      @( [26-Nov-1013] @)
  @}

  @{
    @d f H @.  @d f K @.  @d f T @.  @d f W @.
    dih1.h @e |- H = ( LHyp ` K ) @.
    dih1.t @e |- T = ( ( LTrn ` K ) ` W ) @.
    dih1.i @e |- I = ( ( DIsoH ` K ) ` W ) @.
    @( The value of the  isomorphism H at the fiducial co-atom is the
       set of all translations i.e. the entire vector space. @)
    dih1 @p |- ( ( K e. HL /\ W e. H ) -> ( I ` W ) = T ) @=
      ? @.
      @( [26-Nov-1013] @)
  @}

  @{
    @d f K @.  @d f T @.  @d f W @.  @d f X @.
    dihss.b @e |- B = ( Base ` K ) @.
    dihss.l @e |- .<_ = ( le ` K ) @.
    dihss.h @e |- H = ( LHyp ` K ) @.
    dihss.t @e |- T = ( ( LTrn ` K ) ` W ) @.
    dihss.i @e |- I = ( ( DIsoH ` K ) ` W ) @.
    @( The value of the  isomorphism H is a set of translations i.e. a
       set of vectors. @)
    dihss @p |- ( ( ( K e. HL /\ W e. H ) /\ ( X e. B /\ X .<_ W ) )
            -> ( I ` X ) C_ T ) @=
      ? @.
      @( [26-Nov-1013] @)
  @}

  @{
    @d x H @.  @d x I @.  @d x y K @.  @d x S @.  @d x T @.  @d x y W @.
    dihelrn.h @e |- H = ( LHyp ` K ) @.
    dihelrn.t @e |- T = ( ( LTrn ` K ) ` W ) @.
    dihelrn.i @e |- I = ( ( DIsoH ` K ) ` W ) @.
    @( Any value of the  isomorphism H is a set of translations i.e. a
       set of vectors. @)
    dihelrn @p |- ( ( ( K e. HL /\ W e. H ) /\ S e. ran I )
            -> S C_ T ) @=
      ? @.
      @( [26-Nov-1013] @)
  @}

  @{
    @d f g u .<_ @.  @d f g u B @.  @d f g u H @.  @d f g u K @.  @d f g u W @.
    @d f g u X @.  @d f g u Y @.
    dih11.b @e |- B = ( Base ` K ) @.
    dih11.l @e |- .<_ = ( le ` K ) @.
    dih11.h @e |- H = ( LHyp ` K ) @.
    dih11.i @e |- I = ( ( DIsoH ` K ) ` W ) @.
    @( The  isomorphism H for a lattice ` K ` is order-preserving in the
       region under co-atom ` W ` .  Part of Lemma M of [Crawley] p. 120 line
       28. @)
    dihord @p |- ( ( ( K e. HL /\ W e. H ) /\ ( X e. B /\ X .<_ W )
      /\ ( Y e. B /\ Y .<_ W ) ) -> ( ( I ` X ) C_ ( I ` Y ) <-> X .<_ Y ) ) @=
      ? @.
      @( [26-Nov-1013] @)

    @( The  isomorphism H for a lattice ` K ` is one-to-one in the
       region under co-atom ` W ` .  Part of Lemma M of [Crawley] p. 120 line
       28. @)
    dih11 @p |- ( ( ( K e. HL /\ W e. H ) /\ ( X e. B /\ X .<_ W )
        /\ ( Y e. B /\ Y .<_ W ) ) -> ( ( I ` X ) = ( I ` Y ) <-> X = Y ) ) @=
      ? @.
      @( [25-Nov-1013] @)
  @}
$)

$(
  @{
    @d t w S @.  @d s w V @.  @d s t w W @.
    lspvalx.v @e |- V = ( vbase ` W ) @.
    lspvalx.s @e |- S = ( LSubSp ` W ) @.
    lspvalx.n @e |- N = ( LSpan ` W ) @.
    @( The span of a set of vectors (in a left module). @)
    lspvalx @p |- ( ( W e. LMod /\ U C_ V )
       -> ( N ` U ) = |^| { t e. S | U C_ t } ) @=
      ? @.
  @}

  @{
    @d s .<_ @.  @d s E @.  @d g s F @.  @d g s H @.  @d g s K @.  @d g s R @.
    @d g s T @.  @d g s W @.
    dia1dimx.l @e |- .<_ = ( le ` K ) @.
    dia1dimx.h @e |- H = ( LHyp ` K ) @.
    dia1dimx.t @e |- T = ( ( LTrn ` K ) ` W ) @.
    dia1dimx.r @e |- R = ( ( trL ` K ) ` W ) @.
    dia1dimx.e @e |- E = ( ( TEndo ` K ) ` W ) @.
    dia1dimx.i @e |- I = ( ( DIsoA ` K ) ` W ) @.
    @( Two expressions for the 1-dimensional subspaces of partial vector space
       A (when ` F ` is a nonzero vector i.e. non-identity translation).
       Remark after Lemma L in [Crawley] p. 120 line 21. @)
    dia1dimx @p |- ( ( ( K e. HL /\ W e. H ) /\ F e. T )
      -> ( I ` ( R ` F ) ) = { g e. T | E. s e. E g = ( s ` F ) } ) @=
?      @.
  @}

  @{
    @d a b c K @.  @d a b c W @.
    dvalvecxx.h @e |- H = ( LHyp ` K ) @.
    dvalvecxx.v @e |- U = ( ( DVecA ` K ) ` W ) @.
    @( The constructed partial vector space A for a lattice ` K ` is a left
       vector space. @)
    dvalvecx @p |- ( ( K e. HL /\ W e. H ) -> U e. LVec ) @=
      ? @.
  @}
