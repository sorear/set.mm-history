  ${
    $d r s .<_ $.  $d r s .\/ $.  $d r s A $.  $d r s H $.  $d r s K $.
    $d r s P $.  $d r s Q $.  $d r s W $.
    lhp2lt.l $e |- .<_ = ( le ` K ) $.
    lhp2lt.s $e |- S = ( lt ` K ) $.
    lhp2lt.j $e |- .\/ = ( join ` K ) $.
    lhp2lt.a $e |- A = ( Atoms ` K ) $.
    lhp2lt.h $e |- H = ( LHyp ` K ) $.
    $( The join of two atoms under a co-atom is strictly less than it. $)
    lhp2lt $p |- ( ( ( K e. HL /\ W e. H ) /\ ( P e. A /\ P .<_ W )
          /\ ( Q e. A /\ Q .<_ W ) )
       -> ( P .\/ Q ) S W ) $=
      ( vr vs chlt wcel wa wbr w3a co wne cbs cfv wb simp1l simp2l simp3l eqid
