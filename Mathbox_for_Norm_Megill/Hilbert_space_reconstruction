    cdlemeg47rv2 $p |- ( ( ( ( K e. HL /\ W e. H )
        /\ ( P e. A /\ -. P .<_ W )
        /\ ( Q e. A /\ -. Q .<_ W ) )
     /\ ( P =/= Q /\ ( R e. A /\ -. R .<_ W ) /\ ( S e. A /\ -. S .<_ W ) )
     /\ ( R .<_ ( P .\/ Q ) /\ -. S .<_ ( P .\/ Q ) ) )
