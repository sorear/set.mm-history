    $( TODO: fix comment. $)
    cdlemg17i $p |- ( ( ( K e. HL /\ W e. H
              /\ ( F e. ( T ` W ) /\ G e. ( T ` W ) ) )
           /\ ( ( P e. A /\ -. P L W ) /\ ( Q e. A /\ -. Q L W ) /\ P =/= Q )
           /\ ( ( ( R ` W ) ` G ) L ( P J Q )
             /\ -. E. r e. A ( -. r L W /\ ( P J r ) = ( Q J r ) )
             /\ ( G ` P ) =/= P ) )
      -> ( G ` ( F ` P ) ) = ( F ` Q ) ) $=
