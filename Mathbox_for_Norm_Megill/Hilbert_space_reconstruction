    $( Lemma for ~ 4at .  Combine all four possible cases. $)
    4atlem12 $p |- ( ( ( ( K e. HL /\ P e. A /\ Q e. A ) /\ ( R e. A
         /\ S e. A /\ T e. A ) /\ ( U e. A /\ V e. A /\ W e. A ) )
          /\ ( P =/= Q /\ -. R L ( P J Q ) /\ -. S L ( ( P J Q ) J R ) ) )
     -> ( ( ( P J Q ) J ( R J S ) ) L ( ( T J U ) J ( V J W ) )
          -> ( ( P J Q ) J ( R J S ) ) = ( ( T J U ) J ( V J W ) ) ) ) $=
