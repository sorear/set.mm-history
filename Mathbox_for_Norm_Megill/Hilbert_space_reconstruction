    $d a b c u v V $.  $d a b c u v w W $.
    cdlemef47.b $e |- B = ( Base ` K ) $.
    cdlemef47.l $e |- L = ( le ` K ) $.
    cdlemef47.j $e |- J = ( join ` K ) $.
    cdlemef47.m $e |- M = ( meet ` K ) $.
    cdlemef47.a $e |- A = ( Atoms ` K ) $.
    cdlemef47.h $e |- H = ( LHyp ` K ) $.
    cdlemef47.v $e |- V = ( ( Q J P ) M W ) $.
    $( g(t), ` -. ` v ` <_ ` p ` \/ ` q. $)
    cdlemef47.n $e |- N = ( ( v J V ) M ( P J ( ( Q J v ) M W ) ) ) $.
    $( g_t(s), u ` <_ ` p ` \/ ` q $)
    cdlemefs47.o $e |- O = ( ( Q J P ) M ( N J ( ( u J v ) M W ) ) ) $.
    cdlemef47.g $e |- G = ( a e. B |->
         if ( ( Q =/= P /\ -. a L W ) ,
           $( g(x) when p ` =/= ` q and ` -. ` a ` <_ ` w. $)
           ( iota_ c e. B A. u e. A
                ( ( -. u L W /\ ( u J ( a M W ) ) = a ) -> c = (
                    $( g_t(s), u ` <_ ` p ` \/ ` q $)
                    if ( u L ( Q J P ) ,
                       ( iota_ b e. B A. v e. A
                             ( ( -. v L W /\ -. v L ( Q J P ) )
                             -> b = O ) ) ,
                    $( g(s), ` -. ` u ` <_ ` p ` \/ ` q. $)
                       [_ u / v ]_ N )
                J ( a M W ) ) ) ) ,
           $( g(x) when p = q or a ` <_ ` w is just x. $)
           a ) ) $.
    $d a c u v X $.
