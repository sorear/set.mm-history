    cdlemg1.h $e |- H = ( LHyp ` K ) $.
    ${
      cdlemg1.u $e |- U = ( ( P .\/ Q ) ./\ W ) $.
      $( f(t), ` -. ` t ` <_ ` p ` \/ ` q. $)
      cdlemg1.d $e |- D
          = ( ( t .\/ U ) ./\ ( Q .\/ ( ( P .\/ t ) ./\ W ) ) ) $.
      $( f_t(s), s ` <_ ` p ` \/ ` q $)
      cdlemg1.e $e |- E
          = ( ( P .\/ Q ) ./\ ( D .\/ ( ( s .\/ t ) ./\ W ) ) ) $.
      cdlemg1.g $e |- G = ( x e. B |->
         if ( ( P =/= Q /\ -. x .<_ W ) ,
           $( Value when p ` =/= ` q and ` -. ` x ` <_ ` w. $)
           ( iota_ z e. B A. s e. A
                ( ( -. s .<_ W /\ ( s .\/ ( x ./\ W ) ) = x ) -> z = (
                    if ( s .<_ ( P .\/ Q ) ,
                       $( Unique f_t(s) for all t, s ` <_ ` p ` \/ ` q $)
                       ( iota_ y e. B A. t e. A
                             ( ( -. t .<_ W /\ -. t .<_ ( P .\/ Q ) ) -> y =
                          $( f_t(s) $)
                          E )
                       ) ,
                       $( f(s), ` -. ` s ` <_ ` p ` \/ ` q. $)
                       [_ s / t ]_ D
                    )
                .\/ ( x ./\ W ) ) ) ) ,
           $( Value when p = q or x ` <_ ` w is just x. $)
           x
         ) ) $.

      $d w G $.  $d w P $.  $d w Q $.  $d w T $.  $d w W $.  $d f w $.
      cdlemg1.t $e |- T = ( ( LTrn ` K ) ` W ) $.
      $( Shorter expression for ` G ` .  TODO: fix comment.  TODO: shorten
         using ~ cdleme or vice-versa?  Also, if not shortened with ~ cdleme ,
         then it can be moved up to save repeating hypotheses. $)
      cdlemg1a $p |- ( ( ( K e. HL /\ W e. H )
