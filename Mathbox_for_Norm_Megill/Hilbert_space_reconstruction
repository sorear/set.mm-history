    $( There is only one atom under both ` P J Q ` and co-atom ` W ` . $)
    lhpat3 $p |- ( ( ( K e. HL /\ W e. H ) /\ ( P e. A /\ -. P L W /\ Q e. A )
        /\ ( S e. A /\ P =/= Q /\ S L ( P J Q ) ) )
           -> ( -. S L W <-> S =/= R ) ) $=
      ( chlt wcel wa wbr wn w3a wne co wceq clat cbs cfv wb simp1l hllat syl
      adantr simp31 eqid atbase simp21 simp23 hlatjcl syl111anc simp1r lhpbase
      latlem12 syl13anc simpl33 simpr mpbi2and syl6breqr cal simpl1l hlatl
