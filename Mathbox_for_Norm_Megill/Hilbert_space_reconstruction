    lhpocat $p |- ( ( K e. HL /\ W e. H ) -> ( O ` W ) e. A ) $=
      ( chlt wcel wa cfv simpr cbs wb eqid lhpbase lhpoc sylan2 mpbid ) CKLZFBL
