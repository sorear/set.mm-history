    $( A lattice dilation is a one-to-one onto function. $)
    ldil1o $p |- ( ( K e. V /\ W e. H /\ F e. D )
      -> F : B -1-1-onto-> B ) $=
      ( wcel w3a claut cfv wf1o simp1 eqid ldillaut laut1o syl11anc ) EFKZGDKZC
