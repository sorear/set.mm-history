  ${
    $d c d A $.  $d d C $.  $d c d D $.  $d c d E $.  $d c d F $.
    $d c d .\/ $.  $d c d K $.  $d c d .<_ $.  $d c P $.  $d c Q $.  $d c R $.
    $d c d Y $.  $d c d Z $.  $d c d ph $.
    dalem62.ph $e |- ( ph <-> ( ( ( K e. HL /\ C e. ( Base ` K ) )
    /\ ( P e. A /\ Q e. A /\ R e. A )
    /\ ( S e. A /\ T e. A /\ U e. A ) )
 /\ ( Y e. O /\ Z e. O )
 /\ ( ( -. C .<_ ( P .\/ Q ) /\ -. C .<_ ( Q .\/ R ) /\ -. C .<_ ( R .\/ P ) )
    /\ ( -. C .<_ ( S .\/ T ) /\ -. C .<_ ( T .\/ U ) /\ -. C .<_ ( U .\/ S ) )
    /\ ( C .<_ ( P .\/ S ) /\ C .<_ ( Q .\/ T ) /\ C .<_ ( R .\/ U ) ) ) ) ) $.
    dalem62.l $e |- .<_ = ( le ` K ) $.
    dalem62.j $e |- .\/ = ( join ` K ) $.
    dalem62.a $e |- A = ( Atoms ` K ) $.
    dalem62.m $e |- ./\ = ( meet ` K ) $.
    dalem62.o $e |- O = ( LPlanes ` K ) $.
    dalem62.y $e |- Y = ( ( P .\/ Q ) .\/ R ) $.
    dalem62.z $e |- Z = ( ( S .\/ T ) .\/ U ) $.
    dalem62.d $e |- D = ( ( P .\/ Q ) ./\ ( S .\/ T ) ) $.
    dalem62.e $e |- E = ( ( Q .\/ R ) ./\ ( T .\/ U ) ) $.
    dalem62.f $e |- F = ( ( R .\/ P ) ./\ ( U .\/ S ) ) $.
