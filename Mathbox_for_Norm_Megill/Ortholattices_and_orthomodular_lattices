  ${
    atomcmp.l $e |- .<_ = ( le ` K ) $.
    atomcmp.a $e |- A = ( Atoms ` K ) $.
    $( If two atoms are comparable, they are equal.  (Th. ~ atsseq analog.) $)
    atcmpOLD $p |- ( ( K e. OP /\ P e. A /\ Q e. A ) -> ( P .<_ Q <-> P = Q ) )
        $=
      ( cops wcel w3a cpo cbs cfv cp0 ccvr wbr wa wceq wb opposet 3ad2ant1
      eqid atbase 3ad2ant2 3ad2ant3 op0cl atcvr0 3adant3 3adant2 jca cvrcmp
      syl131anc ) DHIZBAIZCAIZJZDKIZBDLMZIZCURIZDNMZURIZVABDOMZPZVACVCPZQBCEPBC
      RSUMUNUQUODTUAUNUMUSUOAURBDURUBZGUCUDUOUMUTUNAURCDVFGUCUEUMUNVBUOURDVAVFV
      AUBZUFUAUPVDVEUMUNVDUOAVCHBDVAVGVCUBZGUGUHUMUOVEUNAVCHCDVAVGVHGUGUIUJURVC
      DEBCVAVFFVHUKUL $.
      $( [13-Oct-2011] $)

    $( Frequently-used variation of ~ atcmpOLD . $)
    atncmpOLD $p |- ( ( K e. OP /\ P e. A /\ Q e. A )
             -> ( -. P .<_ Q <-> P =/= Q ) ) $=
      ( cops wcel w3a wbr atcmpOLD necon3bbid ) DHIBAICAIJBCEKBCABCDEFGLM $.
