$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
               Ortholattices and orthomodular lattices
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c oc $.
  $c OP $.
  $c cm $.
  $c OL $.
  $c OML $.

  $( Extend class notation with the class of orthocomplementation
     extractors. $)
  coc $a class oc $.

  $( Extend class notation with orthoposets. $)
  cops $a class OP $.

  $( Extend class notation with the commutes relation. $)
  ccmt $a class cm $.

  $( Extend class notation with orthlattices. $)
  col $a class OL $.

  $( Extend class notation with orthomodular lattices. $)
  coml $a class OML $.

  $( Def. 8.4.9 p. 208. $)
  $( Define the class of orthocomplementation extractors for posets and related
     structures. $)
  df-ocomp $a |- oc = ( p e. _V |-> ( p ` 3 ) ) $.

  ${
    $d a b c p o $.
    $( Def. 8.4.10 p. 209. $)
    $( Define the class of orthoposets. $)
    df-oposet $a |- OP = Struct ( 3 , p ,
       ( ( p e. PosetNEW
        /\ ( 0. ` p ) e. ( Base ` p )
       /\ ( 1. ` p ) e. ( Base ` p ) )
        /\ E. o
           ( o = ( oc ` p )
          /\ A. a e. ( Base ` p ) A. b e. ( Base ` p )
               ( ( ( o ` a ) e. ( Base ` p ) /\ ( o ` ( o ` a ) ) = a
                /\ ( a ( le ` p ) b -> ( o ` b ) ( le ` p ) ( o ` a ) ) )
                /\ ( a ( join ` p ) ( o ` a ) ) = ( 1. ` p )
                   /\ ( a ( meet ` p ) ( o ` a ) ) = ( 0. ` p ) )
          ) ) ) $.
  $}

  ${
    $d p x y $.
    $( Define the commutes relation for orthoposets.  Definition of commutes in
       [Kalmbach] p. 20. $)
    df-cmt $a |- cm = ( p e. _V |-> { <. x , y >. | ( x e. ( Base ` p )
             /\ y e. ( Base ` p )
             /\ x = ( ( x ( meet ` p ) y )
                ( join ` p ) ( x ( meet ` p ) ( ( oc ` p ) ` y ) ) ) ) } ) $.
  $}

  $( Define the class of ortholattices.  Definition from [Kalmbach] p. 16. $)
  df-ol $a |- OL = ( LatNEW i^i OP ) $.

  ${
    $d a b c l $.
    $( Define the class of orthomodular lattices.  Definition from [Kalmbach]
       p. 16. $)
    df-oml $a |- OML = { l e. OL | A. a e. ( Base ` l ) A. b e. ( Base ` l )
       ( a ( le ` l ) b -> b = ( a ( join ` l ) ( b ( meet ` l )
          ( ( oc ` l ) ` a ) ) ) ) } $.
  $}

  ${
    $d n p x y B $.  $d n p F $.  $d n p J $.  $d n p M $.  $d n p x y O $.
    $d n p x y K $.  $d n p L $.  $d n p U $.
    isopos.0 $e |- S = Struct ( 3 , p , T. ) $.
    isopos.b $e |- B = ( Base ` K ) $.
    isopos.l $e |- L = ( le ` K ) $.
    isopos.o $e |- O = ( oc ` K ) $.
    isopos.j $e |- J = ( join ` K ) $.
    isopos.m $e |- M = ( meet ` K ) $.
    isopos.f $e |- F = ( 0. ` K ) $.
    isopos.u $e |- U = ( 1. ` K ) $.
    $( The predicate "is an orthoposet." $)
    isopos $p |- ( K e. OP <-> ( K e. S
            /\ ( K e. PosetNEW /\ F e. B /\ U e. B )
            /\ A. x e. B A. y e. B ( ( ( O ` x ) e. B
                 /\ ( O ` ( O ` x ) ) = x
                 /\ ( x L y -> ( O ` y ) L ( O ` x ) ) )
               /\ ( x J ( O ` x ) ) = U /\ ( x M ( O ` x ) ) = F ) ) ) $=
      ( vn cops wcel cv cpo cp0 cfv cbs cp1 w3a coc wceq cple wbr wi cjn co
      cmee wral wa wex c3 cstru df-oposet eleq2i eleq1 fveq2 syl6eqr eleq12d
      3anbi123d eqeq2d eleq2d breqd imbi12d 3anbi13d opreqd eqeq12d raleqbidv
      anbi12d exbidv elstr2 cvv fvex eqeltri fveq1 eleq1d id fveq12d eqeq1d
      breq12d imbi2d opreq2d 2ralbidv ceqsexv anbi2i 3anass bitr4i 3bitri ) HUB
      UCHLUDZUEUCZWSUFUGZWSUHUGZUCZWSUIUGZXBUCZUJZUAUDZWSUKUGZULZAUDZXGUGZXBUCZ
      XKXGUGZXJULZXJBUDZWSUMUGZUNZXOXGUGZXKXPUNZUOZUJZXJXKWSUPUGZUQZXDULZXJXKWS
      URUGZUQZXAULZUJZBXBUSZAXBUSZUTZUAVAZUTZLVBVCZUCHDUCZHUEUCZFCUCZECUCZUJZXG
      KULZXKCUCZXNXJXOIUNZXRXKIUNZUOZUJZXJXKGUQZEULZXJXKJUQZFULZUJZBCUSZACUSZUT
      ZUAVAZUTZUTZYOYSXJKUGZCUCZUUQKUGZXJULZUUBXOKUGZUUQIUNZUOZUJZXJUUQGUQZEULZ
      XJUUQJUQZFULZUJZBCUSACUSZUJZUBYNHUALABVDVEYMUUODLHVBMWSHULZXFYSYLUUNUVLWT
      YPXCYQXEYRWSHUEVFUVLXAFXBCUVLXAHUFUGFWSHUFVGSVHZUVLXBHUHUGCWSHUHVGNVHZVIU
      VLXDEXBCUVLXDHUIUGEWSHUIVGTVHZUVNVIVJUVLYKUUMUAUVLXIYTYJUULUVLXHKXGUVLXHH
      UKUGZKWSHUKVGPVHVKUVLYIUUKAXBCUVNUVLYHUUJBXBCUVNUVLYAUUEYDUUGYGUUIUVLXLUU
      AXTUUDXNUVLXBCXKUVNVLUVLXQUUBXSUUCUVLXPIXJXOUVLXPHUMUGIWSHUMVGOVHZVMUVLXP
      IXRXKUVQVMVNVOUVLYCUUFXDEUVLYBGXJXKUVLYBHUPUGGWSHUPVGQVHVPUVOVQUVLYFUUHXA
      FUVLYEJXJXKUVLYEHURUGJWSHURVGRVHVPUVMVQVJVRVRVSVTVSWAUUPYOYSUVJUTZUTUVKUU
      OUVRYOUUNUVJYSUULUVJUAKKUVPWBPHUKWCWDYTUUJUVIABCCYTUUEUVDUUGUVFUUIUVHYTUU
      AUURXNUUTUUDUVCYTXKUUQCXJXGKWEZWFYTXMUUSXJYTXKUUQXGKYTWGUVSWHWIYTUUCUVBUU
      BYTXRUVAXKUUQIXOXGKWEUVSWJWKVJYTUUFUVEEYTXKUUQXJGUVSWLWIYTUUHUVGFYTXKUUQX
      JJUVSWLWIVJWMWNWOWOYOYSUVJWPWQWR $.
      $( [20-Oct-2011] $)
  $}

  ${
    $d p x y B $.  $d p Z $.  $d p J $.  $d p M $.  $d p x y O $.
    $d p x y K $.  $d p L $.  $d p U $.
    isopi.a $e |- K e. Struct ( 3 , p , T. ) $.
    isopi.0 $e |- K e. PosetNEW $.
    isopi.b $e |- B = ( Base ` K ) $.
    isopi.l $e |- L = ( le ` K ) $.
    isopi.o $e |- O = ( oc ` K ) $.
    isopi.j $e |- J = ( join ` K ) $.
    isopi.m $e |- M = ( meet ` K ) $.
    isopi.z $e |- Z = ( 0. ` K ) $.
    isopi.u $e |- U = ( 1. ` K ) $.
    isopi.8 $e |- Z e. B $.
    isopi.9 $e |- U e. B $.
    isopi.9a $e |- ( x e. B -> ( O ` x ) e. B ) $.
    isopi.10 $e |- ( x e. B -> ( O ` ( O ` x ) ) = x ) $.
    isopi.11 $e |- ( ( x e. B /\ y e. B ) ->
                 ( x L y -> ( O ` y ) L ( O ` x ) ) ) $.
    isopi.12 $e |- ( x e. B -> ( x J ( O ` x ) ) = U ) $.
    isopi.13 $e |- ( x e. B -> ( x M ( O ` x ) ) = Z ) $.
    $( Properties that determine an orthoposet (constructed structure
       version). $)
    isopi $p |- K e. OP $=
      ( cops wcel wtru c3 cstru cpo w3a cv cfv wceq wbr wi co wral eqid isopos
      3pm3.2i wa adantr 3jca rgen2a mpbir3an ) FUHUIFUJKUKULZUIFUMUIZJCUIZDCUIZ
      UNAUOZIUPZCUIZVOIUPVNUQZVNBUOZGURVRIUPVOGURUSZUNZVNVOEUTDUQZVNVOHUTJUQZUN
      ZBCVAACVAABCVJDJEFGHIKVJVBNOPQRSTVCLVKVLVMMUAUBVDWCABCVNCUIZVRCUIZVEZVTWA
      WBWFVPVQVSWDVPWEUCVFWDVQWEUDVFUEVGWDWAWEUFVFWDWBWEUGVFVGVHVI $.
      $( [13-Sep-2011] $)
  $}

  ${
    $d p x y B $.  $d p Z $.  $d p J $.  $d p M $.  $d p x y O $.
    $d p x y K $.  $d p L $.  $d p U $.
    isopix.a $e |- K = { <. 1 , B >. , <. 2 , L >. , <. 3 , O >. } $.
    isopix.0 $e |- K e. PosetNEW $.
    isopix.1 $e |- B e. _V $.
    isopix.2 $e |- L e. _V $.
    isopix.3 $e |- O e. _V $.
    isopix.j $e |- J = ( join ` K ) $.
    isopix.m $e |- M = ( meet ` K ) $.
    isopix.z $e |- Z = ( 0. ` K ) $.
    isopix.u $e |- U = ( 1. ` K ) $.
    isopix.8 $e |- Z e. B $.
    isopix.9 $e |- U e. B $.
    isopix.9a $e |- ( x e. B -> ( O ` x ) e. B ) $.
    isopix.10 $e |- ( x e. B -> ( O ` ( O ` x ) ) = x ) $.
    isopix.11 $e |- ( ( x e. B /\ y e. B ) ->
                    ( x L y -> ( O ` y ) L ( O ` x ) ) ) $.
    isopix.12 $e |- ( x e. B -> ( x J ( O ` x ) ) = U ) $.
    isopix.13 $e |- ( x e. B -> ( x M ( O ` x ) ) = Z ) $.
    $( Properties that determine an orthoposet (explicit structure version). $)
    isopix $p |- K e. OP $=
      ( vp stb3strx cbs df-base stb3v1x cple df-ple stb3v2x coc df-ocomp
      stb3v3x isopi ) ABCDEFGHIJUGUGFCGIMNOKUHLUIUGFCGIMNOKUGUJUKULUGFCGIMNOKUG
      UMUNUOUGFCGIMNOKUGUPUQPQRSTUAUBUCUDUEUFUR $.
      $( [14-Sep-2011] $)
  $}

  ${
    $d p x y K $.
    $( Every orthoposet is a poset. $)
    opposet $p |- ( K e. OP -> K e. PosetNEW ) $=
      ( vp vx vy cops wcel wtru c3 cstru cpo cp0 cfv cbs cp1 w3a cv coc wceq
      cple wbr wi cjn co cmee wral eqid isopos simpl1 3adant1 sylbi ) AEFAGBHIZ
      FZAJFZAKLZAMLZFZANLZUOFZOZCPZAQLZLZUOFVBVALUTRUTDPZASLZTVCVALVBVDTUAOUTVB
      AUBLZUCUQRUTVBAUDLZUCUNRODUOUECUOUEZOUMCDUOUKUQUNVEAVDVFVABUKUFUOUFVDUFVA
      UFVEUFVFUFUNUFUQUFUGUSVGUMULUMUPURVGUHUIUJ $.
      $( [12-Oct-2011] $)
  $}

  ${
    $d p x y B $.  $d p x y F $.  $d p x y J $.  $d p x y M $.  $d p x y O $.
    $d p x y K $.  $d p x y L $.  $d p x y U $.  $d x y X $.  $d y Y $.
    oposlem.b $e |- B = ( Base ` K ) $.
    oposlem.l $e |- L = ( le ` K ) $.
    oposlem.o $e |- O = ( oc ` K ) $.
    oposlem.j $e |- J = ( join ` K ) $.
    oposlem.m $e |- M = ( meet ` K ) $.
    oposlem.f $e |- F = ( 0. ` K ) $.
    oposlem.u $e |- U = ( 1. ` K ) $.
    $( Lemma for orthoposet properties. $)
    oposlem $p |- ( ( K e. OP /\ X e. B /\ Y e. B )
                -> ( ( ( O ` X ) e. B /\ ( O ` ( O ` X ) ) = X
               /\ ( X L Y -> ( O ` Y ) L ( O ` X ) ) )
               /\ ( X J ( O ` X ) ) = U /\ ( X M ( O ` X ) ) = F ) ) $=
      ( vx vy vp cops wcel cfv wceq wbr wi w3a co cv wral wa wtru c3 cstru cpo
      eqid isopos simp3bi fveq2 eleq1d fveq2d id eqeq12d breq1 breq2d imbi12d
      3anbi123d opreq12d eqeq1d breq2 breq1d 3anbi3d 3anbi1d rcla42v mpan9
      3impb ) EUAUBZIAUBZJAUBZIHUCZAUBZVTHUCZIUDZIJFUEZJHUCZVTFUEZUFZUGZIVTDUHZ
      BUDZIVTGUHZCUDZUGZVQRUIZHUCZAUBZWOHUCZWNUDZWNSUIZFUEZWSHUCZWOFUEZUFZUGZWN
      WODUHZBUDZWNWOGUHZCUDZUGZSAUJRAUJZVRVSUKWMVQEULTUMUNZUBEUOUBCAUBBAUBUGXJR
      SAXKBCDEFGHTXKUPKLMNOPQUQURXIWMWAWCIWSFUEZXAVTFUEZUFZUGZWJWLUGRSIJAAWNIUD
      ZXDXOXFWJXHWLXPWPWAWRWCXCXNXPWOVTAWNIHUSZUTXPWQWBWNIXPWOVTHXQVAXPVBZVCXPW
      TXLXBXMWNIWSFVDXPWOVTXAFXQVEVFVGXPXEWIBXPWNIWOVTDXRXQVHVIXPXGWKCXPWNIWOVT
      GXRXQVHVIVGWSJUDZXOWHWJWLXSXNWGWAWCXSXLWDXMWFWSJIFVJXSXAWEVTFWSJHUSVKVFVL
      VMVNVOVP $.
      $( [20-Oct-2011] $)
  $}

  ${
    $d p x y B $.  $d p x y K $.  $d p Z $.
    op0cl.b $e |- B = ( Base ` K ) $.
    op0cl.z $e |- Z = ( 0. ` K ) $.
    $( An orthoposet has a zero element.  (Th. ~ h0elch analog.) $)
    op0cl $p |- ( K e. OP -> Z e. B ) $=
      ( vp vx vy cops wcel wtru c3 cstru cpo cp1 cfv w3a cv coc wceq cple wbr
      wi cjn co cmee wral eqid isopos simpl2 3adant1 sylbi ) BIJBKFLMZJZBNJZCAJ
      ZBOPZAJZQZGRZBSPZPZAJVBVAPUTTUTHRZBUAPZUBVCVAPVBVDUBUCQUTVBBUDPZUEUQTUTVB
      BUFPZUECTQHAUGGAUGZQUPGHAUMUQCVEBVDVFVAFUMUHDVDUHVAUHVEUHVFUHEUQUHUIUSVGU
      PUNUOUPURVGUJUKUL $.
      $( [12-Oct-2011] $)
  $}

  ${
    $d p x y B $.  $d p x y K $.  $d p U $.
    op1cl.b $e |- B = ( Base ` K ) $.
    op1cl.u $e |- U = ( 1. ` K ) $.
    $( An orthoposet has a unit element.  (Th. ~ helch analog.) $)
    op1cl $p |- ( K e. OP -> U e. B ) $=
      ( vp vx vy cops wcel wtru c3 cstru cpo cp0 cfv w3a cv coc wceq cple wbr
      wi cjn co cmee wral eqid isopos simpl3 3adant1 sylbi ) CIJCKFLMZJZCNJZCOP
      ZAJZBAJZQZGRZCSPZPZAJVBVAPUTTUTHRZCUAPZUBVCVAPVBVDUBUCQUTVBCUDPZUEBTUTVBC
      UFPZUEUPTQHAUGGAUGZQURGHAUMBUPVECVDVFVAFUMUHDVDUHVAUHVEUHVFUHUPUHEUIUSVGU
      RUNUOUQURVGUJUKUL $.
      $( [22-Oct-2011] $)
  $}

  ${
    op0le.b $e |- B = ( Base ` K ) $.
    op0le.l $e |- L = ( le ` K ) $.
    op0le.z $e |- Z = ( 0. ` K ) $.
    $( Orthoposet zero is less than or equal to any element.  (Th. ~ ch0le
       analog.) $)
    op0le $p |- ( ( K e. OP /\ X e. B ) -> Z L X ) $=
      ( cops wcel wbr op0cl adantr cpo p0le opposet syl3an1 3com23 mpd3an3 ) BI
      JZDAJZEAJZEDCKZTUBUAABEFHLMTUBUAUCBNJUBUAUCTABCDEFGHOBPQRS $.
      $( [12-Oct-2011] $)

    $( An element less than or equal to zero equals zero.  (Th. ~ chle0
       analog.) $)
    ople0 $p |- ( ( K e. OP /\ X e. B ) -> ( X L Z <-> X = Z ) ) $=
      ( cops wcel wa wbr wceq op0le biantrud cpo wb opposet adantr simpr op0cl
      posasymb syl111anc bitrd ) BIJZDAJZKZDECLZUHEDCLZKZDEMZUGUIUHABCDEFGHNOUG
      BPJZUFEAJZUJUKQUEULUFBRSUEUFTUEUMUFABEFHUASABCDEFGUBUCUD $.
      $( [21-Oct-2011] $)
  $}

  ${
    opltne0.b $e |- B = ( Base ` K ) $.
    opltne0.s $e |- S = ( lt ` K ) $.
    opltne0.z $e |- Z = ( 0. ` K ) $.
    $( Todo: is this needed? $)
    $( A lattice element greater than zero is nonzero. $)
    opltne0 $p |- ( ( K e. OP /\ X e. B ) -> ( Z S X <-> X =/= Z ) ) $=
      ( cops wcel wa wbr cple cfv wne wb simpl op0cl adantr simpr eqid pltval
      syl111anc op0le biantrurd necom syl5rbb bitrd ) CIJZDAJZKZEDBLZEDCMNZLZED
      OZKZDEOZUKUIEAJZUJULUPPUIUJQUIURUJACEFHRSUIUJTIAABCUMEDUMUAZGUBUCUKUOUPUQ
      UKUNUOACUMDEFUSHUDUEDEUFUGUH $.
      $( [1-Jun-2012] $)
  $}

  ${
    ople1.b $e |- B = ( Base ` K ) $.
    ople1.l $e |- L = ( le ` K ) $.
    ople1.u $e |- U = ( 1. ` K ) $.
    $( Any element is less than the orthoposet unit.  (Th. ~ chss analog.) $)
    ople1 $p |- ( ( K e. OP /\ X e. B ) -> X L U ) $=
      ( cops wcel wbr op1cl adantr cpo ple1 opposet syl3an1 mpd3an3 ) CIJZEAJZB
      AJZEBDKZSUATABCFHLMCNJTUAUBSABCDEFGHOCPQR $.
      $( [23-Oct-2011] $)

    $( If the orthoposet unit is less than or equal to an element, the element
       equals the unit.  (Th. ~ chle0 analog.) $)
    op1le $p |- ( ( K e. OP /\ X e. B ) -> ( U L X <-> X = U ) ) $=
      ( cops wcel wa wbr wceq ople1 biantrurd cpo wb opposet adantr simpr
      op1cl posasymb syl111anc bitrd ) CIJZEAJZKZBEDLZEBDLZUHKZEBMZUGUIUHABCDEF
      GHNOUGCPJZUFBAJZUJUKQUEULUFCRSUEUFTUEUMUFABCFHUASACDEBFGUBUCUD $.
      $( [5-Dec-2011] $)
  $}

  ${
    $d x y z K $.  $d x z U $.
    glb0.g $e |- G = ( glb ` K ) $.
    glb0.u $e |- U = ( 1. ` K ) $.
    $( The greatest lower bound of the empty set is the unit element. $)
    glb0 $p |- ( K e. OP -> ( G ` (/) ) = U ) $=
      ( vx vy vz cops wcel c0 cfv cv cple wbr wral wi cbs wa crio wss wceq 0ss
      eqid glbval mpan2 op1cl w3a breq1 rcla4v 3ad2ant2 wb op1le 3adant2
      sylibd breq2 ople1 3ad2antl1 syl5cbir r19.21adva impbid ral0 a1bi ralbii
      biantrur bitri syl5bbr riota5 mpdan eqtrd ) CIJZKBLZFMZGMZCNLZOZGKPZHMZVN
      VOOZGKPZVRVMVOOZQZHCRLZPZSZFWCTZAVKKWCUAVLWFUBWCUCFGHIWCKBCVOWCUDZVOUDZDU
      EUFVKAWCJZWFAUBWCACWGEUGVKWEFWCAVKWIVMWCJZUHZWAHWCPZVMAUBZWEWKWLWMWKWLAVM
      VOOZWMWIVKWLWNQWJWAWNHAWCVRAVMVOUIUJUKVKWJWNWMULWIWCACVOVMWGWHEUMUNUOWKWM
      WAHWCWMWAVRAVOOZWKVRWCJZSVMAVRVOUPVKWIWPWOWJWCACVOVRWGWHEUQURUSUTVAWLWDWE
      WAWBHWCVTWAVSGVBVCVDVQWDVPGVBVEVFVGVHVIVJ $.
