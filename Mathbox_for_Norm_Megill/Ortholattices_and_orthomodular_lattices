      AUPUAUBUCUDUE $.
      $( [13-Sep-2011] $)

    $( Contraposition law for orthoposets.  (Th. ~ chcon3i analog.) $)
    opcon3b $p |- ( ( K e. OP /\ X e. B /\ Y e. B )
                -> ( X = Y <-> ( O ` Y ) = ( O ` X ) ) ) $=
      ( cops wcel w3a wceq cfv fveq2 eqcoms opococ 3adant3 3adant2 eqeq12d
      syl5ib impbid2 ) BHIZDAIZEAIZJZDEKZECLZDCLZKZUHEDEDCMNUHUGCLZUFCLZKZUDUEU
      KUGUFUGUFCMNUDUIDUJEUAUBUIDKUCABCDFGOPUAUCUJEKUBABCEFGOQRST $.
      $( [8-Nov-2011] $)

    $( Orthocomplement contraposition law.  (Th. ~ negcon2 analog.) $)
    opcon2b $p |- ( ( K e. OP /\ X e. B /\ Y e. B )
       -> ( X = ( O ` Y ) <-> Y = ( O ` X ) ) ) $=
      ( cops wcel w3a cfv wceq wb opoccl 3adant2 opcon3b syld3an3 opococ
      eqeq1d bitrd ) BHIZDAIZEAIZJZDECKZLZUECKZDCKZLZEUHLUAUBUCUEAIZUFUIMUAUCUJ
