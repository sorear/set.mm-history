$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
               Ortholattices and orthomodular lattices
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c oc $.
  $c OP $.
  $c cm $.
  $c OL $.
  $c OML $.

  $( Extend class notation with the class of orthocomplementation
     extractors. $)
  coc $a class oc $.

  $( Extend class notation with orthoposets. $)
  cops $a class OP $.

  $( Extend class notation with the commutes relation. $)
  ccmt $a class cm $.

  $( Extend class notation with orthlattices. $)
  col $a class OL $.

  $( Extend class notation with orthomodular lattices. $)
  coml $a class OML $.

  $( Def. 8.4.9 p. 208. $)
  $( Define the orthocomplementation extractor for posets and related
     structures. $)
  df-ocomp $a |- oc = ( p e. _V |-> ( p ` ( 10 + 1 ) ) ) $.

  ${
    $d a b c p o $.
    $( Def. 8.4.10 p. 209. $)
    $( Define the class of orthoposets. $)
    df-oposet $a |- OP = { p e. Poset |
       ( ( ( 0. ` p ) e. ( Base ` p )
       /\ ( 1. ` p ) e. ( Base ` p ) )
        /\ E. o
           ( o = ( oc ` p )
          /\ A. a e. ( Base ` p ) A. b e. ( Base ` p )
               ( ( ( o ` a ) e. ( Base ` p ) /\ ( o ` ( o ` a ) ) = a
                /\ ( a ( le ` p ) b -> ( o ` b ) ( le ` p ) ( o ` a ) ) )
                /\ ( a ( join ` p ) ( o ` a ) ) = ( 1. ` p )
                   /\ ( a ( meet ` p ) ( o ` a ) ) = ( 0. ` p ) )
          ) ) } $.
  $}

  ${
    $d p x y $.
    $( Define the commutes relation for orthoposets.  Definition of commutes in
       [Kalmbach] p. 20. $)
    df-cmt $a |- cm = ( p e. _V |-> { <. x , y >. | ( x e. ( Base ` p )
             /\ y e. ( Base ` p )
             /\ x = ( ( x ( meet ` p ) y )
                ( join ` p ) ( x ( meet ` p ) ( ( oc ` p ) ` y ) ) ) ) } ) $.
  $}

  $( Define the class of ortholattices.  Definition from [Kalmbach] p. 16. $)
  df-ol $a |- OL = ( Lat i^i OP ) $.

  ${
    $d a b c l $.
    $( Define the class of orthomodular lattices.  Definition from [Kalmbach]
       p. 16. $)
    df-oml $a |- OML = { l e. OL | A. a e. ( Base ` l ) A. b e. ( Base ` l )
       ( a ( le ` l ) b -> b = ( a ( join ` l ) ( b ( meet ` l )
          ( ( oc ` l ) ` a ) ) ) ) } $.
  $}

  ${
    $d n p x y B $.  $d n p F $.  $d n p .\/ $.  $d n p ./\ $.  $d n p x y O $.
    $d n p x y K $.  $d n p .<_ $.  $d n p U $.
    isopos.b $e |- B = ( Base ` K ) $.
    isopos.l $e |- .<_ = ( le ` K ) $.
    isopos.o $e |- O = ( oc ` K ) $.
    isopos.j $e |- .\/ = ( join ` K ) $.
    isopos.m $e |- ./\ = ( meet ` K ) $.
    isopos.f $e |- F = ( 0. ` K ) $.
    isopos.u $e |- U = ( 1. ` K ) $.
    $( The predicate "is an orthoposet." $)
    isopos $p |- ( K e. OP <-> ( ( K e. Poset /\ F e. B /\ U e. B )
            /\ A. x e. B A. y e. B ( ( ( O ` x ) e. B
                 /\ ( O ` ( O ` x ) ) = x
                 /\ ( x .<_ y -> ( O ` y ) .<_ ( O ` x ) ) )
               /\ ( x .\/ ( O ` x ) ) = U /\ ( x ./\ ( O ` x ) ) = F ) ) ) $=
      ( vn vp cops wcel cpo wa cv wceq cfv wbr wi w3a co wral wex cp0 cbs cp1
      coc cple cjn cmee fveq2 syl6eqr eleq12d anbi12d eqeq2d eleq2d breqd
      imbi12d 3anbi13d opreqd eqeq12d 3anbi123d raleqbidv exbidv df-oposet
      elrab2 anass 3anass bicomi cvv fvex eqeltri fveq1 eleq1d id fveq12d
      eqeq1d breq12d imbi2d opreq2d 2ralbidv ceqsexv anbi12i 3bitr2i ) GTUAGUBU
      AZECUAZDCUAZUCZRUDZJUEZAUDZWRUFZCUAZXAWRUFZWTUEZWTBUDZHUGZXEWRUFZXAHUGZUH
      ZUIZWTXAFUJZDUEZWTXAIUJZEUEZUIZBCUKZACUKZUCZRULZUCZUCWNWQUCZXSUCWNWOWPUIZ
      WTJUFZCUAZYCJUFZWTUEZXFXEJUFZYCHUGZUHZUIZWTYCFUJZDUEZWTYCIUJZEUEZUIZBCUKA
      CUKZUCSUDZUMUFZYQUNUFZUAZYQUOUFZYSUAZUCZWRYQUPUFZUEZXAYSUAZXDWTXEYQUQUFZU
      GZXGXAUUGUGZUHZUIZWTXAYQURUFZUJZUUAUEZWTXAYQUSUFZUJZYRUEZUIZBYSUKZAYSUKZU
      CZRULZUCXTSGUBTYQGUEZUUCWQUVBXSUVCYTWOUUBWPUVCYREYSCUVCYRGUMUFEYQGUMUTPVA
      ZUVCYSGUNUFCYQGUNUTKVAZVBUVCUUADYSCUVCUUAGUOUFDYQGUOUTQVAZUVEVBVCUVCUVAXR
      RUVCUUEWSUUTXQUVCUUDJWRUVCUUDGUPUFZJYQGUPUTMVAVDUVCUUSXPAYSCUVEUVCUURXOBY
      SCUVEUVCUUKXJUUNXLUUQXNUVCUUFXBUUJXIXDUVCYSCXAUVEVEUVCUUHXFUUIXHUVCUUGHWT
      XEUVCUUGGUQUFHYQGUQUTLVAZVFUVCUUGHXGXAUVHVFVGVHUVCUUMXKUUADUVCUULFWTXAUVC
      UULGURUFFYQGURUTNVAVIUVFVJUVCUUPXMYREUVCUUOIWTXAUVCUUOGUSUFIYQGUSUTOVAVIU
      VDVJVKVLVLVCVMVCRSABVNVOWNWQXSVPYAYBXSYPYBYAWNWOWPVQVRXQYPRJJUVGVSMGUPVTW
      AWSXOYOABCCWSXJYJXLYLXNYNWSXBYDXDYFXIYIWSXAYCCWTWRJWBZWCWSXCYEWTWSXAYCWRJ
      WSWDUVIWEWFWSXHYHXFWSXGYGXAYCHXEWRJWBUVIWGWHVKWSXKYKDWSXAYCWTFUVIWIWFWSXM
      YMEWSXAYCWTIUVIWIWFVKWJWKWLWM $.
      $( [20-Oct-2011] $)
  $}

  ${
    $d p x y B $.  $d p Z $.  $d p .\/ $.  $d p ./\ $.  $d p x y O $.
    $d p x y K $.  $d p .<_ $.  $d p U $.
    isopi.0 $e |- K e. Poset $.
    isopi.b $e |- B = ( Base ` K ) $.
    isopi.l $e |- .<_ = ( le ` K ) $.
    isopi.o $e |- O = ( oc ` K ) $.
    isopi.j $e |- .\/ = ( join ` K ) $.
    isopi.m $e |- ./\ = ( meet ` K ) $.
    isopi.z $e |- Z = ( 0. ` K ) $.
    isopi.u $e |- U = ( 1. ` K ) $.
    isopi.8 $e |- Z e. B $.
    isopi.9 $e |- U e. B $.
    isopi.9a $e |- ( x e. B -> ( O ` x ) e. B ) $.
    isopi.10 $e |- ( x e. B -> ( O ` ( O ` x ) ) = x ) $.
    isopi.11 $e |- ( ( x e. B /\ y e. B ) ->
                 ( x .<_ y -> ( O ` y ) .<_ ( O ` x ) ) ) $.
    isopi.12 $e |- ( x e. B -> ( x .\/ ( O ` x ) ) = U ) $.
    isopi.13 $e |- ( x e. B -> ( x ./\ ( O ` x ) ) = Z ) $.
    $( Properties that determine an orthoposet (constructed structure
       version). $)
    isopi $p |- K e. OP $=
      ( cops wcel cpo w3a cv cfv wceq wbr wi co wral isopos 3pm3.2i wa adantr
      3jca rgen2a mpbir2an ) FUFUGFUHUGZJCUGZDCUGZUIAUJZIUKZCUGZVHIUKVGULZVGBUJ
      ZGUMVKIUKVHGUMUNZUIZVGVHEUODULZVGVHHUOJULZUIZBCUPACUPABCDJEFGHILMNOPQRUQV
      DVEVFKSTURVPABCVGCUGZVKCUGZUSZVMVNVOVSVIVJVLVQVIVRUAUTVQVJVRUBUTUCVAVQVNV
      RUDUTVQVOVRUEUTVAVBVC $.
      $( [13-Sep-2011] $)
  $}

  ${
    $d p x y K $.
    $( Every orthoposet is a poset. $)
    opposet $p |- ( K e. OP -> K e. Poset ) $=
      ( vx vy cops wcel cpo cp0 cfv cbs cp1 w3a cv coc wceq cple wbr wi cjn co
      cmee wral wa eqid isopos simp1 adantr sylbi ) ADEAFEZAGHZAIHZEZAJHZUJEZKZ
      BLZAMHZHZUJEUQUPHUONUOCLZAOHZPURUPHUQUSPQKUOUQARHZSULNUOUQATHZSUINKCUJUAB
      UJUAZUBUHBCUJULUIUTAUSVAUPUJUCUSUCUPUCUTUCVAUCUIUCULUCUDUNUHVBUHUKUMUEUFU
      G $.
      $( [12-Oct-2011] $)
  $}

  ${
    $d p x y B $.  $d p x y F $.  $d p x y .\/ $.  $d p x y ./\ $.
    $d p x y O $.  $d p x y K $.  $d p x y .<_ $.  $d p x y U $.  $d x y X $.
    $d y Y $.
    oposlem.b $e |- B = ( Base ` K ) $.
    oposlem.l $e |- .<_ = ( le ` K ) $.
    oposlem.o $e |- O = ( oc ` K ) $.
    oposlem.j $e |- .\/ = ( join ` K ) $.
    oposlem.m $e |- ./\ = ( meet ` K ) $.
    oposlem.f $e |- F = ( 0. ` K ) $.
    oposlem.u $e |- U = ( 1. ` K ) $.
    $( Lemma for orthoposet properties. $)
    oposlem $p |- ( ( K e. OP /\ X e. B /\ Y e. B )
                -> ( ( ( O ` X ) e. B /\ ( O ` ( O ` X ) ) = X
               /\ ( X .<_ Y -> ( O ` Y ) .<_ ( O ` X ) ) )
               /\ ( X .\/ ( O ` X ) ) = U /\ ( X ./\ ( O ` X ) ) = F ) ) $=
      ( vx vy cops wcel cfv wceq wbr wi w3a co cv wral wa cpo isopos simprbi
      fveq2 eleq1d fveq2d id eqeq12d breq1 breq2d imbi12d 3anbi123d opreq12d
      eqeq1d breq2 breq1d 3anbi3d 3anbi1d rcla42v mpan9 3impb ) ETUAZIAUAZJAUAZ
