  ${
    opoccl.b $e |- B = ( Base ` K ) $.
    opoccl.o $e |- O = ( oc ` K ) $.
    $( Closure of orthocomplement operation.  (Th. ~ choccl analog.) $)
    opoccl $p |- ( ( K e. OP /\ X e. B ) -> ( O ` X ) e. B ) $=
      ( cops wcel wa cfv wceq cple wbr wi w3a cjn co cp1 cmee cp0 eqid oposlem
      3anidm23 simp1d ) BGHZDAHZIZDCJZAHZUHCJDKZDDBLJZMUHUHUKMNZUGUIUJULOZDUHBP
      JZQBRJZKZDUHBSJZQBTJZKZUEUFUMUPUSOAUOURUNBUKUQCDDEUKUAFUNUAUQUAURUAUOUAUB
      UCUDUD $.
      $( [20-Oct-2011] $)

    $( Double negative law for orthoposets.  (Th. ~ ococ analog.) $)
    opococ $p |- ( ( K e. OP /\ X e. B ) -> ( O ` ( O ` X ) ) = X ) $=
      ( cops wcel wa cfv wceq cple wbr wi w3a cjn co cp1 cmee cp0 eqid oposlem
      3anidm23 simp1d simp2d ) BGHZDAHZIZDCJZAHZUICJDKZDDBLJZMUIUIULMNZUHUJUKUM
      OZDUIBPJZQBRJZKZDUIBSJZQBTJZKZUFUGUNUQUTOAUPUSUOBULURCDDEULUAFUOUAURUAUSU
      AUPUAUBUCUDUE $.
      $( [13-Sep-2011] $)

    $( Contraposition law for orthoposets.  (Th. ~ chcon3i analog.) $)
    opcon3b $p |- ( ( K e. OP /\ X e. B /\ Y e. B )
                -> ( X = Y <-> ( O ` Y ) = ( O ` X ) ) ) $=
      ( cops wcel w3a wceq cfv fveq2 eqcoms opococ 3adant3 3adant2 eqeq12d
      syl5bi impbid2 ) BHIZDAIZEAIZJZDEKZECLZDCLZKZUHEDEDCMNUDUGCLZUFCLZKZUEUHU
      DUIDUJEUAUBUIDKUCABCDFGOPUAUCUJEKUBABCEFGOQRUKUGUFUGUFCMNST $.
      $( [8-Nov-2011] $)

    $( Orthocomplement contraposition law.  (Th. ~ negcon2 analog.) $)
    opcon2b $p |- ( ( K e. OP /\ X e. B /\ Y e. B )
       -> ( X = ( O ` Y ) <-> Y = ( O ` X ) ) ) $=
      ( cops wcel w3a cfv wceq wb opcon3b opoccl 3adant2 syld3an3 opococ
      eqeq1d bitrd ) BHIZDAIZEAIZJZDECKZLZUECKZDCKZLZEUHLUAUBUEAIZUFUIMUCABCDUE
      FGNUAUCUJUBABCEFGOPQUDUGEUHUAUCUGELUBABCEFGRPST $.
      $( [16-Jan-2012] $)

    $( Orthocomplement contraposition law.  (Th. ~ negcon1 analog.) $)
    opcon1b $p |- ( ( K e. OP /\ X e. B /\ Y e. B )
       -> ( ( O ` X ) = Y <-> ( O ` Y ) = X ) ) $=
      ( cops wcel w3a cfv wceq opcon2b eqcom 3bitr4g bicomd ) BHIDAIEAIJZECKZDL
      ZDCKZELZQDRLETLSUAABCDEFGMRDNTENOP $.
      $( [24-Jan-2012] $)
  $}

  ${
    opcon3.b $e |- B = ( Base ` K ) $.
    opcon3.l $e |- L = ( le ` K ) $.
    opcon3.o $e |- O = ( oc ` K ) $.
    $( Contraposition law for orthoposets. $)
    oplecon3 $p |- ( ( K e. OP /\ X e. B /\ Y e. B )
