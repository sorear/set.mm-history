      $.
      $( [20-Oct-2011] $)
  $}

  $( A Hilbert lattice is orthomodular. $)
  hloml $p |- ( K e. HL -> K e. OML ) $=
    ( chlt wcel coml ccla cal hllem1 simp1d ) ABCADCAECAFCAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is complete. $)
  hlclat $p |- ( K e. HL -> K e. CLat ) $=
    ( chlt wcel coml ccla cal hllem1 simp2d ) ABCADCAECAFCAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is atomic. $)
  hlatl $p |- ( K e. HL -> K e. AtLat ) $=
    ( chlt wcel coml ccla cal hllem1 simp3d ) ABCADCAECAFCAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is an ortholattice. $)
  hlol $p |- ( K e. HL -> K e. OL ) $=
    ( chlt wcel coml col hloml omlol syl ) ABCADCAECAFAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is an orthoposet. $)
  hlop $p |- ( K e. HL -> K e. OP ) $=
    ( chlt wcel col cops hlol olop syl ) ABCADCAECAFAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is a lattice. $)
  hllat $p |- ( K e. HL -> K e. LatNEW ) $=
    ( chlt wcel cal clat hlatl atlatlat syl ) ABCADCAECAFAGH $.
    $( [20-Oct-2011] $)

  $( A Hilbert lattice is a poset. $)
  hlpos $p |- ( K e. HL -> K e. PosetNEW ) $=
    ( chlt wcel clat cpo hllat latpos syl ) ABCADCAECAFAGH $.
    $( [20-Oct-2011] $)

  ${
    hlatjcl.b $e |- B = ( Base ` K ) $.
    hlatjcl.j $e |- J = ( join ` K ) $.
    hlatjcl.a $e |- A = ( Atoms ` K ) $.
    $( Closure of join operation.  Frequently-used special case of ~ latjcl for
       atoms. $)
    hlatjcl $p |- ( ( K e. HL /\ X e. A /\ Y e. A ) -> ( X J Y ) e. B ) $=
      ( clat wcel co chlt latjcl hllat atombase syl3an ) DJKEBKFBKEFCLBKDMKEAKF
      AKBCDEFGHNDOABEDGIPABFDGIPQ $.
      $( [15-Jun-2012] $)
  $}

  ${
    hlatjcom.j $e |- J = ( join ` K ) $.
    hlatjcom.a $e |- A = ( Atoms ` K ) $.
    $( Commutatitivity of join operation.  Frequently-used special case of
       ~ latjcom for atoms. $)
    hlatjcom $p |- ( ( K e. HL /\ X e. A /\ Y e. A )
               -> ( X J Y ) = ( Y J X ) ) $=
      ( clat wcel cbs cfv co wceq chlt eqid latjcom hllat atombase syl3an ) CHI
      DCJKZIETIDEBLEDBLMCNIDAIEAITBCDETOZFPCQATDCUAGRATECUAGRS $.
      $( [15-Jun-2012] $)

    $( TODO: Use in place of ~ latjcom when it shortens proofs $)
    $( Idempotence of join operation.  Frequently-used special case of
       ~ latjcom for atoms. $)
    hlatjidm $p |- ( ( K e. HL /\ X e. A ) -> ( X J X ) = X ) $=
      ( clat wcel cbs cfv co wceq chlt eqid latjidm hllat atombase syl2an ) CGH
      DCIJZHDDBKDLCMHDAHSBCDSNZEOCPASDCTFQR $.
      $( [15-Ju1-2012] $)

    $( Lattice join is associative.  Frequently-used special case of ~ latjass
       for atoms. $)
    hlatjass $p |- ( ( K e. HL /\ ( P e. A /\ Q e. A /\ R e. A ) ) ->
             ( ( P J Q ) J R ) = ( P J ( Q J R ) ) ) $=
      ( chlt wcel w3a wa clat cbs cfv co wceq simpl hllat syl simpr1 eqid
      atombase simpr2 simpr3 latjass syl13anc ) FIJZBAJZCAJZDAJZKZLZFMJZBFNOZJZ
      CUOJZDUOJZBCEPDEPBCDEPEPQUMUHUNUHULRFSTUMUIUPUHUIUJUKUAAUOBFUOUBZHUCTUMUJ
      UQUHUIUJUKUDAUOCFUSHUCTUMUKURUHUIUJUKUEAUODFUSHUCTUOEFBCDUSGUFUG $.
      $( [27-Jul-2012] $)

    $( Swap 2nd and 3rd members of lattice join.  Frequently-used special case
       of ~ latj32 for atoms. $)
    hlatj32 $p |- ( ( K e. HL /\ ( P e. A /\ Q e. A /\ R e. A ) ) ->
             ( ( P J Q ) J R ) = ( ( P J R ) J Q ) ) $=
      ( chlt wcel w3a wa clat cbs cfv co wceq simpl hllat syl simpr1 eqid
      atombase simpr2 simpr3 latj32 syl13anc ) FIJZBAJZCAJZDAJZKZLZFMJZBFNOZJZC
      UOJZDUOJZBCEPDEPBDEPCEPQUMUHUNUHULRFSTUMUIUPUHUIUJUKUAAUOBFUOUBZHUCTUMUJU
      QUHUIUJUKUDAUOCFUSHUCTUMUKURUHUIUJUKUEAUODFUSHUCTUOEFBCDUSGUFUG $.
      $( [21-Jul-2012] $)

    $( Rotate lattice join of 3 classes.  Frequently-used special case of
       ~ latjrot for atoms. $)
    hlatjrot $p |- ( ( K e. HL /\ ( P e. A /\ Q e. A /\ R e. A ) ) ->
             ( ( P J Q ) J R ) = ( ( R J P ) J Q ) ) $=
      ( chlt wcel w3a wa co hlatj32 wceq hlatjcom 3adant3r2 opreq1d eqtrd ) FIJ
      ZBAJZCAJZDAJZKLZBCEMDEMBDEMZCEMDBEMZCEMABCDEFGHNUDUEUFCETUAUCUEUFOUBAEFBD
      GHPQRS $.
      $( [2-Aug-2012] $)

    $( Rearrangement of lattice join of 4 classes.  Frequently-used special
       case of ~ latj4 for atoms. $)
    hlatj4 $p |- ( ( K e. HL /\ ( P e. A /\ Q e. A )
               /\ ( R e. A /\ S e. A ) )
        -> ( ( P J Q ) J ( R J S ) ) = ( ( P J R ) J ( Q J S ) ) ) $=
