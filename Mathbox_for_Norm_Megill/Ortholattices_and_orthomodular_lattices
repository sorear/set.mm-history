    $( Lemma for ~ 3at . $)
    3atlem5 $p |- ( ( ( K e. HL /\ ( P e. A /\ Q e. A /\ R e. A )
        /\ ( S e. A /\ T e. A /\ U e. A ) ) /\ ( -. R .<_ ( P .\/ Q )
            /\ P =/= Q /\ -. Q .<_ ( P .\/ U ) )
        /\ ( ( P .\/ Q ) .\/ R ) .<_ ( ( S .\/ T ) .\/ U ) )
      -> ( ( P .\/ Q ) .\/ R ) = ( ( S .\/ T ) .\/ U ) ) $=
      ( chlt wcel w3a co wbr wn wne wceq wa wi opreq2 eqcoms breq2d eqeq2d
      imbi12d simp1l simp1r1 simp2 simp1r3 simp3 3atlem3 syl131anc 3expia
