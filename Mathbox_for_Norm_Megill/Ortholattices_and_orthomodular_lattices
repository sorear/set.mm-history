    $( The covers relation implies no in-betweenness.  (Th. ~ cvnbtwn2
       analog.) $)
    cvrnbtwn2 $p |- ( ( K e. PosetNEW /\ ( X e. B /\ Y e. B /\ Z e. B )
                        /\ X C Y )
