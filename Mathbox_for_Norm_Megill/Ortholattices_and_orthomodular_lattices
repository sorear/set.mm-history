  $c <o $.
  $c Atoms $.
  $c AtLat $.
  $(
  $c AtsLat $.
  $c CvLat $.
  $)
  $c HL $.

  $( Extend class notation with covers relation. $)
  ccvr $a class <o $.

  $( Extend class notation with atoms. $)
  catm $a class Atoms $.

  $( Extend class notation with atomic lattices. $)
  cal $a class AtLat $.

  $( Extend class notation with atomistic lattices. $)
  $(
  cas $a class AtsLat $.
  $)

  $( Extend class notation with lattices with the covering property. $)
  $(
  clc $a class CvLat $.
  $)

  $( Extend class notation with Hilbert lattices. $)
  chlt $a class HL $.

  ${
    $d p l a b $.
    $( Def. 8.4.13 p. 209. $)
    $( Define the covers relation ("is covered by") for posets. $)
    df-covers $a |- <o = ( p e. _V |-> { <. a , b >. | ( ( a e. ( Base ` p )
               /\ b e. ( Base ` p ) )
           /\ a ( lt ` p ) b /\ -. E. z e. ( Base ` p )
              ( a ( lt ` p ) z /\ z ( lt ` p ) b ) ) } ) $.

    $( Def. 8.4.14 p. 210. $)
    $( Define the class of poset atoms. $)
    df-atoms $a |- Atoms = ( p e. _V |-> { a e. ( Base ` p ) |
                      ( 0. ` p ) ( <o ` p ) a } ) $.

    $( Def. 8.4.15 p. 210. $)
    $( Define the class of atomic lattices. $)
    df-atlat $a |- AtLat = { l e. LatNEW | A. a e. ( Base ` l )
       ( a =/= ( 0. ` l ) ->
          E. b e. ( Atoms ` l ) b ( le ` l ) a ) } $.

    $( Def. 8.4.16 p. 210. $)
    $( Define the class of atomistic lattices.  Every non-zero element
       is the join of all atoms less than it.  If an
        orthomodular lattice is atomic, then it is also atomistic
      (Beltrametti_Cassinelli p. 98). $)
    $(
    df-atslat $a |- AtsLat = { l e. LatNEW | A. a e. ( Base ` l )
         a = ( ( lub ` l ) ` { p e. ( Atoms ` l ) | p ( le ` l ) a } ) } $.
    $)
  $}

  ${
    $d p x y z B $.  $d p x y z K $.  $d p S $.
    cvrfval.b $e |- B = ( Base ` K ) $.
    cvrfval.s $e |- S = ( lt ` K ) $.
    cvrfval.c $e |- C = ( <o ` K ) $.
    $( Value of covers relation "is covered by". $)
