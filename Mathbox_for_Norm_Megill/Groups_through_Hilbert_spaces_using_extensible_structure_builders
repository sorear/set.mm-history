$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
       Groups through Hilbert spaces using extensible structure builders
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c +g $.
  $c GrpNEW $.
  $c 0g $.
  $c -g $.
  $c AbelNEW $.
  $c .r $.
  $c RingNEW $.
  $c 1rNEW $.

  $( Extend class notation with group operation. $)
  cplusg $a class +g $.

  $( Extend class notation with class of all groups. $)
  cgrp $a class GrpNEW $.

  $( Extend class notation with group identity element. $)
  c0g $a class 0g $.

  $( Extend class notation with inverse of group element. $)
  cminusg $a class -g $.

  $( Extend class notation with class of all Abelian groups. $)
  cabel $a class AbelNEW $.

  $( Extend class notation with ring multiplication. $)
  cmulr $a class .r $.

  $( Extend class notation with class of all rings. $)
  crg $a class RingNEW $.

  $( Extend class notation with ring unit. $)
  cur $a class 1rNEW $.

  ${
    $d a b c e f g h i k m p q r s t u v w x y z $.
    $( Def. 8.5.2 p. 214 $)
    $( Define group operation. $)
    df-plusg $a |- +g = ( g e. _V |-> ( g ` 2 ) ) $.

    $( Def. 8.5.1 p. 213 $)
    $( Define class of all groups. $)
    df-grpNEW $a |- GrpNEW = Struct ( 2 , f , E. g E. p ( g = ( base ` f )
       /\ p = ( +g ` f ) /\ ( A. a e. g A. b e. g A. c e. g ( ( a p b ) e. g
            /\ ( ( a p b ) p c ) = ( a p ( b p c ) ) ) /\
          E. e e. g A. a e. g ( ( e p a ) = a /\
            E. m e. g ( m p a ) = e ) ) ) ) $.

    $( Def. 8.5.4 p. 215 $)
    $( Define group identity element. $)
    df-0g $a |- 0g = ( g e. _V |-> ( iota e ( e e. ( base ` g ) /\
      A. x e. ( base ` g ) ( e ( +g ` g ) x ) = x ) ) ) $.

    $( Def. 8.5.4 p. 215 $)
    $( Define inverse of group element. $)
    df-minusg $a |- -g = ( g e. _V |-> ( x e. ( base ` g ) |-> ( iota w
          ( w e. ( base ` g ) /\ ( w ( +g ` g ) x ) = ( 0g ` g ) ) ) ) ) $.

    $( Def. 8.5.5 p. 215 $)
    $( Define class of all Abelian groups. $)
    df-ablNEW $a |- AbelNEW = { g e. GrpNEW | A. a e. ( base ` g )
      A. b e. ( base ` g ) ( a ( +g ` g ) b ) = ( b ( +g ` g ) a ) } $.

    $( Def. 8.5.7 p. 216 $)
    $( Define ring multiplication. $)
    df-mulr $a |- .r = ( r e. _V |-> ( r ` 3 ) ) $.

    $( Def. 8.5.6 p. 216 $)
    $( Define class of all rings (with unit). $)
    df-ringNEW $a |- RingNEW = Struct ( 3 , f , ( f e. AbelNEW /\
        E. r E. p E. t ( ( r = ( base ` f ) /\ p = ( +g ` f )
           /\ t = ( .r ` f ) )
       /\ A. x e. r A. y e. r A. z e. r
           ( ( x t y ) e. r /\ ( ( ( x t y ) t z ) = ( x t ( y t z ) )
            /\ ( x t ( y p z ) ) = ( ( x t y ) p ( x t z ) )
            /\ ( ( x p y ) t z ) = ( ( x t z ) p ( y t z ) ) ) )
        /\ E. u e. r A. x e. r ( ( u t x ) = x /\ ( x t u ) = x ) ) ) ) $.

    $( Def. 8.5.7 p. 216 $)
    $( Define ring unit. $)
    df-ur $a |- 1rNEW = ( r e. _V |-> ( iota u ( u e. ( base ` r ) /\
      A. x e. ( base ` r ) ( ( u ( .r ` r ) x ) = x
           /\ ( x ( .r ` r ) u ) = x ) ) ) ) $.
  $}

  ${
    $d g G $.
    plusgval.1 $e |- G e. _V $.
    $( Value of the group operation extractor. $)
    plusgval $p |- ( +g ` G ) = ( G ` 2 ) $=
      ( vg cplusg c2 df-plusg strcval ) DCAEBCFG $.
      $( [4-Sep-2011] $)
  $}

  ${
    $d g B $.  $d g P $.
    grpstr.1 $e |- B e. _V $.
    grpstr.2 $e |- P e. _V $.
    grpstr.3 $e |- G
               = StrBldr ( 2 , g , ( ( base ` g ) = B /\ ( +g ` g ) = P ) ) $.
    $( Structure scaffold property of a constructed group. $)
    grpstr $p |- G e. Struct ( 2 , g , T. ) $=
      ( cv cbs cfv wceq cplusg wa c1 c2 visset baseval eqeq1i plusgval anbi12i
      stb2cl ) CHZIJZAKZUBLJZBKZMABCDEFUDNUBJZAKUFOUBJZBKUCUGAUBCPZQRUEUHBUBUIS
      RTGUA $.
      $( [8-Sep-2011] $)

    $( The base set of a constructed group. $)
    grpbase $p |- B = ( base ` G ) $=
      ( cbs cplusg df-base df-plusg stb2v1 ) HICCDABCJCKEFGL $.
      $( [8-Sep-2011] $)

    $( The operation of a constructed group. $)
    grpplusg $p |- P = ( +g ` G ) $=
      ( cbs cplusg df-base df-plusg stb2v2 ) HICCDABCJCKEFGL $.
      $( [8-Sep-2011] $)
  $}

  ${
    grpstrx.1 $e |- B e. _V $.
    grpstrx.2 $e |- P e. _V $.
    grpstrx.3 $e |- G = { <. 1 , B >. , <. 2 , P >. } $.
    $( Structure scaffold property of an explicitly given group. $)
    grpstrx $p |- G e. Struct ( 2 , g , T. ) $=
      ( stb2strx ) CDABEFGH $.
      $( [8-Sep-2011] $)

    $( The base of an explicitly given group. $)
    grpbasex $p |- B = ( base ` G ) $=
      ( vf cbs df-base stb2v1x ) HGCABDEFGIJ $.
      $( [8-Sep-2011] $)

    $( The operation of an explicitly given group. $)
    grpplusgx $p |- P = ( +g ` G ) $=
      ( vg cplusg df-plusg stb2v2x ) HGCABDEFGIJ $.
      $( [8-Sep-2011] $)
  $}

  ${
    $d g B $.  $d g P $.
    grpstrv.1 $e |- B e. _V $.
    grpstrv.2 $e |- P e. _V $.
    grpstrv.3 $e |- G
               = StrBldr ( 2 , g , ( ( base ` g ) = B /\ ( +g ` g ) = P ) ) $.
    $( The explicit value of a constructed group structure.  This theorem lets
       us go back and forth between the two representations. $)
    grpstrval $p |- G = { <. 1 , B >. , <. 2 , P >. } $=
      ( cbs cplusg df-base df-plusg stb2xpl ) HICCDABCJCKEFGL $.
      $( [8-Sep-2011] $)
  $}

  ${
    $d a b c e f g m p B $.  $d f g p G $.  $d a b c e f g m p P $.
    isgrp.0NEW $e |- S = Struct ( 2 , f , T. ) $.
    isgrp.1NEW $e |- B = ( base ` G ) $.
    isgrp.2NEW $e |- P = ( +g ` G ) $.
    $( The predicate "is a group." $)
    isgrpNEW $p |- ( G e. GrpNEW <-> ( G e. S
            /\ A. a e. B A. b e. B A. c e. B
            ( ( a P b ) e. B /\ ( ( a P b ) P c ) = ( a P ( b P c ) ) )
   /\ E. e e. B A. a e. B ( ( e P a ) = a /\ E. m e. B ( m P a ) = e ) ) ) $=
