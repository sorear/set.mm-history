  ${
    $d x y B $.  $d y A $.
    $( Not in [WhiteheadRussell] but it serves as a lemma for ~ sbceqal .
       Notice that ` B ` and ` x ` do not have to be distinct. $)
    eqsbc3 $p |- ( A e. C -> ( [ A / x ] x = B <-> A = B ) ) $=
      ( vy cv wceq wsbc dfsbcq eqeq1 eqsb3 vtoclbg ) AFCGZAEFZHNCGMABHBCGEBDMAN
      BINBCJEACKL $.
      $( [29-Jun-2011] $) $( [29-Jun-2011] $)
  $}

  ${
    $d x B $.  $d x A $.
    $( Not in [WhiteheadRussell] but is useful for proving the theorems in the
       section.  It is the set theory version of ~ sbeqal .  Notice that ` B `
       and ` x ` do not have to be distinct. $)
    sbceqal $p |- ( A e. C -> ( A. x ( x = A -> x = B ) -> A = B ) ) $=
      ( wcel cv wceq wi wal wsbc a4sbc sbcimg wb eqid eqeq1 sbcieg mpbiri 
      pm5.5 syl eqsbc3 3bitrd sylibd ) BDEZAFZBGZUDCGZHZAIUGABJZBCGZUGABDKUCUHU
      EABJZUFABJZHZUKUIUEUFABDLUCUJULUKMUCUJBBGZBNUEUMABDUDBBOPQUJUKRSABCDTUAUB
      $.
      $( [28-Jun-2011] $) $( [28-Jun-2011] $)
  $}

  ${
    $d x A $.  $d x B $.
    $( Theorem *14.121 in [WhiteheadRussell] p. 185. $)
