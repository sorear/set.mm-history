    ${
      $d A x $.
      $( Any set that contains one element less than the universe is not equal
         to it. $)
      elnev $p |- ( A e. _V <-> { x | -. x = A } =/= _V ) $=
        ( cv wceq wex wn cab cvv wcel wne wal df-v eqeq2i wb eq2ab equid tbt 
        albii bitr4i alnex 3bitri con2bii isset df-ne 3bitr4i ) ACZBDZAEZUGFZAGZH
        DZFBHIUJHJUKUHUKUJUFUFDZAGZDZUIAKZUHFHUMUJALMUNUIULNZAKUOUIULAOUIUPAULUIA
        PQRSUGATUAUBABUCUJHUDUE $.
        $( [16-Jun-2011] $) $( [16-Jun-2011] $)
    $}
  
  
    ${
      $d A y $.    $d x y $.
      isseta.1 $e |- ( y e. A -> A. x y e. A ) $.
      isseta.2 $e |- ( x = A -> A. y x = A ) $.
      $( A version of isset that does not require x and A to be distinct. $)
      isseta $p |- ( A e. _V <-> E. x x = A ) $=
        ( cvv wcel cv wceq wex isset hbeleq eqeq1 cbvex bitri ) CFGBHZCIZBJAHZCIZ
        AJBCKQSBAABCDLEPRCMNO $.
        $( [6-Jun-2011] $) $( [6-Jun-2011] $)
    $}
  
    ${
      $d x y $.    $d y A $.
      cla4gft.1 $e |- ( ps -> A. x ps ) $.
      cla4gft.2 $e |- ( y e. A -> A. x y e. A ) $.
      $( A closed version of cla4gf. $)
      cla4gft $p |- ( A. x ( x = A -> ( ph <-> ps ) ) -> ( A e. B ->
                      ( A. x ph -> ps ) ) ) $=
        ( cv wceq wb wi wal cvv wcel wex hba1 bi1 imim2i a4s 19.22d ax-17 isseta
        syl5ib 19.36 syl6ib elisset syl5 ) CIEJZABKZLZCMZENOZACMBLZEFOULUMABLZCPZ
        UNULUICPUPUMULUIUOCUKCQUKUIUOLCUJUOUIABRSTUACDEHUIDUBUCUDABCGUEUFEFUGUH
        $.
        $( [6-Jun-2011] $) $( [6-Jun-2011] $)
    $}
  
    ${
      $d x A $.    $d x B $.    $d A z $.  $d x z $.
      rcla4t.1 $e |- ( ps -> A. x ps ) $.
      $( A closed version of rcla4. $)
      rcla4t $p |- ( A. x ( x = A -> ( ph <-> ps ) ) -> ( A e. B ->
                   ( A. x e. B ph -> ps ) ) ) $=
        ( vz cv wceq wb wi wal wcel wral wa eleq1 adantr pm3.27 imbi12d ex a2i
        19.20i ax-17 hbim cla4gft syl df-ral syl7ib com34 pm2.43d ) CHZDIZABJZKZC
        LZDEMZACENZBKUOUPUQUPBUOUPUKEMZAKZCLZUPBKZUQUOULUSVAJZKZCLUPUTVAKKUNVCCUL
        UMVBULUMVBULUMOURUPABULURUPJUMUKDEPQULUMRSTUAUBUSVACGDEUPBCUPCUCFUDGHDMCU
        CUEUFACEUGUHUIUJ $.
        $( [6-Jun-2011] $) $( [6-Jun-2011] $)
