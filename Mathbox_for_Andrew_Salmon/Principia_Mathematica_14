    ${
      $( Distribute proper substitution through negated membership. $)
      sbcnel12g $p |- ( A e. D -> ( [ A / x ] B e/ C <-> [_ A / x ]_ B e/
                      [_ A / x ]_ C ) ) $=
        ( wcel wnel wsbc wn csb df-nel sbcbii sbcng sbcel12g notbid syl6bbr 
        3bitrd ) BEFZCDGZABHCDFZIZABHTABHZIZABCJZABDJZGZSUAABECDKLTABEMRUCUDUEFZI
        UFRUBUGABCDENOUDUEKPQ $.
        $( [28-Jun-2011] $) $( [18-Jun-2011] $)
    $}
  
    ${
      $( Distribute proper substitution through an inequality. $)
      sbcne12g $p |- ( A e. D -> ( [ A / x ] B =/= C <-> [_ A / x ]_ B =/=
                     [_ A / x ]_ C ) ) $=
        ( wcel wne wsbc csb wn wceq nne sbcbii sbcng sbceqdig syl6bbr 3bitr3d 
        con4bid ) BEFZCDGZABHZABCIZABDIZGZSTJZABHCDKZABHZUAJUDJZUEUFABECDLMTABENS
        UGUBUCKUHABCDEOUBUCLPQR $.
        $( [28-Jun-2011] $) $( [18-Jun-2011] $)
    $}

    ${
      $( A version of Russell's paradox which is proven using proper
         substitution. $)
      rusbcALT $p |- { x | x e/ x } e/ _V $=
        ( cv wnel cab cvv wcel wn wb pm5.19 wsbc csb elabsg sbcnel12g csbvarg 
        eleq12d notbid df-nel syl5bb 3bitrd mto mpbir ) ABZUBCZADZECUDEFZGUEUDUDF
        ZUFGZHUFIUEUFUCAUDJAUDUBKZUHCZUGUCAUDELAUDUBUBEMUEUHUHFZGUGUIUEUJUFUEUHUD
        UHUDAUDENZUKOPUHUHQRSTUDEQUA $.
        $( [29-Jun-2011] $) $( [18-Jun-2011] $)
    $}
  
