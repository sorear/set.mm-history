  ${
    $d A x $.
    $( Any set that contains one element less than the universe is not equal to
       it. $)
    elnev $p |- ( A e. _V <-> { x | -. x = A } =/= _V ) $=
      ( cv wceq wex wn cab cvv wcel wne wal df-v eqeq2i wb eq2ab equid tbt
      albii bitr4i alnex 3bitri con2bii isset df-ne 3bitr4i ) ACZBDZAEZUGFZAGZH
      DZFBHIUJHJUKUHUKUJUFUFDZAGZDZUIAKZUHFHUMUJALMUNUIULNZAKUOUIULAOUIUPAULUIA
      PQRSUGATUAUBABUCUJHUDUE $.
      $( [16-Jun-2011] $) $( [16-Jun-2011] $)
  $}

  ${
    $d A y $.  $d x y $.
    isseta.1 $e |- ( y e. A -> A. x y e. A ) $.
    isseta.2 $e |- ( x = A -> A. y x = A ) $.
    $( A version of isset that does not require x and A to be distinct. $)
    isseta $p |- ( A e. _V <-> E. x x = A ) $=
      ( cvv wcel cv wceq wex isset hbeleq eqeq1 cbvex bitri ) CFGBHZCIZBJAHZCIZ
      AJBCKQSBAABCDLEPRCMNO $.
      $( [6-Jun-2011] $) $( [6-Jun-2011] $)
  $}

