  ${
    $d A x $.
    $( Any set that contains one element less than the universe is not equal to
       it. $)
    elnev $p |- ( A e. _V <-> { x | -. x = A } =/= _V ) $=
      ( cv wceq wex wn cab cvv wcel wne wal df-v eqeq2i wb eq2ab equid tbt
      albii bitr4i alnex 3bitri con2bii isset df-ne 3bitr4i ) ACZBDZAEZUGFZAGZH
      DZFBHIUJHJUKUHUKUJUFUFDZAGZDZUIAKZUHFHUMUJALMUNUIULNZAKUOUIULAOUIUPAULUIA
      PQRSUGATUAUBABUCUJHUDUE $.
      $( [16-Jun-2011] $)
  $}

  ${
    $( A version of Russell's paradox which is proven using proper
       substitution. $)
    rusbcALT $p |- { x | x e/ x } e/ _V $=
      ( cv wnel cab cvv wcel wn wb pm5.19 wsbc csb elabsg sbcnel12g df-nel
      csbvarg eleq12d notbid syl5bb 3bitrd mto mpbir ) ABZUBCZADZECUDEFZGUEUDUD
      FZUFGZHUFIUEUFUCAUDJAUDUBKZUHCZUGUCAUDELAUDUBUBEMUIUHUHFZGUEUGUHUHNUEUJUF
      UEUHUDUHUDAUDEOZUKPQRSTUDENUA $.
      $( [18-Jun-2011] $)
  $}

  ${
    $d x A $.
    $( Equivalence between two ways of saying "is a member of the complement of
       ` A ` ." $)
    compel $p |- ( x e. ( _V \ A ) <-> -. x e. A ) $=
      ( cv cvv cdif wcel wn wa eldif visset biantrur bitr4i ) ACZDBEFMDFZMBFGZH
      OMDBINOAJKL $.
      $( [15-Jul-2011] $)

    $( Equality between two ways of saying "the complement of ` A ` ." $)
    compeq $p |- ( _V \ A ) = { x | -. x e. A } $=
      ( cv wcel wn cvv cdif compel abbi2i ) ACBDEAFBGABHI $.
      $( [15-Jul-2011] $)
  $}

  ${
    $d A x y $.
    $( The complement of ` A ` is not equal to ` A ` . $)
    compne $p |- ( _V \ A ) =/= A $=
      ( vx vy cvv cdif wne wceq wn cv wcel cab wb wal wsbc pm5.19 ax-4 df-clab
      bibi12i sbn bibi1i bicom 3bitri sylib mto dfcleq mtbir compeq eqcomi
      abid2 eqeq12i mtbi df-ne mpbir ) DAEZAFUNAGZHBIAJZHZBKZUPBKZGZUOUTCIZURJZ
      VAUSJZLZCMZVEUPBVANZVFHZLZVFOVEVDVHVDCPVDUQBVANZVFLVGVFLVHVBVIVCVFUQCBQUP
      CBQRVIVGVFUPBCSTVGVFUAUBUCUDCURUSUEUFURUNUSAUNURBAUGUHBAUIUJUKUNAULUM $.
      $( [15-Jul-2011] $)
  $}

  ${
    $d x ph $.  $d x z $.
    $( Two ways of saying "the complement of a class abstraction". $)
    compab $p |- ( _V \ { z | ph } ) = { z | -. ph } $=
      ( vx cvv cab cdif cv wcel wn compeq hbab1 hbn ax-17 wceq wsbc df-clab
      sbequ12r syl5bb notbid cbvab eqtri ) DABEZFCGZUBHZIZCEAIZBECUBJUEUFCBUDBA
      BCKLUFCMUCBGNZUDAUDABUCOUGAACBPACBQRSTUA $.
      $( [15-Jul-2011] $)
  $}

  ${
    $d A x $.  $d B x $.
    $( Contrpositive law for subsets. $)
    conss34 $p |- ( A C_ B <-> ( _V \ B ) C_ ( _V \ A ) ) $=
      ( vx cv wcel wi wal cvv cdif wss wn con34b compel imbi12i bitr4i albii
      dfss2 3bitr4i ) CDZAEZSBEZFZCGSHBIZEZSHAIZEZFZCGABJUCUEJUBUGCUBUAKZTKZFUG
      TUALUDUHUFUICBMCAMNOPCABQCUCUEQR $.
      $( [15-Jul-2011] $)

    $( Contrapositive law for subsets. $)
    conss2 $p |- ( A C_ ( _V \ B ) <-> B C_ ( _V \ A ) ) $=
      ( cvv wss cdif wb ssv ssconb mp2an ) ACDBCDACBEDBCAEDFAGBGABCHI $.
      $( [15-Jul-2011] $)

    $( Contrapositive law for subsets. $)
    conss1 $p |- ( ( _V \ A ) C_ B <-> ( _V \ B ) C_ A ) $=
      ( vx cv cvv cdif wcel wi wal wss wn con1b compel imbi1i 3bitr4i albii
      dfss2 ) CDZEAFZGZRBGZHZCIREBFZGZRAGZHZCISBJUCAJUBUFCUEKZUAHUAKZUEHUBUFUEU
      ALTUGUACAMNUDUHUECBMNOPCSBQCUCAQO $.
      $( [15-Jul-2011] $)
  $}

  ${
    ralbidar.1 $e |- ( ph -> A. x e. A ph ) $.
    ralbidar.2 $e |- ( ( ph /\ x e. A ) -> ( ps <-> ch ) ) $.
    $( More general form of ~ ralbida . $)
    ralbidar $p |- ( ph -> ( A. x e. A ps <-> A. x e. A ch ) ) $=
      ( cv wcel wi wal wral wb ex ralimi syl df-ral sylib pm2.43 pm5.74d alimi
      albi 3syl 3bitr4g ) ADHEIZBJZDKZUECJZDKZBDELCDELAUEUEBCMZJZJZDKZUFUHMZDKU
      GUIMAUKDELZUMAADELUOFAUKDEAUEUJGNOPUKDEQRULUNDULUEBCUEUJSTUAUFUHDUBUCBDEQ
      CDEQUD $.
      $( [25-Jul-2011] $)

    $( More general form of ~ rexbida . $)
    rexbidar $p |- ( ph -> ( E. x e. A ps <-> E. x e. A ch ) ) $=
      ( cv wcel wa wex wrex wb wi wal wral ex ralimi syl df-ral sylib pm2.43
      pm5.32d alimi exbi 3syl df-rex 3bitr4g ) ADHEIZBJZDKZUICJZDKZBDELCDELAUIU
      IBCMZNZNZDOZUJULMZDOUKUMMAUODEPZUQAADEPUSFAUODEAUIUNGQRSUODETUAUPURDUPUIB
      CUIUNUBUCUDUJULDUEUFBDEUGCDEUGUH $.
      $( [25-Jul-2011] $)
  $}

  ${
    $d ph w $.  $d w x $.  $d w y $.  $d w z $.
    $( Theorem to aid use of the distinctor reduction theorem with ordered pair
       class abstraction. $)
    dropab1 $p |- ( A. x x = y -> { <. x , z >. | ph } =
        { <. y , z >. | ph } ) $=
      ( vw cv wceq wal cop wa wex cab copab opeq1 a4s eqeq2d anbi1d drex2
      drex1 abbidv df-opab 3eqtr4g ) BFZCFZGZBHZEFZUCDFZIZGZAJZDKZBKZELUGUDUHIZ
      GZAJZDKZCKZELABDMACDMUFUMUREULUQBCUKUPBCDUFUJUOAUFUIUNUGUEUIUNGBUCUDUHNOP
      QRSTABDEUAACDEUAUB $.
      $( [25-Jul-2011] $)

    $( Theorem to aid use of the distinctor reduction theorem with ordered pair
       class abstraction. $)
    dropab2 $p |- ( A. x x = y -> { <. z , x >. | ph } =
        { <. z , y >. | ph } ) $=
      ( vw cv wceq wal cop wa wex cab copab opeq2 a4s eqeq2d anbi1d drex1
      drex2 abbidv df-opab 3eqtr4g ) BFZCFZGZBHZEFZDFZUCIZGZAJZBKZDKZELUGUHUDIZ
      GZAJZCKZDKZELADBMADCMUFUMUREULUQBCDUKUPBCUFUJUOAUFUIUNUGUEUIUNGBUCUDUHNOP
      QRSTADBEUAADCEUAUB $.
      $( [25-Jul-2011] $)
  $}

  ${
    $d x A $.
    $( If the identity relation partially orders any class, then that class is
       the null class. $)
    ipo0 $p |- ( _I Po A <-> A = (/) ) $=
      ( vx cid wpo c0 wceq wne cv wcel wex wn n0 wbr weq equid visset ideq
      mpbir poirr ex mt2i con2i 19.23aiv sylbi necon4ai po0 poeq2 mpbiri
      impbii ) ACDZAEFZUJAEAEGBHZAIZBJUJKZBALUMUNBUJUMUJUMULULCMZUOBBNBOULULBPQ
      RUJUMUOKAULCSTUAUBUCUDUEUKUJECDCUFAECUGUHUI $.
      $( [25-Jul-2011] $)

    $( A class that is founded by the identity relation is null. $)
    ifr0 $p |- ( _I Fr A <-> A = (/) ) $=
      ( vx cid wfr c0 wceq wne cv wcel wex wn n0 wbr equid visset ideq mpbir
      frirr ex mt2i con2i 19.23aiv sylbi necon4ai fr0 freq2 mpbiri impbii ) ACD
      ZAEFZUIAEAEGBHZAIZBJUIKZBALULUMBUIULUIULUKUKCMZUNUKUKFBNUKUKBOPQUIULUNKBA
      CRSTUAUBUCUDUJUIECDCUEAECUFUGUH $.
      $( [25-Jul-2011] $)

    $( ~ ordelpss with an antecedent removed. $)
    ordpss $p |- ( Ord B -> ( A e. B -> A C. B ) ) $=
      ( word wcel wa wpss ordelord ex ancrd wb ordelpss ancoms biimpd expimpd
      syld ) BCZABDZACZQEABFZPQRPQRBAGHIPRQSPREQSRPQSJABKLMNO $.
      $( [25-Jul-2011] $)
  $}

  ${
    $d A x y $.  $d F x y $.
    fvsb.1 $e |- A e. _V $.
    $( Explicit substitution of a value of a function into a wff. $)
    fvsb $p |- ( E! y A F y -> ( [ ( F ` A ) / x ] ph <->
        E. x ( A. y ( A F y <-> y = x ) /\ ph ) ) ) $=
      ( cfv wsbc cv wbr cio weu wceq wb wal wa wex fv4 dfsbcq ax-mp iotasbc
      syl5bb ) ABDEGZHZABDCIZEJZCKZHZUFCLUFUEBIMNCOAPBQUCUGMUDUHNCDEFRABUCUGSTU
      FACBUAUB $.
      $( [1-Aug-2011] $)

    fveqsb.2 $e |- ( x = ( F ` A ) -> ( ph <-> ps ) ) $.
    fveqsb.3 $e |- ( ps -> A. x ps ) $.
    $( Implicit substitution of a value of a function into a wff. $)
    fveqsb $p |- ( E! y A F y -> ( ps <->
        E. x ( A. y ( A F y <-> y = x ) /\ ph ) ) ) $=
      ( cfv wsbc cv wbr weu wceq wb wal wa wex cvv wcel fvex wi a1i sbciegf
      ax-mp fvsb syl5bbr ) BACEFJZKZEDLZFMZDNULUKCLOPDQARCSUITUAZUJBPEFUBABCUIT
      BBCQUCUMIUDHUEUFACDEFGUGUH $.
      $( [1-Aug-2011] $)
  $}

  ${
    $d x y z A $.  $d x y B $.
    $( 'Less than' implies not equal for extended reals. $)
    xrltneNEW $p |- ( ( A e. RR* /\ A < B ) -> A =/= B ) $=
      ( cxr wcel clt wbr wne wn wceq xrltnr breq2 notbid syl5ibcom necon2ad
      imp ) ACDZABEFZABGPQABPAAEFZHABIZQHAJSRQABAEKLMNO $.
      $( [11-Nov-2011] $)

    $( A Cartesian product exists iff its converse does.  Corollary 6.9(1) in
       [TakeutiZaring] p. 26. $)
    xpexb $p |- ( ( A X. B ) e. _V <-> ( B X. A ) e. _V ) $=
      ( cxp cvv wcel ccnv cnvxp cnvexg syl5eqelr impbii ) ABCZDEZBACZDEZLMKFDAB
      GKDHINKMFDBAGMDHIJ $.
      $( [13-Nov-2011] $)

    $( A condition where the converse of ~ xpex holds as well.  Corollary
       6.9(2) in [TakeutiZaring] p. 26. $)
    xpexcnv $p |- ( ( B =/= (/) /\ ( A X. B ) e. _V ) -> A e. _V ) $=
      ( c0 wne cxp cvv wcel cdm dmexg dmxp eleq1d syl5ib imp ) BCDZABEZFGZAFGZP
      OHZFGNQOFINRAFABJKLM $.
      $( [13-Nov-2011] $)

    $( An element of a transitive set is a proper subset of it.  Theorem 7.2 in
       [TakeutiZaring] p. 35.  Unlike ~ tz7.2 , ~ ax-reg is required for its
       proof. $)
    trelpss $p |- ( ( Tr A /\ B e. A ) -> B C. A ) $=
      ( wtr wcel wa wss wne wpss cep wfr zfregfr tz7.2 mp3an2 df-pss sylibr )
      ACZBADZEBAFBAGEZBAHPAIJQRAKABLMBANO $.
      $( [13-Nov-2011] $)
  $}


