  $( The sine function is periodic.  (Contributed by Paul Chapman,
     23-Jan-2008.) $)
  sinper $p |- ( ( A e. CC /\ K e. ZZ ) ->
                 ( sin ` ( A + ( K x. ( 2 x. pi ) ) ) ) = ( sin ` A ) ) $=
    ( c2 ci cmul co csin cmin sinval cpi caddc sinperlem ) ACDEFGBHAIABCJEFEFKF
    IL $.
    $( [10-May-2014] $) $( [23-Jan-2008] $)

  $( The cosine function is periodic.  (Contributed by Paul Chapman,
     23-Jan-2008.) $)
  cosper $p |- ( ( A e. CC /\ K e. ZZ ) ->
                 ( cos ` ( A + ( K x. ( 2 x. pi ) ) ) ) = ( cos ` A ) ) $=
    ( c2 ccos caddc cosval cpi cmul co sinperlem ) ACDBEAFABCGHIHIEIFJ $.
    $( [10-May-2014] $) $( [23-Jan-2008] $)

  $( If ` K ` is an integer, the sine of ` 2 K pi ` is 0.  (Contributed by Paul
     Chapman, 23-Jan-2008.) $)
