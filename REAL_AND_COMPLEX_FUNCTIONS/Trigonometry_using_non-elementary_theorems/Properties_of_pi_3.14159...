  $( The exponential function is periodic.  (Contributed by Paul Chapman,
     21-Apr-2008.)  (The proof was shortened by Mario Carneiro,
     10-May-2014.) $)
  efper $p |- ( ( A e. CC /\ K e. ZZ ) ->
           ( exp ` ( A + ( ( _i x. ( 2 x. pi ) ) x. K ) ) ) = ( exp ` A ) ) $=
    ( cc wcel cz wa ci c2 cpi cmul co caddc ce cfv wceq ax-icn 2cn pire mulcli
    c1 recni zcn mulcl sylancr efadd sylan2 ef2kpi oveq2d efcl mulid1 sylan9eqr
    syl eqtrd ) ACDZBEDZFAGHIJKZJKZBJKZLKMNZAMNZURMNZJKZUTUOUNURCDZUSVBOUOUQCDB
    CDVCGUPPHIQIRUASSBUBUQBUCUDAURUEUFUOUNVBUTTJKZUTUOVATUTJBUGUHUNUTCDVDUTOAUI
    UTUJULUKUM $.
    $( [10-May-2014] $) $( [21-Apr-2008] $)

  ${
    sinperlem.1 $e |- ( A e. CC -> ( F ` A ) =
      ( ( ( exp ` ( _i x. A ) ) O ( exp ` ( -u _i x. A ) ) ) / D ) ) $.
    sinperlem.2 $e |- ( ( A + ( K x. ( 2 x. pi ) ) ) e. CC ->
      ( F ` ( A + ( K x. ( 2 x. pi ) ) ) ) =
        ( ( ( exp ` ( _i x. ( A + ( K x. ( 2 x. pi ) ) ) ) ) O
          ( exp ` ( -u _i x. ( A + ( K x. ( 2 x. pi ) ) ) ) ) ) / D ) ) $.
    $( Lemma for ~ sinper and ~ cosper . $)
    sinperlem $p |- ( ( A e. CC /\ K e. ZZ ) ->
                 ( F ` ( A + ( K x. ( 2 x. pi ) ) ) ) = ( F ` A ) ) $=
