  $( Real part of a logarithm.  (Contributed by Mario Carneiro,
     15-Sep-2014.) $)
  relog $p |- ( ( A e. CC /\ A =/= 0 ) ->
                  ( Re ` ( log ` A ) ) = ( log ` ( abs ` A ) ) ) $=
    ( cc wcel cc0 wne wa clog cfv ce cabs cr wceq logcl recl relogef 3syl absef
    cre fveq2d eqtr3d syl eflog ) ABCADEFZAGHZRHZIHZGHZUEAJHZGHUCUDBCZUEKCUGUEL
