$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        The Division algorithm for polynomials
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c quot $.

  $( Extend class notation to include the quotient of a polynomial division. $)
  cquot $a class quot $.

  ${
    $d f g q r $.
    $( Define the quotient function on polynomials.  This is the ` q ` of the
       expression ` f = g x. q + r ` in the division algorithm. $)
    df-quot $a |- quot = ( f e. ( Poly ` CC ) ,
      g e. ( ( Poly ` CC ) \ { 0p } ) |->
      ( iota_ q e. ( Poly ` CC ) [ ( f oF - ( g oF x. q ) ) / r ]
        ( r = 0p \/ ( deg ` r ) < ( deg ` g ) ) ) ) $.
  $}

  ${
    $d f g q r F $.  $d f g q G $.  $d f g r R $.
    quotval.1 $e |- R = ( F oF - ( G oF x. q ) ) $.
    $( Value of the quotient function. $)
    quotval $p |- ( ( F e. ( Poly ` S ) /\ G e. ( Poly ` S ) /\ G =/= 0p ) ->
                    ( F quot G ) = ( iota_ q e. ( Poly ` CC )
                      ( R = 0p \/ ( deg ` R ) < ( deg ` G ) ) ) ) $=
      ( vf vg vr cply cfv wcel c0p co wceq cdgr clt wbr wo cv cc wne cquot crio
      plyssc sseli wa csn cdif eldifsn cmul cof cmin wsbc oveq12 sylan2 syl6eqr
      oveq1 dfsbcq syl cvv ovex eqeltri eqeq1 fveq2 breq1d orbi12d sbcie fveq2d
      simpr breq2d orbi2d syl5bb bitrd riotabidv df-quot riotaex sylan2br 3impb
      wb ovmpt2a syl3an2 syl3an1 ) CBJKZLCUAJKZLZDWDLZDMUBZCDUCNAMOZAPKZDPKZQRZ
      SZEWEUDZOZWDWECBUEZUFWGWFDWELZWHWOWDWEDWPUFWFWQWHWOWQWHUGWFDWEMUHUIZLWODW
      EMUJGHCDWEWRITZMOZWSPKZHTZPKZQRZSZIGTZXBETZUKULZNZUMULZNZUNZEWEUDWNUCXFCO
      ZXBDOZUGZXLWMEWEXOXLXEIAUNZWMXOXKAOXLXPVTXOXKCDXGXHNZXJNZAXNXMXIXQOXKXROX
      BDXGXHURXFCXIXQXJUOUPFUQXEIXKAUSUTXPWIWJXCQRZSZXOWMXEXTIAAXRVAFCXQXJVBVCW
      SAOZWTWIXDXSWSAMVDYAXAWJXCQWSAPVEVFVGVHXOXSWLWIXOXCWKWJQXOXBDPXMXNVJVIVKV
      LVMVNVOGHIEVPWMEWEVQWAVRVSWBWC $.
      $( [23-Jul-2014] $)
  $}

  ${
    $d x y z A $.  $d d f p q x y z F $.  $d f p q x y z H $.  $d d x y z ph $.
    $d x y z B $.  $d f z D $.  $d x y z M $.  $d f p q x y z N $.  $d x y T $.
    $d d f g p q w x y z G $.  $d d f p x y R $.  $d d f g p q x y z S $.
    plydiv.pl $e |- ( ( ph /\ ( x e. S /\ y e. S ) ) -> ( x + y ) e. S ) $.
    plydiv.tm $e |- ( ( ph /\ ( x e. S /\ y e. S ) ) -> ( x x. y ) e. S ) $.
    plydiv.rc $e |- ( ( ph /\ ( x e. S /\ x =/= 0 ) ) -> ( 1 / x ) e. S ) $.
    plydiv.m1 $e |- ( ph -> -u 1 e. S ) $.
    $( Lemma for ~ plydivalg . $)
    plydivlem1 $p |- ( ph -> 0 e. S ) $=
      ( cc0 c1 caddc co ax-1cn wcel cmul wa caovclg ex mp2and syl5eqelr mulid1i
      cneg negidi mul2negi eqtri ) AIJJUBZKLZDJMUCAJDNZUFDNZUGDNZAJUFUFOLZDUKJJ
      OLJJJMMUDJMUAUEAUIUIUKDNZHHAUIUIPULABCUFUFDDDOFQRSTHAUHUIPUJABCJUFDDDKEQR
      ST $.
      $( [24-Jul-2014] $)

    plydiv.f $e |- ( ph -> F e. ( Poly ` S ) ) $.
    plydiv.g $e |- ( ph -> G e. ( Poly ` S ) ) $.
    plydiv.z $e |- ( ph -> G =/= 0p ) $.
    ${
      plydiv.r $e |- R = ( F oF - ( G oF x. q ) ) $.
      $( Lemma for ~ plydivalg . $)
      plydivlem2 $p |- ( ( ph /\ q e. ( Poly ` S ) ) -> R e. ( Poly ` S ) ) $=
        ( cv wcel co adantr cply cfv wa cmul cmin simpr caddc adantlr plymul c1
        cof cneg plysub syl5eqel ) AHQZEUAUBZRZUCZDFGUOUDUKSZUEUKSUPPURBCEFUSAF
        UPRUQMTURBCEGUOAGUPRUQNTAUQUFABQZERCQZERUCZUTVAUGSERUQIUHZAVBUTVAUDSERU
        QJUHZUIVCVDAUJULERUQLTUMUN $.
        $( [24-Jul-2014] $)

      ${
        plydiv.0 $e |- ( ph ->
                         ( F = 0p \/ ( ( deg ` F ) - ( deg ` G ) ) < 0 ) ) $.
        $( Lemma for ~ plydivex .  Base case of induction. $)
        plydivlem3 $p |- ( ph -> E. q e. ( Poly ` S )
          ( R = 0p \/ ( deg ` R ) < ( deg ` G ) ) ) $=
          ( c0p wcel cc vz cply cfv cmul cof co cmin wceq cdgr clt wbr wrex wss
          wo plybss ply0 3syl cc0 cv cvv cnex a1i wf wfn plyf ffn inidm offn wa
