$(
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
                             Polynomials
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
$)

  $c Poly $.
  $c Xp $.
  $c coeff $.
  $c deg $.

  $( Extend class notation to include the set of complex polynomials. $)
  cply $a class Poly $.

  $( Extend class notation to include the identity polynomial. $)
  cidp $a class Xp $.

  $( Extend class notation to include the coefficient function on
     polynomials. $)
  ccoe $a class coeff $.

  $( Extend class notation to include the degree function on polynomials. $)
  cdgr $a class deg $.

  ${
    $d a f k n x z $.
    $( Define the set of polynomials on the complexes with coefficients in the
       given subset. $)
    df-ply $a |- Poly = ( x e. ~P CC |-> { f | E. n e. NN0
      E. a e. ( ( x u. { 0 } ) ^m NN0 ) f =
        ( z e. CC |-> sum_ k e. ( 0 ... n ) ( ( a ` k ) x. ( z ^ k ) ) ) } ) $.

    $( Define the identity polynomial. $)
    df-idp $a |- Xp = ( _I |` CC ) $.

    $( Define the coefficient function for a polynomial. $)
    df-coe $a |- coeff = ( f e. ( Poly ` CC ) |-> ( iota_ a e. ( CC ^m NN0 )
        E. n e. NN0 ( ( a " ( ZZ>= ` ( n + 1 ) ) ) = { 0 } /\ f =
    ( z e. CC |-> sum_ k e. ( 0 ... n ) ( ( a ` k ) x. ( z ^ k ) ) ) ) ) ) $.

    $( Define the degree of a polynomial. $)
    df-dgr $a |- deg = ( f e. ( Poly ` CC ) |->
      sup ( ( `' ( coeff ` f ) " ( CC \ { 0 } ) ) , NN0 , < ) ) $.
  $}

