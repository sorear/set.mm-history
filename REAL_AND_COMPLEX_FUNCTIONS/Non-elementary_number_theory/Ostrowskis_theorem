  ${
    $d k n p y G $.  $d a g p y z J $.  $d k n K $.  $d a b k n p q x y z ph $.
    $d j k n x M $.  $d k n x y z N $.  $d a b k S $.  $d n x U $.  $d k x X $.
    $d a b g j k n p q y z F $.  $d k n x y z Q $.  $d j k n x T $.  $d k Y $.
    $d a k n p q x y z A $.  $d a b k p q x y z P $.  $d a p q y z R $.
    qrng.q $e |- Q = ( CCfld |`s QQ ) $.
    $( The base set of the field of rationals. $)
    qrngbas $p |- QQ = ( Base ` Q ) $=
      ( cq cc wss cbs cfv wceq qsscn ccnfld cnfldbas ressbas2 ax-mp ) CDECAFGHI
      CDAJBKLM $.
      $( [8-Sep-2014] $)

    $( The rationals form a division ring. $)
    qdrng $p |- Q e. DivRing $=
      ( ccnfld cq cress co cdr csubrg cfv wcel qsubdrg simpri eqeltri ) ACDEFZG
      BDCHIJNGJKLM $.
      $( [8-Sep-2014] $)

    $( The zero element of the field of rationals. $)
    qrng0 $p |- 0 = ( 0g ` Q ) $=
      ( cq ccnfld csubg cfv wcel cc0 c0g wceq csubrg cress co qsubdrg subrgsubg
      cdr simpli ax-mp cnfld0 subg0 ) CDEFGZHAIFJCDKFGZUAUBDCLMPGNQCDORCDAHBSTR
      $.
      $( [8-Sep-2014] $)

    $( The unit element of the field of rationals. $)
    qrng1 $p |- 1 = ( 1r ` Q ) $=
      ( cq ccnfld csubrg cfv wcel c1 cur wceq cress co cdr simpli cnfld1 subrg1
      qsubdrg ax-mp ) CDEFGZHAIFJSDCKLMGQNCDAHBOPR $.
      $( [8-Sep-2014] $)

    $( The additive inverse in the field of rationals. $)
    qrngneg $p |- ( X e. QQ -> ( ( invg ` Q ) ` X ) = -u X ) $=
      ( wcel ccnfld cminusg cfv cneg csubg wceq csubrg cress cdr qsubdrg simpli
      cq co subrgsubg ax-mp eqid subginv mpan cc qcn cnfldneg syl eqtr3d ) BPDZ
      BEFGZGZBAFGZGZBHZPEIGDZUHUJULJPEKGDZUNUOEPLQMDNOPERSPEAUIUKBCUITUKTUAUBUH
      BUCDUJUMJBUDBUEUFUG $.
      $( [8-Sep-2014] $)

    $( The division operation in the field of rationals. $)
    qrngdiv $p |- ( ( X e. QQ /\ Y e. QQ /\ Y =/= 0 ) ->
                    ( X ( /r ` Q ) Y ) = ( X / Y ) ) $=
      ( cq wcel cc0 wne w3a cdiv cdvr cfv ccnfld csubrg csn cdif wceq cress cdr
