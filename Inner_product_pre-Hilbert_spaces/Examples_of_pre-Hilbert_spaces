$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                     Examples of pre-Hilbert spaces
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y $.
    cnph.6 $e |- U = <. <. + , x. >. , abs >. $.
    $( The set of complex numbers is an inner product (pre-Hilbert) space.
       (Contributed by Steve Rodriguez, 28-Apr-2007; revised by nm
       24-Jul-2008.) $)
    cnph $p |- U e. CPreHil $=
      ( vx vy caddc cmul cop cabs cphl wcel cnv cv co cfv c2 cexp c1 cneg wceq
      cc wral cvv wa wb addex mulex ccj csqr axcnex df-abs fopabex2 cabl cgr
      cnaddabl ablgrp ax-mp cxp axaddopr fdmi grprn isphg mp3an eqid cnnv cmin
      mulm1 adantl opreq2d negsub eqtrd fveq2d opreq1d cre sqabsadd sqabssub
      opreq12d ppncan addcl abscl recnd sqcl syl syl2an mulcl cjcl sylan2 recl
      2cn mpan 3syl syl3ancOLD 2times eqcomd 3eqtrd rgen2a mpbir2an eqeltri ) AEFG
      HGZIBWRIJZWRKJZCLZDLZEMHNOPMZXAQRXBFMZEMZHNZOPMZEMZOXAHNZOPMZXBHNZOPMZEMZ
      FMZSZDTUACTUAZEUBJFUBJHUBJWSWTXPUCUDUEUFCDTXAXAUGNFMUHNHUICDUJUKCDUBUBUBF
      EHTETEULJEUMJUNEUOUPTTUQTEURUSUTVAVBWRWRVCVDXOCDTXATJZXBTJZUCZXHXCXAXBVEM
      ZHNZOPMZEMZXMXMEMZXNXSXGYBXCEXSXFYAOPXSXEXTHXSXEXAXBRZEMXTXSXDYEXAEXRXDYE
      SXQXBVFVGVHXAXBVIVJVKVLVHXSYCXMOXAXBUGNZFMZVMNZFMZEMZXMYIVEMZEMZYDXSXCYJY
      BYKEXAXBVNXAXBVOVPXMTJZYITJZYMYLYDSXSXMYIXMVQXJTJZXLTJZYMXQXRXJXLVRXQXITJ
      YOXQXIXAVSVTXIWAWBXRXKTJYPXRXKXBVSVTXKWAWBWCZXSYGTJZYHTJZYNXQYFTJYRXRXAYF
      WDXBWEWFYRYHYGWGVTOTJYSYNWHOYHWDWIWJYQWKVJXSYMYDXNSYQYMXNYDXMWLWMWBWNWOWP
      WQ $.
      $( [25-Jul-2008] $) $( [28-Apr-2007] $)
  $}

  ${
    elimph.1 $e |- X = ( BaseSet ` U ) $.
    elimph.5 $e |- Z = ( 0v ` U ) $.
    elimph.6 $e |- U e. CPreHil $.
    $( MAY BE REVISED - use elimnvOLD ? $)
    $( Hypothesis elimination lemma for complex inner product spaces to assist
       weak deduction theorem. $)
    elimph $p |- if ( A e. X , A , Z ) e. X $=
      ( phnvi elimnv ) ABCDEFBGHI $.
      $( [27-Apr-2007] $)
  $}

  $( MAY BE REVISED - use elimnvu ? $)
  $( Hypothesis elimination lemma for complex inner product spaces to assist
     weak deduction theorem. $)
  elimphu $p |- if ( U e. CPreHil , U , <. <. + , x. >. , abs >. )
                 e. CPreHil $=
    ( caddc cmul cop cabs cphl eqid cnph elimel ) ABCDEDZFJJGHI $.
    $( [6-May-2007] $)

