$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                     Examples of pre-Hilbert spaces
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y $.
    cnph.6 $e |- U = <. <. + , x. >. , abs >. $.
    $( The set of complex numbers is an inner product (pre-Hilbert) space.
       (Contributed by Steve Rodriguez, 28-Apr-2007; revised by nm
       15-Jan-2008.) $)
    cnph $p |- U e. CPreHil $=
      ( vx vy caddc cmul cop cabs cphl wcel cnv cv co cfv c2 cexp c1 cneg wceq
      cc wral cvv wa wb addex mulex ccj csqr axcnex df-abs fopabex2 cabl cgr
      cnaddabl ablgrp ax-mp axaddopr grprnOLD isphg mp3an eqid cnnv cmin mulm1t
      adantl opreq2d negsubt eqtrd fveq2d opreq1d cre sqabsaddt sqabssubt
      opreq12d ppncant axaddcl absclt recnd sqclt syl syl2an axmulcl cjclt
      sylan2 reclt 2cn mpan 3syl syl3anc 2timest eqcomd 3eqtrd rgen2a mpbir2an
      eqeltr ) AEFGHGZIBWPIJZWPKJZCLZDLZEMHNOPMZWSQRWTFMZEMZHNZOPMZEMZOWSHNZOPM
      ZWTHNZOPMZEMZFMZSZDTUACTUAZEUBJFUBJHUBJWQWRXNUCUDUEUFCDTWSWSUGNFMUHNHUICD
      UJUKCDUBUBUBFEHTETEULJEUMJUNEUOUPUQURUSUTWPWPVAVBXMCDTWSTJZWTTJZUCZXFXAWS
      WTVCMZHNZOPMZEMZXKXKEMZXLXQXEXTXAEXQXDXSOPXQXCXRHXQXCWSWTRZEMXRXQXBYCWSEX
      PXBYCSXOWTVDVEVFWSWTVGVHVIVJVFXQYAXKOWSWTUGNZFMZVKNZFMZEMZXKYGVCMZEMZYBXQ
      XAYHXTYIEWSWTVLWSWTVMVNXKTJZYGTJZYKYJYBSXQXKYGXKVOXHTJZXJTJZYKXOXPXHXJVPX
      OXGTJYMXOXGWSVQVRXGVSVTXPXITJYNXPXIWTVQVRXIVSVTWAZXQYETJZYFTJZYLXOYDTJYPX
      PWSYDWBWTWCWDYPYFYEWEVROTJYQYLWFOYFWBWGWHYOWIVHXQYKYBXLSYOYKXLYBXKWJWKVTW
      LWMWNWO $.
      $( [15-Jan-2008] $) $( [28-Apr-2007] $)
  $}

  ${
    elimph.1 $e |- X = ( Base ` U ) $.
    elimph.5 $e |- Z = ( 0v ` U ) $.
    elimph.6 $e |- U e. CPreHil $.
    $( MAY BE REVISED - use elimnvOLD ? $)
    $( Hypothesis elimination lemma for complex inner product spaces to assist
       weak deduction theorem. $)
    elimph $p |- if ( A e. X , A , Z ) e. X $=
      ( phnvi elimnv ) ABCDEFBGHI $.
      $( [3-Nov-2007] $) $( [27-Apr-2007] $)
  $}

  $( MAY BE REVISED - use elimnvu ? $)
  $( Hypothesis elimination lemma for complex inner product spaces to assist
     weak deduction theorem. $)
  elimphu $p |- if ( U e. CPreHil , U , <. <. + , x. >. , abs >. )
                 e. CPreHil $=
    ( caddc cmul cop cabs cphl eqid cnph elimel ) ABCDEDZFJJGHI $.
    $( [16-May-2007] $) $( [6-May-2007] $)

