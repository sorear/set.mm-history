      hvsubcl ffvelrni sylanc fopab ) EFGGEHZAIZUDBIZJKZABLKZGGAMGGBMUHUDGNZFHU
      GOPEFQOCDEFABRSUEGNUFGNUGGNUIUEUFTGGUDACUAGGUDBDUAUBUC $.
      $( [14-Nov-2000] $)

    $( Functionality of sum of Hilbert space operators. $)
    hoaddfni $p |- ( S +op T ) Fn ~H $=
      ( chil chos co wf wfn hoaddcli ffn ax-mp ) EEABFGZHMEIABCDJEEMKL $.
      $( [14-Nov-2000] $)

    $( Functionality of difference of Hilbert space operators. $)
    hosubfni $p |- ( S -op T ) Fn ~H $=
      ( chil chod co wf wfn hosubcli ffn ax-mp ) EEABFGZHMEIABCDJEEMKL $.
      $( [30-Jun-2006] $) $( [2-Jun-2006] $)

    $( Commutativity of sum of Hilbert space operators. $)
    hoaddcomi $p |- ( S +op T ) = ( T +op S ) $=
      ( vx cv chos co cfv wceq chil wral wcel cva ax-hvcom ffvelrni sylanc wf
      hosvalOLD mpanl12 3eqtr4d rgen hoaddcli hoeqi mpbi ) EFZABGHZIZUFBAGHZIZJ
