$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                     Operations on Hilbert space operators
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y A $.  $d x y S $.  $d x y T $.
    $( The sum of Hilbert space operators is an operator. $)
    hoaddclt $p |- ( ( S : H~ --> H~ /\ T : H~ --> H~ ) ->
                ( S +op T ) : H~ --> H~ ) $=
      ( vx vy chil wf wa chos co cv wcel cfv cva wceq copab wral hvaddclt
      ffvelrn syl2an anandirs r19.21aiva eqid fopab2 sylib hosmvalt feq1d
      mpbird ) EEAFZEEBFZGZEEABHIZFEECJZEKZDJULALZULBLZMIZNGCDOZFZUJUPEKZCEPURU
      JUSCEUHUIUMUSUNEKUOEKUSUHUMGUIUMGUNUOQEEULAREEULBRSTUACDEEUPUQUQUBUCUDUJE
      EUKUQCDABUEUFUG $.
      $( [22-Feb-2006] $) $( [21-Feb-2006] $)

    $( The scalar product of a Hilbert space operator is an operator. $)
    homulclt $p |- ( ( A e. CC /\ T : H~ --> H~ ) ->
                   ( A .op T ) : H~ --> H~ ) $=
      ( vx vy cc wcel chil wf wa chot co cv cfv csm wceq copab wral hvmulclt
      ffvelrn sylan2 anassrs r19.21aiva eqid fopab2 sylib hommvalt feq1d
      mpbird ) AEFZGGBHZIZGGABJKZHGGCLZGFZDLAUMBMZNKZOICDPZHZUKUPGFZCGQURUKUSCG
      UIUJUNUSUIUOGFUSUJUNIAUORGGUMBSTUAUBCDGGUPUQUQUCUDUEUKGGULUQCDABUFUGUH $.
      $( [22-Mar-2006] $) $( [21-Feb-2006] $)
  $}

  ${
    $d x T $.  $d x U $.
    $( Equality of Hilbert space operators. $)
    hoeqt $p |- ( ( T : H~ --> H~ /\ U : H~ --> H~ ) ->
                ( A. x e. H~ ( T ` x ) = ( U ` x ) <-> T = U ) ) $=
      ( chil wfn cv cfv wceq wral wb wf wa eqfnfv eqid biantrur syl6rbbr ffn
      syl2an ) BDEZCDEZAFZBGUACGHADIZBCHZJDDBKDDCKSTLUCDDHZUBLUBADDBCMUDUBDNOPD
      DBQDDCQR $.
      $( [13-Aug-2006] $) $( [12-Aug-2006] $)
  $}

  ${
    $d x y S $.  $d x y T $.
    hoeq.1 $e |- S : H~ --> H~ $.
    hoeq.2 $e |- T : H~ --> H~ $.
    $( Equality of Hilbert space operators. $)
    hoeq $p |- ( A. x e. H~ ( S ` x ) = ( T ` x ) <-> S = T ) $=
      ( chil wf cv cfv wceq wral wb hoeqt mp2an ) FFBGFFCGAHZBIOCIJAFKBCJLDEABC
      MN $.
      $( [14-Nov-2000] $)

    $( Closure of Hilbert space operator sum. $)
    hoscl $p |- ( A e. H~ -> ( ( S +op T ) ` A ) e. H~ ) $=
      ( chil wf wcel chos co cfv hosclt mpanl12 ) FFBGFFCGAFHABCIJKFHDEABCLM $.
      $( [12-Nov-2000] $)

    $( Closure of Hilbert space operator difference. $)
    hodcl $p |- ( A e. H~ -> ( ( S -op T ) ` A ) e. H~ ) $=
      ( chil wf wcel chod co cfv hodclt mpanl12 ) FFBGFFCGAFHABCIJKFHDEABCLM $.
      $( [12-Nov-2000] $)

    $( Composition of Hilbert space operators. $)
    hoco $p |- ( A e. H~ -> ( ( S o. T ) ` A ) = ( S ` ( T ` A ) ) ) $=
      ( chil wcel cdm ccom cfv wceq wf fdm ax-mp eleq2i wfun ffun fvco mp3an12
      sylbir ) AFGACHZGZABCIJACJBJKZUAFAFFCLZUAFKEFFCMNOBPZCPZUBUCFFBLUEDFFBQNU
      DUFEFFCQNABCRST $.
      $( [12-Nov-2000] $)

    $( Closure of composition of Hilbert space operators. $)
    hococl $p |- ( A e. H~ -> ( ( S o. T ) ` A ) e. H~ ) $=
      ( chil wcel ccom cfv hoco ffvelrni syl eqeltrd ) AFGZABCHIACIZBIZFABCDEJN
      OFGPFGFFACEKFFOBDKLM $.
      $( [12-Nov-2000] $)

    $( Mapping of composition of Hilbert space operators. $)
    hocof $p |- ( S o. T ) : H~ --> H~ $=
      ( chil wf ccom fco mp2an ) EEAFEEBFEEABGFCDEEEABHI $.
      $( [14-Nov-2000] $)

    $( Functionality of composition of Hilbert space operators. $)
