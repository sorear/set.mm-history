    $( Commutative/associative law for Hilbert space operator sum that swaps
       the first two terms. $)
    hoadd12i $p |- ( R +op ( S +op T ) ) = ( S +op ( R +op T ) ) $=
      ( chos co hoaddcomi opreq1i hoaddassi 3eqtr3i ) ABGHZCGHBAGHZCGHABCGHGHBA
      CGHGHMNCGABDEIJABCDEFKBACEDFKL $.
      $( [21-Sep-2004] $) $( [27-Aug-2004] $)

    $( Commutative/associative law for Hilbert space operator sum that swaps
       the second and third terms. $)
    hoadd23i $p |- ( ( R +op S ) +op T ) = ( ( R +op T ) +op S ) $=
      ( chos co hoaddcomi opreq2i hoaddassi 3eqtr4i ) ABCGHZGHACBGHZGHABGHCGHAC
      GHBGHMNAGBCEFIJABCDEFKACBDFEKL $.
      $( [28-Jul-2006] $) $( [27-Jul-2006] $)

    $( Distributive law for Hilbert space operator sum. $)
    hocadddiri $p |- ( ( R +op S ) o. T ) = ( ( R o. T ) +op ( S o. T ) ) $=
