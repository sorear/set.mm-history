    CUAUCBUBTELKTUBCCAGMNCCBGMNOP $.
    $( [25-Aug-2006] $) $( [23-Aug-2006] $)

  $( Commutativity of sum of Hilbert space operators. $)
  hoaddcomt $p |- ( ( S : H~ --> H~ /\ T : H~ --> H~ ) ->
                ( S +op T ) = ( T +op S ) ) $=
    ( chil wf chos co wceq ch0o cif opreq1 opreq2 eqeq12d ho0f elimf hoaddcom
