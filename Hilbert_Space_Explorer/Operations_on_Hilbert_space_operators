    hoadd23 $p |- ( ( R +op S ) +op T ) = ( ( R +op T ) +op S ) $=
      ( chos co hoaddcom opreq2i hoaddass 3eqtr4 ) ABCGHZGHACBGHZGHABGHCGHACGHB
      GHMNAGBCEFIJABCDEFKACBDFEKL $.
      $( [28-Jul-2006] $) $( [27-Jul-2006] $)

