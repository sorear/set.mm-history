    $( The modular pair property expressed in terms of the dual modular pair
       property. $)
    mddmd $p |- ( ( A e. CH /\ B e. CH ) -> ( A MH B <->
                   ( _|_ ` A ) MH* ( _|_ ` B ) ) ) $=
      ( cch wcel wa cort cfv cdmd wbr cmd wb dmdmd choccl syl2an ococ
      breqan12d bitr2d ) ACDZBCDZEAFGZBFGZHIZTFGZUAFGZJIZABJITCDUACDUBUEKRSTUAL
      AMBMNRSUCAUDBJAOBOPQ $.
      $( [27-Apr-2006] $)

    $( Consequence of the dual modular pair property. $)
    dmdi $p |- ( ( ( A e. CH /\ B e. CH /\ C e. CH ) /\ ( A MH* B /\
              B C_ C ) ) -> ( ( C i^i A ) vH B ) = ( C i^i ( A vH B ) ) ) $=
      ( vx cch wcel w3a cdmd wbr wss cin chj co wceq wi wa cv wral dmdbr
      biimpd sseq2 ineq1 opreq1d eqeq12d imbi12d rcla4v sylan9 3impa imp32 ) AE
      FZBEFZCEFZGABHIZBCJZCAKZBLMZCABLMZKZNZUJUKULUMUNUSOZOUJUKPZUMBDQZJZVBAKZB
