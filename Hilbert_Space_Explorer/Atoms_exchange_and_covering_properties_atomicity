    $( [21-Jun-2004] $)

  $( The meet of a Hilbert lattice element and an incomparable atom is the zero
     subspace. $)
  atnssm0 $p |- ( ( A e. CH /\ B e. Atoms ) ->
             ( -. B C_ A <-> ( A i^i B ) = 0H ) ) $=
    ( cch wcel cat wa wss wn chj co ccv wbr cin c0h wceq chcv1 cvp bitr4d ) ACD
    BEDFBAGHAABIJKLABMNOABPABQR $.
    $( [30-Jun-2004] $)

  $( The meet of distinct atoms is the zero subspace. $)
  atnem0 $p |- ( ( A e. Atoms /\ B e. Atoms ) ->
             ( A =/= B <-> ( A i^i B ) = 0H ) ) $=
    ( cat wcel wa wss wn wne cin c0h wceq wb atsseq eqcom syl6bb ancoms
    necon3bbid cch atnssm0 atelch sylan bitr3d ) ACDZBCDZEZBAFZGZABHABIJKZUEUFA
    BUDUCUFABKZLUDUCEUFBAKUIBAMBANOPQARDUDUGUHLUCABSATUAUB $.
    $( [25-Sep-2009] $) $( [25-Jun-2004] $)

  $( The meet with an atom's superset is the atom. $)
  atssma $p |- ( ( A e. Atoms /\ B e. CH ) ->
            ( A C_ B <-> ( A i^i B ) e. Atoms ) ) $=
    ( cat wcel cch wa wss cin wi wceq df-ss biimpi eleq1d biimprcd adantr c0h
    wn wb atnssm0 ancoms biimpd con1d incom eleq1i wne atn0 df-ne sylib sylbi
    syl5 impbid ) ACDZBEDZFZABGZABHZCDZULUOUQIUMUOUQULUOUPACUOUPAJABKLMNOUNBAHZ
