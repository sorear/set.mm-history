  $( Associativity of sum and difference of Hilbert space vectors. $)
  hvaddsubass $p |- ( ( A e. ~H /\ B e. ~H /\ C e. ~H ) -> ( ( A +h B ) -h C )
                     = ( A +h ( B -h C ) ) ) $=
    ( chil wcel w3a cva co c1 cneg csm cmv wceq cc ax-1cn hvmulcl mpan ax-hvass
    negcli hvsubval syl3an3 wa hvaddcl sylan 3impa 3adant1 opreq2d 3eqtr4d ) AD
    EZBDEZCDEZFZABGHZIJZCKHZGHZABUOGHZGHZUMCLHZABCLHZGHUKUIUJUODEZUPURMUNNEUKVA
    IOSUNCPQABUORUAUIUJUKUSUPMZUIUJUBUMDEUKVBABUCUMCTUDUEULUTUQAGUJUKUTUQMUIBCT
    UFUGUH $.
    $( [27-Aug-2004] $)

  $( Subtraction and addition of equal Hilbert space vectors.. $)
  hvpncan3 $p |- ( ( A e. ~H /\ B e. ~H ) -> ( A +h ( B -h A ) ) = B ) $=
    ( chil wcel wa cva co cmv wceq hvaddsubass 3anidm13 hvpncan2 eqtr3d ) ACDZB
    CDZEABFGAHGZABAHGFGZBNOPQIABAJKABLM $.
    $( [27-Aug-2004] $)

  $( Scalar multiplication commutative law. $)
  hvmulcom $p |- ( ( A e. CC /\ B e. CC /\ C e. ~H ) ->
                 ( A .h ( B .h C ) ) = ( B .h ( A .h C ) ) ) $=
    ( cc wcel chil w3a cmul co csm wa mulcom opreq1d 3adant3 ax-hvmulass 3com12
    wceq 3eqtr3d ) ADEZBDEZCFEZGABHIZCJIZBAHIZCJIZABCJIJIBACJIJIZSTUCUEQUASTKUB
    UDCJABLMNABCOTSUAUEUFQBACOPR $.
