    ax-hvmulid 3eqtr3d hvmul0t syl ex df-ne syl5ibr orrd wi opreq1 eqeq1d
    ax-hvmul0 syl5cbir adantl adantr jaod impbid ) ACDZBEDZFZABGHZIJZAKJZBIJZLZ
    VGVIVLVGVIFZVJVKVMAKMZVKVJNVMVNVKVMVNFOAPHZVHGHZVOIGHZBIVIVPVQJVGVNVHIVOGQR
    VGVNVPBJVIVGVNFZVOASHZBGHZOBGHZVPBVEVNVTWAJVFVEVNFZVSOBGATUAUBVOCDZVEVFVTVP
    JVRVOABUCVEVNWCVFAUDZUBVEVFVNUEVEVFVNUFUGVFWABJVEVNBUHRUIUBVGVNVQIJZVIVEVNW
    EVFWBWCWEWDVOUJUKUBUBUIULAKUMUNUOULVGVJVIVKVFVJVIUPVEVJVIKBGHZIJVFVJVHWFIAK
    BGUQURBUSUTVAVEVKVIUPVFVKVIAIGHZIJVEVKVHWGIBIAGQURAUJUTVBVCVD $.
    $( [21-May-2005] $) $( [19-May-2005] $)

  $( Subtraction of a vector from itself. $)
  hvsubidt $p |- ( A e. H~ -> ( A -h A ) = 0h ) $=
    ( chil wcel cmv co cc0 csm c0v c1 cneg caddc cva ax-hvmulid opreq1d cc
    wceq ax1cn negcl ax-hvdistr2 mp3an12 hvsubvalt anidms 3eqtr4rd negid
    opreq1i syl6eq ax-hvmul0 eqtrd ) ABCZAADEZFAGEZHUIUJIIJZKEZAGEZUKUIIAGEZULA
    GEZLEZAUPLEZUNUJUIUOAUPLAMNIOCULOCUIUNUQPQIQRIULASTUIUJURPAAUAUBUCUMFAGIQUD
    UEUFAUGUH $.
    $( [30-May-1999] $)

  $( Addition of negative of a vector to itself. $)
  hvnegidt $p |- ( A e. H~ -> ( A +h ( -u 1 .h A ) ) = 0h ) $=
    ( chil wcel cmv co c1 cneg csm cva c0v wceq hvsubvalt anidms hvsubidt
    eqtr3d ) ABCZAADEZAFGAHEIEZJPQRKAALMANO $.
    $( [3-Jun-2004] $) $( [4-Jun-2004] $)

  $( Two ways to express the negative of a vector. $)
  hv2negt $p |- ( A e. H~ -> ( 0h -h A ) = ( -u 1 .h A ) ) $=
    ( chil wcel c0v cmv co c1 cneg csm cva wceq ax-hv0cl hvsubvalt mpan cc
    ax1cn negcl hvmulclt hvaddid2t syl eqtrd ) ABCZDAEFZDGHZAIFZJFZUEDBCUBUCUFK
    LDAMNUBUEBCZUFUEKUDOCUBUGGPQUDARNUESTUA $.
    $( [24-May-2005] $) $( [23-May-2005] $)

  ${
    hvaddid2.1 $e |- A e. H~ $.
    $( Addition with the zero vector. $)
    hvaddid2 $p |- ( 0h +h A ) = A $=
      ( chil wcel c0v cva co wceq hvaddid2t ax-mp ) ACDEAFGAHBAIJ $.
      $( [18-Aug-1999] $)

    $( Addition of negative of a vector to itself. $)
