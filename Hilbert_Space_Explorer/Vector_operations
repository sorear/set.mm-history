$)

  $( Closure of vector addition. $)
  hvaddclt $p |- ( ( A e. H~ /\ B e. H~ ) -> ( A +h B ) e. H~ ) $=
    ( chil cva ax-hfvadd foprcl ) ABCCCDEF $.
    $( [18-Apr-2007] $) $( [18-Apr-2007] $)

  $( Closure of scalar multiplication. $)
  hvmulclt $p |- ( ( A e. CC /\ B e. H~ ) -> ( A .h B ) e. H~ ) $=
    ( chil cc csm ax-hfvmul foprcl ) ABCDCEFG $.
    $( [19-Apr-2007] $) $( [19-Apr-2007] $)

  ${
    hvmulcl.1 $e |- A e. CC $.
    hvmulcl.2 $e |- B e. H~ $.
    $( Closure inference for scalar multiplication. $)
    hvmulcl $p |- ( A .h B ) e. H~ $=
      ( cc wcel chil csm co hvmulclt mp2an ) AEFBGFABHIGFCDABJK $.
      $( [1-Aug-1999] $)
  $}

  ${
    $d x y z $.
    $( Define vector subtraction.  See ~ hvsubval for its value and
       ~ hvsubcl for its closure. $)
    df-hvsub $a |- -h = { <. <. x , y >. , z >. | ( ( x e. H~ /\ y e. H~ )
                   /\ z = ( x +h ( -u 1 .h y ) ) ) } $.

    $( Mapping domain and codomain of vector subtraction. $)
    hvsubopr $p |- -h : ( H~ X. H~ ) --> H~ $=
      ( vx vy vz chil cv c1 cneg csm co cva cmv df-hvsub wcel hvaddclt cc
      ax1cn negcl hvmulclt mpan sylan2 foprab ) ABCDDAEZFGZBEZHIZJIZDKABCLUBDMU
      EDMZUFDMUDDMZUBUENUCOMUHUGFPQUCUDRSTUA $.
      $( [19-Sep-2007] $) $( [6-Sep-2007] $)
  $}

  ${
    $d x y z A $.  $d x y z B $.
    $( Value of vector subtraction. $)
    hvsubvalt $p |- ( ( A e. H~ /\ B e. H~ ) ->
                    ( A -h B ) = ( A +h ( -u 1 .h B ) ) ) $=
      ( vx vy vz chil cv c1 cneg csm co cva cmv oprex opreq1 wceq opreq2
      opreq2d df-hvsub oprabval2 ) CDEABFFCGZHIZDGZJKZLKAUBBJKZLKMAUDLKAUELNUAA
      UDLOUCBPUDUEALUCBUBJQRCDEST $.
      $( [5-Sep-1999] $)
  $}

  $( Closure of vector subtraction. $)
  hvsubclt $p |- ( ( A e. H~ /\ B e. H~ ) -> ( A -h B ) e. H~ ) $=
    ( chil wcel wa cmv co c1 cneg csm cva hvsubvalt hvaddclt cc ax1cn negcl
    hvmulclt mpan sylan2 eqeltrd ) ACDZBCDZEABFGAHIZBJGZKGZCABLUAUDCDZUECDUBAUD
    MUCNDUBUFHOPUCBQRST $.
    $( [17-Aug-1999] $)

  ${
    hvaddcl.1 $e |- A e. H~ $.
    hvaddcl.2 $e |- B e. H~ $.
    $( Closure of vector addition. $)
    hvaddcl $p |- ( A +h B ) e. H~ $=
      ( chil wcel cva co hvaddclt mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [1-Aug-1999] $)

    $( Commutation of vector addition. $)
    hvcom $p |- ( A +h B ) = ( B +h A ) $=
      ( chil wcel cva co wceq ax-hvcom mp2an ) AEFBEFABGHBAGHICDABJK $.
      $( [3-Sep-1999] $)

    $( Value of vector subtraction definition. $)
    hvsubval $p |- ( A -h B ) = ( A +h ( -u 1 .h B ) ) $=
      ( chil wcel cmv co c1 cneg csm cva wceq hvsubvalt mp2an ) AEFBEFABGHAIJBK
      HLHMCDABNO $.
      $( [3-Sep-1999] $)

    $( Closure of vector subtraction. $)
    hvsubcl $p |- ( A -h B ) e. H~ $=
      ( chil wcel cmv co hvsubclt mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [2-Aug-1999] $)
  $}

  $( Addition with the zero vector. $)
  hvaddid2t $p |- ( A e. H~ -> ( 0h +h A ) = A ) $=
    ( chil wcel c0v cva co wceq ax-hv0cl ax-hvcom mpan2 ax-hvaddid eqtr3d ) ABC
    ZADEFZDAEFZAMDBCNOGHADIJAKL $.
    $( [18-Oct-1999] $)

  $( Scalar multiplication with the zero vector. $)
  hvmul0t $p |- ( A e. CC -> ( A .h 0h ) = 0h ) $=
    ( cc wcel c0v cc0 csm co cmul mul01t opreq1d chil wceq ax-hv0cl ax-hvmul0
    ax-mp syl6eq 0cn ax-hvmulass mp3an23 eqtr3d opreq2i syl6req ) ABCZDAEDFGZFG
    ZADFGUCAEHGZDFGZDUEUCUGUDDUCUFEDFAIJDKCZUDDLMDNOZPUCEBCUHUGUELQMAEDRSTUDDAF
    UIUAUB $.
    $( [30-May-1999] $)

  $( If a scalar product is zero, one of its factors must be zero. $)
  hvmul0ort $p |- ( ( A e. CC /\ B e. H~ ) ->
                  ( ( A .h B ) = 0h <-> ( A = 0 \/ B = 0h ) ) ) $=
    ( cc wcel chil wa csm co c0v wceq cc0 wo wne wn c1 cdiv opreq2 ad2antlr
