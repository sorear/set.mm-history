  $( Convert minus one times a scalar product to the negative of the scalar. $)
  hvm1neg $p |- ( ( A e. CC /\ B e. ~H ) ->
                 ( -u 1 .h ( A .h B ) ) = ( -u A .h B ) ) $=
    ( cc wcel chil wa c1 cneg cmul co csm wceq ax1cn negcli ax-hvmulass mp3an1
    mulm1 adantr opreq1d eqtr3d ) ACDZBEDZFZGHZAIJZBKJZUDABKJKJZAHZBKJUDCDUAUBU
    FUGLGMNUDABOPUCUEUHBKUAUEUHLUBAQRST $.
