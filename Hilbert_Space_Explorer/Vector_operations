$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                      Vector operations
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( The Hilbert space scalar product operation is a set. $)
  hvmulex $p |- .h e. _V $=
    ( cc chil cxp csm wf cvv wcel ax-hfvmul cnex ax-hilex xpex fex mp2an ) ABCZ
    BDENFGDFGHABIJKNBFDLM $.
    $( [17-Apr-2007] $)

  $( Closure of vector addition. $)
  hvaddcl $p |- ( ( A e. ~H /\ B e. ~H ) -> ( A +h B ) e. ~H ) $=
    ( chil cva ax-hfvadd foprcl ) ABCCCDEF $.
    $( [18-Apr-2007] $)

  $( Closure of scalar multiplication. $)
  hvmulcl $p |- ( ( A e. CC /\ B e. ~H ) -> ( A .h B ) e. ~H ) $=
    ( chil cc csm ax-hfvmul foprcl ) ABCDCEFG $.
    $( [19-Apr-2007] $)

  ${
    hvmulcl.1 $e |- A e. CC $.
    hvmulcl.2 $e |- B e. ~H $.
    $( Closure inference for scalar multiplication. $)
    hvmulcli $p |- ( A .h B ) e. ~H $=
      ( cc wcel chil csm co hvmulcl mp2an ) AEFBGFABHIGFCDABJK $.
      $( [1-Aug-1999] $)
  $}

  ${
    $d x y $.
    $( Mapping domain and codomain of vector subtraction. $)
    hvsubopr $p |- -h : ( ~H X. ~H ) --> ~H $=
      ( vx vy cv c1 cneg csm co cva chil wcel wral cxp cmv wf cc ax-1cn hvmulcl
      negcli mpan hvaddcl sylan2 rgen2a df-hvsub fmpt2 mpbi ) ACZDEZBCZFGZHGZIJ
      ZBIKAIKIILIMNUKABIUHIJZUFIJUIIJZUKUGOJULUMDPRUGUHQSUFUITUAUBABIIUJIMABUCU
      DUE $.
      $( [6-Sep-2007] $)
  $}

  ${
    $d x y A $.  $d y B $.
    $( Value of vector subtraction.  (Revised by Mario Carneiro,
       23-Dec-2013.) $)
    hvsubval $p |- ( ( A e. ~H /\ B e. ~H ) ->
                    ( A -h B ) = ( A +h ( -u 1 .h B ) ) ) $=
      ( vx vy chil cv c1 cneg csm co cva cmv opreq1 wceq opreq2d df-hvsub oprex
      opreq2 ovmpt2 ) CDABEECFZGHZDFZIJZKJAUABIJZKJLAUCKJTAUCKMUBBNUCUDAKUBBUAI
      ROCDPAUDKQS $.
      $( [23-Dec-2013] $) $( [5-Sep-1999] $)
  $}

  $( Closure of vector subtraction. $)
  hvsubcl $p |- ( ( A e. ~H /\ B e. ~H ) -> ( A -h B ) e. ~H ) $=
    ( chil wcel wa cmv co c1 cneg csm hvsubval cc ax-1cn negcli hvmulcl hvaddcl
    cva mpan sylan2 eqeltrd ) ACDZBCDZEABFGAHIZBJGZQGZCABKUBUAUDCDZUECDUCLDUBUF
    HMNUCBORAUDPST $.
    $( [17-Aug-1999] $)

  ${
    hvaddcl.1 $e |- A e. ~H $.
    hvaddcl.2 $e |- B e. ~H $.
    $( Closure of vector addition. $)
    hvaddcli $p |- ( A +h B ) e. ~H $=
      ( chil wcel cva co hvaddcl mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [1-Aug-1999] $)

    $( Commutation of vector addition. $)
    hvcomi $p |- ( A +h B ) = ( B +h A ) $=
      ( chil wcel cva co wceq ax-hvcom mp2an ) AEFBEFABGHBAGHICDABJK $.
      $( [3-Sep-1999] $)

    $( Value of vector subtraction definition. $)
    hvsubvali $p |- ( A -h B ) = ( A +h ( -u 1 .h B ) ) $=
      ( chil wcel cmv co c1 cneg csm cva wceq hvsubval mp2an ) AEFBEFABGHAIJBKH
      LHMCDABNO $.
      $( [3-Sep-1999] $)

    $( Closure of vector subtraction. $)
    hvsubcli $p |- ( A -h B ) e. ~H $=
      ( chil wcel cmv co hvsubcl mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [2-Aug-1999] $)
  $}

  $( Addition with the zero vector. $)
  hvaddid2 $p |- ( A e. ~H -> ( 0h +h A ) = A ) $=
    ( chil wcel c0v cva co wceq ax-hv0cl ax-hvcom mpan2 ax-hvaddid eqtr3d ) ABC
    ZADEFZDAEFZAMDBCNOGHADIJAKL $.
    $( [18-Oct-1999] $)

  $( Scalar multiplication with the zero vector. $)
  hvmul0 $p |- ( A e. CC -> ( A .h 0h ) = 0h ) $=
    ( cc wcel c0v cc0 csm cmul mul01 opreq1d chil wceq ax-hv0cl ax-hvmul0 ax-mp
    co syl6eq 0cn ax-hvmulass mp3an23 eqtr3d opreq2i syl6req ) ABCZDAEDFOZFOZAD
    FOUCAEGOZDFOZDUEUCUGUDDUCUFEDFAHIDJCZUDDKLDMNZPUCEBCUHUGUEKQLAEDRSTUDDAFUIU
    AUB $.
    $( [30-May-1999] $)

  $( If a scalar product is zero, one of its factors must be zero. $)
  hvmul0or $p |- ( ( A e. CC /\ B e. ~H ) ->
                  ( ( A .h B ) = 0h <-> ( A = 0 \/ B = 0h ) ) ) $=
    ( cc wcel wa csm co c0v cc0 c1 opreq2 ad2antlr adantlr 3eqtr3d hvmul0 ex wi
    wceq eqeq1d syl5ibrcom chil wo wn wne df-ne cdiv cmul recid2 opreq1d simpll
    reccl simplr ax-hvmulass syl3anc ax-hvmulid syl5bir ax-hvmul0 opreq1 adantl
    syl orrd adantr jaod impbid ) ACDZBUADZEZABFGZHRZAIRZBHRZUBZVGVIVLVGVIEZVJV
    KVJUCAIUDZVMVKAIUEVMVNVKVMVNEJAUFGZVHFGZVOHFGZBHVIVPVQRVGVNVHHVOFKLVGVNVPBR
    VIVGVNEZVOAUGGZBFGZJBFGZVPBVEVNVTWARVFVEVNEZVSJBFAUHUIMVRVOCDZVEVFVTVPRVEVN
    WCVFAUKZMVEVFVNUJVEVFVNULVOABUMUNVFWABRVEVNBUOLNMVGVNVQHRZVIVEVNWEVFWBWCWEW
    DVOOUTMMNPUPVAPVGVJVIVKVFVJVIQVEVFVIVJIBFGZHRBUQVJVHWFHAIBFURSTUSVEVKVIQVFV
    EVIVKAHFGZHRAOVKVHWGHBHAFKSTVBVCVD $.
    $( [19-May-2005] $)

  $( Subtraction of a vector from itself. $)
  hvsubid $p |- ( A e. ~H -> ( A -h A ) = 0h ) $=
    ( chil wcel cmv co cc0 csm c0v c1 cneg caddc cva ax-hvmulid opreq1d cc wceq
    ax-1cn negcli ax-hvdistr2 mp3an12 hvsubval anidms 3eqtr4rd negidi ax-hvmul0
    opreq1i syl6eq eqtrd ) ABCZAADEZFAGEZHUIUJIIJZKEZAGEZUKUIIAGEZULAGEZLEZAUPL
    EZUNUJUIUOAUPLAMNIOCULOCUIUNUQPQIQRIULASTUIUJURPAAUAUBUCUMFAGIQUDUFUGAUEUH
    $.
    $( [30-May-1999] $)

  $( Addition of negative of a vector to itself. $)
  hvnegid $p |- ( A e. ~H -> ( A +h ( -u 1 .h A ) ) = 0h ) $=
    ( chil wcel cmv co c1 cneg csm cva c0v wceq hvsubval anidms hvsubid eqtr3d
    ) ABCZAADEZAFGAHEIEZJPQRKAALMANO $.
    $( [4-Jun-2004] $)

  $( Two ways to express the negative of a vector. $)
  hv2neg $p |- ( A e. ~H -> ( 0h -h A ) = ( -u 1 .h A ) ) $=
    ( chil wcel c0v cmv co c1 cneg csm cva wceq ax-hv0cl hvsubval ax-1cn negcli
    mpan cc hvmulcl hvaddid2 syl eqtrd ) ABCZDAEFZDGHZAIFZJFZUEDBCUBUCUFKLDAMPU
    BUEBCZUFUEKUDQCUBUGGNOUDARPUESTUA $.
    $( [23-May-2005] $)

  ${
    hvaddid2.1 $e |- A e. ~H $.
    $( Addition with the zero vector. $)
    hvaddid2i $p |- ( 0h +h A ) = A $=
      ( chil wcel c0v cva co wceq hvaddid2 ax-mp ) ACDEAFGAHBAIJ $.
      $( [18-Aug-1999] $)

    $( Addition of negative of a vector to itself. $)
    hvnegidi $p |- ( A +h ( -u 1 .h A ) ) = 0h $=
      ( chil wcel c1 cneg csm co cva c0v wceq hvnegid ax-mp ) ACDAEFAGHIHJKBALM
      $.
      $( [18-Aug-1999] $)

    $( Two ways to express the negative of a vector. $)
    hv2negi $p |- ( 0h -h A ) = ( -u 1 .h A ) $=
      ( chil wcel c0v cmv co c1 cneg csm wceq hv2neg ax-mp ) ACDEAFGHIAJGKBALM
      $.
      $( [31-Oct-1999] $)
  $}

  $( Convert minus one times a scalar product to the negative of the scalar. $)
  hvm1neg $p |- ( ( A e. CC /\ B e. ~H ) ->
                 ( -u 1 .h ( A .h B ) ) = ( -u A .h B ) ) $=
    ( cc wcel chil wa c1 cneg cmul co csm wceq ax-1cn negcli ax-hvmulass mp3an1
    mulm1 adantr opreq1d eqtr3d ) ACDZBEDZFZGHZAIJZBKJZUDABKJKJZAHZBKJUDCDUAUBU
    FUGLGMNUDABOPUCUEUHBKUAUEUHLUBAQRST $.
    $( [4-Jun-2004] $)

  $( Value of vector addition in terms of vector subtraction. $)
  hvaddsubval $p |- ( ( A e. ~H /\ B e. ~H ) ->
                  ( A +h B ) = ( A -h ( -u 1 .h B ) ) ) $=
    ( chil wcel wa c1 cneg csm cmv cva wceq ax-1cn negcli hvmulcl mpan hvsubval
    co cc sylan2 hvm1neg negnegi opreq1i syl6eq ax-hvmulid eqtrd adantl opreq2d
    eqtr2d ) ACDZBCDZEZAFGZBHQZIQZAULUMHQZJQZABJQUJUIUMCDZUNUPKULRDZUJUQFLMZULB
    NOAUMPSUKUOBAJUJUOBKUIUJUOFBHQZBUJUOULGZBHQZUTURUJUOVBKUSULBTOVAFBHFLUAUBUC
    BUDUEUFUGUH $.
    $( [10-Jun-2006] $)

  $( Commutative/associative law. $)
  hvadd32 $p |- ( ( A e. ~H /\ B e. ~H /\ C e. ~H ) ->
               ( ( A +h B ) +h C ) = ( ( A +h C ) +h B ) ) $=
    ( chil wcel w3a co wceq wa ax-hvcom opreq2d 3adant1 ax-hvass 3com23 3eqtr4d
    cva ) ADEZBDEZCDEZFABCPGZPGZACBPGZPGZABPGCPGACPGBPGZRSUAUCHQRSITUBAPBCJKLAB
    CMQSRUDUCHACBMNO $.
    $( [16-Oct-1999] $)

  $( Commutative/associative law. $)
  hvadd12 $p |- ( ( A e. ~H /\ B e. ~H /\ C e. ~H ) ->
               ( A +h ( B +h C ) ) = ( B +h ( A +h C ) ) ) $=
    ( chil wcel w3a co wceq wa ax-hvcom opreq1d 3adant3 ax-hvass 3com12 3eqtr3d
    cva ) ADEZBDEZCDEZFABPGZCPGZBAPGZCPGZABCPGPGBACPGPGZQRUAUCHSQRITUBCPABJKLAB
    CMRQSUCUDHBACMNO $.
    $( [19-Oct-1999] $)

  $( Hilbert vector space addition law. $)
  hvadd4 $p |- ( ( ( A e. ~H /\ B e. ~H ) /\ ( C e. ~H /\ D e. ~H ) ) ->
               ( ( A +h B ) +h ( C +h D ) ) = ( ( A +h C ) +h ( B +h D ) ) ) $=
    ( chil wcel wa cva co wceq w3a hvadd32 opreq1d 3expa adantrr ax-hvass 3expb
    hvaddcl sylan an4s 3eqtr3d ) AEFZBEFZGZCEFZDEFZGZGABHIZCHIZDHIZACHIZBHIZDHI
    ZUHCDHIHIZUKBDHIHIZUDUEUJUMJZUFUBUCUEUPUBUCUEKUIULDHABCLMNOUDUHEFZUGUJUNJZA
    BRUQUEUFURUHCDPQSUBUEUCUFUMUOJZUBUEGUKEFZUCUFGUSACRUTUCUFUSUKBDPQSTUA $.
    $( [16-Oct-1999] $)

  $( Hilbert vector space addition/subtraction law. $)
  hvsub4 $p |- ( ( ( A e. ~H /\ B e. ~H ) /\ ( C e. ~H /\ D e. ~H ) ) ->
               ( ( A +h B ) -h ( C +h D ) ) = ( ( A -h C ) +h ( B -h D ) ) ) $=
    ( chil wcel wa cva co c1 cneg csm cmv wceq cc hvmulcl mpan hvaddcl hvsubval
    anim2i ax-1cn negcli ax-hvdistr1 mp3an1 adantl opreq2d hvadd4 eqtr4d syl2an
    anim12i an4s syl ad2ant2r ad2ant2l opreq12d 3eqtr4d ) AEFZBEFZGZCEFZDEFZGZG
    ZABHIZJKZCDHIZLIZHIZAVECLIZHIZBVEDLIZHIZHIZVDVFMIZACMIZBDMIZHIVCVHVDVIVKHIZ
    HIZVMVCVGVQVDHVBVGVQNZUSVEOFZUTVAVSJUAUBZVECDUCUDUEUFVCUQVIEFZGZURVKEFZGZGZ
    VMVRNUQUTURVAWFUQUTGWCURVAGWEUTWBUQVTUTWBWAVECPQTVAWDURVTVAWDWAVEDPQTUJUKAV
    IBVKUGULUHUSVDEFVFEFVNVHNVBABRCDRVDVFSUIVCVOVJVPVLHUQUTVOVJNURVAACSUMURVAVP
    VLNUQUTBDSUNUOUP $.
    $( [17-Oct-1999] $)

  $( Commutative/associative law. $)
  hvaddsub12 $p |- ( ( A e. ~H /\ B e. ~H /\ C e. ~H ) ->
               ( A +h ( B -h C ) ) = ( B +h ( A -h C ) ) ) $=
