    opreq1i syl6eq ax-hvmul0 eqtrd ) ABCZAADEZFAGEZHUIUJIIJZKEZAGEZUKUIIAGEZULA
    GEZLEZAUPLEZUNUJUIUOAUPLAMNIOCULOCUIUNUQPQIQRIULASTUIUJURPAAUAUBUCUMFAGIQUD
    UEUFAUGUH $.
    $( [30-May-1999] $)

  $( Addition of negative of a vector to itself. $)
  hvnegid $p |- ( A e. ~H -> ( A +h ( -u 1 .h A ) ) = 0h ) $=
    ( chil wcel cmv co c1 cneg csm cva c0v wceq hvsubval anidms hvsubid eqtr3d
    ) ABCZAADEZAFGAHEIEZJPQRKAALMANO $.
    $( [3-Jun-2004] $) $( [4-Jun-2004] $)

  $( Two ways to express the negative of a vector. $)
  hv2neg $p |- ( A e. ~H -> ( 0h -h A ) = ( -u 1 .h A ) ) $=
    ( chil wcel c0v cmv co c1 cneg csm cva wceq ax-hv0cl hvsubval mpan cc
    ax1cn negcli hvmulcl hvaddid2 syl eqtrd ) ABCZDAEFZDGHZAIFZJFZUEDBCUBUCUFKL
    DAMNUBUEBCZUFUEKUDOCUBUGGPQUDARNUESTUA $.
    $( [24-May-2005] $) $( [23-May-2005] $)

  ${
    hvaddid2.1 $e |- A e. ~H $.
    $( Addition with the zero vector. $)
    hvaddid2i $p |- ( 0h +h A ) = A $=
