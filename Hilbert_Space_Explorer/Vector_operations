  $( The Hilbert space scalar product operation is a set. $)
  hvmulex $p |- .h e. _V $=
    ( cc chil cxp csm wf cvv wcel ax-hfvmul axcnex ax-hilex xpex fex mp2an ) AB
    CZBDENFGDFGHABIJKNBFDLM $.
    $( [22-Apr-2007] $) $( [17-Apr-2007] $)

  $( Closure of vector addition. $)
  hvaddcl $p |- ( ( A e. ~H /\ B e. ~H ) -> ( A +h B ) e. ~H ) $=
    ( chil cva ax-hfvadd foprcl ) ABCCCDEF $.
    $( [18-Apr-2007] $) $( [18-Apr-2007] $)

  $( Closure of scalar multiplication. $)
  hvmulcl $p |- ( ( A e. CC /\ B e. ~H ) -> ( A .h B ) e. ~H ) $=
    ( chil cc csm ax-hfvmul foprcl ) ABCDCEFG $.
    $( [19-Apr-2007] $) $( [19-Apr-2007] $)

  ${
    hvmulcl.1 $e |- A e. CC $.
    hvmulcl.2 $e |- B e. ~H $.
    $( Closure inference for scalar multiplication. $)
    hvmulcli $p |- ( A .h B ) e. ~H $=
      ( cc wcel chil csm co hvmulcl mp2an ) AEFBGFABHIGFCDABJK $.
      $( [1-Aug-1999] $)
  $}

  ${
    $d x y z $.
    $( Mapping domain and codomain of vector subtraction. $)
    hvsubopr $p |- -h : ( ~H X. ~H ) --> ~H $=
      ( vx vy vz chil cv c1 cneg csm co cva cmv df-hvsub wcel hvaddcl cc ax1cn
      negcli hvmulcl mpan sylan2 foprab ) ABCDDAEZFGZBEZHIZJIZDKABCLUBDMUEDMZUF
      DMUDDMZUBUENUCOMUHUGFPQUCUDRSTUA $.
      $( [19-Sep-2007] $) $( [6-Sep-2007] $)
  $}

  ${
    $d x y z A $.  $d x y z B $.
    $( Value of vector subtraction. $)
    hvsubval $p |- ( ( A e. ~H /\ B e. ~H ) ->
                    ( A -h B ) = ( A +h ( -u 1 .h B ) ) ) $=
      ( vx vy vz chil cv c1 cneg csm co cva cmv oprex opreq1 wceq opreq2
      opreq2d df-hvsub oprabval2 ) CDEABFFCGZHIZDGZJKZLKAUBBJKZLKMAUDLKAUELNUAA
      UDLOUCBPUDUEALUCBUBJQRCDEST $.
      $( [5-Sep-1999] $)
  $}

  $( Closure of vector subtraction. $)
  hvsubcl $p |- ( ( A e. ~H /\ B e. ~H ) -> ( A -h B ) e. ~H ) $=
    ( chil wcel wa cmv co c1 cneg csm cva hvsubval hvaddcl cc ax1cn negcli
    hvmulcl mpan sylan2 eqeltrd ) ACDZBCDZEABFGAHIZBJGZKGZCABLUAUDCDZUECDUBAUDM
    UCNDUBUFHOPUCBQRST $.
    $( [17-Aug-1999] $)

  ${
    hvaddcl.1 $e |- A e. ~H $.
    hvaddcl.2 $e |- B e. ~H $.
    $( Closure of vector addition. $)
    hvaddcli $p |- ( A +h B ) e. ~H $=
      ( chil wcel cva co hvaddcl mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [1-Aug-1999] $)

    $( Commutation of vector addition. $)
    hvcomi $p |- ( A +h B ) = ( B +h A ) $=
      ( chil wcel cva co wceq ax-hvcom mp2an ) AEFBEFABGHBAGHICDABJK $.
      $( [3-Sep-1999] $)

    $( Value of vector subtraction definition. $)
    hvsubvali $p |- ( A -h B ) = ( A +h ( -u 1 .h B ) ) $=
      ( chil wcel cmv co c1 cneg csm cva wceq hvsubval mp2an ) AEFBEFABGHAIJBKH
      LHMCDABNO $.
      $( [3-Sep-1999] $)

    $( Closure of vector subtraction. $)
    hvsubcli $p |- ( A -h B ) e. ~H $=
      ( chil wcel cmv co hvsubcl mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [2-Aug-1999] $)
  $}

  $( Addition with the zero vector. $)
  hvaddid2 $p |- ( A e. ~H -> ( 0h +h A ) = A ) $=
    ( chil wcel c0v cva co wceq ax-hv0cl ax-hvcom mpan2 ax-hvaddid eqtr3d ) ABC
    ZADEFZDAEFZAMDBCNOGHADIJAKL $.
    $( [18-Oct-1999] $)

  $( Scalar multiplication with the zero vector. $)
  hvmul0 $p |- ( A e. CC -> ( A .h 0h ) = 0h ) $=
    ( cc wcel c0v cc0 csm co cmul mul01 opreq1d chil wceq ax-hv0cl ax-hvmul0
    ax-mp syl6eq 0cn ax-hvmulass mp3an23 eqtr3d opreq2i syl6req ) ABCZDAEDFGZFG
    ZADFGUCAEHGZDFGZDUEUCUGUDDUCUFEDFAIJDKCZUDDLMDNOZPUCEBCUHUGUELQMAEDRSTUDDAF
