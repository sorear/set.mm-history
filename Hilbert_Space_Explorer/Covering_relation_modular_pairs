      KUBUCVBCUQKUBUDUEUFUGUHUI $.
      $( [24-Dec-2006] $) $( [22-Jun-2004] $)

    $( Consequence of the modular pair property. $)
    mditOLD $p |- ( ( A e. CH /\ B e. CH /\ C e. CH ) -> ( A MH B ->
              ( C (_ B -> ( ( C vH A ) i^i B ) = ( C vH ( A i^i B ) ) ) ) ) $=
      ( vx cch wcel cmd wbr wss chj co cin wceq wi wa cv wral mdbrt biimpd
      sseq1 opreq1 ineq1d eqeq12d imbi12d rcla4v sylan9 3impa ) AEFZBEFZCEFZABG
