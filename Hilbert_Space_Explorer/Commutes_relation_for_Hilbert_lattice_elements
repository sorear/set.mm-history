  ${
    cmid.1 $e |- A e. CH $.
    $( The commutes relation is reflexive. $)
    cmidi $p |- A C_H A $=
      ( ssid lecmii ) AABBACD $.
      $( [13-Sep-2004] $) $( [7-Aug-2004] $)
  $}

