  $( Commuted inner products have the same absolute values. $)
  abshicom $p |- ( ( A e. ~H /\ B e. ~H ) ->
                  ( abs ` ( A .ih B ) ) = ( abs ` ( B .ih A ) ) ) $=
    ( chil wcel wa csp co cabs cfv ccj ax-his1 fveq2d cc wceq hicl ancoms abscj
    syl eqtrd ) ACDZBCDZEZABFGZHIBAFGZJIZHIZUDHIZUBUCUEHABKLUBUDMDZUFUGNUATUHBA
    OPUDQRS $.
    $( [26-May-2006] $)

