    $( Commuting subspaces form a modular pair. $)
    cmmdi $p |- ( A C_H B -> A MH B ) $=
      ( ccm wbr cort cfv cdmd cmd cph co chj wceq cmcm4i choccli osumcor2i
      sylbi sumdmdii syl cch wcel wb mddmd mp2an sylibr ) ABEFZAGHZBGHZIFZABJFZ
