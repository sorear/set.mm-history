    $( Commuting subspaces form a modular pair. $)
    cmmd $p |- ( A C_H B -> A MH B ) $=
      ( ccm wbr cort cfv cdmd cmd cph co chj wceq cmcm4 choccl osumcor2 sylbi
      sumdmdi syl cch wcel wb mddmdt mp2an sylibr ) ABEFZAGHZBGHZIFZABJFZUGUHUI
      KLUHUIMLNZUJUGUHUIEFULABCDOUHUIACPZBDPZQRUHUIUMUNSTAUAUBBUAUBUKUJUCCDABUD
      UEUF $.
      $( [13-Aug-2005] $) $( [16-Jan-2005] $)

    $( Commuting subspaces form a dual modular pair. $)
