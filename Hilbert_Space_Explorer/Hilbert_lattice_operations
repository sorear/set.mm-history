$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                     Hilbert lattice operations
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( The zero subspace is the smallest subspace. $)
  sh0let $p |- ( A e. SH -> 0H (_ A ) $=
    ( csh wcel c0v csn c0h wss sh0 snssi syl df-ch0 syl5ss ) ABCZDEZAFMDACNAGAH
    DAIJKL $.
    $( [4-Jun-2004] $) $( [3-Jun-2004] $)

  $( The zero subspace is the smallest member of ` CH ` . $)
  ch0let $p |- ( A e. CH -> 0H (_ A ) $=
    ( cch wcel csh c0h wss chsh sh0let syl ) ABCADCEAFAGAHI $.
    $( [14-Aug-2002] $)

  $( No subspace is smaller than the zero subspace. $)
  shle0t $p |- ( A e. SH -> ( A (_ 0H <-> A = 0H ) ) $=
    ( csh wcel c0h wss wa wceq sh0let biantrud eqss syl6bbr ) ABCZADEZMDAEZFADG
    LNMAHIADJK $.
    $( [26-Nov-2004] $) $( [24-Nov-2004] $)

  $( No Hilbert lattice element is smaller than zero. $)
  chle0t $p |- ( A e. CH -> ( A (_ 0H <-> A = 0H ) ) $=
    ( cch wcel csh c0h wss wceq wb chsh shle0t syl ) ABCADCAEFAEGHAIAJK $.
    $( [14-Aug-2002] $)

  $( A Hilbert lattice element that is not a subset of another is nonzero. $)
  chnlen0 $p |- ( B e. CH -> ( -. A (_ B -> -. A = 0H ) ) $=
    ( cch wcel c0h wceq wss sseq1 ch0let syl5cbir con3d ) BCDZAEFZABGZMNEBGLAEB
    HBIJK $.
    $( [2-Jul-2004] $) $( [30-Jun-2004] $)

  $( The zero subspace is a proper subset of non-zero Hilbert lattice
     elements. $)
  ch0psst $p |- ( A e. CH -> ( 0H (. A <-> A =/= 0H ) ) $=
    ( cch wcel c0h wne wss wa wpss ch0let biantrurd necom syl5bbr df-pss
    syl6rbbr ) ABCZADEZDAFZDAEZGZDAHORSPOQRAIJDAKLDAMN $.
    $( [18-Jun-2006] $) $( [9-Jun-2004] $)

  $( The intersection of orthogonal subspaces is the zero subspace. $)
  orthin $p |- ( ( A e. SH /\ B e. SH ) ->
               ( A (_ ( _|_ ` B ) -> ( A i^i B ) = 0H ) ) $=
    ( csh wcel wa cort cfv wss cin c0h wceq wi ocin sseq2d ssrin incom syl6ss
    syl5bi adantl shinclt sh0let syl jctird eqss syl6ibr ) ACDZBCDZEZABFGZHZABI
    ZJHZJUKHZEUKJKUHUJULUMUGUJULLUFUGUKBUIIZHULUJUGUNJUKBMNUJUKUIBIUNAUIBOUIBPQ
    RSUHUKCDUMABTUKUAUBUCUKJUDUE $.
    $( [24-Jun-2004] $) $( [24-Jun-2004] $)

  ${
    $d x y A $.
    shne0.1 $e |- A e. SH $.
    $( A non-zero subspace has a non-zero vector. $)
    shne0 $p |- ( A =/= 0H <-> E. x e. A x =/= 0h ) $=
      ( c0h wne wceq wn cv wcel wrex c0v df-ne wa wex wss df-rex nss csh wb
      shle0t ax-mp negbii 3bitr2r elch0 necon3bbii rexbii 3bitr ) BDEBDFZGZAHZD
      IZGZABJZUJKEZABJBDLUMUJBIULMANBDOZGUIULABPABDQUOUHBRIUOUHSCBTUAUBUCULUNAB
      UKUJKUJUDUEUFUG $.
      $( [18-Mar-2006] $) $( [25-Feb-2006] $)

    $( Hilbert subspace sum with the zero subspace. $)
    shs0 $p |- ( A +H 0H ) = A $=
      ( c0h cph co cun cspn cfv h0elsh shsumval3 wss wceq csh wcel sh0let
      ax-mp ssequn2 mpbi fveq2i spanid 3eqtr ) ACDEACFZGHAGHZAACBIJUBAGCAKZUBAL
      AMNZUDBAOPCAQRSUEUCALBATPUA $.
      $( [17-Jan-2005] $) $( [14-Jan-2005] $)

    ${
      shs00.2 $e |- B e. SH $.
      $( Two subspaces are zero iff their join is zero. $)
      shs00 $p |- ( ( A = 0H /\ B = 0H ) <-> ( A +H B ) = 0H ) $=
        ( c0h wceq wa cph co opreq12 h0elsh shs0 syl6eq wss shsub1 sseq2 mpbii
        csh wcel wb shle0t ax-mp sylib shsub2 jca impbi ) AEFZBEFZGZABHIZEFZUIU
        JEEHIEAEBEHJEKLMUKUGUHUKAENZUGUKAUJNULABCDOUJEAPQARSULUGTCAUAUBUCUKBENZ
        UHUKBUJNUMBADCUDUJEBPQBRSUMUHTDBUAUBUCUEUF $.
        $( [11-Aug-2004] $) $( [7-Aug-2004] $)
    $}
  $}

  ${
    $d x y A $.
    ch0le.1 $e |- A e. CH $.
    $( The closed subspace zero is the smallest member of ` CH ` . $)
    ch0le $p |- 0H (_ A $=
      ( cch wcel c0h wss ch0let ax-mp ) ACDEAFBAGH $.
      $( [15-Oct-1999] $)

    $( No Hilbert closed subspace is smaller than zero. $)
    chle0 $p |- ( A (_ 0H <-> A = 0H ) $=
      ( cch wcel c0h wss wceq wb chle0t ax-mp ) ACDAEFAEGHBAIJ $.
      $( [7-Apr-2001] $)

    $( A non-zero closed subspace has a non-zero vector. $)
    chne0 $p |- ( A =/= 0H <-> E. x e. A x =/= 0h ) $=
      ( chshi shne0 ) ABBCDE $.
      $( [19-Mar-2006] $) $( [25-Feb-2006] $)

    $( Intersection of a closed subspace and its orthocomplement.  Part of
       Proposition 1 of [Kalmbach] p. 65. $)
    chocin $p |- ( A i^i ( _|_ ` A ) ) = 0H $=
      ( csh wcel cort cfv cin c0h wceq chshi ocin ax-mp ) ACDAAEFGHIABJAKL $.
      $( [6-Aug-2004] $) $( [11-Oct-1999] $)

    $( Join with lattice zero in ` CH ` . $)
    chj0 $p |- ( A vH 0H ) = A $=
      ( c0h chj co cun cort cfv h0elch chjval wss wceq ch0le ssequn2 mpbi
      fveq2i pjococ 3eqtr ) ACDEACFZGHZGHAGHZGHAACBIJTUAGSAGCAKSALABMCANOPPABQR
      $.
      $( [15-Oct-1999] $)

    $( Meet with lattice one in ` CH ` . $)
    chm1 $p |- ( A i^i H~ ) = A $=
      ( chil cin inss1 ssid chssi ssini eqssi ) ACDAACEAACAFABGHI $.
      $( [24-Oct-1999] $)

    ${
      $d x y B $.  $d x y C $.
      chjcl.2 $e |- B e. CH $.
      $( Closure of ` CH ` join. $)
      chjcl $p |- ( A vH B ) e. CH $=
        ( chshi shjcl ) ABACEBDEF $.
        $( [29-Jul-1999] $)

      $( Subspace sum is smaller than subspace join.  Remark in [Kalmbach]
         p. 65. $)
      chslej $p |- ( A +H B ) (_ ( A vH B ) $=
        ( chshi shslej ) ABACEBDEF $.
        $( [17-Oct-1999] $)

      $( Membership in subspace sum. $)
      chsel $p |- ( C e. ( A +H B ) <-> E. x e. A E. y e. B C = ( x +h y ) ) $=
        ( chshi shsel ) ABCDECFHDGHI $.
        $( [19-Oct-1999] $)

      $( Closure of Hilbert lattice intersection. $)
      chincl $p |- ( A i^i B ) e. CH $=
        ( cpr cint cin cch elisseti intpr wss c0 wne wcel wa pm3.2i prss mpbi
        prnz chintcl eqeltrr ) ABEZFABGHABAHCIZBHDIZJUBUBHKZUBLMAHNZBHNZOUEUFUG
        CDPABHUCUDQRABUCSPTUA $.
        $( [15-Oct-1999] $)

      $( Hilbert lattice contraposition law. $)
      chsscon3 $p |- ( A (_ B <-> ( _|_ ` B ) (_ ( _|_ ` A ) ) $=
        ( wss cort cfv chil wi chssi occont mp2an choccl pjococ 3sstr3g impbi
        ) ABEZBFGZAFGZEZAHEBHEQTIACJBDJABKLTSFGZRFGZABRHESHETUAUBEIRBDMJSACMJRS
        KLACNBDNOP $.
        $( [15-Oct-1999] $)

      $( Hilbert lattice contraposition law. $)
      chsscon1 $p |- ( ( _|_ ` A ) (_ B <-> ( _|_ ` B ) (_ A ) $=
        ( cort cfv wss choccl chsscon3 pjococ sseq2i bitr ) AEFZBGBEFZMEFZGNAGM
        BACHDIOANACJKL $.
        $( [15-Oct-1999] $)

      $( Hilbert lattice contraposition law. $)
      chsscon2 $p |- ( A (_ ( _|_ ` B ) <-> B (_ ( _|_ ` A ) ) $=
        ( cort cfv wss choccl chsscon3 pjococ sseq1i bitr ) ABEFZGMEFZAEFZGBOGA
        MCBDHINBOBDJKL $.
        $( [15-Oct-1999] $)

      $( Hilbert lattice contraposition law. $)
      chcon2 $p |- ( A = ( _|_ ` B ) <-> B = ( _|_ ` A ) ) $=
        ( cort cfv wss wa wceq chsscon2 chsscon1 anbi12i eqss 3bitr4 ) ABEFZGZO
        AGZHBAEFZGZRBGZHAOIBRIPSQTABCDJBADCKLAOMBRMN $.
        $( [24-Jun-2004] $) $( [24-Jun-2004] $)

      $( Hilbert lattice contraposition law. $)
      chcon1 $p |- ( ( _|_ ` A ) = B <-> ( _|_ ` B ) = A ) $=
        ( cort cfv wceq chcon2 eqcom 3bitr4 ) BAEFZGABEFZGKBGLAGBADCHKBILAIJ $.
        $( [3-Oct-2006] $) $( [15-Jun-2006] $)

      $( Hilbert lattice contraposition law. $)
      chcon3 $p |- ( A = B <-> ( _|_ ` B ) = ( _|_ ` A ) ) $=
        ( wss wa cort cfv wceq chsscon3 anbi12i eqss 3bitr4 ) ABEZBAEZFBGHZAGHZ
        EZQPEZFABIPQINROSABCDJBADCJKABLPQLM $.
        $( [25-Jun-2004] $) $( [24-Jun-2004] $)

      $( Union is smaller than ` CH ` join. $)
      chunssj $p |- ( A u. B ) (_ ( A vH B ) $=
        ( chshi shunssj ) ABACEBDEF $.
        $( [15-Oct-1999] $)

      $( Commutative law for join in ` CH ` . $)
      chjcom $p |- ( A vH B ) = ( B vH A ) $=
        ( chshi shjcom ) ABACEBDEF $.
        $( [14-Oct-1999] $)

      $( ` CH ` join is an upper bound of two elements. $)
      chub1 $p |- A (_ ( A vH B ) $=
        ( chshi shub1 ) ABACEBDEF $.
        $( [15-Oct-1999] $)

      $( ` CH ` join is an upper bound of two elements. $)
      chub2 $p |- A (_ ( B vH A ) $=
        ( chj co chub1 chjcom sseqtr ) AABEFBAEFABCDGABCDHI $.
        $( [5-Nov-2000] $)

      ${
        $d x y C $.
        chlub.1 $e |- C e. CH $.
