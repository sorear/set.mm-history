$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                 Span (cont.) and one-dimensional subspaces
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( The span of the singleton of the zero vector is the zero subspace. $)
  spansn0 $p |- ( span ` { 0h } ) = 0H $=
    ( c0h cspn cfv c0v csn df-ch0 fveq2i wcel wceq h0elsh spanid ax-mp eqtr3i
    csh ) ABCZDEZBCAAPBFGANHOAIJAKLM $.
    $( [14-Jan-2005] $)

  $( The span of the empty set is the zero subspace.  Remark 11.6.e of
     [Schechter] p. 276. $)
  span0 $p |- ( span ` (/) ) = 0H $=
    ( cspn cfv c0h chil wss h0elsh shssii 0ss spanss mp2an csh wcel wceq spanid
    c0 ax-mp sseqtri spancl sh0le eqssi ) OABZCUACABZCCDEOCEUAUBECFGCHOCIJCKLUB
    CMFCNPQUAKLZCUAEODEUCDHORPUASPT $.
    $( [3-Jun-2004] $)

  ${
    $d x A $.  $d x B $.
    elspan.1 $e |- B e. _V $.
    $( Membership in the span of a subset of Hilbert space. $)
    elspani $p |- ( A C_ ~H ->
                ( B e. ( span ` A ) <-> A. x e. SH ( A C_ x -> B e. x ) ) ) $=
      ( chil wss cspn cfv wcel cv csh crab cint wi wral spanval eleq2d elintrab
      syl6bb ) BEFZCBGHZICBAJZFZAKLMZIUCCUBINAKOTUAUDCABPQUCACKDRS $.
      $( [2-Jun-2004] $)

  $}

  ${
    spanun.1 $e |- A C_ ~H $.
    spanun.2 $e |- B C_ ~H $.
    $d x y z w A $.  $d x y z w B $.
    $( The span of a union is the subspace sum of spans. $)
    spanuni $p |- ( span ` ( A u. B ) ) = ( ( span ` A ) +H ( span ` B ) ) $=
      ( vx vz vw vy cspn cfv chil wss csh wcel ax-mp cv wa wel wi wral co mp2an
      cun spancl shscli shssii spanss2 unss12 shunssi sstri spanss wceq sseqtri
      cph spanid cva wex wrex shseli r2ex bitri wb visset elspani r19.26 bitr4i
      anbi12i r19.27av sylanb unss syl5bir shaddcl 3expib sylan9r eleq1 biimprd
      prth sylan9 expl ralimia unssi sylibr syl 19.23aivv sylbi ssriv eqssi )
      ABUCZIJZAIJZBIJZUNUAZWIWLIJZWLWLKLWHWLLWIWMLWLWJWKAKLZWJMNCAUDOZBKLZWKMND
      BUDOZUEZUFWHWJWKUCZWLAWJLZBWKLZWHWSLWNWTCAUGOWPXADBUGOAWJBWKUHUBWJWKWOWQU
      IUJWHWLUKUBWLMNWMWLULWRWLUOOUMEWLWIEPZWLNZFPZWJNZGPZWKNZQZXBXDXFUPUAZULZQ
      ZGUQFUQZXBWINZXCXJGWKURFWJURXLFGWJWKXBWOWQUSXJFGWJWKUTVAXKXMFGXKAHPZLZFHR
      ZSZBXNLZGHRZSZQZXJQZHMTZXMXHYAHMTZXJYCXHXQHMTZXTHMTZQYDXEYEXGYFWNXEYEVBCH
      AXDFVCVDOWPXGYFVBDHBXFGVCVDOVGXQXTHMVEVFYAXJHMVHVIYCWHXNLZEHRZSZHMTZXMYBY
      IHMXNMNZYAXJYIYKYAQYGXIXNNZXJYHYAYGXPXSQZYKYLYGXOXRQYAYMABXNVJXOXPXRXSVQV
      KYKXPXSYLXDXFXNVLVMVNXJYHYLXBXIXNVOVPVRVSVTWHKLXMYJVBABKCDWAHWHXBEVCVDOWB
      WCWDWEWFWG $.
      $( [2-Jun-2004] $)
  $}

  $( The span of a union is the subspace sum of spans. $)
  spanun $p |- ( ( A C_ ~H /\ B C_ ~H ) ->
                ( span ` ( A u. B ) ) = ( ( span ` A ) +H ( span ` B ) ) ) $=
    ( chil wss cun cspn cfv cph co cif uneq1 fveq2d fveq2 opreq1d eqeq12d uneq2
    wceq opreq2d sseq1 elimhyp ssid spanuni dedth2h ) ACDZBCDZABEZFGZAFGZBFGZHI
    ZQUDACJZBEZFGZUKFGZUIHIZQUKUEBCJZEZFGZUNUPFGZHIZQABCCAUKQZUGUMUJUOVAUFULFAU
    KBKLVAUHUNUIHAUKFMNOBUPQZUMURUOUTVBULUQFBUPUKPLVBUIUSUNHBUPFMROUKUPUDUKCDCC
    DZACAUKCSCUKCSCUAZTUEUPCDVCBCBUPCSCUPCSVDTUBUC $.
    $( [9-Jun-2006] $)

  ${
    sshjococ.1 $e |- A C_ ~H $.
    sshjococ.2 $e |- B C_ ~H $.
    $( The join of two Hilbert space subsets (not necessarily closed subspaces)
       equals the join of their closures (double orthocomplements). $)
    sshhococi $p |- ( A vH B ) =
                 ( ( _|_ ` ( _|_ ` A ) ) vH ( _|_ ` ( _|_ ` B ) ) ) $=
      ( chj cort cfv cun wss chil ococss ax-mp mp2an unssi occli choccli chssii
      co occon2i sstri unss12 sshjval chjvali 3sstr4i ssun1 sseqtr4i cch sshjcl
      wceq wcel ssun2 chlubii ococi sseqtri eqssi ) ABERZAFGZFGZBFGZFGZERZABHZF
      GFGZURUTHZFGFGZUPVAVBVDIZVCVEIAURIZBUTIZVFAJIZVGCAKLBJIZVHDBKLAURBUTUAMVB
      VDABJCDNZURUTJURUQACOPZQUTUSBDOPZQNSLVIVJUPVCUICDABUBMZURUTVLVMUCUDVAUPFG
      ZFGZUPURVPIZUTVPIZVAVPIAUPIVQAVCUPAVBVCABUEVBJIVBVCIVKVBKLZTVNUFAUPCUPVIV
      JUPUGUJCDABUHMZQZSLBUPIVRBVCUPBVBVCBAUKVSTVNUFBUPDWASLURUTVPVLVMVOUPVTPPU
      LMUPVTUMUNUO $.
      $( [1-Jun-2004] $)
  $}

  $( Hilbert space has a nonzero vector iff it is not trivial. $)
  hne0 $p |- ( ~H =/= 0H <-> E. x e. ~H x =/= 0h ) $=
    ( chil helch chne0i ) ABCD $.
    $( [24-Feb-2006] $)

  $( The supremum of the empty set. $)
  chsup0 $p |- ( \/H ` (/) ) = 0H $=
    ( c0 chsup cfv c0h wss wceq csn 0ss cch wi h0elch snssi ax-mp chsupss mp2an
    wcel chsupsn sseqtri chsupcl chle0i mpbi ) ABCZDEUBDFUBDGZBCZDAUCEZUBUDEZUC
    HAIEZUCIEZUEUFJIHZDIPZUHKDILMAUCNOMUJUDDFKDQMRUBUGUBIPUIASMTUA $.
    $( [13-Aug-2002] $)

  ${
    $d x A $.  $d x B $.
    h1deot.1 $e |- B e. ~H $.
    $( Membership in orthocomplement of 1-dimensional subspace. $)
    h1deoi $p |- ( A e. ( _|_ ` { B } ) <->
                 ( A e. ~H /\ ( A .ih B ) = 0 ) ) $=
      ( vx csn cort cfv wcel chil cv csp co cc0 wceq wral wa wss ax-mp wi wal
      snssi ocel df-ral elsn imbi1i albii elisseti opreq2 eqeq1d ceqsalv 3bitri
      wb anbi2i bitri ) ABEZFGHZAIHZADJZKLZMNZDUOOZPZUQABKLZMNZPUOIQZUPVBULBIHV
      ECBIUARDAUOUBRVAVDUQVAURUOHZUTSZDTURBNZUTSZDTVDUTDUOUCVGVIDVFVHUTDBUDUEUF
      UTVDDBBICUGVHUSVCMURBAKUHUIUJUKUMUN $.
      $( [7-Jul-2001] $)

    $( Membership in 1-dimensional subspace. $)
    h1dei $p |- ( A e. ( _|_ ` ( _|_ ` { B } ) ) <->
    ( A e. ~H /\ A. x e. ~H ( ( B .ih x ) = 0 -> ( A .ih x ) = 0 ) ) ) $=
      ( csn cort cfv wcel chil csp co cc0 wceq wral wa wi wss wb ax-mp bitri cv
      snssi occli chssii ocel h1deoi orthcom mpan2 pm5.32i imbi1i impexp anbi2i
      ralbii2 ) BCEZFGZFGHZBIHZBAUAZJKLMZAUONZOZUQCURJKLMZUSPZAINZOUOIQUPVARUOU
      NCIHZUNIQDCIUBSUCUDABUOUESUTVDUQUSVCAUOIURUOHZUSPURIHZVBOZUSPVGVCPVFVHUSV
      FVGURCJKLMZOVHURCDUFVGVIVBVGVEVIVBRDURCUGUHUITUJVGVBUSUKTUMULT $.
      $( [7-Jul-2001] $)
  $}

  $( A generating vector belongs to the 1-dimensional subspace it generates. $)
  h1did $p |- ( A e. ~H -> A e. ( _|_ ` ( _|_ ` { A } ) ) ) $=
    ( chil wcel csn cort cfv wss snssi ococss syl snssg mpbird ) ABCZAADZEFEFZC
    NOGZMNBGPABHNIJAOBKL $.
    $( [22-Jul-2001] $)

  $( A nonzero vector generates a (nonzero) 1-dimensional subspace. $)
  h1dn0 $p |- ( ( A e. ~H /\ A =/= 0h ) ->
              ( _|_ ` ( _|_ ` { A } ) ) =/= 0H ) $=
    ( chil wcel c0v wne csn cort cfv h1did eleq2 syl5ibcom elch0 syl6ib necon3d
    c0h wceq imp ) ABCZADEAFGHGHZOERSOADRSOPZAOCZADPRASCTUAAISOAJKALMNQ $.
    $( [19-Jun-2006] $) $( [22-Jul-2001] $)

  ${
