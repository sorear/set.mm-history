$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                 Span (cont.) and one-dimensional subspaces
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( The span of the singleton of the zero vector is the zero subspace. $)
  spansn0 $p |- ( span ` { 0h } ) = 0H $=
    ( c0h cspn cfv c0v csn df-ch0 fveq2i csh wcel wceq h0elsh spanid ax-mp
    eqtr3i ) ABCZDEZBCAAPBFGAHIOAJKALMN $.
    $( [14-Jan-2005] $)

  $( The span of the empty set is the zero subspace.  Remark 11.6.e of
     [Schechter] p. 276. $)
  span0 $p |- ( span ` (/) ) = 0H $=
    ( c0 cspn cfv c0h chil wss h0elsh shssii 0ss spanss mp2an csh wcel wceq
    spanid ax-mp sseqtri spancl sh0le eqssi ) ABCZDUADBCZDDEFADFUAUBFDGHDIADJKD
    LMUBDNGDOPQUALMZDUAFAEFUCEIARPUASPT $.
    $( [3-Jun-2004] $)

  ${
    $d x A $.  $d x B $.
    elspan.1 $e |- B e. _V $.
    $( Membership in the span of a subset of Hilbert space. $)
    elspani $p |- ( A C_ ~H ->
                ( B e. ( span ` A ) <-> A. x e. SH ( A C_ x -> B e. x ) ) ) $=
      ( chil wss cspn cfv wcel cv csh crab cint wi wral spanval eleq2d
      elintrab syl6bb ) BEFZCBGHZICBAJZFZAKLMZIUCCUBINAKOTUAUDCABPQUCACKDRS $.
      $( [2-Jun-2004] $)

  $}

  ${
    spanun.1 $e |- A C_ ~H $.
    spanun.2 $e |- B C_ ~H $.
    $d x y z w A $.  $d x y z w B $.
    $( The span of a union is the subspace sum of spans. $)
    spanuni $p |- ( span ` ( A u. B ) ) = ( ( span ` A ) +H ( span ` B ) ) $=
      ( vx vz vw vy cun cspn cfv cph co chil wss csh wcel spancl ax-mp shscli
      shssii spanss2 unss12 mp2an shunssi sstri spanss wceq spanid sseqtri cv
      wa cva wex wrex shseli r2ex bitri wi wral r19.27av wb visset elspani
      anbi12i r19.26 bitr4i sylanb prth unss syl5ibr shaddclOLD sylan9r eleq1
      biimprd sylan9 expl ralimia unssi sylibr syl 19.23aivv sylbi ssriv eqssi
      ) ABIZJKZAJKZBJKZLMZWGWJJKZWJWJNOWFWJOWGWKOWJWHWIANOZWHPQCARSZBNOZWIPQDBR
      SZTZUAWFWHWIIZWJAWHOZBWIOZWFWQOWLWRCAUBSWNWSDBUBSAWHBWIUCUDWHWIWMWOUEUFWF
      WJUGUDWJPQWKWJUHWPWJUISUJEWJWGEUKZWJQZFUKZWHQZGUKZWIQZULZWTXBXDUMMZUHZULZ
      GUNFUNZWTWGQZXAXHGWIUOFWHUOXJFGWHWIWTWMWOUPXHFGWHWIUQURXIXKFGXIAHUKZOZXBX
      LQZUSZBXLOZXDXLQZUSZULZXHULZHPUTZXKXSHPUTZXHYAXFXSXHHPVAXFXOHPUTZXRHPUTZU
      LYBXCYCXEYDWLXCYCVBCHAXBFVCVDSWNXEYDVBDHBXDGVCVDSVEXOXRHPVFVGVHYAWFXLOZWT
      XLQZUSZHPUTZXKXTYGHPXLPQZXSXHYGYIXSULYEXGXLQZXHYFXSYEXNXQULZYIYJXSXMXPULY
      KYEXMXNXPXQVIABXLVJVKXBXDXLVLVMXHYFYJWTXGXLVNVOVPVQVRWFNOXKYHVBABNCDVSHWF
      WTEVCVDSVTWAWBWCWDWE $.
      $( [2-Jun-2004] $)
  $}

  $( The span of a union is the subspace sum of spans. $)
  spanun $p |- ( ( A C_ ~H /\ B C_ ~H ) ->
                ( span ` ( A u. B ) ) = ( ( span ` A ) +H ( span ` B ) ) ) $=
    ( chil wss cun cspn cfv cph co wceq cif uneq1 fveq2d fveq2 opreq1d eqeq12d
    uneq2 opreq2d sseq1 ssid elimhyp spanuni dedth2h ) ACDZBCDZABEZFGZAFGZBFGZH
    IZJUDACKZBEZFGZUKFGZUIHIZJUKUEBCKZEZFGZUNUPFGZHIZJABCCAUKJZUGUMUJUOVAUFULFA
    UKBLMVAUHUNUIHAUKFNOPBUPJZUMURUOUTVBULUQFBUPUKQMVBUIUSUNHBUPFNRPUKUPUDUKCDC
    CDZACAUKCSCUKCSCTZUAUEUPCDVCBCBUPCSCUPCSVDUAUBUC $.
    $( [9-Jun-2006] $)

  ${
    sshjococ.1 $e |- A C_ ~H $.
    sshjococ.2 $e |- B C_ ~H $.
    $( The join of two Hilbert space subsets (not necessarily closed subspaces)
       equals the join of their closures (double orthocomplements). $)
    sshhococi $p |- ( A vH B ) =
                 ( ( _|_ ` ( _|_ ` A ) ) vH ( _|_ ` ( _|_ ` B ) ) ) $=
      ( chj co cort cfv cun wss chil ococss ax-mp unss12 mp2an unssi occli
      choccli chssii occon2i wceq sshjval chjvali 3sstr4i ssun1 sstri sseqtr4i
      cch wcel sshjcl ssun2 chlubii ococi sseqtri eqssi ) ABEFZAGHZGHZBGHZGHZEF
      ZABIZGHGHZURUTIZGHGHZUPVAVBVDJZVCVEJAURJZBUTJZVFAKJZVGCALMBKJZVHDBLMAURBU
      TNOVBVDABKCDPZURUTKURUQACQRZSUTUSBDQRZSPTMVIVJUPVCUACDABUBOZURUTVLVMUCUDV
      AUPGHZGHZUPURVPJZUTVPJZVAVPJAUPJVQAVCUPAVBVCABUEVBKJVBVCJVKVBLMZUFVNUGAUP
      CUPVIVJUPUHUICDABUJOZSZTMBUPJVRBVCUPBVBVCBAUKVSUFVNUGBUPDWATMURUTVPVLVMVO
      UPVTRRULOUPVTUMUNUO $.
      $( [1-Jun-2004] $)
  $}

  $( Hilbert space has a nonzero vector iff it is not trivial. $)
  hne0 $p |- ( ~H =/= 0H <-> E. x e. ~H x =/= 0h ) $=
    ( chil helch chne0i ) ABCD $.
    $( [24-Feb-2006] $)

  $( The supremum of the empty set. $)
  chsup0 $p |- ( \/H ` (/) ) = 0H $=
    ( c0 chsup cfv c0h wss wceq csn 0ss cch wi wcel h0elch snssi ax-mp chsupss
    mp2an chsupsn sseqtri chsupcl chle0i mpbi ) ABCZDEUBDFUBDGZBCZDAUCEZUBUDEZU
    CHAIEZUCIEZUEUFJIHZDIKZUHLDIMNAUCOPNUJUDDFLDQNRUBUGUBIKUIASNTUA $.
    $( [13-Aug-2002] $)

  ${
    $d x A $.  $d x B $.
    h1deot.1 $e |- B e. ~H $.
    $( Membership in orthocomplement of 1-dimensional subspace. $)
    h1deoi $p |- ( A e. ( _|_ ` { B } ) <->
                 ( A e. ~H /\ ( A .ih B ) = 0 ) ) $=
      ( vx csn cort cfv wcel chil cv csp co cc0 wceq wral wa wss wb snssi
      ax-mp ocel wi wal df-ral elsn imbi1i albii elisseti opreq2 eqeq1d
      ceqsalv 3bitri anbi2i bitri ) ABEZFGHZAIHZADJZKLZMNZDUOOZPZUQABKLZMNZPUOI
      QZUPVBRBIHVECBISTDAUOUATVAVDUQVAURUOHZUTUBZDUCURBNZUTUBZDUCVDUTDUOUDVGVID
      VFVHUTDBUEUFUGUTVDDBBICUHVHUSVCMURBAKUIUJUKULUMUN $.
      $( [7-Jul-2001] $)

    $( Membership in 1-dimensional subspace. $)
    h1dei $p |- ( A e. ( _|_ ` ( _|_ ` { B } ) ) <->
    ( A e. ~H /\ A. x e. ~H ( ( B .ih x ) = 0 -> ( A .ih x ) = 0 ) ) ) $=
      ( csn cort cfv wcel chil cv csp co cc0 wceq wral wa wi wss wb snssi
      ax-mp occli chssii ocel h1deoi orthcom mpan2 pm5.32i bitri imbi1i impexp
      ralbii2 anbi2i ) BCEZFGZFGHZBIHZBAJZKLMNZAUOOZPZUQCURKLMNZUSQZAIOZPUOIRUP
      VASUOUNCIHZUNIRDCITUAUBUCABUOUDUAUTVDUQUSVCAUOIURUOHZUSQURIHZVBPZUSQVGVCQ
      VFVHUSVFVGURCKLMNZPVHURCDUEVGVIVBVGVEVIVBSDURCUFUGUHUIUJVGVBUSUKUIULUMUI
      $.
      $( [7-Jul-2001] $)
  $}

  $( A generating vector belongs to the 1-dimensional subspace it generates. $)
  h1did $p |- ( A e. ~H -> A e. ( _|_ ` ( _|_ ` { A } ) ) ) $=
    ( chil wcel csn cort cfv wss snssi ococss syl snssg mpbird ) ABCZAADZEFEFZC
    NOGZMNBGPABHNIJAOBKL $.
    $( [22-Jul-2001] $)

  $( A nonzero vector generates a (nonzero) 1-dimensional subspace. $)
  h1dn0 $p |- ( ( A e. ~H /\ A =/= 0h ) ->
              ( _|_ ` ( _|_ ` { A } ) ) =/= 0H ) $=
    ( chil wcel c0v wne csn cort cfv c0h wceq eleq2 h1did syl5cbi elch0 syl6ib
    necon3d imp ) ABCZADEAFGHGHZIERSIADRSIJZAICZADJTASCUARSIAKALMANOPQ $.
    $( [19-Jun-2006] $) $( [22-Jul-2001] $)

