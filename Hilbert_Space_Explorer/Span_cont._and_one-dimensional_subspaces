$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                 Span (cont.) and one-dimensional subspaces
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( The span of the singleton of the zero vector is the zero subspace. $)
  spansn0 $p |- ( span ` { 0h } ) = 0H $=
    ( c0h cspn cfv c0v csn df-ch0 fveq2i csh wcel wceq h0elsh spanid ax-mp
    eqtr3i ) ABCZDEZBCAAPBFGAHIOAJKALMN $.
    $( [14-Jan-2005] $)

  $( The span of the empty set is the zero subspace.  Remark 11.6.e of
     [Schechter] p. 276. $)
  span0 $p |- ( span ` (/) ) = 0H $=
    ( c0 cspn cfv c0h chil wss h0elsh shssii 0ss spanss mp2an csh wcel wceq
    spanid ax-mp sseqtri spancl sh0le eqssi ) ABCZDUADBCZDDEFADFUAUBFDGHDIADJKD
    LMUBDNGDOPQUALMZDUAFAEFUCEIARPUASPT $.
    $( [3-Jun-2004] $)

  ${
    $d x A $.  $d x B $.
    elspan.1 $e |- B e. _V $.
    $( Membership in the span of a subset of Hilbert space. $)
    elspani $p |- ( A C_ ~H ->
                ( B e. ( span ` A ) <-> A. x e. SH ( A C_ x -> B e. x ) ) ) $=
      ( chil wss cspn cfv wcel cv csh crab cint wi wral spanval eleq2d
      elintrab syl6bb ) BEFZCBGHZICBAJZFZAKLMZIUCCUBINAKOTUAUDCABPQUCACKDRS $.
      $( [2-Jun-2004] $)

  $}

  ${
    spanun.1 $e |- A C_ ~H $.
    spanun.2 $e |- B C_ ~H $.
    $d x y z w A $.  $d x y z w B $.
    $( The span of a union is the subspace sum of spans. $)
    spanuni $p |- ( span ` ( A u. B ) ) = ( ( span ` A ) +H ( span ` B ) ) $=
      ( vx vz vw vy cun cspn cfv cph co chil wss csh wcel spancl ax-mp shscli
      shssii spanss2 unss12 mp2an shunssi sstri spanss wceq spanid sseqtri cv
      wa cva wex wrex shseli r2ex bitri wel wi wral wb visset elspani anbi12i
      r19.26 bitr4i r19.27av sylanb unss prth syl5bir shaddcl 3expib sylan9r
      eleq1 biimprd sylan9 expl ralimia unssi sylibr syl 19.23aivv sylbi ssriv
      eqssi ) ABIZJKZAJKZBJKZLMZWIWLJKZWLWLNOWHWLOWIWMOWLWJWKANOZWJPQCARSZBNOZW
      KPQDBRSZTZUAWHWJWKIZWLAWJOZBWKOZWHWSOWNWTCAUBSWPXADBUBSAWJBWKUCUDWJWKWOWQ
      UEUFWHWLUGUDWLPQWMWLUHWRWLUISUJEWLWIEUKZWLQZFUKZWJQZGUKZWKQZULZXBXDXFUMMZ
      UHZULZGUNFUNZXBWIQZXCXJGWKUOFWJUOXLFGWJWKXBWOWQUPXJFGWJWKUQURXKXMFGXKAHUK
      ZOZFHUSZUTZBXNOZGHUSZUTZULZXJULZHPVAZXMXHYAHPVAZXJYCXHXQHPVAZXTHPVAZULYDX
      EYEXGYFWNXEYEVBCHAXDFVCVDSWPXGYFVBDHBXFGVCVDSVEXQXTHPVFVGYAXJHPVHVIYCWHXN
      OZEHUSZUTZHPVAZXMYBYIHPXNPQZYAXJYIYKYAULYGXIXNQZXJYHYAYGXPXSULZYKYLYGXOXR
