    $( Lemma for ~ h1de2ci . $)
    h1de2ctlem $p |- ( A e. ( _|_ ` ( _|_ ` { B } ) ) <->
                 E. x e. CC A = ( x .h B ) ) $=
      ( csn cort cfv wcel csm co wceq c0v cc0 fveq2d chil ax-mp opreq1 eqeq2d
      cc cv wrex wi sneq eleq2d elisseti elsnc hsn0elch eleq2i ax-hvmul0 eqeq2i
