    UAUFLUDBAUCMNOUBUEUFAPAQRST $.
    $( [3-Jun-2004] $)

  $( A member of a span of a singleton is a vector. $)
  elspansncl $p |- ( ( A e. ~H /\ B e. ( span ` { A } ) ) -> B e. ~H ) $=
    ( csn chil wss cspn cfv wcel elspancl snssi sylan ) ACZDEBLFGHBDHADHLBIADJK
    $.
    $( [17-Dec-2004] $)

