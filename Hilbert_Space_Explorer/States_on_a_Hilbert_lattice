    $( Sum of a Hilbert-space-valued state of a lattice element and its
       orthocomplement. $)
    hstoc $p |- ( ( S e. CHStates /\ A e. CH ) ->
                  ( ( S ` A ) +h ( S ` ( _|_ ` A ) ) ) = ( S ` ~H ) ) $=
      ( chst wcel cch wa cort cfv chj co cva chil wss wceq choccl adantl csh
      chsh shococss syl jca hstosum mpdan chjo fveq2d eqtr3d ) BCDZAEDZFZAAGHZI
