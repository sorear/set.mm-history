  $( 4 is not a prime number.  (Contributed by Paul Chapman, 22-Jun-2011.)
     (The proof was shortened by Mario Carneiro, 18-Feb-2014.) $)
  4nprm $p |- -. 4 e. Prime $=
    ( c2 c4 2nn 1lt2 cmul co 2t2e4 eqcomi nprmi ) AABCCDDAAEFBGHI $.
    $( [18-Feb-2014] $) $( [22-Jun-2011] $)

  $( Two ways to write the number 25.  (Contributed by Mario Carneiro,
     18-Feb-2014.) $)
  5t5e25 $p |- ( 5 x. 5 ) = ( ( 4 x. 6 ) + 1 ) $=
    ( c4 c6 cmul co c1 caddc 5nn nnnn0i 1nn0 dec5 df-6 5re recni mulid2i oveq1i
    c5 eqtr4i decmul1c eqcomi ) ABCDEFDPPCDEEBEPEPPGHIIJIIBPEFDEPCDZEFDKTPEFPPL
    MNZOQAECDEFDPTJUAQRS $.
    $( [18-Feb-2014] $)

  ${
    $d N x $.
    prmlem0.1 $e |- ( x e. ( ZZ>= ` M ) ->
                      ( ( x e. Prime /\ ( x ^ 2 ) <_ N ) -> -. x || N ) ) $.
    prmlem0.2 $e |- ( K e. Prime -> -. K || N ) $.
    prmlem0.3 $e |- M = ( K + 1 ) $.
    $( Lemma for ~ prmlem1 and ~ prmlem2 . $)
    prmlem0 $p |- ( x e. ( ZZ>= ` K ) ->
                    ( ( x e. Prime /\ ( x ^ 2 ) <_ N ) -> -. x || N ) ) $=
      ( cv cuz cfv wcel wceq c1 caddc co cprime wbr cdivides wn wi wo c2 cle wa
      cexp uzp1 eleq1 breq1 notbid imbi12d mpbiri eqcomi fveq2i eleq2s jaoi syl
      adantrd ) AHZBIJKURBLZURBMNOZIJZKZUAURPKZURUBUEODUCQZUDURDRQZSZTZBURUFUSV
      GVBUSVCVFVDUSVCVFTBPKZBDRQZSZTFUSVCVHVFVJURBPUGUSVEVIURBDRUHUIUJUKUQVGURC
      IJVAEUTCICUTGULUMUNUOUP $.
      $( [18-Feb-2014] $)
  $}

  ${
    $d N x $.
    prmlem1.n $e |- N e. NN $.
    prmlem1.lt $e |- N < ( ( 4 x. 6 ) + 1 ) $.
    prmlem1.gt $e |- 1 < N $.
    prmlem1.2 $e |- -. 2 || N $.
    prmlem1.3 $e |- -. 3 || N $.
    $( A quick proof skeleton to show that the numbers less than 25 (= 121 in
       base 4) are prime, by trial division.  (Contributed by Mario Carneiro,
       18-Feb-2014.) $)
    prmlem1 $p |- N e. Prime $=
      ( vx cprime wcel c2 co cle wbr cdivides wn clt c3 c4 c5 cr cuz cv cexp wi
