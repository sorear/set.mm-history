  @{
    @d m n x y A @.  @d m n x y M @.  @d m n x y k @.
    @( Series sum with an infinite index set (i.e. an infinite series). @)
    dfisumNEW @p |- ( M e. ZZ -> sum_ k e. ( ZZ>= ` M ) A =
       U. { x |
    ( <. M , + >. seq ( { <. k , y >. | y = A } |` ZZ ) ) ~~> x } ) @=
      ? @.
      @( [11-Dec-2005] @)
  @}

  @{
    @d m n x y A @.  @d m n x y M @.  @d m n x y N @.  @d k m n x y @.
    @( Special case of series sum over a finite index set. @)
    dffsumNEW @p |- ( N e. ( ZZ>= ` M ) -> sum_ k e. ( M ... N ) A =
       ( ( <. M , + >. seq ( { <. k , y >. | y = A } |` ZZ ) ) ` N ) ) @=
      ? @.
      @( [11-Dec-2005] @)
  @}
