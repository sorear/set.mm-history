  ${
    $( A double modus ponens inference. $)
    mp2b.1 $e |- ph $.
    mp2b.2 $e |- ( ph -> ps ) $.
    mp2b.3 $e |- ( ps -> ch ) $.
    $( PLEASE PUT DESCRIPTION HERE. $)
    mp2b $p |- ch $=
      ( ax-mp ) BCABDEGFG $.
