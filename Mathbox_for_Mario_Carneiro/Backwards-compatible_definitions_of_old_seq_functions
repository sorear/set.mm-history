$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
            Backwards-compatible definitions of old "seq" functions
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d f g p $.
    $( Define the 1-based sequence in terms of a general sequence. $)
    df-seq1NEW $a |- seq1 = ( p e. _V , f e. _V |->
      ( seqNEW ( p , f ) ` 1 ) ) $.

    $( Define the 0-based sequence in terms of a general sequence. $)
    df-seq0NEW $a |- seq0 = ( p e. _V , f e. _V |->
      ( seqNEW ( p , f ) ` 0 ) ) $.

    $( Define the old general sequence from the new definition. $)
    df-seqzNEW $a |- seq = ( g e. _V , f e. _V |->
      ( seqNEW ( ( 2nd ` g ) , f ) ` ( 1st ` g ) ) ) $.
  $}

  ${
    $d F y $.  $d S x y $.
    seq1valNEW.1 $e |- S e. _V $.
    seq1valNEW.2 $e |- F e. _V $.
    $( Value of the 1-based recursive sequence builder operation.  (Revised by
       Mario Carneiro, 28-Jun-2013.) $)
    seq1fval $p |- ( S seq1 F ) = ( seqNEW ( S , F ) ` 1 ) $=
      ( vx vy cvv wcel cseq1 co c1 cseq cfv wceq cv seqeq1 fveq1d seqeq2
      df-seq1NEW fvex ovmpt2 mp2an ) AGHBGHABIJKABLZMZNCDEFABGGKEOZFOZLZMUDIKAU
      FLZMUEANKUGUHUEAUFPQUFBNKUHUCAUFBRQFESKUCTUAUB $.
      $( [28-Jun-2013] $) $( [3-Aug-2005] $)

    $( Value of the recursive sequence builder at 1.  See description in
       ~ df-seq1NEW .  (Revised by Mario Carneiro, 28-Jun-2013.) $)
    seq11NEW $p |- ( ( S seq1 F ) ` 1 ) = ( F ` 1 ) $=
      ( c1 cseq1 co cfv cseq seq1fval fveq1i cz wcel wceq 1z seq1NEW ax-mp
      eqtri ) EABFGZHEEABIHZHZEBHZESTABCDJKELMUAUBNOABEPQR $.
      $( [28-Jun-2013] $) $( [5-Oct-2004] $)

    $( Value of the recursive sequence builder at a successor.  See description
       in ~ df-seq1NEW .  (Revised by Mario Carneiro, 28-Jun-2013.) $)
    seq1p1NEW $p |- ( A e. NN -> ( ( S seq1 F ) ` ( A + 1 ) ) =
                ( ( ( S seq1 F ) ` A ) S ( F ` ( A + 1 ) ) ) ) $=
      ( cn wcel c1 caddc co cseq cfv cseq1 wceq cuz seqp1 nnuz eleq2s seq1fval
      fveq1i opreq1i 3eqtr4g ) AFGAHIJZHBCKLZLZAUDLZUCCLZBJZUCBCMJZLAUILZUGBJUE
      UHNAHOLFBCHAPQRUCUIUDBCDESZTUJUFUGBAUIUDUKTUAUB $.
      $( [28-Jun-2013] $) $( [5-Oct-2004] $)

    $( Functionality and domain of the recursive sequence builder.  (Revised by
       Mario Carneiro, 28-Jun-2013.) $)
    seq1fnNEW $p |- ( S seq1 F ) Fn NN $=
      ( cseq1 co cn wfn c1 cseq cfv cuz cz wcel 1z seqfn ax-mp seq1fval fneq1i
      nnuz fneq2i bitri mpbir ) ABEFZGHZIABJKZILKZHZIMNUHOABIPQUEUFGHUHGUDUFABC
      DRSGUGUFTUAUBUC $.
      $( [28-Jun-2013] $) $( [8-Oct-2004] $)
  $}

  ${
    $d F m y $.  $d M m $.  $d S m x y $.
    seq0valNEW.1 $e |- S e. _V $.
    seq0valNEW.2 $e |- F e. _V $.
    $( Value of the 0-based recursive sequence builder operation.  (Revised by
       Mario Carneiro, 28-Jun-2013.) $)
    seq0fvalNEW $p |- ( S seq0 F ) = ( seqNEW ( S , F ) ` 0 ) $=
      ( vx vy cvv wcel cseq0 co cc0 cseq cfv wceq cv seqeq1 fveq1d seqeq2
      df-seq0NEW fvex ovmpt2 mp2an ) AGHBGHABIJKABLZMZNCDEFABGGKEOZFOZLZMUDIKAU
      FLZMUEANKUGUHUEAUFPQUFBNKUHUCAUFBRQFESKUCTUAUB $.
      $( [28-Jun-2013] $) $( [3-Aug-2005] $)

    $( Value of the arbitrary-based recursive sequence builder operation.
       (Revised by Mario Carneiro, 28-Jun-2013.) $)
    seqzfvalNEW $p |- ( M e. ZZ ->
