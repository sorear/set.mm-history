      ( cz wcel cop cseqz co cuz cfv wfn cseq seqfn seqzfvalNEW fneq1d mpbird
      ) CFGZCAHBIJZCKLZMCABNLZUAMABCOSUATUBABCDEPQR $.
      $( [28-Jun-2013] $) $( [2-Sep-2005] $)

    $( The 1-based recursive sequence in terms of the arbitrary-based one.
       (Revised by Mario Carneiro, 28-Jun-2013.) $)
    seq1seqzNEW $p |- ( S seq1 F ) = ( <. 1 , S >. seq F ) $=
      ( cseq1 co c1 cseq cfv cop cseqz seq1fval cz wcel wceq 1z seqzfvalNEW
      ax-mp eqtr4i ) ABEFGABHIZGAJBKFZABCDLGMNUATOPABGCDQRS $.
      $( [28-Jun-2013] $) $( [1-Sep-2005] $)

    $( The 0-based recursive sequence in terms of the arbitrary-based one.
       (Revised by Mario Carneiro, 28-Jun-2013.) $)
    seq0seqzNEW $p |- ( S seq0 F ) = ( <. 0 , S >. seq F ) $=
      ( cseq0 co cc0 cseq cfv cop cseqz seq0fvalNEW cz wcel wceq 0z
      seqzfvalNEW ax-mp eqtr4i ) ABEFGABHIZGAJBKFZABCDLGMNUATOPABGCDQRS $.
      $( [28-Jun-2013] $) $( [1-Sep-2005] $)

    $( Functionality and domain of the 0-based recursive sequence builder.
