      @( Lemma for ~ ovoliun . @)
      ovoliunlem2 @p |- ( A. n e. NN ( A C_ RR /\ ( vol* ` A ) e. RR ) ->
                      ( vol* ` U_ n e. NN A ) <_ sup ( ran T , RR* , < ) ) @=
        ? @.
    @}

    @( The Lebesgue outer measure function is countably sub-additive. (Many
       books allow ` +oo ` as a value for one of the sets in the sum, but in
       our setup we can't do arithmetic on infinity, and in any case the volume
       of a union containing an infinitely large set is already infinitely
       large by monotonicity ~ ovolss , so we need not consider this case
       here, although we do allow the sum itself to be infinite.) @)
    ovoliun @p |- ( A. n e. NN ( A C_ RR /\ ( vol* ` A ) e. RR ) ->
                    ( vol* ` U_ n e. NN A ) <_ sup ( ran T , RR* , < ) ) @=
      ? @.

    @( The Lebesgue outer measure function is countably sub-additive. (This
       version is a little easier to read, but does not allow infinite values
       like ~ ovoliun .) @)
    ovoliun2 @p |- ( ( A. n e. NN ( A C_ RR /\ ( vol* ` A ) e. RR ) /\
                       T e. dom ~~> ) ->
                     ( vol* ` U_ n e. NN A ) <_ sum_ n e. NN ( vol* ` A ) ) @=
      ? @.
  @}
  $)

