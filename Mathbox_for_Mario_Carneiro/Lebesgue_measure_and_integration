  $(
  @{
    ovolun.1 @e |- M = { y e. RR* |
      E. f e. ( ( < i^i ( RR X. RR ) ) ^m NN ) ( A C_ U. ran ( (,) o. f ) /\
          y = sup ( ran seq 1 ( + , ( ( abs o. - ) o. f ) ) , RR* , < ) ) } @.
    ovolun.2 @e |- S = seq 1 ( + , ( ( abs o. - ) o. F ) ) @.
    ovolun.3 @e |- T = seq 1 ( + , ( ( abs o. - ) o. G ) ) @.
    ovolun.4 @e |- ( ph -> ( A C_ RR /\ ( vol* ` A ) e. RR ) ) @.
    ovolun.5 @e |- ( ph -> ( B C_ RR /\ ( vol* ` B ) e. RR ) ) @.
    ovolun.6 @e |- ( ph -> C e. RR+ ) @.
    ovolun.7 @e |- ( ph -> ( F e. ( ( < i^i ( RR X. RR ) ) ^m NN ) /\
     ( A C_ U. ran ( (,) o. F ) /\ sup ( ran T , RR* , < ) <
            ( ( vol* ` A ) + ( C / 2 ) ) ) ) ) @.
    ovolun.8 @e |- ( ph -> ( G e. ( ( < i^i ( RR X. RR ) ) ^m NN ) /\
     ( A C_ U. ran ( (,) o. G ) /\ sup ( ran U , RR* , < ) <
            ( ( vol* ` B ) + ( C / 2 ) ) ) ) ) @.
    @( Lemma for ~ ovolun . @)
    ovolunlem1 @p |- ( ph ->
        ( vol* ` ( A u. B ) ) <_ ( ( ( vol* ` A ) + ( vol* ` B ) ) + C ) ) @=
      ? @.
  @}

  @{
    @( The Lebesgue outer measure function is finitely sub-additive. (Unlike
       the stronger ~ ovoliun , this does not require any choice principles.)
       @)
    ovolun @p |- ( ( ( A C_ RR /\ ( vol* ` A ) e. RR ) /\
                      ( B C_ RR /\ ( vol* ` B ) e. RR ) ) ->
                  ( vol* ` ( A u. B ) ) <_ ( ( vol* ` A ) + ( vol* ` B ) ) ) @=
      ? @.
  @}

  @{
    fubsum.1 @e |- ( ph -> J : NN -1-1-onto-> ( NN x. NN ) ) @.
    fubsum.2 @e |- ( ph -> A. x e. NN A. y e. NN
                     ( ( x F y ) e. RR /\ 0 <_ ( x F y ) ) ) @.
    @{
      fubsum.3 @e |- ( ph -> A. x e. NN A. y e. NN
                       ( ( x F y ) e. RR /\ 0 <_ ( x F y ) ) ) @.
      @( Lemma for ~ fubsum . @)
      fubsumlem1 @p |- ( ph -> sum_ n e. NN ( F ` ( J ` n ) ) <_
                     sum_ i e. NN sum_ j e. NN ( i F j ) ) @=
        ? @.
    @}

    @( Fubini's theorem for sums. @)
    fubsum @p |- ( ph -> sum_ n e. NN ( F ` ( J ` n ) ) =
                   sum_ i e. NN sum_ j e. NN ( i F j ) ) @=
      ? @.
  @}

  @{
    ovoliun.t @e |- T = seq 1 ( + , G ) @.
    ovoliun.g @e |- G = ( n e. NN |-> ( vol* ` A ) ) @.
    @{
      ovoliun.s @e |- S = seq 1 ( + , ( ( abs o. - ) o. ( F ` n ) ) ) @.
      ovoliun.m @e |- M = { y e. RR* |
        E. f e. ( ( < i^i ( RR X. RR ) ) ^m NN ) ( A C_ U. ran ( (,) o. f ) /\
           y = sup ( ran seq 1 ( + , ( ( abs o. - ) o. f ) ) , RR* , < ) ) } @.
      @{
        ovoliun.u @e |- U = seq 1 ( + , ( ( abs o. - ) o. H ) ) @.
        ovoliun.n @e |- N = { y e. RR* |
                           E. f e. ( ( < i^i ( RR X. RR ) ) ^m NN )
                             ( U_ n e. NN A C_ U. ran ( (,) o. f ) /\
           y = sup ( ran seq 1 ( + , ( ( abs o. - ) o. f ) ) , RR* , < ) ) } @.
        ovoliun.h @e |- H = ( k e. NN |-> ( ( F ` ( 1st ` ( J ` k ) ) ) `
                                                   ( 2nd ` ( J ` k ) ) ) ) @.
        ovoliun.dj @e |- ( ph -> A. n ph ) @.
        ovoliun.a @e |- ( ph -> A. n e. NN
                          ( A C_ RR /\ ( vol* ` A ) e. RR ) ) @.
        ovoliun.r @e |- ( ph -> sup ( ran T , RR* , < ) e. RR ) @.
        ovoliun.b @e |- ( ph -> B e. RR+ ) @.
        ovoliun.j @e |- ( ph -> J : NN -1-1-onto-> ( NN X. NN ) ) @.
        ovoliun.f @e |- ( ph
               -> F : NN --> ( ( < i^i ( RR X. RR ) ) ^m NN ) ) @.
        ovoliun.x @e |- ( ph -> A. n e. NN
              ( A C_ U. ran ( (,) o. ( F ` n ) ) /\ sup ( ran S , RR* , < ) <
                ( ( vol* ` A ) + ( B / ( 2 ^ n ) ) ) ) ) @.
        @{
          ovoliun.k @e |- ( ( ph /\ ps ) -> K e. NN ) @.
          ovoliun.l1 @e |- ( ( ph /\ ps ) -> L e. ZZ ) @.
          ovoliun.l2 @e |- ( ( ph /\ ps ) ->
                            A. w e. ( 1 ... K ) ( 1st ` ( J ` w ) ) <_ L ) @.
          @( Lemma for ~ ovoliun . @)
          ovoliunlm3 @p |- ( ( ph /\ ps ) ->
                ( U ` K ) <_ ( sup ( ran T , RR* , < ) + B ) ) @=
            ? @.
        @}

        @( Lemma for ~ ovoliun . @)
        ovoliunlem1 @p |- ( ph ->
              ( vol* ` U_ n e. NN A ) <_ ( sup ( ran T , RR* , < ) + B ) ) @=
          ( vm vz vj vw vx cn ciun covol cfv crn cxr clt csup cle caddc co ccnv
          wbr cr wss wceq wral wcel wa simpl ralimi syl iunss sylibr ovolval cv
          cioo ccom cuni cabs cmin c1 cseq cxp cmap wrex crab ssrab2 eqsstri wf
          cin c2nd c1st adantr wf1o f1of ffvelrn sylan xp1st syl11anc reex xpex
          inex2 nnex elmap sylib xp2nd ex fmptd eliun wi ra4 imp simpld 3adant3
