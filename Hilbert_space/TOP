      JABCDUBAUICURUBUCUDUEUFUG $.
      $( [12-Aug-04] $) $( [7-Aug-04] $)

    ${
      cmcmi.1 $e |- A Com B $.
      $( Commutation is symmetric.  Theorem 2(v) of [Kalmbach] p. 22. $)
      cmcmi $p |- B Com A $=
        ( ccm wbr cmcm mpbi ) ABFGBAFGEABCDHI $.
        $( [12-Aug-04] $) $( [7-Aug-04] $)

