$(
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
                            Hilbert space
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
$)

  $( Include set.mm as the starting point (commented out since it's above) $)
  $( $[ set.mm $] $)

  $( Axioms for a Hilbert space $)
  $( We assume we've constructed a fixed constant class ` H~ ` and
     associated operations, and we postulate their properties. $)

  $c H~ $.
  $c +v $.
  $c .s $.
  $c 0v $.
  $c -v $.
  $c .i $.
  $c norm $.
  $c Cauchy $.
  $c ~~>v $.
  $c SH $.
  $c CH $.
  $c _|_ $.
  $c +H $.
  $c span $.
  $c vH $.
  $c \/H $.
  $c 0H $.
  $c Com $.
  $c Proj $.
  $c +P $.
  $c -P $.
  $c States $.
  $c Atoms $.
  $c Mod $.
  $c <o $.

  $( Extend class notation with Hilbert vector space. $)
  chil $a class H~ $.
  $( Extend class notation with vector addition on Hilbert space.  In
     the literature, the subscript "v" is omitted, but we need it to
     avoid ambiguity with complex number addition ` + ` ~ caddc . $)
  cva $a class +v $.
  $( Extend class notation with scalar multiplication on Hilbert space.
     In the literature scalar multiplication is usually indicated by
     juxtaposition, but we need an explicit symbol to prevent ambiguity. $)
  csm $a class .s $.
  $( Extend class notation with zero vector in Hilbert space. $)
  c0v $a class 0v $.
  $( Extend class notation with vector subtraction in Hilbert space. $)
  cmv $a class -v $.
  $( Extend class notation with inner (scalar) product in Hilbert space.
     In the literature, the inner product of ` A ` and ` B ` is usually
     written ` <. A , B >. ` but our operation notation allows us to use
     existing theorems about operations and also eliminates ambiguity with
     the definition of an ordered pair ~ df-op . $)
  csp $a class .i $.
  $( Extend class notation with the norm function in Hilbert space.
     In the literature, the norm of ` A ` is usually  written "|| ` A ` ||",
     but we use function notation to take advantage of our existing theorems
     concerning functions. $)
  cno $a class norm $.
  $( Extend class notation with set of Cauchy sequence in Hilbert space. $)
  ccau $a class Cauchy $.
  $( Extend class notation with convergence relation in Hilbert space. $)
  chli $a class ~~>v $.
  $( Extend class notation with set of subspaces of a Hilbert space. $)
  csh $a class SH $.
  $( Extend class notation with set of closed subspaces of a Hilbert space. $)
  cch $a class CH $.
  $( Extend class notation with orthogonal complement in ` CH ` . $)
  cort $a class _|_ $.
  $( Extend class notation with subspace sum in ` CH ` . $)
  cph $a class +H $.
  $( Extend class notation with subspace span in ` CH ` . $)
  cspn $a class span $.
  $( Extend class notation with join in ` CH ` . $)
  chj $a class vH $.
  $( Extend class notation with supremum of collection in ` CH ` . $)
  chsup $a class \/H $.
  $( Extend class notation with zero of ` CH ` . $)
  c0h $a class 0H $.
  $( Extend class notation with the commutes relation on a Hilbert lattice. $)
  ccm $a class Com $.
  $( Extend class notation with set of projections on a Hilbert space. $)
  cpj $a class Proj $.
  $( Extend class notation with sum of projections on a Hilbert space. $)
  chos $a class +P $.
  $( Extend class notation with projection difference on a Hilbert space. $)
  cpjd $a class -P $.
  $( Extend class notation with set of states on a Hilbert lattice. $)
  cst $a class States $.
  $( Extend class notation with set of atoms on a Hilbert lattice. $)
  cat $a class Atoms $.
  $( Extend class notation with the covers relation on a Hilbert lattice. $)
  ccv $a class <o $.
  $( Extend class notation with the modular pair relation on a Hilbert
     lattice. $)
  cmd $a class Mod $.

  $( This is our primary postulate for Hilbert space, which is the foundation
     for quantum mechanics and quantum field theory.  We assume that there
     exists a primitive class, ` H~ ` , which contains objects called vectors
     whose properties are postulated in this axiom (which asserts that it
     is a set) and the axioms ~ ax-hvaddcl , ~ ax-hvcom , ~ ax-hvass ,
     ~ ax-hvzercl , ~ ax-hvaddid , ~ ax-hvmulcl , ~ ax-hvmulid ,
     ~ ax-hvmulass , ~ ax-hvdistr1 , ~ ax-hvdistr2 , ~ ax-hvmulzer ,
     ~ ax-hicl , ~ ax-his1 , ~ ax-his2 , ~ ax-his3 , ~ ax-his4 , and
     ~ ax-hcompl . . $)
  ax-hilex $a |- H~ e. V $.

  $( Closure of vector addition. $)
  ax-hvaddcl $a |- ( ( A e. H~ /\ B e. H~ ) -> ( A +v B ) e. H~ ) $.

  $( Vector addition is commutative. $)
  ax-hvcom $a |- ( ( A e. H~ /\ B e. H~ ) -> ( A +v B ) = ( B +v A ) ) $.

  $( Vector addition is associative. $)
  ax-hvass $a |- ( ( A e. H~ /\ B e. H~ /\ C e. H~ ) ->
                 ( ( A +v B ) +v C ) = ( A +v ( B +v C ) ) ) $.

  $( The zero vector is in the vector space. $)
  ax-hvzercl $a |- 0v e. H~ $.

  $( Addition with the zero vector. $)
  ax-hvaddid $a |- ( A e. H~ -> ( A +v 0v ) = A ) $.

  $( Closure of scalar multiplication. $)
  ax-hvmulcl $a |- ( ( A e. CC /\ B e. H~ ) -> ( A .s B ) e. H~ ) $.

  ${
    hvmulcl.1 $e |- A e. CC $.
    hvmulcl.2 $e |- B e. H~ $.
    $( Closure inference for scalar multiplication. $)
    hvmulcl $p |- ( A .s B ) e. H~ $=
      ( cc wcel chil csm co ax-hvmulcl mp2an ) AEFBGFABHIGFCDABJK $.
      $( [1-Aug-99] $)
  $}

  $( Scalar multiplication by one. $)
  ax-hvmulid $a |- ( A e. H~ -> ( 1 .s A ) = A ) $.

  $( Scalar multiplication associative law $)
  ax-hvmulass $a |- ( ( A e. CC /\ B e. CC /\ C e. H~ ) ->
                 ( ( A x. B ) .s C ) = ( A .s ( B .s C ) ) ) $.

  $( Scalar multiplication distributive law $)
  ax-hvdistr1 $a |- ( ( A e. CC /\ B e. H~ /\ C e. H~ ) ->
                ( A .s ( B +v C ) ) = ( ( A .s B ) +v ( A .s C ) ) ) $.

  $( Scalar multiplication distributive law $)
  ax-hvdistr2 $a |- ( ( A e. CC /\ B e. CC /\ C e. H~ ) ->
                ( ( A + B ) .s C ) = ( ( A .s C ) +v ( B .s C ) ) ) $.

  $( Scalar multiplication by zero.  We can derive the existence of the
     negative of a vector from this axiom (see ~ hvsubidt and ~ hvsubvalt ). $)
  ax-hvmulzer $a |- ( A e. H~ -> ( 0 .s A ) = 0v ) $.

  ${
    $d x y z $.
    $( Define vector subtraction.  See ~ hvsubval for its value and
       ~ hvsubcl for its closure. $)
    df-hvsub $a |- -v = { <. <. x , y >. , z >. | ( ( x e. H~ /\ y e. H~ )
                   /\ z = ( x +v ( -u 1 .s y ) ) ) } $.
  $}

  ${
    $d x y z A $.  $d x y z B $.
    $( Value of vector subtraction. $)
    hvsubvalt $p |- ( ( A e. H~ /\ B e. H~ ) ->
                    ( A -v B ) = ( A +v ( -u 1 .s B ) ) ) $=
      ( vx vy vz chil cv c1 cneg csm co cva cmv oprex opreq1 wceq opreq2
      opreq2d df-hvsub oprabval2 ) CDEABFFCGZHIZDGZJKZLKAUBBJKZLKMAUDLKAUELNUAA
      UDLOUCBPUDUEALUCBUBJQRCDEST $.
      $( [5-Sep-99] $)
  $}

  $( Closure of vector subtraction. $)
  hvsubclt $p |- ( ( A e. H~ /\ B e. H~ ) -> ( A -v B ) e. H~ ) $=
    ( chil wcel wa cmv co c1 cneg csm cva hvsubvalt ax-hvaddcl cc 1cn negcl
    ax-hvmulcl mpan sylan5 eqeltrd ) ACDZBCDZEABFGAHIZBJGZKGZCABLUAUDCDZUECDUBA
    UDMUCNDUBUFHOPUCBQRST $.
    $( [17-Aug-99] $)

  ${
    hvaddcl.1 $e |- A e. H~ $.
    hvaddcl.2 $e |- B e. H~ $.
    $( Closure of vector addition. $)
    hvaddcl $p |- ( A +v B ) e. H~ $=
      ( chil wcel cva co ax-hvaddcl mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [1-Aug-99] $)

    $( Commutation of vector addition. $)
    hvcom $p |- ( A +v B ) = ( B +v A ) $=
      ( chil wcel cva co wceq ax-hvcom mp2an ) AEFBEFABGHBAGHICDABJK $.
      $( [3-Sep-99] $)

    $( Value of vector subtraction definition. $)
    hvsubval $p |- ( A -v B ) = ( A +v ( -u 1 .s B ) ) $=
      ( chil wcel cmv co c1 cneg csm cva wceq hvsubvalt mp2an ) AEFBEFABGHAIJBK
      HLHMCDABNO $.
      $( [3-Sep-99] $)

    $( Closure of vector subtraction. $)
    hvsubcl $p |- ( A -v B ) e. H~ $=
      ( chil wcel cmv co hvsubclt mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [2-Aug-99] $)
  $}

  $( Addition with the zero vector. $)
  hvaddid2t $p |- ( A e. H~ -> ( 0v +v A ) = A ) $=
    ( chil wcel c0v cva co wceq ax-hvzercl ax-hvcom mpan2 ax-hvaddid eqtr3d )
    ABCZADEFZDAEFZAMDBCNOGHADIJAKL $.
    $( [18-Oct-99] $)

  $( Scalar multiplication with the zero vector. $)
  hvmul0t $p |- ( A e. CC -> ( A .s 0v ) = 0v ) $=
    ( cc wcel c0v cc0 csm co cmulc mulzer1t opreq1d chil wceq ax-hvzercl
    ax-hvmulzer ax-mp syl6eq 0cn ax-hvmulass mp3an2 mpan2 eqtr3d opreq2i
    syl6req ) ABCZDAEDFGZFGZADFGUDAEHGZDFGZDUFUDUHUEDUDUGEDFAIJDKCZUEDLMDNOZPUD
    UIUHUFLZMUDEBCUIUKQAEDRSTUAUEDAFUJUBUC $.
    $( [30-May-99] $)

  $( Subtraction of a vector from itself. $)
  hvsubidt $p |- ( A e. H~ -> ( A -v A ) = 0v ) $=
    ( chil wcel cmv co cc0 csm c0v c1 cneg caddc cva ax-hvmulid opreq1d cc
    wceq 1cn negcl ax-hvdistr2 mp3an1 mpan hvsubvalt anidms 3eqtr4rd negid
    opreq1i syl6eq ax-hvmulzer eqtrd ) ABCZAADEZFAGEZHUJUKIIJZKEZAGEZULUJIAGEZU
    MAGEZLEZAUQLEZUOUKUJUPAUQLAMNUMOCZUJUOURPZIQRIOCUTUJVAQIUMASTUAUJUKUSPAAUBU
    CUDUNFAGIQUEUFUGAUHUI $.
    $( [30-May-99] $)

  $( Addition of negative of a vector to itself. $)
  hvnegidt $p |- ( A e. H~ -> ( A +v ( -u 1 .s A ) ) = 0v ) $=
    ( chil wcel cmv co c1 cneg csm cva c0v wceq hvsubvalt anidms hvsubidt
    eqtr3d ) ABCZAADEZAFGAHEIEZJPQRKAALMANO $.
    $( [3-Jun-04] $) $( [4-Jun-04] $)

  $( Convert minus one times a scalar product to the negative of the scalar. $)
  hvm1negt $p |- ( ( A e. CC /\ B e. H~ ) ->
                 ( -u 1 .s ( A .s B ) ) = ( -u A .s B ) ) $=
    ( cc wcel chil wa c1 cneg cmulc co csm wceq 1cn negcl ax-hvmulass mp3an1
    mulm1t adantr opreq1d eqtr3d ) ACDZBEDZFZGHZAIJZBKJZUDABKJKJZAHZBKJUDCDUAUB
    UFUGLGMNUDABOPUCUEUHBKUAUEUHLUBAQRST $.
    $( [8-Jun-04] $) $( [4-Jun-04] $)

  ${
    hvaddid2.1 $e |- A e. H~ $.
    $( Addition with the zero vector. $)
    hvaddid2 $p |- ( 0v +v A ) = A $=
      ( chil wcel c0v cva co wceq hvaddid2t ax-mp ) ACDEAFGAHBAIJ $.
      $( [18-Aug-99] $)

    $( Addition of negative of a vector to itself. $)
    hvnegid $p |- ( A +v ( -u 1 .s A ) ) = 0v $=
      ( chil wcel c1 cneg csm co cva c0v wceq hvnegidt ax-mp ) ACDAEFAGHIHJKBAL
      M $.
      $( [18-Aug-99] $)

    $( Two ways of expressing negative. $)
    hv2neg $p |- ( 0v -v A ) = ( -u 1 .s A ) $=
      ( c0v cmv co c1 cneg csm cva ax-hvzercl hvsubval 1cn negcl hvmulcl
      hvaddid2 eqtr ) CADECFGZAHEZIERCAJBKRQAFLMBNOP $.
      $( [31-Oct-99] $)
  $}

  $( Commutative/associative law. $)
  hvadd23t $p |- ( ( A e. H~ /\ B e. H~ /\ C e. H~ ) ->
               ( ( A +v B ) +v C ) = ( ( A +v C ) +v B ) ) $=
    ( chil wcel w3a cva co wceq wa ax-hvcom opreq2d 3adant1 ax-hvass 3com23
    3eqtr4d ) ADEZBDEZCDEZFABCGHZGHZACBGHZGHZABGHCGHACGHBGHZRSUAUCIQRSJTUBAGBCK
    LMABCNQSRUDUCIACBNOP $.
    $( [16-Oct-99] $)

  $( Commutative/associative law. $)
  hvadd12t $p |- ( ( A e. H~ /\ B e. H~ /\ C e. H~ ) ->
               ( A +v ( B +v C ) ) = ( B +v ( A +v C ) ) ) $=
    ( chil wcel w3a cva co wceq wa ax-hvcom opreq1d 3adant3 ax-hvass 3com12
    3eqtr3d ) ADEZBDEZCDEZFABGHZCGHZBAGHZCGHZABCGHGHBACGHGHZQRUAUCISQRJTUBCGABK
    LMABCNRQSUCUDIBACNOP $.
    $( [19-Oct-99] $)

  $( Hilbert vector space addition law. $)
  hvadd4t $p |- ( ( ( A e. H~ /\ B e. H~ ) /\ ( C e. H~ /\ D e. H~ ) ) ->
               ( ( A +v B ) +v ( C +v D ) ) = ( ( A +v C ) +v ( B +v D ) ) ) $=
    ( chil wcel wa cva co wceq w3a hvadd23t opreq1d 3expa adantrr ax-hvass
    3expb ax-hvaddcl sylan an4s 3eqtr3d ) AEFZBEFZGZCEFZDEFZGZGABHIZCHIZDHIZACH
    IZBHIZDHIZUHCDHIHIZUKBDHIHIZUDUEUJUMJZUFUBUCUEUPUBUCUEKUIULDHABCLMNOUHEFZUG
    UJUNJZUDUQUEUFURUHCDPQABRSUBUEUCUFUMUOJZUKEFZUCUFGUSUBUEGUTUCUFUSUKBDPQACRS
    TUA $.
    $( [16-Oct-99] $)

  $( Hilbert vector space addition/subtraction law. $)
  hvsub4t $p |- ( ( ( A e. H~ /\ B e. H~ ) /\ ( C e. H~ /\ D e. H~ ) ) ->
               ( ( A +v B ) -v ( C +v D ) ) = ( ( A -v C ) +v ( B -v D ) ) ) $=
    ( chil wcel wa cva co c1 cneg csm cmv wceq cc 1cn negcl ax-hvdistr1 mp3an1
    adantl opreq2d ax-hvmulcl mpan imlan im2an an4s hvadd4t syl eqtr4d
    hvsubvalt ax-hvaddcl syl2an adantrr adantlr adantrl adantll opreq12d
    3eqtr4d ) AEFZBEFZGZCEFZDEFZGZGZABHIZJKZCDHIZLIZHIZAVGCLIZHIZBVGDLIZHIZHIZV
    FVHMIZACMIZBDMIZHIVEVJVFVKVMHIZHIZVOVEVIVSVFHVDVIVSNZVAVGOFZVBVCWAJPQZVGCDR
    STUAVEUSVKEFZGZUTVMEFZGZGZVOVTNUSVBUTVCWHUSVBGWEUTVCGWGVBWDUSWBVBWDWCVGCUBU
    CUDVCWFUTWBVCWFWCVGDUBUCUDUEUFAVKBVMUGUHUIVFEFVHEFVPVJNVAVDVFVHUJABUKCDUKUL
    VEVQVLVRVNHUSVDVQVLNZUTUSVBWIVCACUJUMUNUTVDVRVNNZUSUTVCWJVBBDUJUOUPUQUR $.
    $( [17-Oct-99] $)

  $( Commutative/associative law. $)
  hvaddsub12t $p |- ( ( A e. H~ /\ B e. H~ /\ C e. H~ ) ->
               ( A +v ( B -v C ) ) = ( B +v ( A -v C ) ) ) $=
    ( chil wcel w3a c1 cneg csm co cva cmv wceq hvadd12t cc 1cn negcl
    ax-hvmulcl mpan syl3an3 wa hvsubvalt opreq2d 3adant1 3adant2 3eqtr4d ) ADEZ
    BDEZCDEZFABGHZCIJZKJZKJZBAUKKJZKJZABCLJZKJZBACLJZKJZUGUHUKDEZUMUOMUIABUKNUJ
    OEUIUTGPQUJCRSTUHUIUQUMMUGUHUIUAUPULAKBCUBUCUDUGUIUSUOMUHUGUIUAURUNBKACUBUC
    UEUF $.
    $( [19-Oct-99] $)

  $( Addition/subtraction cancellation law for vectors in Hilbert space. $)
  hvsubcan1t $p |- ( ( A e. H~ /\ B e. H~ ) -> ( ( A +v B ) -v B ) = A ) $=
    ( chil wcel wa cva co cmv c1 cneg csm c0v wceq hvsubvalt ax-hvaddcl pm3.27
    sylan13 ax-hvass 3expb cc 1cn negcl ax-hvmulcl mpan ancli sylan5 eqtrd
    hvnegidt opreq2d adantl ax-hvaddid adantr 3eqtrd ) ACDZBCDZEZABFGZBHGZABIJZ
    BKGZFGZFGZALFGZAUPURUQUTFGZVBUQCDUOURVDMUPUQBNABOUNUOPQUNUOUTCDZEVDVBMZUOUN
    UOVEVFABUTRSUOVEUSTDUOVEIUAUBUSBUCUDUEUFUGUOVBVCMUNUOVALAFBUHUIUJUNVCAMUOAU
    KULUM $.
    $( [8-Jun-04] $) $( [7-Jun-04] $)

  $( Addition/subtraction cancellation law for vectors in Hilbert space. $)
  hvsubcan2t $p |- ( ( A e. H~ /\ B e. H~ ) -> ( ( A +v B ) -v A ) = B ) $=
    ( chil wcel cva co cmv wceq wa ax-hvcom opreq1d cleqcomd hvsubcan1t eqtrd
    ancoms ) BCDZACDZABEFZAGFZBHPQIZSBAEFZAGFZBTUBSTUARAGBAJKLBAMNO $.
    $( [8-Jun-04] $) $( [7-Jun-04] $)

  $( Associativity of sum and difference of Hilbert space vectors. $)
  hvaddsubasst $p |- ( ( A e. H~ /\ B e. H~ /\ C e. H~ ) -> ( ( A +v B ) -v C )
                     = ( A +v ( B -v C ) ) ) $=
    ( chil wcel w3a cva co c1 cneg csm cmv wceq ax-hvass cc 1cn negcl
    ax-hvmulcl mpan syl3an3 wa hvsubvalt ax-hvaddcl sylan 3impa 3adant1
    opreq2d 3eqtr4d ) ADEZBDEZCDEZFZABGHZIJZCKHZGHZABUOGHZGHZUMCLHZABCLHZGHUIUJ
    UODEZUPURMUKABUONUNOEUKVAIPQUNCRSTUIUJUKUSUPMZUMDEUKVBUIUJUAUMCUBABUCUDUEUL
    UTUQAGUJUKUTUQMUIBCUBUFUGUH $.
    $( [28-Aug-04] $) $( [27-Aug-04] $)

  $( Subtraction and addition of equal Hilbert space vectors.. $)
  hvsubaddeqt $p |- ( ( A e. H~ /\ B e. H~ ) -> ( A +v ( B -v A ) ) = B ) $=
    ( chil wcel wa cva co cmv w3a wceq pm3.26 pm3.27 3jca hvaddsubasst syl
    hvsubcan2t eqtr3d ) ACDZBCDZEZABFGAHGZABAHGFGZBTRSRIUAUBJTRSRRSKZRSLUCMABAN
    OABPQ $.
    $( [2-Sep-04] $) $( [27-Aug-04] $)

  ${
    hvmulcom.1 $e |- A e. CC $.
    hvmulcom.2 $e |- B e. CC $.
    hvmulcom.3 $e |- C e. H~ $.
    $( Scalar multiplication associative law. $)
    hvmulass $p |- ( ( A x. B ) .s C ) = ( A .s ( B .s C ) ) $=
      ( cc wcel chil cmulc co csm wceq ax-hvmulass mp3an ) AGHBGHCIHABJKCLKABCL
      KLKMDEFABCNO $.
      $( [3-Sep-99] $)

    $( Scalar multiplication commutative law. $)
    hvmulcom $p |- ( A .s ( B .s C ) ) = ( B .s ( A .s C ) ) $=
      ( cmulc co csm mulcom opreq1i hvmulass 3eqtr3 ) ABGHZCIHBAGHZCIHABCIHIHBA
      CIHIHNOCIABDEJKABCDEFLBACEDFLM $.
      $( [3-Sep-99] $)

    $( Double negative in scalar multiplication. $)
    hvmul2neg $p |- ( -u A .s ( -u B .s C ) ) = ( A .s ( B .s C ) ) $=
      ( cneg cmulc co csm mul2neg opreq1i negcl hvmulass 3eqtr3 ) AGZBGZHIZCJIA
      BHIZCJIPQCJIJIABCJIJIRSCJABDEKLPQCADMBEMFNABCDEFNO $.
      $( [3-Sep-99] $)
  $}

  ${
    hvdistr1.1 $e |- A e. CC $.
    hvdistr1.2 $e |- B e. H~ $.
    hvdistr1.3 $e |- C e. H~ $.
    $( Scalar multiplication distributive law. $)
    hvdistr1 $p |- ( A .s ( B +v C ) ) = ( ( A .s B ) +v ( A .s C ) ) $=
      ( cc wcel chil cva co csm wceq ax-hvdistr1 mp3an ) AGHBIHCIHABCJKLKABLKAC
      LKJKMDEFABCNO $.
      $( [3-Sep-99] $)

    $( Scalar multiplication distributive law. $)
    hvsubdistr1 $p |- ( A .s ( B -v C ) ) = ( ( A .s B ) -v ( A .s C ) ) $=
      ( c1 cneg csm co cva cmv 1cn negcl hvmulcl hvdistr1 hvmulcom opreq2i
      eqtr hvsubval 3eqtr4 ) ABGHZCIJZKJZIJZABIJZUBACIJZIJZKJZABCLJZIJUFUGLJUEU
      FAUCIJZKJUIABUCDEUBCGMNZFOPUKUHUFKAUBCDULFQRSUJUDAIBCEFTRUFUGABDEOACDFOTU
      A $.
      $( [3-Sep-99] $)
  $}

  ${
    hvass.1 $e |- A e. H~ $.
    hvass.2 $e |- B e. H~ $.
    hvass.3 $e |- C e. H~ $.
    $( Hilbert vector space associative law. $)
    hvass $p |- ( ( A +v B ) +v C ) = ( A +v ( B +v C ) ) $=
      ( chil wcel cva co wceq ax-hvass mp3an ) AGHBGHCGHABIJCIJABCIJIJKDEFABCLM
      $.
      $( [3-Sep-99] $)

    $( Hilbert vector space commutative/associative law. $)
    hvadd23 $p |- ( ( A +v B ) +v C ) = ( ( A +v C ) +v B ) $=
      ( chil wcel cva co wceq hvadd23t mp3an ) AGHBGHCGHABIJCIJACIJBIJKDEFABCLM
      $.
      $( [18-Aug-99] $)

    $( Hilbert vector space associative law for subtraction. $)
    hvsubass $p |- ( ( A -v B ) -v C ) = ( A -v ( B +v C ) ) $=
      ( cmv co c1 cneg csm cva hvsubcl hvsubval 1cn negcl hvmulcl hvass
      hvdistr1 opreq2i eqtr4 opreq1i hvaddcl 3eqtr4 eqtr ) ABGHZCGHUFIJZCKHZLHZ
      ABCLHZGHZUFCABDEMFNAUGBKHZLHZUHLHZAUGUJKHZLHZUIUKUNAULUHLHZLHUPAULUHDUGBI
      OPZEQUGCURFQRUOUQALUGBCUREFSTUAUFUMUHLABDENUBAUJDBCEFUCNUDUE $.
      $( [7-Oct-99] $)

