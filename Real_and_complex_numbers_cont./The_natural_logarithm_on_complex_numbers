  ${
    relogoprlem.1 $e |- ( ( ( log ` A ) e. CC /\ ( log ` B ) e. CC ) ->
                        ( exp ` ( ( log ` A ) F ( log ` B ) ) ) =
                        ( ( exp ` ( log ` A ) ) G ( exp ` ( log ` B ) ) ) ) $.
    relogoprlem.2 $e |- ( ( ( log ` A ) e. RR /\ ( log ` B ) e. RR ) ->
                          ( ( log ` A ) F ( log ` B ) ) e. RR ) $.
    $( Lemma for ~ relogmul and ~ relogdiv .  Remark of [Cohen] p. 301 ("The
       proof of Property 3 is quite similar to the proof given for Property
       2"). $)
    relogoprlem $p |- ( ( A e. RR+ /\ B e. RR+ ) ->
        ( log ` ( A G B ) ) = ( ( log ` A ) F ( log ` B ) ) ) $=
      ( crp wcel wa clog cfv ce co reeflog opreqan12d fveq2d cr wceq relogcl
      cc recn syl2an relogef syl eqtr3d ) AGHZBGHZIZAJKZLKZBJKZLKZDMZJKZABDMZJK
      UIUKCMZUHUMUOJUFUGUJAULBDANBNOPUFUIQHZUKQHZUNUPRUGASBSUQURIZUPLKZJKZUNUPU
      QUITHZUKTHZVAUNRURUIUAUKUAVBVCIUTUMJEPUBUSUPQHVAUPRFUPUCUDUEUBUE $.
      $( [25-Nov-2007] $)
  $}

  $( The natural logarithm of the product of two positive real numbers is the
     sum of natural logarithms.  Property 2 of [Cohen] p. 301, restricted to
     natural logarithms.  (Contributed by Steve Rodriguez, 25-Nov-2007.) $)
  relogmul $p |- ( ( A e. RR+ /\ B e. RR+ ) ->
                    ( log ` ( A x. B ) ) = ( ( log ` A ) + ( log ` B ) ) ) $=
    ( caddc cmul clog cfv efadd readdcl relogoprlem ) ABCDAEFZBEFZGJKHI $.
    $( [25-Nov-2007] $)

  $( The natural logarithm of the quotient of two positive real numbers is the
     difference of natural logarithms.  Exercise 72(a) and Property 3 of
     [Cohen] p. 301, restricted to natural logarithms.  (Contributed by Steve
     Rodriguez, 25-Nov-2007.) $)
  relogdiv $p |- ( ( A e. RR+ /\ B e. RR+ ) ->
                    ( log ` ( A / B ) ) = ( ( log ` A ) - ( log ` B ) ) ) $=
    ( cmin cdiv clog cfv efsub resubcl relogoprlem ) ABCDAEFZBEFZGJKHI $.
    $( [25-Nov-2007] $)

  $( Exponentiation of a nonzero complex number to a nonnegative integer
     power.  (Contributed by Paul Chapman, 21-Apr-2008.) $)
  explog $p |- ( ( A e. CC /\ A =/= 0 /\ N e. NN0 ) ->
                  ( A ^ N ) = ( exp ` ( N x. ( log ` A ) ) ) ) $=
    ( vx cc wcel cc0 wne cn0 w3a clog cfv cmul co ce cexp wceq wa crn logcl cv
    cim cpi cneg cico crab logrn ssrab2 eqsstri sseli syl efexp sylan 3impa
    eflog opreq1d 3adant3 eqtr2d ) ADEZAFGZBHEZIBAJKZLMNKZVANKZBOMZABOMZURUSUTV
    BVDPZURUSQZVADEZUTVFVGVAJRZEVHASVIDVAVICTUAKUBUCUBUDMEZCDUEDCUFVJCDUGUHUIUJ
    VABUKULUMURUSVDVEPUTVGVCABOAUNUOUPUQ $.
    $( [21-Apr-2008] $)

  $( Exponentiation of a positive real number to a nonnegative integer power.
     (Contributed by Steve Rodriguez, 25-Nov-2007.) $)
  reexplog $p |- ( ( A e. RR+ /\ N e. NN0 ) ->
                    ( A ^ N ) = ( exp ` ( N x. ( log ` A ) ) ) ) $=
    ( crp wcel cn0 wa clog cfv cmul co ce cexp cc wceq relogcl recnd efexp
    sylan reeflog opreq1d adantr eqtr2d ) ACDZBEDZFBAGHZIJKHZUEKHZBLJZABLJZUCUE
    MDUDUFUHNUCUEAOPUEBQRUCUHUINUDUCUGABLASTUAUB $.
