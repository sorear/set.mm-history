$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        The natural logarithm on complex numbers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c log $.

  $( Extend class notation with the natural logarithm function on complex
     numbers. $)
  clog $a class log $.

  $( Define the natural logarithm function on complex numbers.  See
     ~ http://en.wikipedia.org/wiki/Natural_logarithm ("The natural logarithm
     function can also be defined as the inverse function of the exponential
     function"). $)
  df-log $a |- log =
               `' ( exp |` { x e. CC | ( Im ` x ) e. ( -u pi [,) pi ) } ) $.

  $( The range of the natural logarithm function, also the principal domain of
     the exponential function.  This allows us to write the longer class
     abstraction as simply ` ran log ` .  (Contributed by Paul Chapman,
     21-Apr-2008.) $)
  logrn $p |- ran log = { x e. CC | ( Im ` x ) e. ( -u pi [,) pi ) } $=
    ( cc cc0 csn cdif cv cim cfv cpi cneg cico wcel crab clog wf1 crn wceq wf1o
    co ax-mp wa ce cres ccnv eff1o f1ocnv wb df-log f1oeq1 mpbir dff1o5 simpri
    mpbi ) BCDEZAFGHIJIKSLABMZNOZNPUOQZUNUONRZUPUQUAURUNUOUBUOUCZUDZRZUOUNUSRVA
    AUEUOUNUSUFTNUTQURVAUGAUHUNUONUTUITUJUNUONUKUMUL $.
    $( [21-Apr-2008] $)

  $( The natural logarithm function in terms of the exponential function
     restricted to its principal domain.  (Contributed by Paul Chapman,
     21-Apr-2008.) $)
  dflog2 $p |- log = `' ( exp |` ran log ) $=
    ( vx clog ce cv cim cfv cpi cneg cico co wcel cc crab cres crn df-log logrn
    ccnv reseq2i cnveqi eqtr4i ) BCADEFGHGIJKALMZNZRCBOZNZRAPUEUCUDUBCAQSTUA $.
    $( [21-Apr-2008] $)

  $( The range of the natural logarithm function includes the real numbers.
     (Contributed by Paul Chapman, 21-Apr-2008.) $)
  resslogrn $p |- RR C_ ran log $=
    ( vx cr clog crn cv wcel cim cfv cpi cneg cico co cc cc0 wbr clt pire pipos
    0re wb crab recn reim0 cle renegcli lt0neg2 ax-mp ltleii w3a elico2 biimpri
    mpbi mp2an mp3an syl6eqel rabid sylanbrc logrn syl6eleqr ssriv ) ABCDZAEZBF
    ZVBVBGHZIJZIKLZFZAMUAZVAVCVBMFVGVBVHFVBUBVCVDNVFVBUCNBFZVENUDOZNIPOZNVFFZSV
    ENIQUEZSVKVENPOZRIBFZVKVNTQIUFUGULUHRVLVIVJVKUIZVEBFVOVLVPTVMQVEINUJUMUKUNU
    OVGAMUPUQAURUSUT $.
    $( [21-Apr-2008] $)

  $( The exponential function restricted to its principal domain maps
     one-to-one onto the nonzero complex numbers.  (Contributed by Paul
     Chapman, 21-Apr-2008.) $)
  eff1o2 $p |- ( exp |` ran log ) : ran log -1-1-onto-> ( CC \ { 0 } ) $=
    ( vx clog crn cc cc0 csn cdif ce cres wf1o cv cim cfv cpi cneg cico wceq wb
    ax-mp mpbir co wcel crab eff1o logrn reseq2i f1oeq1 f1oeq2 ) BCZDEFGZHUIIZJ
    ZAKLMNONPUAUBADUCZUJUKJZUNUMUJHUMIZJZAUDUKUOQUNUPRUIUMHAUEZUFUMUJUKUOUGSTUI
    UMQULUNRUQUIUMUJUKUHST $.
    $( [21-Apr-2008] $)

  $( The natural logarithm function maps the nonzero complex numbers one-to-one
     onto its range.  (Contributed by Paul Chapman, 21-Apr-2008.) $)
  logf1o $p |- log : ( CC \ { 0 } ) -1-1-onto-> ran log $=
    ( cc cc0 csn cdif clog wf1o ce cres ccnv eff1o2 f1ocnv ax-mp wceq wb dflog2
    crn f1oeq1 mpbir ) ABCDZEPZEFZSTGTHZIZFZTSUBFUDJTSUBKLEUCMUAUDNOSTEUCQLR $.
    $( [21-Apr-2008] $)

  $( The natural logarithm function on the positive reals in terms of the real
     exponential function.  (Contributed by Paul Chapman, 21-Apr-2008.) $)
  dfrelog $p |- ( log |` RR+ ) = `' ( exp |` RR ) $=
    ( ce clog crn cres ccnv cr cima crp df-ima wss wceq resslogrn resabs1 ax-mp
    rneqi wfn wfun wf1o mpbi dflog2 reeff1o dff1o2 simp3i 3eqtri reseq2i cnveqi
    w3a cc cc0 csn cdif logf1o f1ofun wb funeq funcnvres eqtr3i reseq1 3eqtr4ri
    ) ABCZDZEZVAFGZDZVBHDZAFDZEZBHDZVCHVBVCVAFDZCVFCZHVAFIVIVFFUTJVIVFKLAFUTMNZ
    OVFFPZVGQZVJHKZFHVFRVLVMVNUGUAFHVFUBSUCUDUEVIEZVGVDVIVFVKUFVBQZVOVDKBQZVPUH
    UIUJUKZUTBRVQULVRUTBUMNBVBKZVQVPUNTBVBUONSFVAUPNUQVSVHVEKTBVBHURNUS $.
    $( [21-Apr-2008] $)

  $( The natural logarithm function maps the positive reals one-to-one onto the
     real numbers.  (Contributed by Paul Chapman, 21-Apr-2008.) $)
  relogf1o $p |- ( log |` RR+ ) : RR+ -1-1-onto-> RR $=
    ( crp cr clog cres wf1o ce crn ccnv wfun cc0 csn cdif eff1o2 wfo ax-mp wceq
    cc wb f1oeq1 mpbir dff1o3 simprbi reeff1o resslogrn resabs1 f1orescnv mp2an
    wss dflog2 reseq1 ) ABCADZEZABFCGZDZHZADZEZUOIZBAUNBDZEZUQUMQJKLZUNEZURMVBU
    MVAUNNURUMVAUNUAUBOUTBAFBDZEZUCUSVCPZUTVDRBUMUHVEUDFBUMUEOBAUSVCSOTABUNUFUG
    UKUPPZULUQRCUOPVFUICUOAUJOABUKUPSOT $.
    $( [21-Apr-2008] $)

  $( Closure of the natural logarithm function.  (Contributed by Paul Chapman,
     21-Apr-2008.) $)
  logcl $p |- ( ( A e. CC /\ A =/= 0 ) -> ( log ` A ) e. ran log ) $=
    ( cc wcel cc0 wne wa csn cdif clog cfv eldifsn wf1o wf logf1o f1of ffvelrni
    crn ax-mp sylbir ) ABCADEFABDGHZCAIJIQZCABDKTUAAITUAILTUAIMNTUAIORPS $.
    $( [21-Apr-2008] $)

  $( Closure of the natural logarithm function on positive reals.  (Contributed
     by Steve Rodriguez, 25-Nov-2007.) $)
  relogcl $p |- ( A e. RR+ -> ( log ` A ) e. RR ) $=
    ( crp wcel clog cres cr fvres wf1o wf relogf1o f1of ax-mp ffvelrni eqeltrrd
    cfv ) ABCADBEZOADOFABDGBFAPBFPHBFPIJBFPKLMN $.
    $( [25-Nov-2007] $)

  $( Relationship between the natural logarithm function and the exponential
     function.  (Contributed by Paul Chapman, 21-Apr-2008.) $)
  eflog $p |- ( ( A e. CC /\ A =/= 0 ) -> ( exp ` ( log ` A ) ) = A ) $=
    ( cc wcel cc0 wne wa clog cfv ce crn cres wceq logcl syl ccnv dflog2 fveq1i
    fvres fveq2i csn cdif eldifsn wf1o eff1o2 f1ocnvfv2 sylbir syl5eq eqtr3d
    mpan ) ABCADEFZAGHZIGJZKZHZUKIHZAUJUKULCUNUOLAMUKULIRNUJUNAUMOZHZUMHZAUKUQU
    MAGUPPQSUJABDTUAZCZURALZABDUBULUSUMUCUTVAUDULUSAUMUEUIUFUGUH $.
    $( [21-Apr-2008] $)

  $( Relationship between the natural logarithm function and the exponential
     function.  (Contributed by Steve Rodriguez, 25-Nov-2007.) $)
  reeflog $p |- ( A e. RR+ -> ( exp ` ( log ` A ) ) = A ) $=
    ( crp wcel cc cc0 wne wa clog cfv ce wceq rpcnne0 eflog syl ) ABCADCAEFGAHI
    JIAKALAMN $.
    $( [25-Nov-2007] $)

  $( Relationship between the natural logarithm function and the exponential
     function.  (Contributed by Paul Chapman, 21-Apr-2008.) $)
  logef $p |- ( A e. ran log -> ( log ` ( exp ` A ) ) = A ) $=
    ( clog crn wcel ce cres cfv fvres fveq2d ccnv dflog2 fveq1i cc cc0 csn cdif
    wf1o wceq eff1o2 f1ocnvfv1 mpan syl5eq eqtr3d ) ABCZDZAEUDFZGZBGZAEGZBGAUEU
