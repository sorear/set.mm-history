  $( Relationship between the natural logarithm function and the exponential
     function.  (Contributed by Paul Chapman, 21-Apr-2008.) $)
  logeftb $p |- ( ( A e. CC /\ A =/= 0 /\ B e. ran log ) ->
                  ( ( log ` A ) = B <-> ( exp ` B ) = A ) ) $=
    ( cc wcel cc0 wne clog crn cfv wceq ce wb wa csn cdif eldifsn cres ccnv
    fvres eqeq1d adantr wf1o eff1o2 f1ocnvfvb mp3an1 bitr3d ancoms dflog2
    fveq1i eqeq1i syl6rbbr sylanbr 3impa ) ACDZAEFZBGHZDZAGIZBJZBKIZAJZLZUNUOMA
