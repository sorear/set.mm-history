    $( Lemma for ~ pire , ~ pigt2lt4 and ~ sinpi . $)
    pilem1 $p |- ( A e. ( RR+ i^i ( `' sin " { 0 } ) ) <->
                   ( A e. RR+ /\ ( sin ` A ) = 0 ) ) $=
      ( crp csin ccnv cc0 csn cima cin wcel wa cfv wceq elin rpcn biantrurd wfn
      cc wb wf sinf ffn ax-mp 0cn fniniseg mp2an syl6rbbr pm5.32i bitri ) ABCDE
