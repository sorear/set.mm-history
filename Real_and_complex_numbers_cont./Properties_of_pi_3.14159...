    LUTUIUJUKUM $.
    $( [10-May-2014] $) $( [21-Apr-2008] $)

  ${
    sinperlem.1 $e |- ( A e. CC -> ( F ` A ) =
      ( ( ( exp ` ( _i x. A ) ) O ( exp ` ( -u _i x. A ) ) ) / D ) ) $.
    sinperlem.2 $e |- ( ( A + ( K x. ( 2 x. pi ) ) ) e. CC ->
      ( F ` ( A + ( K x. ( 2 x. pi ) ) ) ) =
        ( ( ( exp ` ( _i x. ( A + ( K x. ( 2 x. pi ) ) ) ) ) O
          ( exp ` ( -u _i x. ( A + ( K x. ( 2 x. pi ) ) ) ) ) ) / D ) ) $.
    $( Lemma for ~ sinper and ~ cosper . $)
    sinperlem $p |- ( ( A e. CC /\ K e. ZZ ) ->
                 ( F ` ( A + ( K x. ( 2 x. pi ) ) ) ) = ( F ` A ) ) $=
      ( cc wcel ci cpi cmul co caddc ce cfv cneg wceq ax-icn eqtrd cz wa c2 zcn
