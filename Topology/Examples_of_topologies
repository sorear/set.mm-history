    $( The indiscrete topology on a set ` A ` expressed as a topological space.
       (Contributed by FL, 19-Jul-2006.) $)
    indistps $p |- <. A , { (/) , A } >. e. TopSp $=
      ( c0 cpr cuni cop ctps cun 0ex unipr uncom un0 3eqtri opeq1i wcel ctop
      indistop eltopsp mpbir eqeltrri ) CADZEZUAFZAUAFGUBAUAUBCAHACHACAIBJCAKAL
      MNUCGOUAPOAQUARST $.
      $( [19-Jul-2006] $) $( [17-Jul-2006] $)

    $( The discrete topology on a set ` A ` expressed as a topological space.
       (Contributed by FL, 20-Aug-2006.) $)
    distps $p |- <. A , P~ A >. e. TopSp $=
      ( cpw cuni cop ctps unipw opeq1i wcel ctop distop eltopsp mpbir eqeltrri
      ) ACZDZOEZAOEFPAOAGHQFIOJIABKOLMN $.
      $( [20-Aug-2006] $) $( [19-Aug-2006] $)
  $}

  ${
    $d x y z w v u t f $.
    $( A basis for the standard topology on the reals. $)
