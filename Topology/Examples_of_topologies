  $( The standard topology on the reals.  (Contributed by FL, 4-Jun-2007.) $)
  retop $p |- ( topGen ` ran (,) ) e. Top $=
    ( cioo crn ctb wcel ctg cfv ctop retopbas tgcl ax-mp ) ABZCDKEFGDHKIJ $.
    $( [22-Jun-2007] $) $( [29-May-2007] $)

  $( The underlying set of the standard topology on the reals is the reals.
     (Contributed by FL, 4-Jun-2007.) $)
  uniretop $p |- U. ( topGen ` ran (,) ) = RR $=
    ( cioo crn ctg cfv cuni cr ctb wcel wceq retopbas unitg ax-mp unirnioo
    eqtri ) ABZCDEZOEZFOGHPQIJOKLMN $.
    $( [22-Jun-2007] $) $( [29-May-2007] $)

  $( The standard topological space on the reals. $)
  retps $p |- <. RR , ( topGen ` ran (,) ) >. e. TopSp $=
    ( cr cioo crn ctg cfv cop ctps wcel ctop cuni wceq istps retop uniretop
    eqcomi mpbir2an ) ABCDEZFGHQIHAQJZKAQLMRANOP $.
    $( [15-Feb-2007] $) $( [10-Feb-2007] $)

  $( Open intervals are open sets of the standard topology on the reals .
     (Contributed by FL, 18-Jun-2007.) $)
  iooretop $p |- ( A (,) B ) e. ( topGen ` ran (,) ) $=
    ( cxr wcel wa cioo co crn ctg cfv wss ctb retopbas bastg ax-mp a1i cxp wfn
    cr cpw wf ioof ffn fnoprvrn mp3an1 sseldd wn c0 ndmioo ctop retop 0opn
    syl6eqel pm2.61i ) ACDZBCDZEZABFGZFHZIJZDUQUSUTURUSUTKZUQUSLDVAMUSNOPFCCQZR
    ZUOUPURUSDVBSTZFUAVCUBVBVDFUCOCCABFUDUEUFUQUGURUHUTABUIUTUJDUHUTDUKUTULOUMU
    N $.
    $( [22-Jun-2007] $) $( [21-Jun-2007] $)


