    $( The indiscrete topology on a set ` A ` expressed as a topological space.
       (Contributed by FL, 19-Jul-2006.) $)
    indistps $p |- <. A , { (/) , A } >. e. TopSp $=
      ( c0 cpr cuni cop ctps cun 0ex unipr uncom un0 3eqtr opeq1i wcel ctop
      indistop eltopsp mpbir eqeltrr ) CADZEZUAFZAUAFGUBAUAUBCAHACHACAIBJCAKALM
      NUCGOUAPOABQUARST $.
      $( [19-Jul-2006] $) $( [17-Jul-2006] $)

    $( The discrete topology on a set ` A ` expressed as a topological space.
       (Contributed by FL, 20-Aug-2006.) $)
    distps $p |- <. A , P~ A >. e. TopSp $=
      ( cpw cuni cop ctps unipw opeq1i wcel ctop distop eltopsp mpbir eqeltrr
      ) ACZDZOEZAOEFPAOAGHQFIOJIABKOLMN $.
      $( [20-Aug-2006] $) $( [19-Aug-2006] $)
  $}

  ${
    $d x y z w v u t f $.
    $( A basis for the standard topology on the reals. $)
    retopbas $p |- ran (,) e. Bases $=
      ( vz vw vx vy vv vu vt vf cioo crn ctb wcel cv cin wss wa wrex wral wceq
      eleq2 sseq1 anbi12d rcla4ev co cxr wi cle wbr cif opreq1 eqeq1d opreq2
      rcla42ev ifcl ad2ant2r adantr ancoms ad2ant2l iooint ineq12 adantl
      eqtr3d syl3anc exp32 exp31 com4t imp31 r19.23adv r19.23adva r19.23aiv
      imp cxp wfn wb cr cpw wf ioof ffn ax-mp oprvalelrn 3imtr4 ssid jctr
      syl2an r19.21aiva rgen2 cvv iooex rnexg isbasis2g mpbir ) IJZKLZAMZBMZLZW
