      coeeu.1 $e |- ( ph -> F e. ( Poly ` S ) ) $.
      coeeu.2 $e |- ( ph -> A e. ( CC ^m NN0 ) ) $.
      coeeu.3 $e |- ( ph -> B e. ( CC ^m NN0 ) ) $.
      coeeu.4 $e |- ( ph -> M e. NN0 ) $.
      coeeu.5 $e |- ( ph -> N e. NN0 ) $.
      coeeu.6 $e |- ( ph -> ( A " ( ZZ>= ` ( M + 1 ) ) ) = { 0 } ) $.
      coeeu.7 $e |- ( ph -> ( B " ( ZZ>= ` ( N + 1 ) ) ) = { 0 } ) $.
      coeeu.8 $e |- ( ph -> F =
