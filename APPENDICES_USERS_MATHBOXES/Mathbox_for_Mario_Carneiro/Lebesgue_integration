      $( Lemma for ~ iblre . $)
      iblrelem2 $p |- ( ( ph /\ K = 2 ) -> G = ( S.2 ` ( x e. RR |->
            if ( ( x e. A /\ 0 <_ -u B ) , -u B , 0 ) ) ) ) $=
        ( wceq wa cr wcel cc0 cdiv co cfv c1 syl c2 cre cle wbr cmpt citg2 cneg
