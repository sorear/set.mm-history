      $( Lemma for ~ iblre . $)
      iblrelem0 $p |- ( ( ph /\ K = 0 ) -> G = ( S.2 ` ( x e. RR |->
            if ( ( x e. A /\ 0 <_ B ) , B , 0 ) ) ) ) $=
        ( cc0 wceq wa cr wcel cdiv co cfv c1 ci cre cle wbr cif cmpt citg2 cexp
        cv simplr oveq2d ax-icn exp0 ax-mp 3eqtr3g adantlr recnd div1 syl eqtrd
        cc fveq2d rere itgvallem2 syl5eq ) AGKLZMZFBNBUHCOZKDEPQZUARZUBUCMVIKUD
