$(
    @( The integral of a sum of simple functions is the sum of the integrals.
       @)
    itg1add @p |- ( ph -> ( S.1 ` H ) = ( ( S.1 ` F ) + ( S.1 ` G ) ) ) @=
      ? @.
      @( [18-Jun-2014] @)
  @}

  @{
    i1fmulc.1 @e |- G = ( x e. RR |-> ( A x. ( F ` x ) ) ) @.
    i1fmulc.2 @e |- ( ph -> F e. dom S.1 ) @.
    i1fmulc.3 @e |- ( ph -> A e. ( 0 [,) +oo ) ) @.
    @( A nonnegative constant times a simple function gives another simple
       function. @)
    i1fmulc @p |- ( ph -> G e. dom S.1 ) @=
      ? @.
      @( [18-Jun-2014] @)

    @( The integral of a constant times a simple function is the constant
       times the original integral. @)
    itg1mulc @p |- ( ph -> ( S.1 ` H ) = ( A x. ( S.1 ` F ) ) ) @=
      ? @.
      @( [18-Jun-2014] @)
  @}

  @{
    @( If one simple function dominates another, then the integral of the
       larger is also larger. @)
    itg1le @p |- ( ( F e. dom S.1 /\ G e. dom S.1 /\
                     A. x e. RR ( F ` x ) <_ ( G ` x ) ) ->
                     ( S.1 ` F ) <_ ( S.1 ` G ) ) @=
      ? @.
$)
  $}

