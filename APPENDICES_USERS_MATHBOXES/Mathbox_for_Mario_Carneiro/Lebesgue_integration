  $(
  @{
    itg2mono.1 @e |- G = ( x e. RR |-> sup ( ran
                           ( n e. NN |-> ( ( F ` n ) ` x ) ) , RR* , < ) ) @.
    itg2mono.2 @e |- ( ( ph /\ n e. NN ) -> ( F ` n ) e. MblFn ) @.
    itg2mono.3 @e |- ( ( ph /\ n e. NN ) ->
                       ( F ` n ) : RR --> ( 0 [,] +oo ) ) @.
    itg2mono.4 @e |- ( ( ph /\ ( n e. NN /\ x e. RR ) ) ->
                       ( ( F ` n ) ` x ) <_ ( ( F ` ( n + 1 ) ) ` x ) ) @.
    @( The Monotone Convergence Theorem for nonnegative functions.  If
       ` { ( F `` n ) : n e. NN } ` is a monotone increasing sequence of
       positive, measurable, extended real-valued functions, and ` G ` is the
       pointwise limit of the sequence, then ` ( S.2 ` G ) ` is the limit of
       the sequence ` { ( S.2 `` ( F `` n ) ) : n e. NN } ` . @)
    itg2mono @p |- ( ph -> ( S.2 ` G ) =
      sup ( ran ( n e. NN |-> ( S.2 ` ( F ` n ) ) ) , RR* , < ) ) @=
      ? @.
  @}

  @{
    itg2add.1 @e |- H = ( x e. RR |-> ( ( F ` x ) + ( G ` x ) ) ) @.
    itg2add.2 @e |- ( ph -> F : RR --> ( 0 [,) +oo ) ) @.
    itg2add.3 @e |- ( ph -> G : RR --> ( 0 [,) +oo ) ) @.
    @( The ` S.2 ` integral is linear. @)
    itg2add @p |- ( ph -> ( S.2 ` H ) = ( ( S.2 ` F ) + ( S.2 ` G ) ) ) @=
      ? @.
  @}
  $)

