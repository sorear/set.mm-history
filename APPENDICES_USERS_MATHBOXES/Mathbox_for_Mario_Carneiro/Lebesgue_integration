        $( [7-Jul-2014] $)

      $( Lemma for ~ iblre . $)
      iblrelem3 $p |- ( ( ph /\ K = 3 ) -> G = 0 ) $=
        ( c3 wceq wa wcel cc0 cdiv co cfv ci syl cr cre cle wbr cmpt citg2 cneg
        cv cif cim cc adantlr recnd negneg cexp simplr oveq2d eqcomd i3 3eqtr3g
        oveq12d renegcl ax-icn ine0 div2neg mp3an23 eqtr3d fveq2d imval 3eqtr2d
        wne reim0 itgvallem3 syl5eq ) AGKLZMZFBUABUHCNZODEPQZUBRZUCUDMVSOUIUEUF
        ROJVPBCVSVPVQMZVSDUGZSPQZUBRZWAUJRZOVTVRWBUBVTWAUGZSUGZPQZVRWBVTWEDWFEP
        VTDUKNWEDLVTDAVQDUANZVOHULZUMDUNTVTSKUOQZSGUOQZWFEVTWKWJVTGKSUOAVOVQUPU
