    $(
    @( Integrability of a real function. @)
    iblre @p |- ( ph -> ( ( x e. A |-> B ) e. Itgbl <->
      ( ( S.2 ` ( x e. RR |->
          if ( ( x e. A /\ 0 <_ B ) , B , 0 ) ) ) e. RR /\
        ( S.2 ` ( x e. RR |->
          if ( ( x e. A /\ 0 <_ -u B ) , -u B , 0 ) ) ) e. RR ) ) ) @=
      ? @.
      @( [7-Jul-2014] @)

    itgreval.2 @e |- ( ph -> ( x e. A |-> B ) e. Itgbl ) @.
    @( Lemma for ~ itgposval and ~ itgreval . @)
    itgrevallem1 @p |- ( ph -> S. A B _d x =
 ( ( S.2 ` ( x e. RR |-> if ( ( x e. A /\ 0 <_ B ) , B , 0 ) ) ) -
   ( S.2 ` ( x e. RR |-> if ( ( x e. A /\ 0 <_ -u B ) , -u B , 0 ) ) ) ) ) @=
      ? @.
      @( [7-Jul-2014] @)

    @{
      itgposval.3 @e |- ( ( ph /\ x e. A ) -> 0 <_ B ) @.
      @( The integral of a nonnegative function.
         (Contributed by Mario Carneiro, 7-Jul-2014.) @)
      itgposval @p |- ( ph -> S. A B _d x =
                        ( S.2 ` ( x e. RR |-> if ( x e. A , B , 0 ) ) ) ) @=
        ? @.
        @( [7-Jul-2014] @)
    @}

    @( Decompose the integral of a real function into positive and negative
       parts.
       (Contributed by Mario Carneiro, 29-Jun-2014.) @)
    itgreval @p |- ( ph -> S. A B _d x =
                    ( S. A if ( 0 <_ B , B , 0 ) _d x -
                      S. A if ( 0 <_ -u B , -u B , 0 ) _d x ) ) @=
      ? @.
      @( [29-Jun-2014] @)
  @}

  @{
    itgcnval.1 @e |- ( ( ph /\ x e. A ) -> B e. V ) @.
    itgcnval.2 @e |- ( ph -> ( x e. A |-> B ) e. Itgbl ) @.
    @( Decompose the integral of a complex function into real and imaginary
       parts.
       (Contributed by Mario Carneiro, 29-Jun-2014.) @)
    itgcnval @p |- ( ph -> S. A B _d x =
      ( S. A ( Re ` B ) _d x + ( _i x. S. A ( Im ` B ) _d x ) ) ) @=
      ? @.
      @( [29-Jun-2014] @)
$)
  $}

