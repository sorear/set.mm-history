$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Order theory
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c mxl $.
  $c mnl $.
  $c ub $.
  $c lb $.
  $c Preset $.
  $c ge $.
  $c se $.
  $c AntiDir $.

  $( Extend class notation with the class of all the presets. $)
  cpreset $a class Preset $.

  $( Extend class notation with a function that returns the maximal elements of
     a preset. $)
  cmxl $a class mxl $.

  $( Extend class notation with a function that returns the minimal elements of
     a preset. $)
  cmnl $a class mnl $.

  $( Extend class notation with a function that returns the upper bounds of a
     part of a preset. $)
  cub $a class ub $.

  $( Extend class notation with a function that returns the lower bounds of a
     part of a preset. $)
  clb $a class lb $.

  $( Extend class notation with a function that returns the greatest element of
     a poset. $)
  cge $a class ge $.

  $( Extend class notation with a function that returns the smallest element of
     a poset. $)
  cse $a class se $.

  $( Extend class notation with the class of all the anti-directions. $)
  cantidir $a class AntiDir $.

  $( Define the class of all presets.  A preset is a transitive and reflexive
     relation.  ("preset" is the short for preordered set.) $)
  df-prs $a |- Preset = { r | ( Rel r /\ ( r o. r ) C_ r /\
              ( _I |` U. U. r ) C_ r ) } $.

  ${
    $d R r $.
    $( The predicate "is a preset".  (Contributed by FL, 1-May-2011.) $)
    isprs $p |- ( R e. A -> ( R e. Preset <->
       ( Rel R /\ ( R o. R ) C_ R /\ ( _I |` U. U. R ) C_ R ) ) ) $=
      ( vr cv wrel ccom wss cid cuni cres w3a cpreset wceq releq wb coeq1 coeq2
      eqtrd sseq12 mpancom unieq unieqd reseq2d 3anbi123d df-prs elab2g ) CDZEZ
      UGUGFZUGGZHUGIZIZJZUGGZKBEZBBFZBGZHBIZIZJZBGZKCBLAUGBMZUHUOUJUQUNVAUGBNUI
      UPMVBUJUQOVBUIBUGFUPUGBUGPUGBBQRUIUPUGBSTUMUTMVBUNVAOVBULUSHVBUKURUGBUAUB
      UCUMUTUGBSTUDCUEUF $.
      $( [1-May-2011] $)
  $}

  $( A preset is a relation.  (Contributed by FL, 18-May-2011.) $)
  preorel $p |- ( R e. Preset -> Rel R ) $=
    ( cpreset wcel wrel ccom wss cid cuni cres w3a isprs ibi simp1d ) ABCZADZAA
    EAFZGAHHIAFZNOPQJBAKLM $.
    $( [18-May-2011] $)

  ${
    $d R x $.
    $( The domain of a preset equals its field.  (Contributed by FL,
       22-May-2011.) $)
    preodom2 $p |- ( R e. Preset -> dom R = U. U. R ) $=
      ( vx cpreset wcel cdm cuni wceq wrel ccom wss cid cres w3a isprs wbr wral
      cv ref4w domfldref sylan2br 3adant2 syl6bi pm2.43i ) ACDZAEAFFZGZUDUDAHZA
      AIAJZKUELAJZMUFCANUGUIUFUHUIUGBQZUJAOBUEPUFBUEARBASTUAUBUC $.
      $( [22-May-2011] $)
  $}

  ${
    preoref12.1 $e |- X = dom R $.
    $( The field of a preset is a set.  (Contributed by FL, 19-Sep-2011.) $)
    ppldrels $p |- ( R e. Preset -> X e. _V ) $=
      ( cpreset wcel cdm cvv dmexg syl5eqel ) ADEBAFGCADHI $.
      $( [19-Sep-2011] $)

    $( A preset is reflexive.  (Contributed by FL, 18-May-2011.) $)
    preoref12 $p |- ( R e. Preset -> ( _I |` X ) C_ R ) $=
      ( cpreset wcel cid cres cuni cdm preodom2 reseq2d wrel ccom wss w3a isprs
      syl5eq ibi simp3d eqsstrd ) ADEZFBGFAHHZGZAUABUBFUABAIUBCAJQKUAALZAAMANZU
      CANZUAUDUEUFODAPRST $.
      $( [18-May-2011] $)

    ${
      $d A x $.  $d R x $.
      $( A preset is reflexive.  (Contributed by FL, 22-May-2011.) $)
      preoref22 $p |- ( ( R e. Preset /\ A e. X ) -> A R A ) $=
        ( vx cpreset wcel wbr cdm cuni wceq cid cres wi preodom2 preoref12 eqtr
        wss wa cv wral ref4w wb breq12 anidms expcom sylbir reseq2 sseq1d eleq2
        rcla4va imbi1d imbi12d mpbiri syl mpan sylc imp ) BFGZACGZAABHZUSBIZBJJ
        ZKZLCMZBRZUTVANZBOBCDPCVBKZVDVFVGNZDVHVDSCVCKZVICVBVCQVJVILVCMZBRZAVCGZ
        VANZNVLETZVOBHZEVCUAZVNEVCBUBVMVQVAVPVAEAVCVOAKVPVAUCVOAVOABUDUEUKUFUGV
        JVFVLVGVNVJVEVKBCVCLUHUIVJUTVMVACVCAUJULUMUNUOUPUQUR $.
        $( [22-May-2011] $)
    $}

    ${
      $d R x $.
      $( The range of a preset equals its field.  (Contributed by FL,
         22-May-2011.) $)
      preoran2 $p |- ( R e. Preset -> ran R = X ) $=
        ( vx cpreset wcel crn wceq wrel ccom wss cid cuni cres w3a isprs cv wbr
        wral ref4w wa cdm domrngref syl5req sylan2br 3adant2 syl6bi pm2.43i ) A
        EFZAGZBHZUIUIAIZAAJAKZLAMMZNAKZOUKEAPULUOUKUMUOULDQZUPARDUNSZUKDUNATULU
        QUABAUBUJCDAUCUDUEUFUGUH $.
        $( [22-May-2011] $)
    $}

    $( If ` A <_ B ` then ` A ` belongs to the field of the preset.
       (Contributed by FL, 23-May-2011.) $)
    pre1befi2 $p |- ( ( R e. Preset /\ A R B ) -> A e. X ) $=
      ( cpreset wcel wbr wa cdm wrel preorel releldm sylan syl6eleqr ) CFGZABCH
      ZIACJZDPCKQARGCLABCMNEO $.
      $( [23-May-2011] $)

    $( If ` A <_ B ` then ` B ` belongs to the field of the preset.
       (Contributed by FL, 23-May-2011.) $)
    pre2befi2 $p |- ( ( R e. Preset /\ B e. C /\ A R B ) -> B e. X ) $=
      ( cpreset wcel wbr w3a crn wrel wi preorel relelrng 3exp syl5com preoran2
      3imp wb eqcomd eleq2d 3ad2ant1 mpbird ) DGHZBCHZABDIZJBEHZBDKZHZUEUFUGUJU
      EDLZUFUGUJMDNUFUKUGUJABCDOPQSUEUFUHUJTUGUEEUIBUEUIEDEFRUAUBUCUD $.
      $( [23-May-2011] $)
  $}

  ${
    domcprere.1 $e |- X = dom R $.
    $( If ` R ` is a preset, its domain and the domain of its converse are
       equal.  (Contributed by FL, 19-Sep-2011.) $)
    domcnvpre $p |- ( R e. Preset -> X = dom `' R ) $=
      ( cpreset wcel ccnv cdm crn df-rn preoran2 syl5reqr ) ADEAFGAHBAIABCJK $.
      $( [19-Sep-2011] $)
  $}

  $( A preset is transitive.  (Contributed by FL, 22-May-2011.) $)
  preotr1 $p |- ( R e. Preset -> ( R o. R ) C_ R ) $=
    ( cpreset wcel wrel ccom wss cid cuni cres w3a isprs ibi simp2d ) ABCZADZAA
    EAFZGAHHIAFZNOPQJBAKLM $.
    $( [22-May-2011] $)

  ${
    $d A x y z $.  $d B x y z $.  $d C x y z $.  $d R x y z $.
    $( A preset is transitive.  (Contributed by FL, 23-May-2011.) $)
    preotr2 $p |- ( ( R e. Preset /\ ( A R B /\ B R C /\ C e. D ) )
      -> A R C ) $=
      ( vx vy vz wcel wbr w3a wi cv wa wal cvv wceq breq12d ex pm2.43i ccom wss
      cpreset preotr1 preorel brrelex simp1 simp2 simp3 anbi12d imbi12d cla43gv
      cotr wrel com23 3exp com12 com4r com13 a1dd com4l com3l com14 sylbi mpcom
      syl 3imp imp ) EUCIZABEJZBCEJZCDIZKZACEJZEEUAEUBZVIVMVNLZEUDVOFMZGMZEJZVR
      HMZEJZNZVQVTEJZLZHOGOFOZVIVPLFGHEUMVIWEVPVIEUNZWEVPLEUEVMWFWEVNVJVKVLWFWE
      VNLZLWFVJVKVLWGWFVJVKVLWGLZLZLVJWFWFWIVJWFWFWILZLWFVJVJWFWIWFVJVJWJLZWFVJ
      NAPIZWKABEUFVKVJWFWLWHWFVKVJWLWHLZWFVKVJWMLZLVKWFWFWNVKWFWFWNLZLWFVKVKWFW
      NWFVKVKWOLZWFVKNBPIZWPBCEUFWQVKWNWFVJVKWQWMVJVKWQWMLWQWLVLVJVKNZWGWLWQVLW
      RWGLZLWLWQVLWSWLWQVLKWEWRVNWDWRVNLFGHABCPPDVQAQZVRBQZVTCQZKZWBWRWCVNXCVSV
      JWAVKXCVQAVRBEWTXAXBUGZWTXAXBUHZRXCVRBVTCEXEWTXAXBUIZRUJXCVQAVTCEXDXFRUKU
      LUOUPUQURSUSUTVFSVATUSTVBVCVFSVATVBTVAVGVBVFUQVDVEVH $.
      $( [23-May-2011] $)
  $}

  $( The composite of a preset with itself.  (Contributed by FL,
     13-May-2011.) $)
  altprs2 $p |- ( R e. Preset -> ( R o. R ) = R ) $=
    ( cpreset wcel ccom wceq wrel wss cid cuni w3a isprs simp2 reflincror eqssd
    cres 3adant2 syl6bi pm2.43i ) ABCZAADZAEZSSAFZTAGZHAIIOAGZJZUABAKUETAUBUCUD
    LUBUDATGUCAMPNQR $.
    $( [13-May-2011] $)

  $( The intersection of two presets is a preset.  (Contributed by FL,
     28-Dec-2011.) $)
  int2pre $p |- ( ( R e. Preset /\ S e. Preset )
    -> ( R i^i S ) e. Preset ) $=
    ( cpreset wcel wa cin wrel ccom wss cid cuni cres w3a isprs adantr 3ad2ant2
    wi expcom com12 imp bi2anan9 relin1 3ad2ant1 inttr wceq resindi ss2in sseq1
    eqcoms uuniin ssres2 ax-mp sstr mpan syl6bi mpsyl 3ad2ant3 3jca pm2.43i cvv
    wb inex1g syl mpbird ) ACDZBCDZEZABFZCDZVHGZVHVHHVHIZJVHKKZLZVHIZMZVGVOVGVG
    AGZAAHAIZJAKKZLZAIZMZBGZBBHBIZJBKKZLZBIZMZEZVOVEVEWAVFVFWGCANCBNUAWHVJVKVNW
    AVJWGVPVQVJVTABUBUCOWAWGVKVQVPWGVKQVTWGVQVKWCWBVQVKQWFVQWCVKABUDRPSPTWAWGVN
    VTVPWGVNQVQWGVTVNWFWBVTVNQWCVTWFVNJVRWDFZLZVSWEFZUEZVTWFEWKVHIZVNJVRWDUFVSA
    WEBUGWLWMWJVHIZVNWMWNVAWKWJWKWJVHUHUIVMWJIZWNVNVLWIIWOABUJVLWIJUKULVMWJVHUM
    UNUOUPRUQSUQTURUOUSVGVHUTDZVIVOVAVEWPVFABCVBOUTVHNVCVD $.
    $( [28-Dec-2011] $)

  $( A square product is a preset.  (Contributed by FL, 28-Dec-2011.) $)
  sqpre $p |- ( A e. V -> ( A X. A ) e. Preset ) $=
    ( wcel cxp cvv cpreset xpexg anidms wrel ccom wss cid cuni cres relxp cpref
    w3a a1i scprefat 3jca isprs mpbird syl ) ABCZAADZECZUEFCZUDUFAABBGHUFUGUEIZ
    UEUEJUEKZLUEMMNUEKZQUFUHUIUJUHUFAAORUIUFAPRUJUFASRTEUEUAUBUC $.
    $( [28-Dec-2011] $)

  $( The relation induced by a preset on a part of its field is a preset.
     (Contributed by FL, 28-Dec-2011.) $)
  indpre $p |- ( ( R e. Preset /\ A e. B )
        -> ( R i^i ( A X. A ) ) e. Preset ) $=
    ( wcel cpreset cxp cin sqpre int2pre sylan2 ) ABDCEDAAFZEDCKGEDABHCKIJ $.
    $( [28-Dec-2011] $)

  $( A partial order is a preset.  (Contributed by FL, 1-May-2011.) $)
  posprs $p |- PosetRel C_ Preset $=
    ( vr cv wrel ccom wss ccnv cin cid cuni cres wceq w3a cab cps cpreset inss1
    sseq1 mpbiri eqcoms 3anim3i ss2abi df-ps df-prs 3sstr4i ) ABZCZUEUEDUEEZUEU
    EFZGZHUEIIJZKZLZAMUFUGUJUEEZLZAMNOULUNAUKUMUFUGUMUJUIUJUIKUMUIUEEUEUHPUJUIU
    EQRSTUAAUBAUCUD $.
    $( [1-May-2011] $)

  $( A poset is a preset.  (Contributed by FL, 19-Sep-2011.) $)
  posispre $p |- ( A e. PosetRel -> A e. Preset ) $=
    ( cps cpreset posprs sseli ) BCADE $.
    $( [19-Sep-2011] $)

  $( The empty set is a poset.  (Contributed by FL, 6-Oct-2008.) $)
  empos $p |- (/) e. PosetRel $=
    ( c0 cps wcel wrel ccom wss ccnv cin cid cuni cres wceq cvv w3a wb 0ex isps
    ax-mp rel0 uni0 co01 eqimssi cnv0 ineq2i inidm unieqi reseq2i eqtr2i 3eqtri
    eqtri res0 mpbir3an ) ABCZADZAAEZAFZAAGZHZIAJZJZKZLZAMCUMUNUPVBNOPMAQRSUOAA
    UAUBURAAHAVAUQAAUCUDAUEVAIAKAUTAIUTUSAUSATUFTUJUGIUKUHUIUL $.
    $( [6-Oct-2008] $)

  $( The converse of a preset is a preset.  The case
     ` ( ``' R e. Preset -> R e. Preset ) ` is true only if ` R ` is a
     relation.  See ~ dupre2 .  (Contributed by FL, 5-Jan-2009.) $)
  dupre1 $p |- ( R e. Preset -> `' R e. Preset ) $=
    ( cpreset wcel ccnv wrel ccom wss cid cuni cres relcnv a1i12 preorel impbid
    w3a a1d wb syl isprs cvv relcnvtr relrefcnv 3anbi123d cnvexg 3bitr4d ibi )
    ABCZADZBCZUGAEZAAFAGZHAIIJAGZOUHEZUHUHFUHGZHUHIIJUHGZOZUGUIUGUJUMUKUNULUOUG
    UJUMUGUJUMAKLUGUJUMAMZPNUGUJUKUNQUQAUARUGUJULUOQUQAUBRUCBASUGUHTCUIUPQABUDT
    UHSRUEUF $.
    $( [5-Jan-2009] $)

  $( The converse of a preset is a preset.  (Contributed by FL,
     19-Sep-2011.) $)
  dupre2 $p |- ( Rel R -> ( R e. Preset <-> `' R e. Preset ) ) $=
    ( wrel cpreset wcel ccnv dupre1 wceq wi dfrel2 eleq1 syl5ibr eqcoms impbid2
    sylbi ) ABZACDZAEZCDZAFOQEZAGRPHZAITASRPASGSCDQFASCJKLNM $.
    $( [19-Sep-2011] $)

  $( The converse of a poset is a poset.  In the general case
     ` ( ``' R e. PosetRel -> R e. PosetRel ) ` is not true.  See ~ dupos2 for
     a special case where the property holds.  (Contributed by FL,
     5-Jan-2009.) $)
  dupos1 $p |- ( R e. PosetRel -> `' R e. PosetRel ) $=
    ( cps wcel ccnv wrel ccom wss cin cid cuni cres wceq w3a isps wi relcnv a1i
    cnvco syl cvv cnvss syl5eqssr psrel relcnvfld dfrel2 wa incom reseq2 eqeq2d
    biimpa syl5eq ex ineq2 eqeq1d imbi2d syl5ibr sylbi 3anim123d sylbid pm2.43i
    sylc wb cnvexg mpbird ) ABCZADZBCZVFEZVFVFFZVFGZVFVFDZHZIVFJJZKZLZMZVEVPVEV
    EAEZAAFZAGZAVFHZIAJJZKZLZMVPBANVEVQVHVSVJWCVOVQVHOVEVHVQAPQQVSVJOVEVSVIVRDV
    FAARVRAUAUBQVEVQWAVMLZWCVOOZAUCZVEVQWDWFAUDSVQVKALZWDWEOAUEWDWEWGWCVFAHZVNL
    ZOWDWCWIWDWCUFWHVTVNVFAUGWDWCVTVNLWDWBVNVTWAVMIUHUIUJUKULWGVOWIWCWGVLWHVNVK
    AVFUMUNUOUPUQVAURUSUTVEVFTCVGVPVBABVCTVFNSVD $.
    $( [5-Jan-2009] $)

  $( The converse of a poset is a poset.  (Contributed by FL, 5-Jan-2009.) $)
  dupos2 $p |- ( Rel R -> ( R e. PosetRel <-> `' R e. PosetRel ) ) $=
    ( wrel cps wcel ccnv dupos1 wceq wi dfrel2 eleq1 biimpd sylbi syl5 impbid2
    ) ABZACDZAEZCDZAFRQEZCDZOPQFOSAGZTPHAIUATPSACJKLMN $.
    $( [5-Jan-2009] $)


  ${
    $d A r x y $.  $d R r x y $.
    $( An infimum is the supremum of the converse relation.  (Contributed by
       FL, 6-Sep-2009.) $)
    nfwval $p |- ( ( R e. U /\ A e. W ) -> ( R infw A ) = ( `' R supw A ) ) $=
      ( vr vx vy cv ccnv cspw co cinf ovex wceq cnveq oveq1d oveq2 df-nfw ov5 )
      EFGBACDEHZIZFHZJKBIZAJKLUCUBJKUCAJMTBNUAUCUBJTBOPUBAUCJQFGERS $.
      $( [6-Sep-2009] $)
  $}

  ${
    $d r y a b $.
    $( Define the maximal elements of a set.  I.e. the elements of the set that
       are not smaller than the other elements.  Meaningful if ` r ` is at
       least a preset.  Read ` ( mxl `` R ) ` as the maximal elements of the
       set ` U. U. R ` preordered by ` R ` .  Bourbaki E III 8.
       Experimental. $)
    df-mxl $a |- mxl = { <. r , y >. |
       y = { a e. U. U. r | A. b e. U. U. r ( a r b -> a = b ) } } $.
  $}

  ${
    $d r y a b $.
    $( Define the minimal elements of a set.  I.e. the elements of the set that
       are not greater than the other elements.  Meaningful is ` r ` is at
       least a preset.  Read ` ( mnl `` R ) ` as the minimal elements of the
       set ` U. U. R ` preordered by ` R ` .  Bourbaki E III 8.
       Experimental. $)
    df-mnl $a |- mnl = { <. r , y >. |
       y = { a e. U. U. r | A. b e. U. U. r ( b r a -> b = a ) } } $.
  $}

  ${
    $d r y $.
    $( Define the greatest element of a poset.  I.e. the element of the poset
       that is larger than the other elements.  Meaningful is ` r ` is at least
       a poset (otherwise there could be more than one supremum due to
       cycles).  Bourbaki E III 10.  Experimental. $)
    df-ge $a |- ge = { <. r , y >. | y = ( r supw dom r ) } $.
  $}

  ${
    $d r y $.
    $( Define the least element of a poset.  I.e. the element of the poset that
       is smaller than the other elements.  Meaningful is ` r ` is at least a
       poset.  Experimental. $)
    df-se $a |- se = { <. r , y >. | y = ( r infw dom r ) } $.
  $}

  ${
    $d R r x $.  $d X r x $.
    gepsup.1 $e |- X = dom R $.
    $( The greatest element of a poset is the supremum of the poset.
       (Contributed by FL, 19-Sep-2011.) $)
    gepsup $p |- ( R e. A -> ( ge ` R ) = ( R supw X ) ) $=
      ( vx vr wcel cge cfv cv cdm cspw co wceq copab df-ge fveq1i cvv ovex id
      dmeq syl6eqr oveq12d fvopabg mpan2 syl5eq ) BAGZBHIBEJFJZUHKZLMZNFEOZIZBC
      LMZBHUKEFPQUGUMRGULUMNBCLSFEBUJUMARUHBNZUHBUICLUNTUNUIBKCUHBUADUBUCUDUEUF
      $.
      $( [19-Sep-2011] $)
  $}

  ${
    $d R r x $.  $d X r x $.
    seinf.1 $e |- X = dom R $.
    $( The least element of a poset is the infimum of the poset.  (Contributed
       by FL, 19-Sep-2011.) $)
    seinf $p |- ( R e. A -> ( se ` R ) = ( R infw X ) ) $=
      ( vx vr wcel cse cfv cv cdm cinf co wceq copab df-se fveq1i cvv ovex id
      dmeq syl6eqr oveq12d fvopabg mpan2 syl5eq ) BAGZBHIBEJFJZUHKZLMZNFEOZIZBC
      LMZBHUKEFPQUGUMRGULUMNBCLSFEBUJUMARUHBNZUHBUICLUNTUNUIBKCUHBUADUBUCUDUEUF
      $.
      $( [19-Sep-2011] $)
  $}

  $( The least element of a poset is the greatest element of the converse
     poset.  (Contributed by FL, 30-Dec-2011.) $)
  sege $p |- ( R e. PosetRel -> ( se ` R ) = ( ge ` `' R ) ) $=
    ( cps wcel cse cfv cdm cinf ccnv cspw cge eqid seinf cvv dmexg nfwval mpdan
    co wceq cpreset wi posispre domcnvpre dupos1 gepsup syl eqcomd oveq2 eqcoms
    syl5req eqeq1d syl5ibr 3syl pm2.43i 3eqtrd ) ABCZADEAAFZGQZAHZUPIQZURJEZBAU
    PUPKZLUOUPMCUQUSRABNUPABMOPUOUSUTRZUOASCZURFZUPRZUOVBTAUAVCUPUPVDVAAUPVAUBU
    IUOVBVEURVDIQZUTRUOUTVFUOURBCUTVFRAUCBURVDVDKUDUEUFVEUSVFUTUSVFRUPVDUPVDURI
    UGUHUJUKULUMUN $.
    $( [30-Dec-2011] $)

  ${
    $d r x y a b $.
    $( In French, "majorants". $)
    $( Define the upper bounds of a set ` x ` .  Meaningful if ` r ` is at
       least a preset, and ` x ` a subset of the field of ` r ` .  Bourbaki
       E.III.9 def. 5.  Experimental. $)
    df-ub $a |- ub = { <. <. r , x >. , y >. |
      y = { a e. U. U. r | A. b e. x b r a } } $.
  $}

  ${
    $d r x y a b $.
    $( In French, "minorants". $)
    $( Define the lower bounds of a set ` x ` .  Meaningful if ` r ` is at
       least a preset, and ` x ` a subset of the field of ` r ` .
       Experimental. $)
    df-lb $a |- lb = { <. <. r , x >. , y >. |
       y = { a e. U. U. r | A. b e. x a r b } } $.
  $}

  ${
    $d r x y z $.
    $( An antidirected set (also called a set filtering on the left by
       Bourbaki) is a preset whose every pair of elements has a lower bound. $)
    df-antidir $a |- AntiDir = ( Preset i^i
     { r | A. x e. U. U. r A. y e. U. U. r
          E. z e. U. U. r z e. ( r lb { x , y } ) } ) $.
  $}

  ${
    $d A a b r x y $.  $d R a b r x y $.  $d X a $.
    ubos2.1 $e |- X = dom R $.
    $( The upper bounds of ` A ` .  (Contributed by FL, 18-Sep-2011.) $)
    ubos2 $p |- ( ( R e. Preset /\ A e. B ) -> ( R ub A ) = { a e. X |
      A. b e. A b R a } ) $=
      ( vr vx vy cpreset wcel cub cv wral crab wceq cuni wa cvv co wbr preodom2
      cdm wi eqtr anim12i dmexg eqeltrrd adantr rabexg syl unieq unieqd ralbidv
      elex breq rabeqbidv raleq rabbidv cxp cres df-ub wrel reloprab reldmoprab
