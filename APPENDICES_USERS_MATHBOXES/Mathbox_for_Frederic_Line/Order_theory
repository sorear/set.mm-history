    $( A lattice is a direction.  (Contributed by FL, 19-Sep-2011.) $)
    latdir $p |- LatRel C_ DirRel $=
      ( vp vx vy vz cla cdir cv cps wcel cpr cspw co cdm cinf wa wral adantr wi
      cvv ralimdva cpreset cub wrex posispre zfpair eqid 3exp com3l ax-mp eleq1
      supaub rcla4ev ex syld com12 imp jca isla isdir2 3imtr4i ssriv ) AEFAGZHI
      ZVBBGZCGZJZKLZVBMZIZVBVFNLVHIZOZCVHPZBVHPZOZVBUAIZDGZVBVFUBLZIZDVHUCZCVHP
