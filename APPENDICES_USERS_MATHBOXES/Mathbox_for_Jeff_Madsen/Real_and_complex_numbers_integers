  ${
    $d A x y $.
    $( A finite set of real numbers has a maximum.  (Contributed by Jeff
       Madsen, 2-Sep-2009.)  (Moved to ~ fimaxre in main set.mm and may be
       deleted by mathbox owner, JM. --NM 22-Mar-2013.) $)
    fimaxreOLD $p |- ( ( A C_ RR /\ A e. Fin /\ A =/= (/) )
                                            -> E. x e. A A. y e. A y <_ x ) $=
      ( fimaxre ) ABCD $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d A x y $.
    $( A nonempty finite set of real numbers has a maximum.  (Contributed by
       Jeff Madsen, 27-May-2011.)  (Moved to ~ fimaxre2 in main set.mm and may
       be deleted by mathbox owner, JM. --NM 22-Mar-2013.) $)
    fimaxre2OLD $p |- ( ( A C_ RR /\ A e. Fin /\ A =/= (/) )
                                      -> E. x e. RR A. y e. A y <_ x ) $=
      ( cr wss cfn wcel cv cle wbr wral wrex c0 wne fimaxre2 3adant3 ) CDECFGBH
      AHIJBCKADLCMNABCOP $.
      $( [27-May-2011] $)
  $}

