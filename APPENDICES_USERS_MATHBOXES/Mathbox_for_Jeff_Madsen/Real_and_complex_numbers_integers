  ${
    $d M x $.  $d N x $.  $d K x $.
    $( Split a finite interval of integers into two parts.  (Moved to ~ fzsplit
       in main set.mm and may be deleted by mathbox owner, JM. --NM
       16-Sep-2013.) $)
    fzsplitOLD $p |- ( ( N e. A /\ K e. ( M ... N ) ) -> ( M ... N )
                              = ( ( M ... K ) u. ( ( K + 1 ) ... N ) ) ) $=
      ( fzsplit ) ABCDE $.
      $( [17-Jun-2010] $)

    $d J x $.  $d A x $.
    $( Condition for two finite intervals of integers to be disjoint.  (Moved
       to ~ fzdisj in main set.mm and may be deleted by mathbox owner, JM. --NM
       16-Sep-2013.) $)
    fzdisjOLD $p |- ( ( K e. A /\ K < M ) ->
                                ( ( J ... K ) i^i ( M ... N ) ) = (/) ) $=
      ( fzdisj ) ABCDEF $.
      $( [17-Jun-2010] $)
  $}

  $( Add one to an element of a finite set of integers.  (Moved to ~ fzp1elp1
     in main set.mm and may be deleted by mathbox owner, JM. --NM
     28-Feb-2014.) $)
  fzp1elp1OLD $p |- ( ( N e. A /\ K e. ( M ... N ) ) ->
                                  ( K + 1 ) e. ( M ... ( N + 1 ) ) ) $=
    ( fzp1elp1 ) ABCDE $.
    $( [6-Jun-2010] $)

