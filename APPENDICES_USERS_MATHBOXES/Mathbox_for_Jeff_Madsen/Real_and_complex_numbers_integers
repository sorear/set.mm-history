  $( Two nonnegative numbers are zero iff their sum is zero.  (Moved to ~ add20
     in main set.mm and may be deleted by mathbox owner, JM. --NM
     16-Sep-2013.) $)
  add20OLD $p |- ( ( ( A e. RR /\ 0 <_ A ) /\ ( B e. RR /\ 0 <_ B ) )
                        -> ( ( A + B ) = 0 <-> ( A = 0 /\ B = 0 ) ) ) $=
    ( add20 ) ABC $.
    $( [2-Sep-2009] $)

  $( Relation between sums and differences..  (Moved to ~ addsubeq4 in main
     set.mm and may be deleted by mathbox owner, JM. --NM 16-Sep-2013.) $)
  addsubeq4OLD $p |- ( ( ( A e. CC /\ B e. CC ) /\ ( C e. CC /\ D e. CC ) ) ->
                    ( ( A + B ) = ( C + D ) <-> ( C - A ) = ( B - D ) ) ) $=
    ( addsubeq4 ) ABCDE $.
    $( [17-Jun-2010] $)


  $( An integer mod ` B ` lies in the first ` B ` nonnegative integers.
     (Contributed by Jeff Madsen, 17-Jun-2010.) $)
  zmodfzcl $p |- ( ( A e. ZZ /\ B e. NN ) ->
                                  ( A mod B ) e. ( 0 ... ( B - 1 ) ) ) $=
    ( zmodcl2 ) ABC $.
    $( [17-Jun-2010] $)

  $( Two ways of expressing the remainder when ` A ` is divided by ` B ` .
     (Contributed by Jeff Madsen, 17-Jun-2010.) $)
  rdr $p |- ( ( A e. RR /\ B e. RR+ ) ->
                      ( ( A - ( A mod B ) ) / B ) = ( |_ ` ( A / B ) ) ) $=
    ( cr wcel crp wa cmo co cmin cdiv cfl cfv cmul modval oveq2d cc wceq adantl
    syl2anc eqtrd recn adantr rpcn cz rerpdivcl flcl zcn mulcl nncan oveq1d cc0
    3syl wne rpne0 divcan3 syl3anc ) ACDZBEDZFZAABGHZIHZBJHBABJHZKLZMHZBJHZVCUS
    VAVDBJUSVAAAVDIHZIHZVDUSUTVFAIABNOUSAPDZVDPDZVGVDQUQVHURAUAUBUSBPDZVCPDZVIU
    RVJUQBUCRZUSVBCDVCUDDVKABUEVBUFVCUGULZBVCUHSAVDUISTUJUSVKVJBUKUMZVEVCQVMVLU
    RVNUQBUNRVCBUOUPT $.
    $( [17-Jun-2010] $)

  $( Membership in a later set of upper integers.  (Moved to ~ eluzadd in main
     set.mm and may be deleted by mathbox owner, JM. --NM 2-Sep-2013.) $)
  eluzaddOLD $p |- ( ( N e. ( ZZ>= ` M ) /\ K e. ZZ ) ->
                                      ( N + K ) e. ( ZZ>= ` ( M + K ) ) ) $=
    ( eluzadd ) ABCD $.
    $( [2-Sep-2009] $)

  $( Membership in an earlier set of upper integers.  (Moved to ~ eluzsub in
     main set.mm and may be deleted by mathbox owner, JM. --NM 2-Sep-2013.) $)
  eluzsubOLD $p |- ( ( M e. ZZ /\ K e. ZZ /\ N e. ( ZZ>= ` ( M + K ) ) )
                                          -> ( N - K ) e. ( ZZ>= ` M ) ) $=
    ( eluzsub ) ABCD $.
    $( [2-Sep-2009] $)

  $( Choices for an element of an upper interval of integers.  (Moved to ~ uzm1
     in main set.mm and may be deleted by mathbox owner, JM. --NM
     16-May-2013.) $)
  uzm1OLD $p |- ( N e. ( ZZ>= ` M ) -> ( N = M \/ ( N - 1 ) e. ( ZZ>= ` M ) ) )
      $=
    ( uzm1 ) ABC $.
    $( [2-Sep-2009] $)

  $( Choices for an element of an upper interval of integers.  (Moved to ~ uzp1
     in main set.mm and may be deleted by mathbox owner, JM. --NM
     16-May-2013.) $)
  uzp1OLD $p |- ( N e. ( ZZ>= ` M ) -> ( N = M \/ N e. ( ZZ>= ` ( M + 1 ) ) ) )
      $=
    ( uzp1 ) ABC $.
    $( [2-Sep-2009] $)

  ${
    $d M m n $.  $d N m n $.
    $( A finite interval of integers is finite.  (Moved to ~ fzfi in main
       set.mm and may be deleted by mathbox owner, JM. --NM 16-Sep-2013.) $)
    fzfiOLD $p |- ( N e. ( ZZ>= ` M ) -> ( M ... N ) e. Fin ) $=
      ( cfz co cfn wcel cuz cfv fzfi a1i ) ABCDEFBAGHFABIJ $.
      $( [2-Sep-2009] $)
  $}

