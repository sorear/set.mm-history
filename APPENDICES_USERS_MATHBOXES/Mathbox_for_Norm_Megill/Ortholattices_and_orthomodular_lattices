  $}

  $( An atomic lattice with the covering property is a lattice. $)
  cvllat $p |- ( K e. CvLat -> K e. Lat ) $=
    ( clc wcel cal clat cvlatl atllat syl ) ABCADCAECAFAGH $.
    $( [5-Nov-2012] $)

  $( An atomic lattice with the covering property is a poset. $)
  cvlpos $p |- ( K e. CvLat -> K e. Poset ) $=
    ( clc wcel clat cpo cvllat latpos syl ) ABCADCAECAFAGH $.
    $( [5-Nov-2012] $)

