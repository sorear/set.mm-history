    hlexch3.a $e |- A = ( Atoms ` K ) $.
    $( A Hilbert lattice has the exchange property.  ( ~ atexch analog.) $)
    hlexch3 $p |- ( ( K e. HL /\ ( P e. A /\ Q e. A /\ X e. B )
        /\ ( P ./\ X ) = Z ) -> ( P .<_ ( X .\/ Q ) -> Q .<_ ( X .\/ P ) ) ) $=
      ( chlt wcel co wbr clc w3a wceq wi hlcvl cvlexch3 syl3an1 ) FQRFUARCARDAR
      IBRUBCIHSJUCCIDESGTDICESGTUDFUEABCDEFGHIJKLMNOPUFUG $.
      $( [15-Nov-2011] $)

    $( A Hilbert lattice has the exchange property.  Part of Definition 7.8 of
       [MaedaMaeda] p. 32. $)
    hlexch4 $p |- ( ( K e. HL /\ ( P e. A /\ Q e. A /\ X e. B )
       /\ ( P ./\ X ) = Z )
    -> ( P .<_ ( X .\/ Q ) <-> ( X .\/ P ) = ( X .\/ Q ) ) ) $=
      ( chlt wcel co wceq clc w3a wbr wb hlcvl cvlexch4 syl3an1 ) FQRFUARCARDAR
      IBRUBCIHSJTCIDESZGUCICESUHTUDFUEABCDEFGHIJKLMNOPUFUG $.
      $( [15-Nov-2011] $)
  $}

  ${
