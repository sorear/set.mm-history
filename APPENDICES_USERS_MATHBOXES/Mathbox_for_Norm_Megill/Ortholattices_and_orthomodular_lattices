      TT $.
      $( [20-Oct-2011] $)

    $( Double negative law for orthoposets.  ( ~ ococ analog.) $)
    opococ $p |- ( ( K e. OP /\ X e. B ) -> ( O ` ( O ` X ) ) = X ) $=
      ( cops wcel wa cfv wceq cple wbr wi w3a cjn co cp1 cmee eqid cp0 3anidm23
      oposlem simp1d simp2d ) BGHZDAHZIZDCJZAHZUICJDKZDDBLJZMUIUIULMNZUHUJUKUMO
