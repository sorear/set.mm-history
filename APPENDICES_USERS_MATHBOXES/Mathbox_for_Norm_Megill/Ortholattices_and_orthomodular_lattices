    $( Lemma for ~ 3at . $)
    3atlem7 $p |- ( ( ( K e. HL /\ ( P e. A /\ Q e. A /\ R e. A )
        /\ ( S e. A /\ T e. A /\ U e. A ) ) /\ ( -. R .<_ ( P .\/ Q )
            /\ P =/= Q )
        /\ ( ( P .\/ Q ) .\/ R ) .<_ ( ( S .\/ T ) .\/ U ) )
      -> ( ( P .\/ Q ) .\/ R ) = ( ( S .\/ T ) .\/ U ) ) $=
      ( wcel w3a co wbr wn wa simpl1 chlt wne wceq simpl2l simpl2r simpr simpl3
      3atlem6 syl131anc 3atlem5 pm2.61dan ) IUANBANCANDANOEANFANGANOOZDBCHPZJQR
