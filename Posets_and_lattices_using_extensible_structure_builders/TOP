    $( A poset ordering is reflexive. $)
    posref $p |- ( ( K e. Poset /\ X e. B ) -> X L X ) $=
      ( cpo wcel wa wbr wceq wi w3a id 3jca poslem sylan2 simp1 syl ) BGHZDAHZI
