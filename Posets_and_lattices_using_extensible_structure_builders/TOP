$(
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
               Posets and lattices using extensible structure builders
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
$)

  $c le $.
  $c Poset $.
  $c lt $.
  $c lub $.
  $c glb $.
  $c join $.
  $c meet $.

  $( Extend class notation with less-than-or-equal for posets. $)
  cple $a class le $.

  $( Extend class notation with the class of posets. $)
  cpo $a class Poset $.

  $( Extend class notation with less-than for posets. $)
  cplt $a class lt $.

  $( Extend class notation with poset least upper bound. $)
  club $a class lub $.

  $( Extend class notation with poset greatest lower bound. $)
  cglb $a class glb $.

  $( Extend class notation with poset join. $)
  cjn $a class join $.

  $( Extend class notation with poset meet. $)
  cmee $a class meet $.

  $( Def. 8.4.2 p. 204. $)
  $( Define less-than-or-equal ordering for posets and related structures. $)
  df-ple $a |- le = ( p e. _V |-> ( p ` 10 ) ) $.

  $( Utility theorem: self-referencing, index-independent form of ~ df-ple . $)
  pleid $p |- le = ( f e. _V |-> ( f ` ( le ` ndx ) ) ) $=
    ( cple c10 df-ple 10nn ndxid ) BACADEF $.
    $( [9-Nov-2012] $)

  $( Utility theorem for working with explicit posets without direct reference
     to the actual component indices. $)
  poshyp $p |- ( Base ` ndx ) =/= ( le ` ndx ) $=
    ( vf cnx cbs cfv c10 cple c1 cn wcel wceq 1nn df-base ndxarg ax-mp clt wbr
    wne 1lt10 1re 10re ltnei necomi eqnetri 10nn df-ple neeqtrri ) BCDZEBFDZUGG
    EGHIUGGJKCAGALMNEGGEOPEGQRGESTUANUBUCEHIUHEJUDFAEAUEMNUF $.
    $( [9-Nov-2012] $)


  ${
    $d f b r x y z $.
    $( Def. 8.4.1 p. 203. $)
    $( Define the class of posets. $)
    df-poset $a |- Poset = { f | E. b E. r
              ( b = ( Base ` f )
               /\ r = ( le ` f )
              /\ ( b =/= (/)
               /\ A. x e. b A. y e. b A. z e. b ( x r x
                 /\ ( ( x r y /\ y r x ) -> x = y )
                 /\ ( ( x r y /\ y r z ) -> x r z ) ) ) ) } $.
  $}

  ${
    $d b p r x y z B $.  $d b p r K $.  $d b p r x y z L $.
    ispos.b $e |- B = ( Base ` K ) $.
    ispos.l $e |- L = ( le ` K ) $.
    $( The predicate "is a poset." $)
    ispos $p |- ( K e. Poset <-> ( K e. _V /\ B =/= (/)
            /\ A. x e. B A. y e. B A. z e. B ( x L x
                 /\ ( ( x L y /\ y L x ) -> x = y )
                 /\ ( ( x L y /\ y L z ) -> x L z ) ) ) ) $=
      ( vb vr vp cvv wcel cv wceq c0 wne wbr wa wi w3a wral wex cpo cbs cfv
