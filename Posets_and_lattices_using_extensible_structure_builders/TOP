    $( A poset ordering is transitive. $)
    postr $p |- ( ( K e. Poset /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X L Y /\ Y L Z ) -> X L Z ) ) $=
      ( cpo wcel w3a wa wbr wceq wi poslem simp3d ) BIJDAJEAJFAJKLDDCMDECMZEDCM
      LDENOREFCMLDFCMOABCDEFGHPQ $.
