    $( Lattice join distributes over itself. $)
    latjjdir $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B ) ) ->
             ( ( X J Y ) J Z ) = ( ( X J Z ) J ( Y J Z ) ) ) $=
