  ${
    $d x y z w v u t h g f n m k j $.
    $( Lemma for ~ grothprim .  Expand the membership of an unordered pair into
       primitives. $)
    grothprimlem $p |- ( { u , v } e. w <-> E. g ( g e. w /\ A. h
      ( h e. g <-> ( h = u \/ h = v ) ) ) ) $=
      ( cv cpr wcel wceq wo cab wb wal wa wex dfpr2 eleq1i clabel bitr ) CFZBFZ
      GZAFZHEFZTIUDUAIJZEKZUCHDFZUCHUDUGHUELEMNDOUBUFUCETUAPQUEEDUCRS $.
      $( [31-Mar-2007] $) $( [29-Mar-2007] $)

    $( Grothendieck's Axiom ~ ax-groth expanded into set theory primitives
       using 163 symbols.  An open problem is whether a shorter equivalent
       exists (when expanded to primitives). $)
    grothprim $p |- E. y ( x e. y /\ A. z ( ( z e. y -> E. v ( v e. y /\
       A. w ( A. u ( u e. w -> u e. z ) -> ( w e. y /\ w e. v ) ) ) ) /\ E. w
      ( ( w e. z -> w e. y ) -> ( A. v ( ( v e. z -> E. t A. u ( E. g ( g e. w
      /\ A. h ( h e. g <-> ( h = v \/ h = u ) ) ) -> u = t ) ) /\ ( v e. y ->
      ( v e. z \/ E. u ( u e. z /\ E. g ( g e. w /\ A. h ( h e. g <-> ( h = u
      \/ h = v ) ) ) ) ) ) ) \/ z e. y ) ) ) ) $=
      ( cv wcel wss cin wi wal wrex wral cdif cdom wbr wo w3a wex wa wceq wb
      axgroth4 3anass dfss2 elin imbi12i albii rexbii df-rex bitr ralbii
      df-ral cpr wmo cvv visset difexg ax-mp c0 incom difdisj eqtr brdom6disj
      orbi1i ax-17 19.44 bitr4 19.35 grothprimlem mobii mo2 wn impexp eldif
      df-or imbi2i 3bitr4 anbi12i 19.26 exbii 3bitr2 anbi2i mpbi ) AJBJZKZDJZCJ
