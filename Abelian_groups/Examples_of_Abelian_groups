$)

  ${
    $d x y z $.
    $( Complex number addition is an Abelian group operation. $)
    cnaddabl $p |- + e. Abel $=
      ( vx vy vz caddc cc cc0 cv cneg axcnex axaddopr axaddass 0cn addid2t
      negclt wcel co wceq axaddcom mpdan negidt eqtr3d isgrpi isabli ) ABDEABCF
      DAGZHZEIJUDBGZCGKLUDMUDNZUDEOZUDUEDPZUEUDDPZFUHUEEOUIUJQUGUDUERSUDTUAUBJU
      DUFRUC $.
      $( [7-Nov-2006] $) $( [5-Nov-2006] $)
  $}

  ${
    $d u x y z A $.
    ablsn.1 $e |- A e. V $.
    $( The Abelian group operation for the singleton group. $)
    ablsn $p |- { <. <. A , A >. , A >. } e. Abel $=
      ( vx vy cop csn grpsn cxp wf wf1o opex f1osn f1of ax-mp wceq wb xpsn
      feq2 mpbir cv co wcel wa opreq12 opreq2 opreq1 sylan9eq eqtr4d elsn
      syl2anb isabli ) CDAAEZAEFZAFZABGUNUNHZUNUMIZULFZUNUMIZUQUNUMJURULAAAKBLU
      QUNUMMNUOUQOUPURPAABBQUOUQUNUMRNSCTZAOZDTZAOZUSVAUMUAZVAUSUMUAZOUSUNUBVAU
      NUBUTVBUCVCAAUMUAZVDUSAVAAUMUDUTVBVDVAAUMUAVEUSAVAUMUEVAAAUMUFUGUHCAUIDAU
      IUJUK $.
      $( [29-Nov-2006] $) $( [5-Nov-2006] $)
  $}

  ${
    $d u x $.
    $( The group identity element of complex number addition is zero.
       (Contributed by Steve Rodriguez, 3-Dec-2006.) $)
    cnid $p |- 0 = ( Id ` + ) $=
      ( vx vu caddc cgi cfv cc0 cv co wceq cc wral addid2t rgen wcel wb 0cn
      crab cuni opreq1 eqeq1d ralbidv cgr cabl cnaddabl ablgrp ax-mp axaddopr
      grprnOLD eqid grpidval eqcomi a1i id eqeq12d bibi12d wreu grpideu reuuni1
      mpan2 vtoclga mpbi ) CDEZFFAGZCHZVCIZAJKZVBFIZVEAJVCLMFJNVFVGOZPBGZVCCHZV
      CIZAJKZVLBJQRZVIIZOZVHBFJVIFIZVLVFVNVGVPVKVEAJVPVJVDVCVIFVCCSTUAVPVMVBVIF
      VMVBIVPVBVMCUBNZVBVMICUCNVQUDCUEUFZABVBCJCJVRUGUHZVBUIUJUFUKULVPUMUNUOVIJ
      NVLBJUPZVOVQVTVRABCJVSUQUFVLBJURUSUTUFVAUK $.
      $( [14-Jan-2008] $) $( [28-Nov-2006] $)
  $}

  ${
    $d u x $.
    $( The group identity element of complex number addition is zero.
       (Contributed by Steve Rodriguez, 3-Dec-2006.) $)
    cnidOLD $p |- ( Id ` + ) = 0 $=
      ( vx vu cc0 cv caddc co wceq cc wral cgi cfv addid2t rgen wcel wb 0cn
      crab cuni opreq1 eqeq1d ralbidv cgr cabl cnaddabl ablgrp ax-mp axaddopr
      grprnOLD eqid grpidval eqeq12 eqcom sylanb mpan bibi12d wreu grpideu
      reuuni1 mpan2 vtoclga mpbi ) CADZEFZVBGZAHIZEJKZCGZVDAHVBLMCHNVEVGOZPBDZV
      BEFZVBGZAHIZVLBHQRZVIGZOZVHBCHVICGZVLVEVNVGVPVKVDAHVPVJVCVBVICVBESTUAVFVM
      GZVPVNVGOZEUBNZVQEUCNVSUDEUEUFZABVFEHEHVTUGUHZVFUIUJUFVMVFGVPVRVQVMVFVICU
      KVFVMULUMUNUOVIHNVLBHUPZVOVSWBVTABEHWAUQUFVLBHURUSUTUFVA $.
      $( [4-Dec-2006] $) $( [28-Nov-2006] $)
  $}

  ${
    $d y A $.
    $( Value of the group inverse of complex number addition.  (Contributed by
       Steve Rodriguez, 3-Dec-2006.) $)
    addinv $p |- ( A e. CC -> ( ( inv ` + ) ` A ) = -u A ) $=
      ( vy cc wcel caddc cgn cfv cc0 cmin co cneg cv wceq crab cuni wa
      axaddcom eqeq1d rabbidv unieqd 0cn subvalt mpan cgr cabl cnaddabl ablgrp
      ax-mp axaddopr grprnOLD cnid eqid grpinvval 3eqtr4rd df-neg syl6eqr ) ACDZAE
      FGZGZHAIJZAKUQABLZEJZHMZBCNZOZVAAEJZHMZBCNZOZUTUSUQVDVHUQVCVGBCUQVACDPVBV
      FHAVAQRSTHCDUQUTVEMUABHAUBUCEUDDZUQUSVIMEUEDVJUFEUGUHZBAHEURCECVKUIUJUKUR
      ULUMUCUNAUOUP $.
      $( [4-Dec-2006] $) $( [3-Dec-2006] $)
  $}

  ${
    $d x y z $.
    $( Nonzero complex number multiplication is an Abelian group operation.
       (Contributed by Steve Rodriguez, 12-Feb-2007.) $)
