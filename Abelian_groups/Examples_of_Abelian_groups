  ${
    $d x y z $.
    $( Complex number addition is an Abelian group operation. $)
    cnaddabl $p |- + e. Abel $=
      ( vx vy vz caddc cc cc0 cv cneg axcnex axaddopr axaddass 0cn addid2t
      negclt wcel co wceq axaddcom mpdan negidt eqtr3d isgrpi isabli ) ABDEABCF
      DAGZHZEIJUDBGZCGKLUDMUDNZUDEOZUDUEDPZUEUDDPZFUHUEEOUIUJQUGUDUERSUDTUAUBJU
      DUFRUC $.
      $( [7-Nov-2006] $) $( [5-Nov-2006] $)
  $}

  ${
    $d u x y z A $.
    ablsn.1 $e |- A e. V $.
    $( The Abelian group operation for the singleton group. $)
    ablsn $p |- { <. <. A , A >. , A >. } e. Abel $=
      ( vx vy vz cop csn snex cxp wf wf1o opex f1osn f1of ax-mp wceq wb xpsn
      feq2 mpbir cv co wcel w3a wa opreq2 opreq1 cfv df-opr fvsn eqtr syl6eq
      sylan9eq opreq1d sylan9eqr 3impa opreq2d 3impb eqtr4d elsn syl3anb snid
      id 3eqtr4a sylbi a1i isgrpi opreq12 syl2anb isabli ) CDAAFZAFGZAGZCDEAVLA
      VMAHVMVMIZVMVLJZVKGZVMVLJZVPVMVLKVQVKAAALZBMVPVMVLNOVNVPPVOVQQAABBRVNVPVM
