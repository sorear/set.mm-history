      GUHURBEUQTCNOFGJKUOFUDZUPBEOUTEUAABDNFHLUIUJBCEOGIMUIUKUL $.
      $( [8-Jan-2009] $) $( [19-Dec-2008] $)
  $}

  ${
    $d ph z $.  $d ps z $.  $d x z $.  $d y z $.
