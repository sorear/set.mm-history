      SWCNPVKVPWCNUSVKWBBVBUTVCVDVEADVFTEVFTVNVOTJKDEFGVMHIVMVGVHVIVJ $.
      $( [6-May-2014] $)

    $( The projection onto the second coordinate is continuous.  (Contributed
       by Mario Carneiro, 6-May-2014.) $)
    cnmpt2nd $p |- ( ph ->
                     ( x e. X , y e. Y |-> y ) e. ( ( J tX K ) Cn K ) ) $=
      ( vz cv c2nd co cfv cmpt wfn cvv wcel cmpt2 cxp cres ctx ccn wceq wss wfo
      fo2nd fofn ax-mp ssv fnssres mp2an dffn5v mpbi fvres mpteq2ia fveq2 op2nd
      cop vex syl6eq mpt2mpt 3eqtri ctop eqid tx2cn syl2anc syl5eqelr ) ABCFGCM
