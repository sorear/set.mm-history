      VMWLWIWGNZWMWNVQFGZVRFGZWTABVFABVGXAVQTGVRTGWTXBVQVHVRVHVQVRVTVIRVJSVKVL
      $.
      $( [13-Nov-2013] $) $( [18-May-2007] $)

    $( An open interval of reals in terms of a ball. $)
    ioo2bl $p |- ( ( A e. RR /\ B e. RR /\ A < B ) -> ( A (,) B ) =
     ( ( ( A + B ) / 2 ) ( ball ` D ) ( ( B - A ) / 2 ) ) ) $=
      ( cr wcel clt wbr cioo co caddc c2 cdiv cmin wceq wa cc recn 3adant3 crp
