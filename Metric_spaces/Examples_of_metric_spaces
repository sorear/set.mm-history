      UHUIUJ $.
      $( [16-May-2007] $)

    $( The base set for the metric for real numbers. $)
    remetba $p |- RR = dom dom D $=
      ( cdm cr cxp cabs cmin ccom cres cin dmeqi dmres wceq cc ax-resscn xpss12
      wss mp2an wf absf subopr fdmi sseqtr4i df-ss mpbi 3eqtri dmxpid eqtr2i
      fco ) ACZCDDEZCDUJUKUJFGHZUKIZCUKULCZJZUKAUMBKULUKLUKUNQUOUKMUKNNEZUNDNQZ
      UQUKUPQOODNDNPRUPDULNDFSUPNGSUPDULSTUAUPNDFGUIRUBUCUKUNUDUEUFKDUGUH $.
      $( [30-Aug-2007] $)

    $( The absolute value metric determines a metric space on the reals. $)
    remet $p |- D e. Met $=
      ( cabs cmin ccom cr cxp cres cme wcel eqid cnmet metres ax-mp eqeltri ) A
      CDEZFFGHZIBPIJQIJPPKLPFMNO $.
      $( [10-Feb-2007] $)

    $( A ball in terms of an open interval of reals. $)
    bl2ioo $p |- ( ( A e. RR /\ B e. RR /\ 0 < B ) -> ( A ( ball ` D
                  ) B ) = ( ( A - B ) (,) ( A + B ) ) ) $=
      ( vw cr wcel clt wbr w3a cfv co cmin wa wb cxr rexr syl cabs wceq cc0 cbl
      caddc cioo cv cme remet remetba elbl mpanl1 3impb resubcl elioo2 syl11anc
      readdcl 3anass remetdval recn abssub syl2an eqtrd adantlr breq1d absdiflt
      cc 3coml 3expa bitr2d pm5.32da syl5bb bitrd 3adant3 bitr4d eqrdv ) AFGZBF
