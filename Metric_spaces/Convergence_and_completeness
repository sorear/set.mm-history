    $( The set of Cauchy sequences on a metric space. $)
    caufval $p |- ( M e. Met -> ( Cau ` M ) = { f |
           ( f (_ ( CC X. X ) /\ A. x e. RR ( 0 < x ->
          E. j e. ZZ A. k e. ZZ A. m e. ZZ ( ( j <_ k /\ j <_ m ) ->
            ( ( f ` k ) e. X /\ ( f ` m ) e. X /\
             ( ( f ` k ) D ( f ` m ) ) < x ) ) ) ) } ) $=
      ( vz vw cv cc c1st cfv cxp wss cc0 clt wbr cle wa wcel c2nd co w3a wi cz
      wral wrex cr cab cmt cca wceq fveq2 syl6eqr xpeq2 syl sseq2d eleq2d
      opreqd breq1d 3anbi123d imbi2d ralbidv rexralbidv anbi12d abbidv df-cau
