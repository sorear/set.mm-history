$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                       Convergence and completeness
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ~~>m $.  $( Limit arrow. $)
  $c Cau $.
  $c CMet $.

  $( Extend class notation with a function on metric spaces whose value is the
     convergence relation for limit sequences in the space. $)
  clm $a class ~~>m $.

  $( Extend class notation with a function on metric spaces whose value is the
     set of all Cauchy sequences of the space. $)
  cca $a class Cau $.

  $( Extend class notation with class of complete metric spaces. $)
  cms $a class CMet $.

  ${
    $d j k m x y f z w $.
    $( Define a function on metric spaces whose value is the convergence
       relation for the space.  Although ` f ` is typically a function from
       upper integers to the metric space, it doesn't have to be.
       Unfortunately, the expression after " ` w = ` " must exist to use
       ~ fvopab4 , and we use the otherwise unnecessary conjunct
       ` f C_ ( CC X. dom dom z ) ` to ensure that.  This could be changed to
       the more liberal (but more complex)
       ` f C_ ( CC X. ( dom dom z u. { (/) } ) ) ` if we want to allow for
       functions with undefined values. $)
    df-lm $a |- ~~>m = { <. z , w >. | ( z e. Met /\ w = { <. f , y >. |
      ( f C_ ( CC X. dom dom z ) /\ y e. dom dom z /\ A. x e. RR ( 0 < x ->
       E. j e. ZZ A. k e. ZZ ( j <_ k -> ( ( f ` k ) e. dom dom z /\
           ( ( f ` k ) D y ) < x ) ) ) ) } ) } $.

    $( Define a function on metric spaces whose value is the set of Cauchy
       sequences of the space. $)
    df-cau $a |- Cau = { <. z , w >. | ( z e. Met /\ w = { f |
           ( f C_ ( CC X. dom dom z ) /\ A. x e. RR ( 0 < x ->
          E. j e. ZZ A. k e. ZZ A. m e. ZZ ( ( j <_ k /\ j <_ m ) ->
            ( ( f ` k ) e. dom dom z /\ ( f ` m ) e. dom dom z /\
             ( ( f ` k ) D ( f ` m ) ) < x ) ) ) ) } ) } $.

    $( Define the class of complete metrics. $)
    df-cmet $a |- CMet = { x e. Met | A. f e. ( Cau ` x )
           E. y e. dom dom x f ( ~~>m ` x ) y } $.
  $}

  ${
    $d f j k m w x y z D $.  $d f w y z X $.
    lmfval.1 $e |- X = dom dom D $.
    $( The relation "sequence ` f ` converges to point ` y ` " in a metric
       space. $)
    lmfval $p |- ( D e. Met -> ( ~~>m ` D ) =
      { <. f , y >. | ( f C_ ( CC X. X ) /\ y e. X /\ A. x e. RR ( 0 < x ->
             E. j e. ZZ A. k e. ZZ ( j <_ k ->
      ( ( f ` k ) e. X /\ ( ( f ` k ) D y ) < x ) ) ) ) } ) $=
