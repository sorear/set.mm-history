    $( The set of Cauchy sequences on a metric space. $)
    caufval $p |- ( D e. Met -> ( Cau ` D ) = { f |
           ( f C_ ( CC X. X ) /\ A. x e. RR ( 0 < x ->
          E. j e. ZZ A. k e. ZZ A. m e. ZZ ( ( j <_ k /\ j <_ m ) ->
            ( ( f ` k ) e. X /\ ( f ` m ) e. X /\
             ( ( f ` k ) D ( f ` m ) ) < x ) ) ) ) } ) $=
      ( vz cme wcel cv cc wbr wi cz wral cvv wceq cdm vw cxp wss cc0 clt cle wa
