    $( A constant function is continuous (metric space version of
       ~ cnconst ). $)
    metcnconst $p |- ( ( ( C e. Met /\ D e. Met ) /\ ( B e. Y /\
             F : X --> { B } ) ) -> F e. ( J Cn K ) ) $=
      ( cme wcel wa csn wf ctop cuni ccn co opntop anim12i adantr uniopn2
      eleq2d feq2d bi2anan9r biimpar eqid cnconst syl11anc ) BMNZCMNZOZAHNZGAPZ
