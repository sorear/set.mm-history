    $( A continuous function is a mapping (metric space version of ~ cnf ). $)
    metcnf $p |- ( ( C e. Met /\ D e. Met /\
                F e. ( J Cn K ) ) -> F : X --> Y ) $=
      ( cme wcel ccn co w3a cuni wf ctop eqid cnf opntop id syl3an wb uniopn2
      feq2d wceq feq3 syl sylan9bb 3adant3 mpbid ) ALMZBLMZCDENOMZPDQZEQZCRZFGC
