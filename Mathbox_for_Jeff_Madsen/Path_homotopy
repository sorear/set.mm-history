    $( Concatenate two path homotopies. $)
    phtpyco $p |- ( ( J e. Top /\ ( F e. ( II Cn J ) /\ G e. ( II Cn J ) /\
            H e. ( II Cn J ) ) /\ ( ( ( F ` 0 ) = ( G ` 0 ) /\ ( F ` 1 ) =
        ( G ` 1 ) ) /\ ( ( G ` 0 ) = ( H ` 0 ) /\ ( G ` 1 ) = ( H ` 1 ) ) ) )
              -> ( ( K e. ( F ( PHtpy ` J ) G ) /\ L e. ( G ( PHtpy ` J ) H ) )
                                        -> M e. ( F ( PHtpy ` J ) H ) ) ) $=
