$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Path homotopy
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c PHtpy $.
  $c ~=ph $.

  $( Extend class notation with the class of path homotopies between two
     continuous functions. $)
  cphtpy $a class PHtpy $.

  $( Extend class notation with the path homotopy relation. $)
  cphtpc $a class ~=ph $.

  ${
    $d x y f g z h s $.
    $( Define the function which takes a topological space ` X ` and two
       continuous functions ` F : II --> X ` and ` G : II --> X ` and returns
       the class of path homotopies from ` F ` to ` G ` .  Definition of
       [Hatcher] p. 25. $)
    df-phtpy $a |- PHtpy = { <. x , y >. | ( x e. Top /\ y =
        { <. <. f , g >. , z >. | ( ( ( f e. ( II Cn x ) /\ g e. ( II Cn x ) )
              /\ ( ( f ` 0 ) = ( g ` 0 ) /\ ( f ` 1 ) = ( g ` 1 ) ) ) /\ z =
                      { h e. ( ( II tX II ) Cn x ) | A. s e. ( 0 [,] 1 )
                    ( ( ( s h 0 ) = ( f ` s ) /\ ( s h 1 ) = ( g ` s ) ) /\
              ( ( 0 h s ) = ( f ` 0 ) /\ ( 1 h s ) = ( f ` 1 ) ) ) } ) } ) } $.
  $}

  ${
    $d J x y f g z h s $.
    $( Value of the path homotopy function on a topology.  (Contributed by Jeff
       Madsen, 2-Sep-2009.) $)
    phtpyfval $p |- ( J e. Top -> ( PHtpy ` J ) =
        { <. <. f , g >. , z >. | ( ( ( f e. ( II Cn J ) /\ g e. ( II Cn J ) )
              /\ ( ( f ` 0 ) = ( g ` 0 ) /\ ( f ` 1 ) = ( g ` 1 ) ) ) /\ z =
                      { h e. ( ( II tX II ) Cn J ) | A. s e. ( 0 [,] 1 )
                    ( ( ( s h 0 ) = ( f ` s ) /\ ( s h 1 ) = ( g ` s ) ) /\
              ( ( 0 h s ) = ( f ` 0 ) /\ ( 1 h s ) = ( f ` 1 ) ) ) } ) } ) $=
      ( vx vy cv cii ccn co wcel wa cc0 cfv wceq c1 cicc wral ctx crab copab2
      ctop cphtpy opreq2 eleq2d anbi12d anbi1d rabeq syl eqeq2d oprabbidv
      df-phtpy oprex wmo moeq moani a1i anass oprabbii oprabex fvopab4 ) GHEBIZ
      JGIZKLZMZCIZVFMZNZOVDPZOVHPQRVDPZRVHPQNZNZAIZFIZODIZLVPVDPQVPRVQLVPVHPQNO
      VPVQLVKQRVPVQLVLQNNFORSLTZDJJUALZVEKLZUBZQZNZBCAUCVDJEKLZMZVHWDMZNZVMNZVO
      VRDVSEKLZUBZQZNZBCAUCZUDUEVEEQZWCWLBCAWNVNWHWBWKWNVJWGVMWNVGWEVIWFWNVFWDV
      DVEEJKUFZUGWNVFWDVHWOUGUHUIWNWAWJVOWNVTWIQWAWJQVEEVSKUFVRDVTWIUJUKULUHUMG
      HABCDFUNVMWKNZBCAWDWDWMJEKUOZWQWPAUPWGWKVMAAWJUQURUSWLWGWPNBCAWGVMWKUTVAV
      BVC $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d J f g z h s $.  $d F f g z h s $.  $d G f g z h s $.
    $( The class of path homotopies between two continuous functions.
       (Contributed by Jeff Madsen, 2-Sep-2009.) $)
    phtpyval $p |- ( ( ( J e. Top /\ F e. ( II Cn J ) /\ G e. ( II Cn J ) )
                  /\ ( ( F ` 0 ) = ( G ` 0 ) /\ ( F ` 1 ) = ( G ` 1 ) ) ) ->
              ( F ( PHtpy ` J ) G ) = { h e. ( ( II tX II ) Cn J ) | A. s e.
            ( 0 [,] 1 ) ( ( ( s h 0 ) = ( F ` s ) /\ ( s h 1 ) = ( G ` s ) )
              /\ ( ( 0 h s ) = ( F ` 0 ) /\ ( 1 h s ) = ( F ` 1 ) ) ) } ) $=
      ( vf vg vz ctop wcel cii ccn co w3a cc0 cfv wceq c1 wa cphtpy cv cicc
      wral ctx crab copab2 phtpyfval opreqd 3ad2ant1 adantr wi cvv oprex rabex
      fveq1 eqeq2d bi2anan9 wb anbi12d ralbidv rabbidv biimpar 3impa biantrud
      eqeq12 syl2an 3adant3 bitr3d wmo moeq moani a1i anass oprabbii
      oprabvalig mp3an3 3adant1 imp eqtrd ) DIJZBKDLMZJZCWAJZNZOBPZOCPZQZRBPZRC
