$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Path homotopy
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c PHtpy $.
  $c ~=ph $.

  $( Extend class notation with the class of path homotopies between two
     continuous functions. $)
  cphtpy $a class PHtpy $.

  $( Extend class notation with the path homotopy relation. $)
  cphtpc $a class ~=ph $.

  ${
    $d x y f g z h s $.
    $( Define the function which takes a topological space ` X ` and two
       continuous functions ` F : II --> X ` and ` G : II --> X ` and returns
       the class of path homotopies from ` F ` to ` G ` .  Definition of
       [Hatcher] p. 25. $)
    df-phtpy $a |- PHtpy = { <. x , y >. | ( x e. Top /\ y =
        { <. <. f , g >. , z >. | ( ( ( f e. ( II Cn x ) /\ g e. ( II Cn x ) )
              /\ ( ( f ` 0 ) = ( g ` 0 ) /\ ( f ` 1 ) = ( g ` 1 ) ) ) /\ z =
                      { h e. ( ( II tX II ) Cn x ) | A. s e. ( 0 [,] 1 )
                    ( ( ( s h 0 ) = ( f ` s ) /\ ( s h 1 ) = ( g ` s ) ) /\
              ( ( 0 h s ) = ( f ` 0 ) /\ ( 1 h s ) = ( f ` 1 ) ) ) } ) } ) } $.
  $}

  ${
    $d J x y f g z h s $.
    $( Value of the path homotopy function on a topology.  (Contributed by Jeff
       Madsen, 2-Sep-2009.) $)
    phtpyfval $p |- ( J e. Top -> ( PHtpy ` J ) =
        { <. <. f , g >. , z >. | ( ( ( f e. ( II Cn J ) /\ g e. ( II Cn J ) )
              /\ ( ( f ` 0 ) = ( g ` 0 ) /\ ( f ` 1 ) = ( g ` 1 ) ) ) /\ z =
                      { h e. ( ( II tX II ) Cn J ) | A. s e. ( 0 [,] 1 )
                    ( ( ( s h 0 ) = ( f ` s ) /\ ( s h 1 ) = ( g ` s ) ) /\
              ( ( 0 h s ) = ( f ` 0 ) /\ ( 1 h s ) = ( f ` 1 ) ) ) } ) } ) $=
      ( vx vy cv cii ccn co wcel wa cc0 cfv wceq c1 crab copab2 cicc ctx cphtpy
      wral ctop opreq2 eleq2d anbi12d anbi1d rabeq syl oprabbidv df-phtpy oprex
      eqeq2d wmo moeq moani a1i anass oprabbii oprabex fvopab4 ) GHEBIZJGIZKLZM
      ZCIZVFMZNZOVDPZOVHPQRVDPZRVHPQNZNZAIZFIZODIZLVPVDPQVPRVQLVPVHPQNOVPVQLVKQ
      RVPVQLVLQNNFORUALUDZDJJUBLZVEKLZSZQZNZBCATVDJEKLZMZVHWDMZNZVMNZVOVRDVSEKL
      ZSZQZNZBCATZUEUCVEEQZWCWLBCAWNVNWHWBWKWNVJWGVMWNVGWEVIWFWNVFWDVDVEEJKUFZU
      GWNVFWDVHWOUGUHUIWNWAWJVOWNVTWIQWAWJQVEEVSKUFVRDVTWIUJUKUOUHULGHABCDFUMVM
      WKNZBCAWDWDWMJEKUNZWQWPAUPWGWKVMAAWJUQURUSWLWGWPNBCAWGVMWKUTVAVBVC $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d J f g z h s $.  $d F f g z h s $.  $d G f g z h s $.
    $( The class of path homotopies between two continuous functions.
       (Contributed by Jeff Madsen, 2-Sep-2009.) $)
    phtpyval $p |- ( ( ( J e. Top /\ F e. ( II Cn J ) /\ G e. ( II Cn J ) )
                  /\ ( ( F ` 0 ) = ( G ` 0 ) /\ ( F ` 1 ) = ( G ` 1 ) ) ) ->
              ( F ( PHtpy ` J ) G ) = { h e. ( ( II tX II ) Cn J ) | A. s e.
            ( 0 [,] 1 ) ( ( ( s h 0 ) = ( F ` s ) /\ ( s h 1 ) = ( G ` s ) )
              /\ ( ( 0 h s ) = ( F ` 0 ) /\ ( 1 h s ) = ( F ` 1 ) ) ) } ) $=
      ( vf vg vz wcel cii co cc0 cfv wceq c1 wa cv fveq1 eqeq2d wb ctop ccn w3a
      cphtpy cicc wral ctx copab2 phtpyfval opreqd 3ad2ant1 adantr wi cvv oprex
      crab rabex bi2anan9 anbi12d ralbidv rabbidv biimpar 3impa biantrud eqeq12
      syl2an 3adant3 bitr3d wmo moeq moani a1i anass oprabbii oprabvalig mp3an3
      3adant1 imp eqtrd ) DUAIZBJDUBKZIZCWAIZUCZLBMZLCMZNZOBMZOCMZNZPZPBCDUDMZK
