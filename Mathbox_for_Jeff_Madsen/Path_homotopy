    $( Lemma for ~ phtpyco . $)
    phtpycolem2 $p |- ( ( A. w e. ( 0 [,] 1 ) ( w K 1 ) = ( w L 0 ) /\
                        X e. ( 0 [,] 1 ) /\ Y e. ( ( 1 / 2 ) [,] 1 ) ) ->
                                ( X M Y ) = ( X L ( ( 2 x. Y ) - 1 ) ) ) $=
      ( cv c1 co cc0 wceq cicc wral wcel c2 cdiv w3a cle wbr cmul cmin cif
      oprex ifex opreq1 ifeq1d ifeq2d eqtrd breq1 opreq2 opreq2d opreq1d
      ifbieq12d oprabval2 cr wa wss 0re 1re pm3.2i 2re 2ne0 rereccli halfgt0
      ltleii leidi iccss mp3an sseli sylan2 3adant1 wn wo iffalse adantl
      mpbiri iftrue syl 2cn recidi syl6eq opreq1i ax1cn subidi eqtri eqeq12d
      biimparc jaodan rcla4cva wb elicc2 mp2an clt leloe mpan biimpa ltnle
