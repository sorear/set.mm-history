    $( Lemma for ~ phtpyco . $)
    phtpycolem2 $p |- ( ( A. w e. ( 0 [,] 1 ) ( w K 1 ) = ( w L 0 ) /\
                        X e. ( 0 [,] 1 ) /\ Y e. ( ( 1 / 2 ) [,] 1 ) ) ->
                                ( X M Y ) = ( X L ( ( 2 x. Y ) - 1 ) ) ) $=
      ( c1 co cc0 wceq wcel c2 cle wbr cmin wa cv cicc wral cdiv w3a cif cr wss
      cmul 0re 1re pm3.2i 2re 2ne0 halfgt0 ltleii leidi iccss mp3an sseli oprex
      rereccli ifex opreq1 ifeq1d ifeq2d eqtrd opreq2 opreq2d opreq1d ifbieq12d
      breq1 oprabval2 sylan2 3adant1 wn wo eqeq12d rcla4cva wb elicc2 mp2an clt
      leloe mpan biimpa ltnle adantr eqcom orbi12d mpbid 3adant3 iffalse adantl
      a1i sylbi mpbiri iftrue syl 2cn recidi syl6eq opreq1i ax-1cn subidi eqtri
