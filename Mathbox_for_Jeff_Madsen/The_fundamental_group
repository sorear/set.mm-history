$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        The fundamental group
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c pi1b $.
  $c *p $.
  $c pi1 $.

  $( Extend class notation with the base set of the fundamental group. $)
  cpi1b $a class pi1b $.

  $( Extend class notation with the concatenation operation for paths in a
     topological space. $)
  cpco $a class *p $.

  $( Extend class notation with the fundamental group. $)
  cpi1 $a class pi1 $.

  ${
    $d j x f g $.
    $( Define the concatenation of two paths in a topological space ` J ` whose
       endpoints line up.  Definition of [Hatcher] p. 26.  Hatcher denotes path
       concatenation with a square dot; other authors, such as Munkres, use a
       star. $)
    df-pco $a |- *p = ( j e. Top |-> { <. <. f , g >. , h >. |
      ( ( ( f e. ( II Cn j ) /\ g e. ( II Cn j ) ) /\ ( f ` 1 ) = ( g ` 0 ) )
          /\ h = ( x e. ( 0 [,] 1 ) |-> if ( x <_ ( 1 / 2 ) ,
                 ( f ` ( 2 x. x ) ) , ( g ` ( ( 2 x. x ) - 1 ) ) ) ) ) } ) $.
  $}

  ${
    $d f g j y $.
    $( Define the base set of the fundamental group of ` J ` at ` Y ` as the
       set of equivalence classes of loops in ` J ` based at ` Y ` .  The group
       structure is defined in ~ df-pi1 . $)
    df-pi1b $a |- pi1b = ( j e. Top , y e. U. j |->
          { g | E. f e. ( II Cn j ) ( ( ( f ` 0 ) = y /\ ( f ` 1 ) = y )
                                /\ g = [ f ] ( ~=ph ` j ) ) } ) $.
  $}

  ${
    $d j y f g h m n t $.
    $( Define the fundamental group, whose operation is given by concatenation
       of homotopy classes of loops.  Definition of [Hatcher] p. 26. $)
    df-pi1 $a |- pi1 = ( j e. Top , y e. U. j |-> { <. <. f , g >. , h >. |
          E. m e. { t e. ( II Cn j ) | ( ( t ` 0 ) = y /\ ( t ` 1 ) = y ) }
          E. n e. { t e. ( II Cn j ) | ( ( t ` 0 ) = y /\ ( t ` 1 ) = y ) }
                ( ( f = [ m ] ( ~=ph ` j ) /\ g = [ n ] ( ~=ph ` j ) ) /\
                    h = [ ( m ( *p ` j ) n ) ] ( ~=ph ` j ) ) } ) $.
  $}

  ${
    $d J f g h j x $.
    $( The value of the path concatenation function on a topological space.
       (Contributed by Jeff Madsen, 15-Jun-2010.) $)
    pcofval $p |- ( J e. Top -> ( *p ` J ) = { <. <. f , g >. , h >. |
      ( ( ( f e. ( II Cn J ) /\ g e. ( II Cn J ) ) /\ ( f ` 1 ) = ( g ` 0 ) )
          /\ h = ( x e. ( 0 [,] 1 ) |-> if ( x <_ ( 1 / 2 ) ,
                 ( f ` ( 2 x. x ) ) , ( g ` ( ( 2 x. x ) - 1 ) ) ) ) ) } ) $=
      ( vj cv cii ccn co wcel wa c1 cfv cc0 wceq c2 copab2 eleq2d anbi1d opreq2
      cicc cdiv cle wbr cmul cmin cmpt ctop cpco anbi12d oprabbidv df-pco oprex
      cif eqid oprabex2 simpl anim1i ssoprab2i ssexi fvmpt ) FEBGZHFGZIJZKZCGZV
      EKZLZMVCNOVGNPZLZDGAOMUBJAGZMQUCJUDUEQVLUFJZVCNVMMUGJVGNUOUHZPZLZBCDRVCHE
      IJZKZVGVQKZLZVJLZVOLZBCDRZUIUJVDEPZVPWBBCDWDVKWAVOWDVIVTVJWDVFVRVHVSWDVEV
      QVCVDEHIUAZSWDVEVQVGWESUKTTULABCDFUMWCVTVOLZBCDRZBCDVQVQVNWGHEIUNZWHWGUPU
      QWBWFBCDWAVTVOVTVJURUSUTVAVB $.
      $( [20-Dec-2013] $) $( [15-Jun-2010] $)
  $}

  ${
    $d J f g h x $.  $d F f g h x $.  $d G f g h x $.
    $( The concatenation of two paths.  (Contributed by Jeff Madsen,
       15-Jun-2010.) $)
    pcoval $p |- ( ( J e. Top /\ ( F e. ( II Cn J ) /\ G e. ( II Cn J ) /\
            ( F ` 1 ) = ( G ` 0 ) ) ) -> ( F ( *p ` J ) G ) =
                ( x e. ( 0 [,] 1 ) |-> if ( x <_ ( 1 / 2 ) ,
                  ( F ` ( 2 x. x ) ) , ( G ` ( ( 2 x. x ) - 1 ) ) ) ) ) $=
      ( vf vg vh wcel co c1 cfv cc0 wceq w3a cv wa cicc c2 cif cmpt cii ccn cle
      ctop cpco cdiv wbr cmul copab2 pcofval opreqd simpl cvv oprex mptex fveq1
      cmin ifeq12 syl2an adantr mpteq2dva eqeq2d biimp3ar biantrud simp1 eleq1d
      wi simp2 anbi12d fveq1d eqeq12d bitr3d moeq moani eqid oprabvaligg mp3an3
      mpcom 3impa sylan9eq ) DUDHZBUADUBIZHZCWBHZJBKZLCKZMZNBCDUEKZIBCEOZWBHZFO
      ZWBHZPZJWIKZLWKKZMZPZGOZALJQIZAOZJRUFIUCUGZRWTUHIZWIKZXBJUQIZWKKZSZTZMZPZ
      EFGUIZIZAWSXAXBBKZXDCKZSZTZWAWHXJBCAEFGDUJUKWCWDWGXKXOMZWCWDPZXQWGPZXPXQW
      GULWCWDXOUMHXRXPVGAWSXNLJQUNUOXIXREFGBCXOXJWBWBUMWIBMZWKCMZWRXOMZNZWQXIXR
      YBXHWQXSXTXHYAXSXTPZXGXOWRYCAWSXFXNYCXFXNMZWTWSHXSXCXLMXEXMMYDXTXBWIBUPXD
      WKCUPXAXCXLXEXMURUSUTVAVBVCVDYBWMXQWPWGYBWJWCWLWDYBWIBWBXSXTYAVEZVFYBWKCW
      BXSXTYAVHZVFVIYBWNWEWOWFYBJWIBYEVJYBLWKCYFVJVKVIVLXHWQGGXGVMVNXJVOVPVQVRV
      SVT $.
      $( [20-Dec-2013] $) $( [15-Jun-2010] $)
  $}

  ${
    $d J x $.  $d F x y $.  $d G x y $.  $d X x y $.
    $( Evaluate the concatenation of two paths on the first half.  (Contributed
       by Jeff Madsen, 15-Jun-2010.) $)
    pcoval1 $p |- ( ( ( J e. Top /\ ( F e. ( II Cn J ) /\ G e. ( II Cn J ) /\
            ( F ` 1 ) = ( G ` 0 ) ) ) /\ X e. ( 0 [,] ( 1 / 2 ) ) ) ->
                  ( ( F ( *p ` J ) G ) ` X ) = ( F ` ( 2 x. X ) ) ) $=
      ( vx wcel co c1 cfv cc0 wceq w3a wa c2 cle wbr cmul cmin cr 0re ctop cdiv
      cii ccn cicc cpco cv cif cmpt pcoval fveq1d 1re pm3.2i 2re rereccli leidi
      wi halflt1 ltleii iccss sseld mp3an breq1 opreq2 fveq2d opreq1d ifbieq12d
      2ne0 eqid fvex ifex fvmpt syl elicc2 mp2an simp3bi iftrue eqtrd sylan9eq
      wb ) CUAFAUCCUDGZFBWAFHAIJBIKLMZDJHNUBGZUEGZFZDABCUFIGZIDEJHUEGZEUGZWCOPZ
