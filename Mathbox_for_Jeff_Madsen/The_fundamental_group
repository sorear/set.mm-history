$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        The fundamental group
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c pi1b $.
  $c *p $.
  $c pi1 $.

  $( Extend class notation with the base set of the fundamental group. $)
  cpi1b $a class pi1b $.

  $( Extend class notation with the concatenation operation for paths in a
     topological space. $)
  cpco $a class *p $.

  $( Extend class notation with the fundamental group. $)
  cpi1 $a class pi1 $.

  ${
    $d j z f g h x y $.
    $( Define the concatenation of two paths in a topological space ` J ` whose
       endpoints line up.  Definition of [Hatcher] p. 26.  Hatcher denotes path
       concatenation with a square dot; other authors, such as Munkres, use a
       star. $)
    df-pco $a |- *p = { <. j , z >. | ( j e. Top /\ z = { <. <. f , g >. , h >.
      | ( ( ( f e. ( II Cn j ) /\ g e. ( II Cn j ) ) /\ ( f ` 1 ) = ( g ` 0 ) )
          /\ h = { <. x , y >. | ( x e. ( 0 [,] 1 ) /\ y = if ( x <_ ( 1 / 2 )
            , ( f ` ( 2 x. x ) ) , ( g ` ( ( 2 x. x ) - 1 ) ) ) ) } ) } ) } $.
  $}

  ${
    $d f g j p y $.
    $( Define the base set of the fundamental group of ` J ` at ` Y ` as the
       set of equivalence classes of loops in ` J ` based at ` Y ` .  The group
       structure is defined in ~ df-pi1 . $)
    df-pi1b $a |- pi1b = { <. <. j , y >. , p >. | ( ( j e. Top /\ y e. U. j )
          /\ p = { g | E. f e. ( II Cn j ) ( ( ( f ` 0 ) = y /\ ( f ` 1 ) = y )
                                /\ g = [ f ] ( ~=ph ` j ) ) } ) } $.
  $}

  ${
    $d j y p f g h m n t $.
    $( Define the fundamental group, whose operation is given by concatenation
       of homotopy classes of loops.  Definition of [Hatcher] p. 26. $)
    df-pi1 $a |- pi1 = { <. <. j , y >. , p >. | ( ( j e. Top /\ y e. U. j ) /\
                  p = { <. <. f , g >. , h >. | E. m e. { t e. ( II Cn j ) |
            ( ( t ` 0 ) = y /\ ( t ` 1 ) = y ) } E. n e. { t e. ( II Cn j ) |
            ( ( t ` 0 ) = y /\ ( t ` 1 ) = y ) } ( ( f = [ m ] ( ~=ph ` j ) /\
                              g = [ n ] ( ~=ph ` j ) ) /\ h =
                        [ ( m ( *p ` j ) n ) ] ( ~=ph ` j ) ) } ) } $.
  $}

  ${
    $d J j z f g h x y $.
    $( The value of the path concatenation function on a topological space.
       (Contributed by Jeff Madsen, 15-Jun-2010.) $)
    pcofval $p |- ( J e. Top -> ( *p ` J ) = { <. <. f , g >. , h >. |
        ( ( ( f e. ( II Cn J ) /\ g e. ( II Cn J ) ) /\ ( f ` 1 ) = ( g ` 0 ) )
          /\ h = { <. x , y >. | ( x e. ( 0 [,] 1 ) /\ y = if ( x <_ ( 1 / 2 )
              , ( f ` ( 2 x. x ) ) , ( g ` ( ( 2 x. x ) - 1 ) ) ) ) } ) } ) $=
      ( vj vz cv cii ccn co wcel wa c1 cfv cc0 wceq cicc c2 cdiv cle wbr cmul
      cmin cif copab copab2 ctop cpco opreq2 eleq2d anbi12d anbi1d oprabbidv
      df-pco oprex eqid oprabex2 id adantlr ssoprab2i ssexi fvopab4 ) GHFCIZJGI
      ZKLZMZDIZVGMZNZOVEPQVIPRZNZEIAIZQOSLMBIVNOTUALUBUCTVNUDLZVEPVOOUELVIPUFRN
      ABUGZRZNZCDEUHVEJFKLZMZVIVSMZNZVLNZVQNZCDEUHZUIUJVFFRZVRWDCDEWFVMWCVQWFVK
      WBVLWFVHVTVJWAWFVGVSVEVFFJKUKZULWFVGVSVIWGULUMUNUNUOABHCDEGUPWEWBVQNZCDEU
      HZCDEVSVSVPWIJFKUQZWJWIURUSWDWHCDEWBVQWHVLWHUTVAVBVCVD $.
      $( [15-Jun-2010] $)
  $}

  ${
    $d J f g h x y $.  $d F f g h x y $.  $d G f g h x y $.
    $( The concatenation of two paths.  (Contributed by Jeff Madsen,
       15-Jun-2010.) $)
    pcoval $p |- ( ( J e. Top /\ ( F e. ( II Cn J ) /\ G e. ( II Cn J ) /\
            ( F ` 1 ) = ( G ` 0 ) ) ) -> ( F ( *p ` J ) G ) =
                { <. x , y >. | ( x e. ( 0 [,] 1 ) /\ y = if ( x <_ ( 1 / 2 )
              , ( F ` ( 2 x. x ) ) , ( G ` ( ( 2 x. x ) - 1 ) ) ) ) } ) $=
      ( vf vg vh ctop wcel cii ccn co c1 cfv cc0 wceq w3a cpco cv wa cicc c2
      cdiv cle wbr cmul cmin cif copab copab2 pcofval opreqd cvv oprex opabex2
      id mp3an3 3adant3 eleq1 anbi1d fveq1 eqeq1d anbi12d ifeq1d eqeq2d anbi2d
      opabbidv ifeq2d df-3an anbi1i simpl pm3.21 impbid2 syl5bbr moeq moani
      eqid oprabvaligg mpcom sylan9eq ) EIJZCKELMZJZDWCJZNCOZPDOZQZRZCDESOZMCDF
