      FUNUOUP $.
      $( [7-Jun-2014] $) $( [15-Jun-2010] $)

    pcoval.2 $e |- ( ph -> F e. ( II Cn J ) ) $.
    pcoval.3 $e |- ( ph -> G e. ( II Cn J ) ) $.
    $( The concatenation of two paths.  (Contributed by Jeff Madsen,
       15-Jun-2010.)  (Revised by Mario Carneiro, 7-Jun-2014.) $)
    pcoval $p |- ( ph -> ( F ( *p ` J ) G ) =
                ( x e. ( 0 [,] 1 ) |-> if ( x <_ ( 1 / 2 ) ,
                  ( F ` ( 2 x. x ) ) , ( G ` ( ( 2 x. x ) - 1 ) ) ) ) ) $=
      ( vf vg cfv co cc0 c1 cicc cv c2 cif wcel wceq cpco cii ccn cdiv cle cmul
      wbr cmin cmpt cmpt2 pcofval oveqd wa biidd simpll fveq1d simplr ifbieq12d
      mpteq2dva eqid ovex mptex ovmpt2a syl2anc eqtrd ) ACDEUAKZLCDIJUBEUCLZVGB
