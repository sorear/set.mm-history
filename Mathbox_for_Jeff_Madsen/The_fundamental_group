  ${
    $d t J $.  $d t Y $.
    pi1gp.1 $e |- X = U. J $.
    $( The fundamental group is a group.  Proposition 1.3 of [Hatcher] p. 26.
       (Contributed by Jeff Madsen, 19-Jun-2010.)  (The proof was shortened by
       Mario Carneiro, 8-Jun-2014.) $)
    pi1gp $p |- ( ( J e. Top /\ Y e. X ) -> ( pi1 ` <. J , Y >. ) e. GrpOp ) $=
      ( vt ctop wcel wa cc0 cv cfv wceq c1 cii ccn co crab eqid simpl simpr
      pi1gplem ) AFGZCBGZHEIEJZKCLMUDKCLHENAOPQZABCDUERUBUCSUBUCTUA $.
      $( [8-Jun-2014] $) $( [19-Jun-2010] $)
    $( The base set of the fundamental group.  (Contributed by Jeff Madsen,
       11-Jun-2010.) $)
    pi1set $p |- ( ( J e. Top /\ Y e. X ) -> ( pi1b ` <. J , Y >. )
                                                = ran ( pi1 ` <. J , Y >. ) )
        $=
      ( vt ctop wcel wa cop cpi1b cfv cpi1 cdm crn cxp wf wceq cc0 cv syl c1 co
      cii ccn crab eqid simpl simpr fdm dmeqd dmxpid syl6req cgr pi1gp grporndm
      pi1f eqtr4d ) AFGZCBGZHZACIZJKZVALKZMZMZVCNZUTVEVBVBOZMVBUTVDVGUTVGVBVCPV
      DVGQUTERESZKCQUAVHKCQHEUCAUDUBUEZABCDVIUFURUSUGURUSUHUPVGVBVCUITUJVBUKULU
      TVCUMGVFVEQABCDUNVCUOTUQ $.
      $( [11-Jun-2010] $)
