  ${
    $d A x $.  $d B x $.  $d ph x $.
    rabeq12.1OLD $e |- ( ph -> A = B ) $.
    rabeq12.2OLD $e |- ( ph -> ( ps <-> ch ) ) $.
    $( Equality of restricted class abstractions.  (Moved to ~ rabeqbidv in
       main set.mm and may be deleted by mathbox owner, JM. --NM
       15-Sep-2011.) $)
    rabeq12OLD $p |- ( ph -> { x e. A | ps } = { x e. B | ch } ) $=
      ( rabeqbidv ) ABCDEFGHI $.
      $( [1-Dec-2009] $)
  $}

  ${
    $d A x $.
    $( Condition for a restricted class abstraction to be empty.  (Moved to
       ~ rabeq0 in main set.mm and may be deleted by mathbox owner, JM. --NM
       1-Apr-2013.) $)
    rabeq0OLD $p |- ( { x e. A | ph } = (/) <-> A. x e. A -. ph ) $=
      ( rabeq0 ) ABCD $.
      $( [7-Jun-2010] $)
  $}

  ${
    $d A x $.
    $( Law of excluded middle, in terms of restricted class abstractions. $)
    rabxm $p |- A = ( { x e. A | ph } u. { x e. A | -. ph } ) $=
      ( wn wo crab cun wceq rabid2 cv wcel exmid a1i mprgbir unrab eqtr4i ) CAA
      DZEZBCFZABCFQBCFGCSHRBCRBCIRBJCKALMNAQBCOP $.
      $( [20-Jun-2011] $)

    $( Law of noncontradiction, in terms of restricted class abstractions. $)
    rabnc $p |- ( { x e. A | ph } i^i { x e. A | -. ph } ) = (/) $=
      ( crab wn cin wa c0 inrab wceq rabeq0 cv wcel pm3.24 a1i mprgbir eqtri )
      ABCDAEZBCDFARGZBCDZHARBCITHJSEZBCSBCKUABLCMANOPQ $.
      $( [20-Jun-2011] $)
  $}

  ${
    $d x y $.  $d ps y $.
    ralab.1OLD $e |- ( y = x -> ( ph <-> ps ) ) $.
    $( Universal quantification over a class abstraction.  (Moved to ~ ralab in
       main set.mm and may be deleted by mathbox owner, JM. --NM
       20-Oct-2011.) $)
    ralabOLD $p |- ( A. x e. { y | ph } ch <-> A. x ( ps -> ch ) ) $=
      ( ralab ) ABCDEFG $.
      $( [10-Jun-2010] $)

    $d A x y $.  $d ph x $.
    $( Universal quantification over a restricted class abstraction.  (Moved to
       ~ ralrab in main set.mm and may be deleted by mathbox owner, JM. --NM
       20-Oct-2011.) $)
    ralrabOLD $p |- ( A. x e. { y e. A | ph } ch <-> A. x e. A ( ps -> ch ) )
        $=
      ( ralrab ) ABCDEFGH $.
      $( [10-Jun-2010] $)
  $}

  ${
    $d ps x $.  $d A x $.  $d x y $.
    rexrabOLD.1 $e |- ( x = y -> ( ph <-> ps ) ) $.
    $( Existential quantification over a class abstraction.  (Moved to ~ rexrab
       in main set.mm and may be deleted by mathbox owner, JM. --NM
       20-Mar-2013.) $)
    rexrabOLD $p |- ( E. y e. { x e. A | ph } ch <-> E. y e. A ( ps /\ ch ) )
        $=
      ( rexrab ) ABCDEFGH $.
      $( [17-Jun-2011] $)
  $}

  ${
    $d F x y $.  $d A x y $.  $d B x y $.  $d ph x $.  $d ps y $.
    raleqfnOLD.1 $e |- ( y = ( F ` x ) -> ( ph <-> ps ) ) $.
    $( Change the domain of quantification by a function.  (Moved to ~ raleqfn
       in main set.mm and may be deleted by mathbox owner, JM. --NM
       17-Mar-2013.) $)
    raleqfnOLD $p |- ( ( F Fn A /\ B C_ A ) ->
                              ( A. x e. B ps <-> A. y e. ( F " B ) ph ) ) $=
      ( raleqfn ) ABCDEFGHI $.
      $( [2-Sep-2009] $)
  $}

