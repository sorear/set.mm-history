${
  $( Move a conditional outside of a function. $)
  fvif $p |- ( F ` if ( ph , A , B ) ) = if ( ph , ( F ` A ) , ( F ` B ) ) $=
    ( cif cfv wceq iftrue fveq2d eqtr4d wn iffalse pm2.61i ) AABCEZDFZABDFZCDFZ
    EZGAOPRANBDABCHIAPQHJAKZOQRSNCDABCLIAPQLJM $.
    $( [2-Sep-2009] $)
$}

${
  ifeq1da.1 $e |- ( ( ph /\ ps ) -> A = B ) $.
  $( Conditional equality. $)
  ifeq1da $p |- ( ph -> if ( ps , A , C ) = if ( ps , B , C ) ) $=
    ( cif wceq wa ifeq1d wn iffalse eqtr4d adantl pm2.61dan ) ABBCEGZBDEGZHZABI
    BCDEFJBKZRASPEQBCELBDELMNO $.
    $( [2-Sep-2009] $)
$}

${
  ifeq2da.1 $e |- ( ( ph /\ -. ps ) -> A = B ) $.
  $( Conditional equality. $)
  ifeq2da $p |- ( ph -> if ( ps , C , A ) = if ( ps , C , B ) ) $=
    ( cif wceq iftrue eqtr4d adantl wn wa ifeq2d pm2.61dan ) ABBECGZBEDGZHZBRAB
    PEQBECIBEDIJKABLMBCDEFNO $.
    $( [2-Sep-2009] $)
$}

${
  ifclda.1 $e |- ( ( ph /\ ps ) -> A e. C ) $.
  ifclda.2 $e |- ( ( ph /\ -. ps ) -> B e. C ) $.
  $( Conditional closure. $)
  ifclda $p |- ( ph -> if ( ps , A , B ) e. C ) $=
    ( cif wcel wa wceq iftrue adantl eqeltrd wn iffalse pm2.61dan ) ABBCDHZEIAB
    JRCEBRCKABCDLMFNABOZJRDESRDKABCDPMGNQ $.
    $( [2-Sep-2009] $)
$}

${
  ifbieq12d.1 $e |- ( ph -> ( ps <-> ch ) ) $.
  ifbieq12d.2 $e |- ( ph -> A = C ) $.
  ifbieq12d.3 $e |- ( ph -> B = D ) $.
  $( Conditional equality. $)
  ifbieq12d $p |- ( ph -> if ( ps , A , B ) = if ( ch , C , D ) ) $=
    ( cif ifbid ifeq1d ifeq2d 3eqtrd ) ABDEKCDEKCFEKCFGKABCDEHLACDFEIMACEGFJNO
    $.
    $( [2-Sep-2009] $)
$}

${
