  ${
    anass1rs.1 $e |- ( ( ph /\ ( ps /\ ch ) ) -> th ) $.
    $( Commutative-associative law for conjunction in an antecedent. $)
    anass1rs $p |- ( ( ( ph /\ ch ) /\ ps ) -> th ) $=
      ( anassrs an32s ) ABCDABCDEFG $.
      $( [19-Jun-2011] $)
  $}

  ${
    anim12da.1 $e |- ( ( ph /\ ps ) -> th ) $.
    anim12da.2 $e |- ( ( ph /\ ch ) -> ta ) $.
    $( Conjoin antecedents and consequents in a deduction. $)
    anim12da $p |- ( ( ph /\ ( ps /\ ch ) ) -> ( th /\ ta ) ) $=
      ( wa adantrr adantrl jca ) ABCHHDEABDCFIACEBGJK $.
      $( [16-Jun-2011] $)
  $}

  ${
    biadan2.1 $e |- ( ph -> ps ) $.
    biadan2.2 $e |- ( ps -> ( ph <-> ch ) ) $.
    $( Add a conjunction to an equivalence. $)
    biadan2 $p |- ( ph <-> ( ps /\ ch ) ) $=
      ( wa pm4.71ri pm5.32i bitri ) ABAFBCFABDGBACEHI $.
      $( [20-Jun-2011] $)
  $}

  ${
    syldanl.1 $e |- ( ( ph /\ ps ) -> ch ) $.
    syldanl.2 $e |- ( ( ( ph /\ ch ) /\ th ) -> ta ) $.
    $( A syllogism deduction with conjoined antecedents. $)
    syldanl $p |- ( ( ( ph /\ ps ) /\ th ) -> ta ) $=
      ( wa ex imdistani sylan ) ABHACHDEABCABCFIJGK $.
      $( [20-Jun-2011] $)
  $}

  ${
    syl3an2cOLD.1 $e |- ( ( ph /\ ( ps /\ ch /\ th ) ) -> ta ) $.
    syl3an2cOLD.2 $e |- ( et -> ph ) $.
    syl3an2cOLD.3 $e |- ( et -> ps ) $.
    syl3an2cOLD.4 $e |- ( et -> ch ) $.
    syl3an2cOLD.5 $e |- ( et -> th ) $.
    $( A syllogism inference combined with contraction.  (Moved to
       ~ syl3an2cOLD in main set.mm and may be deleted by mathbox owner, JM.
       --NM 8-Sep-2011.) $)
    syl3an2cOLDOLD $p |- ( et -> ta ) $=
      ( syl13anc ) FABCDEHIJKGL $.
      $( [17-Jun-2011] $)
  $}

  ${
    mp4an.1OLD $e |- ph $.
    mp4an.2OLD $e |- ps $.
    mp4an.3OLD $e |- ch $.
    mp4an.4OLD $e |- th $.
    mp4an.5OLD $e |- ( ( ( ph /\ ps ) /\ ( ch /\ th ) ) -> ta ) $.
    $( An inference based on modus ponens.  (Moved to ~ mp4an in main set.mm
       and may be deleted by mathbox owner, JM. --NM 19-Oct-2012.) $)
    mp4anOLD $p |- ta $=
      ( mp4an ) ABCDEFGHIJK $.
      $( [15-Jun-2010] $)
  $}

  ${
    imdistanda.1 $e |- ( ( ph /\ ps ) -> ( ch -> th ) ) $.
    $( Distribution of implication with conjunction (deduction version with
       conjoined antecedent). $)
    imdistanda $p |- ( ph -> ( ( ps /\ ch ) -> ( ps /\ th ) ) ) $=
      ( wi ex imdistand ) ABCDABCDFEGH $.
      $( [19-Jun-2011] $)
  $}

  ${
    $d ph x y $.  $d A y $.
    r19.21aivva.1OLD $e |- ( ( ph /\ ( x e. A /\ y e. B ) ) -> ps ) $.
    $( Inference from Theorem 19.21 of [Margaris] p. 90.  (Restricted
       quantifier version with double quantification.)  (Moved to ~ r19.21aivva
       in main set.mm and may be deleted by mathbox owner, JM. --NM
       17-Jul-2012.) $)
    r19.21aivvaOLD $p |- ( ph -> A. x e. A A. y e. B ps ) $=
      ( cv wcel wa ex r19.21aivv ) ABCDEFACHEIDHFIJBGKL $.
      $( [19-Jun-2011] $)
  $}

  ${
    $d w x z $.  $d w y z $.  $d w ph $.
    $( Substitution into "at most one".  (Moved to ~ sbmo in main set.mm and
       may be deleted by mathbox owner, JM. --NM 29-Jan-2012.) $)
    sbmoOLD $p |- ( [ y / x ] E* z ph <-> E* z [ y / x ] ph ) $=
      ( vw cv wceq wi wal wex wsbc wmo sbex sbim ax-17 sbf imbi2i bitri sbalv
      exbii mo2 sbbii 3bitr4i ) ADFEFGZHZDIZEJZBCFZKZABUHKZUDHZDIZEJZADLZBUHKUJ
      DLUIUFBUHKZEJUMUFEBCMUOULEUEUKBCDUEBUHKUJUDBUHKZHUKAUDBCNUPUDUJUDBCUDBOPQ
      RSTRUNUGBCADEAEOUAUBUJDEUJEOUAUC $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d ph y $.  $d ps x $.  $d A y $.  $d B x $.  $d x y $.
    $( Distribute quantification over "or".  (Moved to ~ 2ralor in main set.mm
       and may be deleted by mathbox owner, JM. --NM 29-Jan-2012.) $)
    2ralorOLD $p |- ( A. x e. A A. y e. B ( ph \/ ps ) <->
                  ( A. x e. A ph \/ A. y e. B ps ) ) $=
      ( wo wral wn wa wrex reeanv ioran rexbii rexnal bitr3i bitri anbi12i
      bitr4i 3bitr3i con4bii ) ABGZDFHZCEHZACEHZBDFHZGZAIZBIZJZDFKZCEKZUHCEKZUI
      DFKZJZUDIZUGIZUHUICDEFLULUCIZCEKUPUKURCEUKUBIZDFKURUSUJDFABMNUBDFOPNUCCEO
      QUOUEIZUFIZJUQUMUTUNVAACEOBDFORUEUFMSTUA $.
      $( [19-Jun-2010] $)
  $}

  $( Restricted existential quantification over union.  (Moved to ~ rexun in
     main set.mm and may be deleted by mathbox owner, JM. --NM 29-Jan-2012.) $)
  rexunOLD $p |- ( E. x e. ( A u. B ) ph <->
                          ( E. x e. A ph \/ E. x e. B ph ) ) $=
    ( cun wrex cv wcel wa wex wo df-rex elun anbi1i andir bitri exbii 19.43
    orbi12i bitr4i 3bitri ) ABCDEZFBGZUBHZAIZBJUCCHZAIZUCDHZAIZKZBJZABCFZABDFZK
    ZABUBLUEUJBUEUFUHKZAIUJUDUOAUCCDMNUFUHAOPQUKUGBJZUIBJZKUNUGUIBRULUPUMUQABCL
    ABDLSTUA $.
    $( [5-Jan-2011] $)

  ${
    $d A x y $.  $d B x y $.  $d ph y $.
    $( Restricted quantification over union.  (Moved to ~ ralun in main set.mm
       and may be deleted by mathbox owner, JM. --NM 29-Jan-2012.) $)
    ralunOLD $p |- ( ( A. x e. A ph /\ A. x e. B ph )
                                            -> A. x e. ( A u. B ) ph ) $=
      ( vy wral wa cv wsbc cun wcel wo ra4sbc com12 jaao elun syl5ib r19.21aiv
      cbvralsv sylibr ) ABCFZABDFZGZABEHZIZECDJZFABUFFUCUEEUFUCUDCKZUDDKZLUEUDU
      FKUAUGUEUBUHUGUAUEABUDCMNUHUBUEABUDDMNOUDCDPQRABEUFST $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d A x $.  $d ps x $.
    rexsnOLD.1 $e |- A e. _V $.
    rexsnOLD.2 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( Restricted existential quantification over a singleton.  (Moved to
       ~ rexsn in main set.mm and may be deleted by mathbox owner, JM. --NM
       29-Jan-2012.) $)
    rexsnOLD $p |- ( E. x e. { A } ph <-> ps ) $=
      ( rexsn ) ABCDEFG $.
      $( [5-Jan-2011] $)
  $}

  ${
    $d A x $.  $d x y $.  $d ph x $.
    $( Specialized existential commutation lemma.  (Moved to ~ rexcom4a in main
       set.mm and may be deleted by mathbox owner, JM. --NM 16-Apr-2012.) $)
    rexcom4aOLD $p |- ( E. x E. y e. A ( ph /\ ps )
                          <-> E. y e. A ( ph /\ E. x ps ) ) $=
      ( wa wrex wex rexcom4 19.42v rexbii bitr3i ) ABFZDEGCHMCHZDEGABCHFZDEGMDC
      EINODEABCJKL $.
      $( [1-Jun-2011] $)

    $d B x $.
    rexcom4bOLD.1 $e |- B e. _V $.
    $( Specialized existential commutation lemma.  (Moved to ~ rexcom4b in main
       set.mm and may be deleted by mathbox owner, JM. --NM 16-Apr-2012.) $)
    rexcom4bOLD $p |- ( E. x E. y e. A ( ph /\ x = B ) <-> E. y e. A ph ) $=
      ( cv wceq wa wrex wex rexcom4a isseti biantru rexbii bitr4i ) ABGEHZICDJB
      KAQBKZIZCDJACDJAQBCDLASCDRABEFMNOP $.
      $( [1-Jun-2011] $)
  $}

  ${
    $d ph y z $.  $d ps x z $.  $d ch x y $.  $d A y $.  $d B x z $.
    $d C x y $.
    $( Rearrange three existential quantifiers. $)
    3reeanv $p |- ( E. x e. A E. y e. B E. z e. C ( ph /\ ps /\ ch )
                      <-> ( E. x e. A ph /\ E. y e. B ps /\ E. z e. C ch ) ) $=
      ( wa wrex w3a r19.41v reeanv anbi1i bitri df-3an 2rexbii rexbii 3bitr4i
      ) ABJZEHKZCFIKZJZDGKZADGKZBEHKZJZUCJZABCLZFIKEHKZDGKUFUGUCLUEUBDGKZUCJUIU
      BUCDGMULUHUCABDEGHNOPUKUDDGUKUACJZFIKEHKUDUJUMEFHIABCQRUACEFHINPSUFUGUCQT
      $.
      $( [11-Jun-2010] $)
  $}

  ${
    $d B x y $.  $d A x y $.  $d ph y $.  $d ps x y $.
    morex.1 $e |- B e. _V $.
    morex.2 $e |- ( x = B -> ( ph <-> ps ) ) $.
    $( Derive membership from uniqueness. $)
    morex $p |- ( ( E. x e. A ph /\ E* x ph ) -> ( ps -> B e. A ) ) $=
      ( wmo wrex wcel wi cv wa wex df-rex exancom bitri wal hbmo1 hbe1 hban
      mopick 19.21ai wceq eleq1 imbi12d cla4v syl sylan2b ancoms ) ACHZACDIZBED
      JZKZULUKACLZDJZMZCNZUNULUPAMCNURACDOUPACPQUKURMZAUPKZCRUNUSUTCUKURCACSUQC
      TUAAUPCUBUCUTUNCEFUOEUDABUPUMGUOEDUEUFUGUHUIUJ $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d A x $.  $d ps x $.
    euuni2OLD.1 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( The unique element such that ` ph ` .  (Moved to ~ euuni2 in main set.mm
       and may be deleted by mathbox owner, JM. --NM 15-Oct-2012.) $)
    euuni2OLD $p |- ( ( A e. B /\ E! x ph ) -> ( ps <-> U. { x | ph } = A ) )
        $=
      ( wcel cvv wreu cab cuni wceq wb weu elisset reuv biimpri wa crab
      reuuni2 rabab unieqi eqeq1i syl6bb syl2an ) DEGDHGZACHIZBACJZKZDLZMACNZDE
      OUGUKACPQUFUGRBACHSZKZDLUJABCHDFTUMUIDULUHACUAUBUCUDUE $.
      $( [1-Jun-2011] $)
  $}

  ${
    $d A w x y z $.  $d B w x z $.  $d C w x y $.  $d D w x y $.  $d F w x y $.
    $d ph w x z $.  $d ps w x y $.  $d ch w x y $.
    unirep.1 $e |- ( y = D -> ( ph <-> ps ) ) $.
    unirep.2 $e |- ( y = D -> B = C ) $.
    unirep.3 $e |- ( y = z -> ( ph <-> ch ) ) $.
    unirep.4 $e |- ( y = z -> B = F ) $.
    unirep.5 $e |- B e. _V $.
    $( Define a quantity whose definition involves a choice of representative,
       but which is uniquely determined regardless of the choice. $)
    unirep $p |- ( ( A. y e. A A. z e. A ( ( ph /\ ch ) -> B = F ) /\
            ( D e. A /\ ps ) ) -> U. { x | E. y e. A ( ph /\ x = B ) } = C ) $=
      ( vw wa wceq wi wral wcel wrex cv cab cuni eqidd ancli eqeq2d anbi12d
      rcla4ev sylan2 adantl cvv weu wb csb wal ax-17 a1i csbiegf ax-gen csbexg
      mpan2 eqeltrrd ad2antrl wex eqeq1 anbi2d rexbidv cla4egv syl adantr mpd
      r19.29 eqeq12 pm3.35 syl5cbir ancoms expimpd an4 syl5ib ancomsd expdimp
      r19.23aivw imp sylan an32s ex 19.21aivv cbvrexv syl6bb eu4 sylanbrc
      euuni2 syl11anc mpbid ) ACRZHKSZTZFGUAZEGUAZJGUBZBRZRZAIHSZRZEGUCZADUDZHS
      ZRZEGUCZDUEUFISZXDXHXBBXCBIISZRZXHBXNBIUGUHXGXOEJGEUDZJSZABXFXNLXQHIIMUIU
      JUKULZUMXEIUNUBZXLDUOZXHXMUPXCXSXBBXCEJHUQZIUNEQJHIGQUDZIUBZYCEURTXCYCEUS
      UTMVAXCHUNUBZEURYAUNUBYDEPVBEJHGUNVCVDVEZVFXEXLDVGZXLCYBKSZRZFGUCZRXIYBSZ
      TZQURDURXTXDYFXBXDXHYFXRXCXHYFTZBXCXSYLYEXLXHDIUNXIISZXKXGEGYMXJXFAXIIHVH
      VIVJZVKVLVMVNUMXEYKDQXBYKXDXBXLYIYJXBXLRXAXKRZEGUCYIYJTZXAXKEGVOYOYPEGYOY
      IYJXAYIXKYJXAYIRWTYHRZFGUCZXKYJWTYHFGVOYRXKYJYQXKYJTFGWTYHXKYJWTXKYHYJWTW
