$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Topology
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( The union of two open sets is open.  (Moved to ~ unopn in main set.mm and
     may be deleted by mathbox owner, JM. --NM 15-Oct-2012.) $)
  unopnOLD $p |- ( ( J e. Top /\ A e. J /\ B e. J ) -> ( A u. B ) e. J ) $=
    ( unopn ) ABCD $.
    $( [2-Sep-2009] $)

  ${
    $d A a $.  $d B a $.
    $( The intersection of two closed sets is closed.  (Moved to ~ incld in
       main set.mm and may be deleted by mathbox owner, JM. --NM
       15-Oct-2012.) $)
    incldOLD $p |- ( ( J e. Top /\ A e. ( Clsd ` J ) /\ B e. ( Clsd ` J ) )
                                      -> ( A i^i B ) e. ( Clsd ` J ) ) $=
      ( incld ) ABCD $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d J v $.  $d X v $.  $d A v $.  $d B v $.
    subspopn.1 $e |- X = U. J $.
    $( An open set is open in the subspace topology.  (Contributed by Jeff
       Madsen, 2-Sep-2009.) $)
    subspopn $p |- ( ( ( J e. Top /\ A C_ X ) /\ ( B e. J /\ B C_ A ) )
                                      -> B e. ( subSp ` <. A , J >. ) ) $=
      ( vv ctop wcel wss wa cop csubsp cfv cv cin wceq wrex simprl dfss biimpi
      ad2antll ineq1 eqeq2d rcla4ev syl11anc cvv wb simpll cuni uniexg
      syl5eqel ssexg sylan2 ancoms adantr issubspt syl111anc mpbird ) CGHZADIZJ
      ZBCHZBAIZJZJZBACKLMHZBFNZAOZPZFCQZVEVBBBAOZPZVJVAVBVCRZVCVLVAVBVCVLBASTUA
      VIVLFBCVGBPVHVKBVGBAUBUCUDUEVEUSVBAUFHZVFVJUGUSUTVDUHVMVAVNVDUTUSVNUSUTDU
      FHVNUSDCUIUFECGUJUKADUFULUMUNUOFBACCUPUQUR $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d J v $.  $d X v $.  $d A v $.  $d B v $.
    subspcld.1 $e |- X = U. J $.
    $( A closed set is closed in the subspace topology.  (Contributed by Jeff
       Madsen, 2-Sep-2009.) $)
    subspcld $p |- ( ( ( J e. Top /\ A C_ X ) /\ ( B e. ( Clsd ` J )
                /\ B C_ A ) ) -> B e. ( Clsd ` ( subSp ` <. A , J >. ) ) ) $=
      ( vv ctop wcel wss wa ccld cfv cop csubsp cv cin wceq wrex simprl dfss
      biimpi ad2antll ineq1 eqeq2d rcla4ev syl11anc wb subcld adantr mpbird )
      CGHADIJZBCKLZHZBAIZJZJZBACMNLKLHZBFOZAPZQZFULRZUPUMBBAPZQZVAUKUMUNSUNVCUK
      UMUNVCBATUAUBUTVCFBULURBQUSVBBURBAUCUDUEUFUKUQVAUGUOFBACDEUHUIUJ $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d A v $.  $d B v $.  $d J v $.
    $( A set which is closed in the subspace topology induced by a closed set
       is closed in the original topology.  (Contributed by Jeff Madsen,
       2-Sep-2009.) $)
    subspcld2 $p |- ( ( J e. Top /\ A e. ( Clsd ` J ) /\
            B e. ( Clsd ` ( subSp ` <. A , J >. ) ) ) -> B e. ( Clsd ` J ) ) $=
      ( vv ctop wcel ccld cfv cop csubsp wa cv cin wceq wrex cuni wss wb eqid
      cldss subcld syldan incld 3expa an32s eleq1 syl5ibrcom r19.23adva sylbid
      3impia ) CEFZACGHZFZBACIJHGHFZBULFZUKUMKZUNBDLZAMZNZDULOZUOUKUMACPZQUNUTR
      ACVAVASZTDBACVAVBUAUBUPUSUODULUPUQULFZKUOUSURULFZUKVCUMVDUKVCUMVDUQACUCUD
      UEBURULUFUGUHUIUJ $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d J w x y z $.  $d X w x y z $.  $d A w x y z $.  $d B w x y z $.
    subspabs.1 $e |- X = U. J $.
    $( Absorption law for subspace.  (Contributed by Jeff Madsen,
       2-Sep-2009.) $)
    subspabs $p |- ( ( J e. Top /\ A C_ X /\ B C_ A ) -> ( subSp ` <. B ,
                  ( subSp ` <. A , J >. ) >. ) = ( subSp ` <. B , J >. ) ) $=
      ( vx vy vz vw ctop wcel wss w3a cop csubsp cfv cv cin wceq wrex wi cvv
      wb simp1 visset a1i cuni uniexg syl5eqel ssexg sylan2 ancoms 3adant3
      issubspt syl111anc wa simpllr ineq1 adantl inass sseqin2 biimpi 3ad2ant3
      ineq2d syl5eq ad2antrr eqtrd eqeq2d biimpa rcla4ev syl11anc exp31
      r19.23adva sylbid r19.23adv eqid mpan2 simpll inex1 adantr mpbird
      3adantl3 eqcomd syl6eqr ex impbid sseq2i stoig3 sylan2b sstr 3impb
      3bitr4d eqrdv ) CJKZADLZBALZMZFBACNOPZNOPZBCNOPZWQFQZGQZBRZSZGWRTZXAHQZBR
      ZSZHCTZXAWSKZXAWTKZWQXEXIWQXDXIGWRWQXBWRKZXBIQZARZSZICTZXDXIUAZWQWNXBUBKZ
      AUBKZXLXPUCWNWOWPUDZXRWQGUEUFWNWOXSWPWOWNXSWNWODUBKZXSWNDCUGZUBECJUHUIZAD
      UBUJUKULZUMIXBAUBCUNUOWQXOXQICWQXMCKZUPZXOXDXIYFXOUPZXDUPYEXAXMBRZSZXIWQY
      EXOXDUQYGXDYIYGXCYHXAYGXCXNBRZYHXOXCYJSYFXBXNBURUSWQYJYHSYEXOWQYJXMABRZRY
      HXMABUTWQYKBXMWPWNYKBSZWOWPYLBAVAVBZVCVDVEVFVGVHVIXHYIHXMCXFXMSXGYHXAXFXM
      BURVHVJVKVLVMVNVOWQXHXEHCWQXFCKZUPZXHXEYOXHUPXFARZWRKZXAYPBRZSZXEYOYQXHWN
      WOYNYQWPWNWOUPZYNUPZYQYPXBARZSZGCTZYNUUDYTYNYPYPSZUUDYPVPUUCUUEGXFCXBXFSU
      UBYPYPXBXFAURVHVJVQUSUUAWNYPUBKZXSYQUUDUCWNWOYNVRUUFUUAXFAHUEVSUFYTXSYNYD
      VTGYPAUBCUNUOWAWBVTYOXHYSYOXGYRXAYOXGXFYKRYRYOBYKXFWQBYKSZYNWPWNUUGWOWPYK
