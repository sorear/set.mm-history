  $( The union of two open sets is open. $)
  unopn $p |- ( ( J e. Top /\ A e. J /\ B e. J ) -> ( A u. B ) e. J ) $=
    ( ctop wcel w3a cpr cuni cun wceq uniprg 3adant1 wss wa uniopn prssg ibi
    sylan2 3impb eqeltrrd ) CDEZACEZBCEZFABGZHZABIZCUBUCUEUFJUAABCCKLUAUBUCUECE
    ZUAUDCMZUGUBUCNZUDCOUIUHABCCCPQRST $.
    $( [2-Sep-2009] $)

  ${
    $d A a $.  $d B a $.
    $( The intersection of two closed sets is closed. $)
    incld $p |- ( ( J e. Top /\ A e. ( Clsd ` J ) /\ B e. ( Clsd ` J ) )
                                      -> ( A i^i B ) e. ( Clsd ` J ) ) $=
      ( va ctop wcel ccld cfv w3a cpr cint cin wceq intprg 3adant1 c0 wne wss
      simp1 cv preq1 neeq1d visset prnz vtoclg 3ad2ant2 wa prssg ibi intcld
      syl111anc eqeltrrd ) CEFZACGHZFZBUNFZIZABJZKZABLZUNUOUPUSUTMUMABUNUNNOUQU
      MURPQZURUNRZUSUNFUMUOUPSUOUMVAUPDTZBJZPQVADAUNVCAMVDURPVCABUAUBVCBDUCUDUE
      UFUOUPVBUMUOUPUGVBABUNUNUNUHUIOURCUJUKUL $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d J v $.  $d X v $.  $d A v $.  $d B v $.
    subspopn.1 $e |- X = U. J $.
    $( An open set is open in the subspace topology. $)
    subspopn $p |- ( ( ( J e. Top /\ A C_ X ) /\ ( B e. J /\ B C_ A ) )
                                      -> B e. ( subSp ` <. A , J >. ) ) $=
      ( vv ctop wcel wss wa cop csubsp cfv cv cin wceq wrex simprl dfss biimpi
      ad2antll ineq1 eqeq2d rcla4ev syl11anc cvv wb simpll ssexg cuni uniexg
      syl5eqel sylan2 ancoms adantr issubspt syl111anc mpbird ) CGHZADIZJZBCHZB
      AIZJZJZBACKLMHZBFNZAOZPZFCQZVEVBBBAOZPZVJVAVBVCRZVCVLVAVBVCVLBASTUAVIVLFB
      CVGBPVHVKBVGBAUBUCUDUEVEUSVBAUFHZVFVJUGUSUTVDUHVMVAVNVDUTUSVNUTDUFHVNUSAD
      UFUIUSCUJUFDCGUKEULUMUNUOFBACCUPUQUR $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d J v $.  $d X v $.  $d A v $.  $d B v $.
    subspcld.1 $e |- X = U. J $.
    $( A closed set is closed in the subspace topology. $)
    subspcld $p |- ( ( ( J e. Top /\ A C_ X ) /\ ( B e. ( Clsd ` J )
                /\ B C_ A ) ) -> B e. ( Clsd ` ( subSp ` <. A , J >. ) ) ) $=
      ( vv ctop wcel wss wa ccld cfv cop csubsp cv cin wceq wrex simprl dfss
      biimpi ad2antll ineq1 eqeq2d rcla4ev syl11anc wb subcld adantr mpbird )
      CGHADIJZBCKLZHZBAIZJZJZBACMNLKLHZBFOZAPZQZFULRZUPUMBBAPZQZVAUKUMUNSUNVCUK
      UMUNVCBATUAUBUTVCFBULURBQUSVBBURBAUCUDUEUFUKUQVAUGUOFBACDEUHUIUJ $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d A v $.  $d B v $.  $d J v $.
    $( A set which is closed in the subspace topology induced by a closed set
       is closed in the original topology. $)
    subspcld2 $p |- ( ( J e. Top /\ A e. ( Clsd ` J ) /\
            B e. ( Clsd ` ( subSp ` <. A , J >. ) ) ) -> B e. ( Clsd ` J ) ) $=
      ( vv ctop wcel ccld cfv cop csubsp wa cv cin wceq wrex cuni wss wb eqid
      cldss subcld syldan eleq1 incld 3expa an1rs syl5cbir r19.23adva sylbid
      3impia ) CEFZACGHZFZBACIJHGHFZBULFZUKUMKZUNBDLZAMZNZDULOZUOUKUMACPZQUNUTR
      ACVAVASZTDBACVAVBUAUBUPUSUODULUSUOURULFZUPUQULFZKBURULUCUKVDUMVCUKVDUMVCU
      QACUDUEUFUGUHUIUJ $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d J w x y z $.  $d X w x y z $.  $d A w x y z $.  $d B w x y z $.
    subspabs.1 $e |- X = U. J $.
    $( Absorption law for subspace. $)
    subspabs $p |- ( ( J e. Top /\ A C_ X /\ B C_ A ) -> ( subSp ` <. B ,
                  ( subSp ` <. A , J >. ) >. ) = ( subSp ` <. B , J >. ) ) $=
      ( vx vy vz vw ctop wcel wss w3a cop csubsp cfv cv cin wceq wrex wi cvv
      wb simp1 visset a1i ssexg cuni uniexg syl5eqel sylan2 ancoms 3adant3
      issubspt syl111anc wa simpllr ineq1 adantl sseqin2 biimpi 3ad2ant3
      ineq2d inass syl5eq ad2antrr eqtrd eqeq2d biimpa rcla4ev syl11anc exp31
      r19.23adva sylbid r19.23adv eqid mpan2 simpll inex1 adantr mpbird
      3adantl3 eqcomd syl6eqr ex impbid stoig3 sseq2i sylan2b sstr 3impb
      3bitr4d eqrdv ) CJKZADLZBALZMZFBACNOPZNOPZBCNOPZWQFQZGQZBRZSZGWRTZXAHQZBR
      ZSZHCTZXAWSKZXAWTKZWQXEXIWQXDXIGWRWQXBWRKZXBIQZARZSZICTZXDXIUAZWQWNXBUBKZ
      AUBKZXLXPUCWNWOWPUDZXRWQGUEUFWNWOXSWPWOWNXSWODUBKZXSWNADUBUGWNCUHZUBDCJUI
