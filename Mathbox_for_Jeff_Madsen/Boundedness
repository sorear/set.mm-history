${
  $d m x r $.
  $( Define the class of bounded metrics. A metric space is bounded iff it
  can be covered by a single ball. $)
  df-bnd $a |- Bnd = { m e. Met | A. x e. dom dom m E. r e. RR+
                                        dom dom m = ( x ( ball ` m ) r ) } $.
$}

${
  $d M m x r $. $d X m x r $.
  isbnd.1 $e |- X = dom dom M $.
  $( The predicate "is a bounded metric space". $)
  isbnd $p |- ( M e. Bnd <-> ( M e. Met /\ A. x e. X E. r e. RR+
                          X = ( x ( ball ` M ) r ) ) ) $=
    ( vm cv cdm cbl cfv co wceq crp wrex wral cme cbnd dmeq dmeqd syl6eqr
    fveq2 opreqd eqeq12d rexbidv raleq12d df-bnd elrab2 ) FGZHZHZAGZDGZUHIJZKZL
    ZDMNZAUJOCUKULBIJZKZLZDMNZACOFBPQUHBLZUPUTAUJCVAUJBHZHCVAUIVBUHBRSETZVAUOUS
    DMVAUJCUNURVCVAUMUQUKULUHBIUAUBUCUDUEAFDUFUG $.
    $( [2-Sep-2009] $)
$}

${
  $d M m x r $. $d X m x r $.
  isbnd2.1 $e |- X = dom dom M $.
  $( The predicate "is a bounded metric space". $)
  isbnd2 $p |- ( M e. Met -> ( M e. Bnd <->
                      A. x e. X E. r e. RR+ X = ( x ( ball ` M ) r ) ) ) $=
    ( cbnd wcel cme cv cbl cfv co wceq crp wrex wral isbnd baib ) BFGBHGCAIDIBJ
    KLMDNOACPABCDEQR $.
    $( [2-Sep-2009] $)
$}

