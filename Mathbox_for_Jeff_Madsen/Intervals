  ${
    $d A x $.  $d B x $.  $d C x $.  $d D x $.
    $( Condition for a closed interval to be a subset of another closed
       interval.  (Moved into main set.mm as ~ iccss and may be deleted by
       mathbox owner, JM. --NM 29-May-2014.) $)
    iccssOLD $p |- ( ( ( A e. RR /\ B e. RR ) /\ ( C e. RR /\ D e. RR ) /\
                    ( A <_ C /\ D <_ B ) ) -> ( C [,] D ) C_ ( A [,] B ) ) $=
      ( iccss ) ABCDE $.
      $( [2-Sep-2009] $)
  $}

  $( Condition for a closed interval to be a subset of another closed
     interval.  (Moved into main set.mm as ~ iccss2 and may be deleted by
     mathbox owner, JM. --NM 29-May-2014.) $)
  iccss2OLD $p |- ( ( A e. RR /\ B e. RR ) -> ( ( C e. ( A [,] B ) /\
                    D e. ( A [,] B ) ) -> ( C [,] D ) C_ ( A [,] B ) ) ) $=
    ( iccss2 ) ABCDE $.
    $( [2-Sep-2009] $)

