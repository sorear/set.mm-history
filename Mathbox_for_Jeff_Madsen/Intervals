  $c II $.
  $( Extend class notation with the unit interval. $)
  cii $a class II $.

  $( Define the unit interval with the Euclidean topology. $)
  df-ii $a |- II = ( MetOpen ` ( ( abs o. - )
                          |` ( ( 0 [,] 1 ) X. ( 0 [,] 1 ) ) ) ) $.

  $( The unit interval is a topological space.  (Contributed by Jeff Madsen,
     2-Sep-2009.) $)
  iitop $p |- II e. Top $=
    ( cabs cmin ccom cc0 c1 cicc co cxp cres cme wcel cii ctop eqid cnmet ax-mp
    metres df-ii opntop ) ABCZDEFGZUAHIZJKZLMKTJKUCTTNOTUAQPUBLRSP $.
    $( [2-Sep-2009] $)

  $( The base set of the unit interval.  (Contributed by Jeff Madsen,
     2-Sep-2009.) $)
  iiuni $p |- ( 0 [,] 1 ) = U. II $=
    ( cabs cmin ccom cme wcel cc0 c1 cicc co cxp cres cii cuni wceq eqid cc wss
    cdm cr mp2an cnmet 0re 1re iccssre ax-resscn sstri cnmetba metssba2 uniopn2
    metres df-ii mp2b ) ABCZDEZUMFGHIZUOJKZDEUOLMNUMUMOZUAZUMUOUJUPLUOUNUOPQUOU
    PRRNURUOSPFSEGSEUOSQUBUCFGUDTUEUFUMPUOUMUQUGUHTUKUIUL $.
    $( [15-Jan-2014] $) $( [2-Sep-2009] $)

  $( Alternate definition of the unit interval.  (Contributed by Jeff Madsen,
     2-Sep-2009.) $)
  dfii2 $p |- II = ( subSp ` <. ( 0 [,] 1 ) , ( topGen ` ran (,) ) >. ) $=
    ( cii cabs cmin ccom cc0 c1 cicc co cxp cres copn cfv cioo crn csubsp df-ii
    ctg cop cr wcel wss wceq 0re 1re iccssre mp2an stioo ax-mp eqtr4i ) ABCDEFG
    HZUJIJKLZUJMNQLROLZPUJSUAZULUKUBESTFSTUMUCUDEFUEUFUJUGUHUI $.
    $( [2-Sep-2009] $)

  $( Alternate definition of the unit interval.  (Contributed by Jeff Madsen,
     11-Jun-2010.) $)
  dfii3 $p |- II = ( subSp ` <. ( 0 [,] 1 ) ,
       ( MetOpen ` ( abs o. - ) ) >. ) $=
    ( cc0 c1 cicc cabs cmin ccom copn cfv cop csubsp cii cme wcel wss wceq eqid
    co cc cr mp2an cnmet 0re 1re iccssre ax-resscn sstri cxp cres cnmetba df-ii
    subtopmet eqcomi ) ABCQZDEFZGHZIJHZKUNLMUMRNUPKOUNUNPZUAUMSRASMBSMUMSNUBUCA
