  $c II $.
  $( Extend class notation with the unit interval. $)
  cii $a class II $.

  $( Define the unit interval with the Euclidean topology. $)
  df-ii $a |- II = ( Open ` ( ( abs o. - )
                          |` ( ( 0 [,] 1 ) X. ( 0 [,] 1 ) ) ) ) $.

  $( The unit interval is a topological space. $)
  iitop $p |- II e. Top $=
    ( cabs cmin ccom cc0 c1 cicc co cxp cres cme wcel cii ctop eqid cnmet
    metres ax-mp df-ii opntop ) ABCZDEFGZUAHIZJKZLMKTJKUCTTNOTUAPQUBLRSQ $.
    $( [2-Sep-2009] $)

  $( The base set of the unit interval. $)
  iiuni $p |- ( 0 [,] 1 ) = U. II $=
    ( cii cuni cc0 c1 cicc co cabs cmin ccom cxp cres cme wcel wceq eqid cnmet
    metres ax-mp cc wss cdm cr 0re 1re iccssre mp2an axresscn sstri cnmetba
    metssba2 df-ii uniopn2 eqcomi ) ABZCDEFZGHIZUOUOJKZLMZUNUONUPLMZURUPUPOZPZU
    PUOQRUQAUOUSUOSTUOUQUAUANVAUOUBSCUBMDUBMUOUBTUCUDCDUEUFUGUHUPSUOUPUTUIUJUFU
    KULRUM $.
    $( [2-Sep-2009] $)

