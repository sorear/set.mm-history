  ${
    $d S s f $.  $d F f $.
    $( Value of the recursive sequence builder at 1.  (Moved to ~ seq11g in
       main set.mm and may be deleted by mathbox owner, JM. --NM
       17-Nov-2012.) $)
    seq11gOLD $p |- ( ( S e. A /\ F e. B ) ->
                                    ( ( S seq1 F ) ` 1 ) = ( F ` 1 ) ) $=
      ( seq11g ) ABCDE $.
      $( [7-Jan-2011] $)
  $}

  ${
    $d S s f $.  $d F f $.  $d A s f $.
    $( Value of the recursive sequence builder at a successor.  (Moved to
       ~ seq1p1g in main set.mm and may be deleted by mathbox owner, JM. --NM
       17-Nov-2012.) $)
    seq1p1gOLD $p |- ( ( S e. B /\ F e. C /\ A e. NN ) -> ( ( S seq1 F ) `
              ( A + 1 ) ) = ( ( ( S seq1 F ) ` A ) S ( F ` ( A + 1 ) ) ) ) $=
      ( seq1p1g ) ABCDEF $.
      $( [7-Jan-2011] $)
  $}

  ${
    $d S s f $.  $d F f $.  $d M s f $.
    $( Value of the arbitrary-based recursive sequence builder at its initial
       value.  (Moved to ~ seqz1g in main set.mm and may be deleted by mathbox
       owner, JM. --NM 17-Nov-2012.) $)
    seqz1gOLD $p |- ( ( S e. A /\ F e. B /\ M e. ZZ ) ->
                              ( ( <. M , S >. seqz F ) ` M ) = ( F ` M ) ) $=
      ( seqz1g ) ABCDEF $.
      $( [7-Jan-2011] $)
  $}

  ${
    $d S s f $.  $d F f $.  $d M s f $.  $d N s f $.
    $( Value of the arbitrary-based recursive seqeunce builder at a successor.
       (Moved to ~ seqzp1g in main set.mm and may be deleted by mathbox owner,
       JM. --NM 17-Nov-2012.) $)
    seqzp1gOLD $p |- ( ( S e. A /\ F e. B /\ N e. ( ZZ>= ` M ) ) ->
                        ( ( <. M , S >. seqz F ) ` ( N + 1 ) ) =
                ( ( ( <. M , S >. seqz F ) ` N ) S ( F ` ( N + 1 ) ) ) ) $=
      ( seqzp1g ) ABCDEFG $.
      $( [7-Jan-2011] $)
  $}

  ${
    $d S s f $.  $d F f $.
    $( The 1-based recursive sequence in terms of the arbitrary-based one.
       (Moved to ~ seq1seqzg in main set.mm and may be deleted by mathbox
       owner, JM. --NM 17-Nov-2012.) $)
    seq1seqzgOLD $p |- ( ( S e. A /\ F e. B ) ->
                                ( S seq1 F ) = ( <. 1 , S >. seqz F ) ) $=
      ( seq1seqzg ) ABCDE $.
      $( [7-Jan-2011] $)
  $}

  ${
    $d M a j k m n p s t $.  $d Z a j k m n p s t $.  $d F a j k m n p s t $.
    $d A a j k m n p s t $.  $d G j k n p s t $.
    sdclem1.1 $e |- Z = ( ZZ>= ` M ) $.
    sdclem1.2 $e |- G = { <. m , a >. |
                                    ( m e. Z /\ a = ( ( F ` m ) ` m ) ) } $.
    $( Lemma for ~ sdc .  Construct a function from a sequence of partial
       functions. $)
    sdclem1 $p |- ( ( M e. ZZ /\ F Fn Z /\
