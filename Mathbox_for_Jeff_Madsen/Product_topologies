  ${
    $d R u v z $.  $d S u v z $.  $d T u v z $.  $d A u v z $.  $d B u v z $.
    txopnOLD.1 $e |- T = ( R tX S ) $.
    $( The product of two open sets is open in the product topology.  (Moved to
       ~ txopn in main set.mm and may be deleted by mathbox owner, JM. --NM
       15-Oct-2012.) $)
    txopnOLD $p |- ( ( ( R e. Top /\ S e. Top ) /\ ( A e. R /\ B e. S ) )
                                                      -> ( A X. B ) e. T ) $=
      ( txopn ) ABCDEFG $.
      $( [2-Sep-2009] $)
  $}

  ${
    txcldOLD.1 $e |- T = ( R tX S ) $.
    $( The product of two closed sets is closed in the product topology.
       (Moved to ~ txcld in main set.mm and may be deleted by mathbox owner,
       JM. --NM 15-Oct-2012.) $)
    txcldOLD $p |- ( ( ( R e. Top /\ S e. Top ) /\ ( A e. ( Clsd ` R )
                  /\ B e. ( Clsd ` S ) ) ) -> ( A X. B ) e. ( Clsd ` T ) ) $=
      ( txcld ) ABCDEFG $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d A w x y z $.  $d B w x y z $.  $d C u v w x y z $.  $d D u v w x y z $.
    $d S w $.  $d T u v w x y $.  $d J w $.  $d K w $.  $d L w $.
    $d F u v w $.  $d R w z $.
    cnresoprab.1 $e |- A = U. J $.
    cnresoprab.2 $e |- B = U. K $.
    cnresoprab.3 $e |- S = U. L $.
    cnresoprab.4 $e |- C C_ A $.
    cnresoprab.5 $e |- D C_ B $.
    cnresoprab.6 $e |- T C_ S $.
    cnresoprab.7 $e |- ( ( x e. C /\ y e. D ) -> R e. T ) $.
    cnresoprab.8 $e |- F = { <. <. x , y >. , z >. |
                                        ( ( x e. A /\ y e. B ) /\ z = R ) } $.
    cnresoprab.9 $e |- G = { <. <. x , y >. , z >. |
                                        ( ( x e. C /\ y e. D ) /\ z = R ) } $.
    cnresoprab.10 $e |- J e. Top $.
    cnresoprab.11 $e |- K e. Top $.
    cnresoprab.12 $e |- L e. Top $.
    cnresoprab.13 $e |- F e. ( ( J tX K ) Cn L ) $.
    $( Continuity of a restricted operation abstraction.  (Contributed by Jeff
       Madsen, 11-Jun-2010.) $)
    cnresoprab $p |- G e. ( ( subSp ` <. ( C X. D ) , ( J tX K ) >. )
                                                Cn ( subSp ` <. T , L >. ) ) $=
      ( vw vu vv cxp cres ctx co cop csubsp cfv ccn cv wcel wa copab2 resoprab2
