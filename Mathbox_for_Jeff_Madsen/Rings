  ${
    ringnegcl.1 $e |- G = ( 1st ` R ) $.
    ringnegcl.2 $e |- X = ran G $.
    ringnegcl.3 $e |- N = ( inv ` G ) $.
    $( A ring is closed under negation. $)
    ringnegcl $p |- ( ( R e. RingOps /\ A e. X ) -> ( N ` A ) e. X ) $=
      ( cgr wcel cfv cring grpoinvcl ringogrpo sylan ) CIJAEJADKEJBLJACDEGHMBCF
      NO $.
      $( [10-Jun-2010] $)

    ${
      ringaddneg.4 $e |- Z = ( Id ` G ) $.
      $( Adding the negative in a ring gives zero. $)
      ringaddneg1 $p |- ( ( R e. RingOps /\ A e. X )
            -> ( A G ( N ` A ) ) = Z ) $=
        ( cgr wcel cfv co wceq cring grporinv ringogrpo sylan ) CKLAELAADMCNFOB
        PLAFCDEHJIQBCGRS $.
        $( [10-Jun-2010] $)

      $( Adding the negative in a ring gives zero. $)
      ringaddneg2 $p |- ( ( R e. RingOps /\ A e. X )
             -> ( ( N ` A ) G A ) = Z ) $=
        ( cgr wcel cfv co wceq cring grpolinv ringogrpo sylan ) CKLAELADMACNFOB
        PLAFCDEHJIQBCGRS $.
        $( [10-Jun-2010] $)
    $}

    ringsub.4 $e |- D = ( /g ` G ) $.
    $( Subtraction in a ring, in terms of addition and negation. $)
    ringsub $p |- ( ( R e. RingOps /\ A e. X /\ B e. X )
                                        -> ( A D B ) = ( A G ( N ` B ) ) ) $=
      ( cgr wcel co cfv wceq cring grpdivval ringogrpo syl3an1 ) ELMAGMBGMABCNA
      BFOENPDQMABCEFGIJKRDEHST $.
      $( [19-Jun-2010] $)
  $}

  ${
