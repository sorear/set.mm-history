    ${
      ringaddneg.4 $e |- Z = ( Id ` G ) $.
      $( Adding the negative in a ring gives zero. $)
      ringaddneg1 $p |- ( ( R e. Ring /\ A e. X ) -> ( A G ( N ` A ) ) = Z ) $=
        ( cgr wcel cfv co wceq cring grprinv ringgrp sylan ) CKLAELAADMCNFOBPLA
        FCDEHJIQBCGRS $.
        $( [25-Jun-2011] $) $( [10-Jun-2010] $)

      $( Adding the negative in a ring gives zero. $)
      ringaddneg2 $p |- ( ( R e. Ring /\ A e. X ) -> ( ( N ` A ) G A ) = Z ) $=
        ( cgr wcel cfv co wceq cring grplinv ringgrp sylan ) CKLAELADMACNFOBPLA
        FCDEHJIQBCGRS $.
        $( [25-Jun-2011] $) $( [10-Jun-2010] $)
    $}

    ringsub.4 $e |- D = ( /g ` G ) $.
    $( Subtraction in a ring, in terms of addition and negation. $)
    ringsub $p |- ( ( R e. Ring /\ A e. X /\ B e. X )
                                        -> ( A D B ) = ( A G ( N ` B ) ) ) $=
      ( cgr wcel co cfv wceq cring grpdivval ringgrp syl3an1 ) ELMAGMBGMABCNABF
      OENPDQMABCEFGIJKRDEHST $.
      $( [25-Jun-2011] $) $( [19-Jun-2010] $)
