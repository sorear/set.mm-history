      UHZVEUIUJ $.
      $( [5-Jun-2014] $) $( [2-Sep-2009] $)
  $}

  ${
    $d N x y d k $.  $d X x y d k $.  $d Y x y d k $.
    $( The value of the Euclidean metric.  (Contributed by Jeff Madsen,
       2-Sep-2009.) $)
    rrnmval $p |- ( ( N e. NN /\ X e. ( RR ^m ( 1 ... N ) )
                  /\ Y e. ( RR ^m ( 1 ... N ) ) ) -> ( X ( Rn ` N ) Y ) =
        ( sqr ` sum_ k e. ( 1 ... N ) ( ( ( X ` k ) - ( Y ` k ) ) ^ 2 ) ) ) $=
      ( vx vy cn wcel cr co cfv cv cmin c2 cexp csu csqr wceq wa fveq1 cfz cmap
      c1 crrn cmpt2 rrnval opreqd opreq12 syl2an opreq1d sumeq2sdv eqid ovmpt2a
      fveq2d fvex sylan9eq 3impb ) BGHZCIUCBUAJZUBJZHZDUTHZCDBUDKZJZUSALZCKZVED
