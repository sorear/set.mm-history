$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                   Subgroups
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c SubGrp $.

  $( Extend class notation with all subgroups of a group. $)
  csubg $a class SubGrp $.

  ${
    $d w s $.
    $( Define a subgroup of a group as a set of elements that is a group in its
       own right. $)
    df-subg $a |- SubGrp = ( w e. Grp |->
        { s e. ~P ( Base ` w ) | ( w |`s s ) e. Grp } ) $.
  $}

  ${
    $d s w B $.  $d s w G $.  $d s S $.
    issubg.b $e |- B = ( Base ` G ) $.
    $( The subgroup predicate.  (Contributed by Mario Carneiro, 2-Dec-2014.) $)
    issubg $p |- ( S e. ( SubGrp ` G ) <->
      ( G e. Grp /\ S C_ B /\ ( G |`s S ) e. Grp ) ) $=
      ( vw vs csubg cfv wcel cgrp wss cress co cv cbs cpw crab wa wceq eleq1d
      w3a cdm df-subg dmmptss elfvdm sseldi simp1 fveq2 syl6eqr pweqd rabeqbidv
      oveq1 fvex eqeltri pwex rabex fvmpt eleq2d oveq2 elrab elpw2 anbi1i bitri
      cvv syl6bb ibar bitrd 3anass syl6bbr pm5.21nii ) BCGHZIZCJIZVMBAKZCBLMZJI
      ZUAZVLGUBJCEJENZFNZLMZJIZFVROHZPZQZGEFUCZUDBCGUEUFVMVNVPUGVMVLVMVNVPRZRZV
      QVMVLWFWGVMVLBCVSLMZJIZFAPZQZIZWFVMVKWKBECWDWKJGVRCSZWAWIFWCWJWMWBAWMWBCO
      HZAVRCOUHDUIUJWMVTWHJVRCVSLULTUKWEWIFWJAAWNVDDCOUMUNZUOUPUQURWLBWJIZVPRWF
      WIVPFBWJVSBSWHVOJVSBCLUSTUTWPVNVPBAWOVAVBVCVEVMWFVFVGVMVNVPVHVIVJ $.
      $( [2-Dec-2014] $)

    $( A subgroup is a subset.  (Contributed by Mario Carneiro, 2-Dec-2014.) $)
    subgss $p |- ( S e. ( SubGrp ` G ) -> S C_ B ) $=
      ( csubg cfv wcel cgrp wss cress co issubg simp2bi ) BCEFGCHGBAICBJKHGABCD
      LM $.
      $( [2-Dec-2014] $)

    $( A group is a subgroup of itself.  (Contributed by Mario Carneiro,
       7-Dec-2014.) $)
    subgid $p |- ( G e. Grp -> B e. ( SubGrp ` G ) ) $=
      ( cgrp wcel wss cress co csubg cfv id a1i ressid eqeltrd issubg syl3anbrc
      ssid ) BDEZRAAFZBAGHZDEABIJERKZSRAQLRTBDABDCMUANAABCOP $.
      $( [7-Dec-2014] $)
  $}

  ${
    subggrp.h $e |- H = ( G |`s S ) $.
    $( A subgroup is a group.  (Contributed by Mario Carneiro, 2-Dec-2014.) $)
    subggrp $p |- ( S e. ( SubGrp ` G ) -> H e. Grp ) $=
      ( csubg cfv wcel cress co cgrp cbs wss eqid issubg simp3bi syl5eqel ) ABE
      FGZCBAHIZJDQBJGABKFZLRJGSABSMNOP $.
      $( [2-Dec-2014] $)

    $( The base of the restricted group in a subgroup.  (Contributed by Mario
       Carneiro, 2-Dec-2014.) $)
    subgbas $p |- ( S e. ( SubGrp ` G ) -> S = ( Base ` H ) ) $=
      ( csubg cfv wcel cbs wss wceq eqid subgss ressbas2 syl ) ABEFGABHFZIACHFJ
      OABOKZLAOCBDPMN $.
      $( [2-Dec-2014] $)
  $}

  $( Reverse closure for the subgroup predicate.  (Contributed by Mario
     Carneiro, 2-Dec-2014.) $)
  subgrcl $p |- ( S e. ( SubGrp ` G ) -> G e. Grp ) $=
    ( csubg cfv wcel cgrp cbs wss cress co eqid issubg simp1bi ) ABCDEBFEABGDZH
    BAIJFENABNKLM $.
    $( [2-Dec-2014] $)

  ${
    $d R x $.  $d S x $.  $d A x $.
    subg0.h $e |- H = ( G |`s S ) $.
    ${
      subg0.i $e |- O = ( 0g ` G ) $.
      $( A subgroup of a group must have the same identity as the group.
         (Contributed by Stefan O'Rear, 27-Nov-2014.)  (Revised by Mario
         Carneiro, 2-Dec-2014.) $)
      subg0 $p |- ( S e. ( SubGrp ` G ) -> O = ( 0g ` H ) ) $=
        ( cfv wcel c0g cplusg co wceq cgrp cbs subggrp eqid grpidcl syl syl2anc
        csubg ressplusg grplid wb subgrcl subgss subgbas eleqtrrd sseldd grpid
        mpbid ) ABTGHZCIGZULBJGZKULLZDULLZUKCMHZULCNGZHZUNABCEOZUKUPURUSUQCULUQ
        PZULPZQRZUQUMCULULUTAUMBCEUMPZUAVAUBSUKBMHULBNGZHUNUOUCABUDUKAVDULVDABV
        DPZUEUKULUQAVBABCEUFUGUHVDUMBDULVEVCFUISUJ $.
        $( [2-Dec-2014] $) $( [27-Nov-2014] $)
    $}

    subginv.i $e |- I = ( invg ` G ) $.
    subginv.j $e |- J = ( invg ` H ) $.
    $( The inverse of an element in a subgroup is the same as the inverse in
       the larger group. $)
    subginv $p |- ( ( S e. ( SubGrp ` G ) /\ X e. S ) ->
      ( I ` X ) = ( J ` X ) ) $=
      ( csubg cfv wcel wceq c0g cgrp cbs adantr eleq2d eqid sselda wa cplusg co
      subggrp subgbas biimpa ressplusg grprinv syl2anc eqtr4d wb subgrcl subgss
      subg0 wi grpinvcl ex syl 3imtr4d imp syldan grpinvid1 syl3anc mpbird ) AB
      JKLZFALZUAZFDKFEKZMZFVHBUBKZUCZBNKZMZVGVKCNKZVLVGCOLZFCPKZLZVKVNMVEVOVFAB
      CGUDZQVEVFVQVEAVPFABCGUEZRZUFVPVJCEVNFVPSZAVJBCGVJSZUGVNSIUHUIVEVLVNMVFAB
      CVLGVLSZUNQUJVGBOLZFBPKZLVHWELZVIVMUKVEWDVFABULQVEAWEFWEABWESZUMZTVEVFVHA
      LZWFVEVFWIVEVQVHVPLZVFWIVEVOVQWJUOVRVOVQWJVPCEFWAIUPUQURVTVEAVPVHVSRUSUTV
      EAWEVHWHTVAWEVJBDVLFVHWGWBWCHVBVCVD $.
      $( [2-Dec-2014] $)
  $}

  ${
    subg0cl.i $e |- O = ( 0g ` G ) $.
    $( The group identity is an element of any subgroup.  (Contributed by Mario
       Carneiro, 2-Dec-2014.) $)
    subg0cl $p |- ( S e. ( SubGrp ` G ) -> O e. S ) $=
      ( csubg cfv wcel cress co c0g cbs cgrp eqid subggrp grpidcl subg0 subgbas
      syl eleq12d mpbird ) ABEFGZCAGBAHIZJFZUBKFZGZUAUBLGUEABUBUBMZNUDUBUCUDMUC
      MORUACUCAUDABUBCUFDPABUBUFQST $.
      $( [2-Dec-2014] $) $( [27-Nov-2014] $)
  $}

  ${
    subginvcl.i $e |- I = ( invg ` G ) $.
    $( The inverse of an element is closed in a subgroup. $)
    subginvcl $p |- ( ( S e. ( SubGrp ` G ) /\ X e. S ) -> ( I ` X ) e. S ) $=
      ( csubg cfv wcel wa cress cminusg cbs cgrp eqid subggrp adantr simpr wceq
      co subgbas eleqtrd grpinvcl syl2anc subginv eleq12d mpbird ) ABFGHZDAHZIZ
      DCGZAHDBAJSZKGZGZUKLGZHZUIUKMHZDUNHUOUGUPUHABUKUKNZOPUIDAUNUGUHQUGAUNRUHA
      BUKUQTPZUAUNUKULDUNNULNZUBUCUIUJUMAUNABUKCULDUQEUSUDURUEUF $.
      $( [2-Dec-2014] $)
  $}

  ${
    subgcl.p $e |- P = ( +g ` G ) $.
    $( A subgroup is closed under group operation.  (Contributed by Mario
       Carneiro, 2-Dec-2014.) $)
    subgcl $p |- ( ( S e. ( SubGrp ` G ) /\ X e. S /\ Y e. S ) ->
      ( X P Y ) e. S ) $=
      ( csubg cfv wcel w3a co cress cbs cgrp eqid subggrp 3ad2ant1 wceq eleqtrd
      simp2 subgbas simp3 ressplusg grpcl syl3anc eleqtrrd ) BCGHIZDBIZEBIZJZDE
      AKZCBLKZMHZBUJULNIZDUMIEUMIUKUMIUGUHUNUIBCULULOZPQUJDBUMUGUHUITUGUHBUMRUI
      BCULUOUAQZSUJEBUMUGUHUIUBUPSUMAULDEUMOBACULUOFUCUDUEUPUF $.
      $( [2-Dec-2014] $)
  $}

  ${
    $d u v w x y G $.  $d u v w x y I $.  $d u v w x y P $.  $d u v w x y S $.
    $d u v w B $.  $d u v w O $.
    issubg2.b $e |- B = ( Base ` G ) $.
    issubg2.o $e |- O = ( 0g ` G ) $.
    issubg2.p $e |- P = ( +g ` G ) $.
    issubg2.i $e |- I = ( invg ` G ) $.
    $( Characterize the subgroups of a group by closure properties.
       (Contributed by Mario Carneiro, 2-Dec-2014.) $)
    issubg2 $p |- ( G e. Grp -> ( S e. ( SubGrp ` G ) <-> ( S C_ B /\ S =/= (/)
      /\ A. x e. S ( A. y e. S ( x P y ) e. S /\ ( I ` x ) e. S ) ) ) ) $=
      ( vu wcel cfv cv co wa wceq eleq1d vv vw cgrp csubg wss c0 wne w3a subgss
