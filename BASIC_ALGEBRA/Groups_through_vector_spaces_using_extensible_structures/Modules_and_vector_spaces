  ${
    lmodvsubeq0.v $e |- V = ( Base ` W ) $.
    lmodvsubeq0.o $e |- O = ( 0g ` W ) $.
    lmodvsubeq0.m $e |- M = ( -g ` W ) $.
    $( If the difference between two vectors is zero, they are equal.
       ( ~ hvsubeq0 analog.) $)
    lmodvsubeq0 $p |- ( ( W e. LMod /\ A e. V /\ B e. V )
                  -> ( ( A M B ) = O <-> A = B ) ) $=
      ( clmod wcel cgrp co wceq wb lmodgrp grpsubeq0 syl3an1 ) FJKFLKAEKBEKABCM
      DNABNOFPEFCDABGHIQR $.
      $( [19-Jun-2014] $) $( [31-Mar-2014] $)

    $( Subtraction of a vector from itself.  ( ~ hvsubid analog.) $)
    lmodvsubid $p |- ( ( W e. LMod /\ A e. V ) -> ( A M A ) = O ) $=
      ( clmod wcel cgrp co wceq lmodgrp grpsubid sylan ) EIJEKJADJAABLCMENDEBCA
      FGHOP $.
      $( [19-Jun-2014] $) $( [16-Apr-2014] $)
  $}

  ${
    $d q r w x y z B $.  $d q r w x y z K $.  $d q r w x y z ph $.
    $d q r w x y z L $.  $d q r w x y z P $.
    lmodpropd.1 $e |- ( ph -> B = ( Base ` K ) ) $.
    lmodpropd.2 $e |- ( ph -> B = ( Base ` L ) ) $.
    lmodpropd.3 $e |- ( ( ph /\ ( x e. B /\ y e. B ) ) ->
      ( x ( +g ` K ) y ) = ( x ( +g ` L ) y ) ) $.
    lmodpropd.4 $e |- ( ph -> F = ( Scalar ` K ) ) $.
    lmodpropd.5 $e |- ( ph -> F = ( Scalar ` L ) ) $.
    lmodpropd.6 $e |- P = ( Base ` F ) $.
    lmodpropd.7 $e |- ( ( ph /\ ( x e. P /\ y e. B ) ) ->
      ( x ( vsca ` K ) y ) = ( x ( vsca ` L ) y ) ) $.
    $( If two structures have the same components (properties), one is a left
       module iff the other one is.  (Contributed by Mario Carneiro,
       8-Feb-2015.) $)
    lmodpropd $p |- ( ph -> ( K e. LMod <-> L e. LMod ) ) $=
      ( wcel cfv co wral wceq vr vw vz vq cgrp csca crg cvsca w3a clmod lmodgrp
      cv wi a1i cbs cplusg cmulr cur wa eqid islmod simplr simprl fveq2d syl5eq
      simp2bi ad2antrr eleqtrd lmodvscl syl3anc eleqtrrd ralrimivva ex grppropd
      simprr syl5ibr eqtr3d eleq1d adantlr eleq12d mpbird adantr simpll simprlr
      3jcad wb simprrr proplem syl12anc simplr1 syl simprrl grpcl eqtrd simplr3
      oveq2d proplem2 syl21anc oveq12d eqeq12d simplr2 simprll rngacl 3anbi123d
      oveqd oveq1d rngcl rngidcl eqeq1d anbi12d anassrs 2ralbidva eleq2d anbi1d
      3anbi1d raleqbidv 3bitr3d 3bitr4g pm5.21ndd ) AGUEPZGUFQZUGPZBULZCULZGUHQ
