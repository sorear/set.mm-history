      FGHUCUDUEUF $.
      $( [24-Aug-2011] $)

    $( Obsolete proof of ~ grpid as of 9-Mar-2014. $)
    grpidOLD $p |- ( ( G e. Grp /\ X e. B ) ->
                  ( X = O <-> ( X P X ) = X ) ) $=
      ( cgrp wcel wa co wceq wb wi grpidcl grprcan 3exp2 mpid pm2.43d grplid
      imp eqeq2d bitr3d ) CIJZEAJZKZEEBLZDEBLZMZEDMZUHEMUEUFUJUKNZUEUFULUEUFDAJ
