    lsmless2.s $e |- P = ( LSSum ` W ) $.
    $( Subspace sum commutes (extended domain version). $)
    lsmcomx $p |- ( ( W e. LMod /\ T C_ V /\ U C_ V )
       -> ( T P U ) = ( U P T ) ) $=
