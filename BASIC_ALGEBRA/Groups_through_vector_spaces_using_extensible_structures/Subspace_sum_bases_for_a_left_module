$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
    Subspace sum; bases for a left module
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c LBasis $.

  $( Extend class notation with the set of bases for a vector space. $)
  clbs $a class LBasis $.

  ${
    $d b n s w x y $.
    $( Define the set of bases to a left module or left vector space. $)
    df-lbs $a |- LBasis = ( w e. _V |-> { b e. ~P ( Base ` w ) |
     [ ( LSpan ` w ) / n ] [ ( Scalar ` w ) / s ] ( ( n ` b ) = ( Base ` w ) /\
       A. x e. b A. y e. ( ( Base ` s ) \ { ( 0g ` s ) } )
       -. ( y ( vsca ` w ) x ) e. ( n ` ( b \ { x } ) ) ) } ) $.

    $(
    df-lbs @a |- LBasis = ( w e. _V |-> { b e. ~P ( Base ` w ) |
      ( ( ( LSpan ` w ) ` b ) = ( Base ` w ) /\ A. x e. ( ~P b i^i Fin )
       A. f e. ( ( Base ` ( Scalar ` w ) ) ^m x )
       ( ( w gsum ( f oF ( vsca ` w ) ( _I |` x ) ) ) = ( 0g ` w ) ->
         f = ( x X. { ( 0g ` w ) } ) ) } ) @.

    df-lbs @a |- LBasis = ( w e. _V |-> { b e. ~P ( Base ` w ) |
      [ ( Base ` ( ( Scalar ` w ) freeLMod b ) ) / k ] ( f e. k |->
        ( w gsum ( f oF ( vsca ` w ) ( _I |` b ) ) ) ) :
           k -1-1-onto-> ( Base ` w ) } ) @.
    $)
  $}

  ${
    $d b x y B $.  $d b f n w y K $.  $d b f n w x y N $.  $d b f n w x y W $.
    $d x y F $.  $d b f n w S $.  $d b f n w V $.  $d b f n w y Z $.
    islbs.v $e |- V = ( Base ` W ) $.
    islbs.f $e |- F = ( Scalar ` W ) $.
    islbs.s $e |- S = ( vsca ` W ) $.
    islbs.k $e |- K = ( Base ` F ) $.
    islbs.j $e |- J = ( LBasis ` W ) $.
    islbs.n $e |- N = ( LSpan ` W ) $.
    islbs.z $e |- Z = ( 0g ` F ) $.
    $( The predicate " ` B ` is a basis for the left module or vector space
       ` W ` ".  A subset of the base set is a basis if zero is not in the set,
       it spans the set, and no nonzero multiple of an element of the basis is
       in the span of the rest of the family.  (Contributed by Mario Carneiro,
       24-Jun-2014.) $)
    islbs $p |- ( W e. X -> ( B e. J <-> ( B C_ V /\ ( N ` B ) = V /\ A. x e. B
      A. y e. ( K \ { Z } ) -. ( y S x ) e. ( N ` ( B \ { x } ) ) ) ) ) $=
      ( cfv vb vw vn vf wcel cv wceq co csn cdif wral cpw crab wss w3a cvv elex
      wn clbs cbs cvsca c0g csca wsbc clspn fveq2 syl6eqr pweqd fvex a1i adantr
      simplr fveq1d ad2antrr eqeq12d simpr fveq2d sneqd difeq12d eleq12d notbid
      wa oveqd raleqbidv ralbidv anbi12d sbcied2 rabeqbidv df-lbs eqeltri rabex
      pwex fvmpt syl5eq syl eleq2d elpw2 anbi1i eqeq1d difeq1 raleqbi1dv 3anass
      elrab 3bitr4i syl6bb ) JKUEZCFUECUAUFZHTZIUGZBUFZAUFZDUHZXGXKUIZUJZHTZUEZ
      URZBGLUIZUJZUKZAXGUKZWBZUAIULZUMZUEZCIUNZCHTZIUGZXLCXMUJZHTZUEZURZBXSUKZA
      CUKZUOZXFFYDCXFJUPUEZFYDUGJKUQYPFJUSTYDQUBJXGUCUFZTZUBUFZUTTZUGZXJXKYSVAT
      ZUHZXNYQTZUEZURZBUDUFZUTTZUUGVBTZUIZUJZUKZAXGUKZWBZUDYSVCTZVDZUCYSVETZVDZ
      UAYTULZUMYDUPUSYSJUGZUURYBUAUUSYCUUTYTIUUTYTJUTTZIYSJUTVFMVGZVHUUTUUPYBUC
      UUQHUPUUQUPUEUUTYSVEVIVJUUTUUQJVETHYSJVEVFRVGUUTYQHUGZWBZUUNYBUDUUOEUPUUO
      UPUEUVDYSVCVIVJUVDUUOJVCTZEUUTUUOUVEUGUVCYSJVCVFVKNVGUVDUUGEUGZWBZUUAXIUU
      MYAUVGYRXHYTIUVGXGYQHUUTUVCUVFVLZVMUUTYTIUGUVCUVFUVBVNVOUVGUULXTAXGUVGUUF
      XQBUUKXSUVGUUHGUUJXRUVGUUHEUTTGUVGUUGEUTUVDUVFVPZVQPVGUVGUUILUVGUUIEVBTLU
