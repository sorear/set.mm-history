      VGADSLBADSUIFUJHGAUTVABVBUQCDVFVCEVATUTTVBTFUQTVCTUKULUMUN $.
      $( [24-Jun-2014] $)

    $( An element of a basis is a vector.  (Contributed by Mario Carneiro,
       24-Jun-2014.) $)
    lbsel $p |- ( ( B e. J /\ E e. B ) -> E e. V ) $=
      ( wcel lbsss sselda ) ACHADBACDEFGIJ $.
      $( [24-Jun-2014] $)

    lbssp.n $e |- N = ( LSpan ` W ) $.
    $( The span of a basis is the whole space. $)
    lbssp $p |- ( B e. J -> ( N ` B ) = V ) $=
      ( vy vx wcel wss cfv wceq cv csn cdif wral clbs eqid cvsca co wn csca cbs
      c0g w3a cdm wb elfvdm eleq2s islbs syl ibi simp2d ) ABKZADLZACMDNZIOJOZEU
      AMZUBAUSPQCMKUCIEUDMZUEMZVAUFMZPQRJARZUPUQURVDUGZUPESUHZKZUPVEUIVGAESMBAE
      SUJGUKJIAUTVABVBCDEVFVCFVATUTTVBTGHVCTULUMUNUO $.
      $( [24-Jun-2014] $)

    lbsind.f $e |- F = ( Scalar ` W ) $.
    lbsind.s $e |- S = ( vsca ` W ) $.
    lbsind.k $e |- K = ( Base ` F ) $.
    lbsind.z $e |- Z = ( 0g ` F ) $.
    $( A basis is linearly independent; that is, every element has a span which
       trivially intersects the span of the remainder of the basis.
       (Contributed by Mario Carneiro, 12-Jan-2015.) $)
    lbsind $p |- ( ( ( B e. J /\ E e. B ) /\ ( A e. K /\ A =/= Z ) ) ->
        -. ( A S E ) e. ( N ` ( B \ { E } ) ) ) $=
      ( vy wcel vx wne wa csn cdif co cfv wn eldifsn wral wss wceq w3a clbs cdm
      cv wb elfvdm eleq2s islbs syl ibi simp3d oveq2 sneq difeq2d fveq2d notbid
      eleq12d oveq1 eleq1d rcla42v syl5com impl sylan2br ) AGTAKUBUCBFTZDBTZUCA
      GKUDUEZTZADCUFZBDUDZUEZHUGZTZUHZAGKUIVPVQVSWEVPSUPZUAUPZCUFZBWGUDZUEZHUGZ
      TZUHZSVRUJUABUJZVQVSUCWEVPBIUKZBHUGIULZWNVPWOWPWNUMZVPJUNUOZTZVPWQUQWSBJU
      NUGFBJUNURMUSUASBCEFGHIJWRKLOPQMNRUTVAVBVCWMWEWFDCUFZWCTZUHUASDABVRWGDULZ
      WLXAXBWHWTWKWCWGDWFCVDXBWJWBHXBWIWABWGDVEVFVGVIVHWFAULZXAWDXCWTVTWCWFADCV
      JVKVHVLVMVNVO $.
      $( [25-Jun-2014] $)
  $}

  ${
    lbsind2.j $e |- J = ( LBasis ` W ) $.
    lbsind2.n $e |- N = ( LSpan ` W ) $.
    lbsind2.f $e |- F = ( Scalar ` W ) $.
    lbsind2.o $e |- O = ( 1r ` F ) $.
    lbsind2.z $e |- Z = ( 0g ` F ) $.
    $( A basis is linearly independent; that is, every element is not in the
       span of the remainder of the basis.  (Revised by Mario Carneiro,
       12-Jan-2015.) $)
    lbsind2 $p |- ( ( ( W e. LMod /\ O =/= Z ) /\ B e. J /\ E e. B ) ->
                   -. E e. ( N ` ( B \ { E } ) ) ) $=
      ( clmod wcel cfv cbs syl eqid syl2anc wne wa w3a cvsca co csn simp2 simp3
      cdif crg simp1l lmodrng rngidcl simp1r lbsind syl22anc wceq lbsel lmodvs1
      wn eleq1d mtbid ) GNOZFHUAZUBZADOZBAOZUCZFBGUDPZUEZABUFUIEPZOZBVKOVHVFVGF
      CQPZOZVDVLUTVEVFVGUGZVEVFVGUHZVHCUJOZVNVHVCVQVCVDVFVGUKZCGKULRVMCFVMSZLUM
      RVCVDVFVGUNFAVIBCDVMEGQPZGHVTSZIJKVISZVSMUOUPVHVJBVKVHVCBVTOZVJBUQVRVHVFV
      GWCVOVPABDVTGWAIURTVIFCVTGBWAKWBLUSTVAVB $.
      $( [12-Jan-2015] $) $( [25-Jun-2014] $)

    $d x y B $.  $d x y C $.  $d x y J $.  $d x N $.  $d x y O $.  $d x y W $.
    $d x V $.  $d x y Z $.
    lbspss.v $e |- V = ( Base ` W ) $.
    $( No proper subset of a basis spans the space. $)
    lbspss $p |- ( ( ( W e. LMod /\ O =/= Z ) /\ B e. J /\ C C. B ) ->
      ( N ` C ) =/= V ) $=
      ( vx wcel wne wa wn vy clmod wpss w3a cv wex pssnel 3ad2ant3 simpl2 lbsss
      cfv wss syl simprl sseldd csn cdif simpl1l simpl1r syl211anc difss syl5ss
      lbsind2 simpl3 pssssd sseld wceq simprr eleq1 syl5ibrcom necon2ad eldifsn
      notbid jcad syl6ibr ssrdv lspss syl3anc mtod nelne1 syl2anc necomd ex mpd
      exlimdv ) HUBQZFIRZSZADQZBAUCZUDZPUEZAQZWLBQZTZSZPUFZBEUKZGRZWJWHWQWIPBAU
      GUHWKWPWSPWKWPWSWKWPSZGWRWTWLGQWLWRQZTGWRRWTAGWLWTWIAGULWHWIWJWPUIZADGHOJ
      UJUMZWKWMWOUNZUOWTXAWLAWLUPZUQZEUKZQZWTWFWGWIWMXHTWFWGWIWJWPURZWFWGWIWJWP
      USXBXDAWLCDEFHIJKLMNVCUTWTWRXGWLWTWFXFGULBXFULWRXGULXIWTXFAGAXEVAXCVBWTUA
      BXFWTUAUEZBQZXJAQZXJWLRZSXJXFQWTXKXLXMWTBAXJWTBAWHWIWJWPVDVEVFWTXKXJWLWTX
      KTXJWLVGZWOWKWMWOVHXNXKWNXJWLBVIVMVJVKVNXJAWLVLVOVPBXFEGHOKVQVRVFVSWLGWRV
      TWAWBWCWEWD $.
      $( [25-Jun-2014] $)
  $}

  $c LSSum $.

  $( Extend class notation with subspace sum. $)
  clsm $a class LSSum $.

  ${
    $d w t u x y z $.
    $( Define subspace sum. $)
    df-lsm $a |- LSSum = ( w e. _V |->
       ( t e. ~P ( Base ` w ) , u e. ~P ( Base ` w ) |->
           { z | E. x e. t E. y e. u z = ( x ( +g ` w ) y ) } ) ) $.
  $}

  ${
    $d t u w x y z A $.  $d t u x y z T $.  $d t u w x y z V $.  $d x y z X $.
    $d t u x y z U $.  $d t u w x y z W $.  $d y z Y $.
    lsmfval.v $e |- V = ( Base ` W ) $.
    lsmfval.a $e |- A = ( +g ` W ) $.
    lsmfval.s $e |- S = ( LSSum ` W ) $.
    $( The subspace sum function (for a left module or left vector space). $)
    lsmfval $p |- ( W e. X -> S = ( t e. ~P V , u e. ~P V |->
                    { z | E. x e. t E. y e. u z = ( x A y ) } ) ) $=
      ( vw cfv cv wceq wrex cvv cbs wcel clsm cpw cab cmpt2 elex cplusg syl6eqr
      co fveq2 pweqd oveqd eqeq2d 2rexbidv abbidv mpt2eq123dv fvex eqeltri pwex
      df-lsm mpt2ex fvmpt syl syl5eq ) IJUAZGIUBOZEDHUCZVGCPZAPZBPZFUIZQZBDPZRA
      EPZRZCUDZUEZMVEISUAVFVQQIJUFNIEDNPZTOZUCZVTVHVIVJVRUGOZUIZQZBVMRAVNRZCUDZ
      UEVQSUBVRIQZEDVTVTWEVGVGVPWFVSHWFVSITOZHVRITUJKUHUKZWHWFWDVOCWFWCVLABVNVM
      WFWBVKVHWFWAFVIVJWFWAIUGOFVRIUGUJLUHULUMUNUOUPABCNDEUTEDVGVGVPHHWGSKITUQU
      RUSZWIVAVBVCVD $.
      $( [21-Jun-2014] $) $( [28-Jan-2014] $)

    $( Subspace sum value (for a left module or left vector space).  Extended
       domain version of ~ lsmval . $)
    lsmvalx $p |- ( ( W e. X /\ T C_ V /\ U C_ V )
       -> ( T S U ) = { z | E. x e. T E. y e. U z = ( x A y ) } ) $=
      ( vt vu wcel cv wceq wrex cvv wss co cab cpw cmpt2 lsmfval oveqd cbs fvex
      wa cfv eqeltri elpw2 wral abrexexg ralrimivw abrexex2g sylan2 simpl simpr
      rexeqdv rexeqbidv abbidv eqid ovmpt2ga mpd3an3 syl2anbr sylan9eq 3impb )
