      $( [21-Jun-2014] $) $( [28-Jan-2014] $)

    $( Subspace sum membership (for a left module or left vector space). $)
    lsmelvalix $p |- ( ( ( W e. LMod /\ T C_ V /\ U C_ V ) /\
               ( X e. T /\ Y e. U ) ) -> ( X A Y ) e. ( T S U ) ) $=
