    $( Ring multiplication distributes over subtraction.  ( ~ subdir analog.)
       (Contributed by Jeff Madsen, 19-Jun-2010.) $)
    rngsubdir $p |- ( ph -> ( ( X M Y ) T Z ) = ( ( X T Z ) M ( Y T Z ) ) ) $=
      ( cfv co crg wcel syl3anc cminusg cplusg wceq rnggrp syl grpinvcl syl2anc
      cgrp rngdir syl13anc rngmneg1 oveq2d eqtrd grpsubval oveq1d rngcl 3eqtr4d
      eqid ) AFGCUAPZPZCUBPZQZHDQZFHDQZGHDQZUSPZVAQZFGEQZHDQVDVEEQZAVCVDUTHDQZV
