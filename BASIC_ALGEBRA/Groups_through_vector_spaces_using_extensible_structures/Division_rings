    $( The multiplicative inverse in a division ring is nonzero.  ( ~ recne0
       analog.) $)
    drnginvrn0 $p |- ( ( R e. DivRing /\ X e. B /\ X =/= Z )
             -> ( I ` X ) =/= Z ) $=
      ( cdr wcel wne w3a cfv wa cui crg wi drngrng eqid drngunit ex syl 3imtr3d
