    $( Lemma for ~ grpoidinv . $)
    grpoidinvlem1 $p |- ( ( ( G e. GrpOp /\ ( Y e. X /\ A e. X ) ) /\
                  ( ( Y G A ) = U /\ ( A G A ) = A ) ) -> ( U G A ) = U ) $=
      ( cgr wcel wa co wceq oveq1 ad2antrl w3a id 3anidm23 sylan2 adantr eqtr3d
