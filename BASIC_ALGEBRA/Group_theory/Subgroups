  $c SubGrpOp $.

  $( Extend class notation to include the class of subgroups. $)
  csubg $a class SubGrpOp $.

  $( Define the set of subgroups of ` g ` . $)
  df-subg $a |- SubGrpOp = ( g e. GrpOp |-> ( GrpOp i^i ~P g ) ) $.

  ${
    $d G g h $.  $d H h $.
    $( The predicate "is a subgroup of ` G ` ."  (Contributed by Paul Chapman,
       3-Mar-2008.) $)
    issubg $p |- ( H e. ( SubGrpOp ` G ) <->
                   ( G e. GrpOp /\ H e. GrpOp /\ H C_ G ) ) $=
      ( vg cgr wcel csubg cfv wa wss w3a cpw cin wceq inss2 pwexg ssexg sylancr
      cvv cv pm5.32i pweq ineq2d df-subg fvmptg mpdan eleq2d elpwg bitri syl6bb
      elin cdm crab dmmpt ssrab2 eqsstri elfvdm sseldi pm4.71ri 3anass 3bitr4i
      ) ADEZBAFGZEZHVABDEZBAIZHZHVCVAVDVEJVAVCVFVAVCBDAKZLZEZVFVAVBVHBVAVHREZVB
      VHMVAVHVGIVGREVJDVGNADOVHVGRPQCADCSZKZLZVHDRFVKAMVLVGDVKAUAUBCUCZUDUEUFVI
      VDBVGEZHVFBDVGUJVDVOVEBADUGTUHUITVCVAVCFUKZDAVPVMREZCDULDCDVMFVNUMVQCDUNU
      OBAFUPUQURVAVDVEUSUT $.
      $( [12-Jul-2014] $) $( [3-Mar-2008] $)
