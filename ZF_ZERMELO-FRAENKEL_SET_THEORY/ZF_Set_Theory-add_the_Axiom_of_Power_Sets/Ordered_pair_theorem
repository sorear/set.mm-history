      AUBQVJVBUNUMURBDRUDSTUQURCDGHUEIUEUFUGUH $.
      $( [14-Oct-2005] $)
  $}

  ${
    otthg.1 $e |- A e. _V $.
    otthg.2 $e |- B e. _V $.
    otthg.3 $e |- R e. _V $.
    $( Ordered triple theorem, with triple express with ordered pairs. $)
    otthg2 $p |- ( ( D e. V /\ S e. W ) ->
                ( <. <. A , B >. , R >. = <. <. C , D >. , S >. <->
                ( A = C /\ B = D /\ R = S ) ) ) $=
      ( wcel wa cop wceq w3a opex opthg anbi1d sylan9bbr df-3an syl6bbr ) DGLZF
      HLZMABNZENCDNZFNOZACOZBDOZMZEFOZMZUHUIUKPUDUGUEUFOZUKMUCULUEEUFFHABQKRUCU
      MUJUKABCDGIJRSTUHUIUKUAUB $.
      $( [1-May-1995] $)

    $( Ordered triple theorem. $)
    otthg $p |- ( ( D e. V /\ S e. W ) ->
     ( <. A , B , R >. = <. C , D , S >. <-> ( A = C /\ B = D /\ R = S ) ) ) $=
      ( cotp wceq cop wcel wa w3a df-ot eqeq12i otthg2 syl5bb ) ABELZCDFLZMABNE
      NZCDNFNZMDGOFHOPACMBDMEFMQUBUDUCUEABERCDFRSABCDEFGHIJKTUA $.
      $( [25-Sep-2014] $)
  $}

  ${
