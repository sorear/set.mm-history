  $( Addition with 1 is same as successor.  Proposition 4.34(a) of [Mendelson]
     p. 266. $)
  oa1suc $p |- ( A e. On ->
                ( A +o 1o ) = suc A ) $=
    ( con0 wcel c1o co c0 csuc df-1o oveq2i wceq 0elon oasuc mpan2 syl5eq suceq
