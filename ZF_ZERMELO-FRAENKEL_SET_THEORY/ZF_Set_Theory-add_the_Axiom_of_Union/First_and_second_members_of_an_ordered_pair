  $( Equality with an ordered pair. $)
  eqopi $p |- ( ( A e. ( V X. W ) /\ ( ( 1st ` A ) = B /\ ( 2nd ` A ) = C ) )
              -> A = <. B , C >. ) $=
    ( cxp wcel cvv c1st cfv wceq c2nd wa cop xpss sseli simplbi opeq12 sylan9eq
    elxp6 sylan ) ADEFZGAHHFZGZAIJZBKALJZCKMZABCNZKUBUCADEOPUDUGAUEUFNZUHUDAUIK
    UEHGUFHGMAHHTQUEUFBCRSUA $.
    $( [23-Feb-2014] $) $( [15-Dec-2008] $)

  ${
    eqop.1 $e |- C e. _V $.
    $( Two ways to express equality with an ordered pair. $)
    eqop $p |- ( A e. ( V X. W ) -> ( A = <. B , C >.
              <-> ( ( 1st ` A ) = B /\ ( 2nd ` A ) = C ) ) ) $=
      ( cxp wcel cop wceq c1st cfv wa eleq1 biimpac opelxp1 fveq2 sylan9eqr cvv
      c2nd syl op1stg op2ndg mpan2 jca sylancom eqopi impbida ) ADEGZHZABCIZJZA
