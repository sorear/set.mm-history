      ( vx cv cop c1st cfv wceq opeq1 fveq2d id eqeq12d vex op1st vtoclg ) DEZB
      FZGHZQIABFZGHZAIDACQAIZSUAQAUBRTGQABJKUBLMQBDNOP $.
      $( [19-Jul-2005] $)

