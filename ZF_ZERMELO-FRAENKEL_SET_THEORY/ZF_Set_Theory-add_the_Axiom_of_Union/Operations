      EXFXGXHXEDWMXFCWMXGBWMVJWNWOABCDEWPWQ $.
      $( [20-Mar-2013] $)
  $}

  $( The result of an operation is a set. $)
  ovex $p |- ( A F B ) e. _V $=
    ( co cop cfv cvv df-ov fvex eqeltri ) ABCDABEZCFGABCHKCIJ $.
    $( [13-Mar-1995] $)

  ${
    ovprc1.1 $e |- Rel dom F $.
    $( The value of an operation when the first argument is a proper class.
       Note: this theorem is dependent on our particular definitions of
       operation value, function value, and ordered pair. $)
    ovprc1 $p |- ( -. A e. _V -> ( A F B ) = (/) ) $=
      ( cvv wcel wn co cop cfv c0 df-ov cdm wceq wbr df-br brrelexi con3i ndmfv
      sylbir syl syl5eq ) AEFZGZABCHABIZCJZKABCLUDUECMZFZGUFKNUHUCUHABUGOUCABUG
      PABUGDQTRUECSUAUB $.
      $( [16-Jun-2004] $)
  $}

  $( The value of an operation when the second argument is a proper class.
     Note: this theorem is dependent on our particular definitions of operation
     value, function value, and ordered pair. $)
  ovprc2 $p |- ( -. B e. _V -> ( A F B ) = ( A F A ) ) $=
    ( cvv wcel wn cop cfv co opprc2 fveq2d df-ov 3eqtr4g ) BDEFZABGZCHAAGZCHABC
    IAACINOPCABJKABCLAACLM $.
    $( [16-Jun-2004] $)

  ${
    $d y z A $.  $d y z B $.  $d y z C $.  $d y z D $.  $d y z F $.
    $d x y z $.
    $( Move class substitution in and out of an operation.  (The proof was
       shortened by Andrew Salmon, 22-Oct-2011.) $)
    csbovg $p |- ( A e. D -> [_ A / x ]_ ( B F C ) =
           ( [_ A / x ]_ B [_ A / x ]_ F [_ A / x ]_ C ) ) $=
      ( vy vz wcel co csb cop cfv df-ov cv wceq csbeq1 opeq12d hbcsb1 csbeq1a
      csbeq2i csbfv12g eqeq12d ax-17 hbop csbief vtoclg fveq2d syl6eqr 3eqtrd
      vex ) BEIZABCDFJZKABCDLZFMZKABUNKZABFKZMZABCKZABDKZUQJZABUMUOECDFNUAABUNE
      FUBULURUSUTLZUQMVAULUPVBUQAGOZUNKZAVCCKZAVCDKZLZPUPVBPGBEVCBPZVDUPVGVBAVC
      BUNQVHVEUSVFUTAVCBCQAVCBDQRUCAHVCUNVGGUKZAHVEVFAHVCCVIHOVCIAUDZSAHVCDVIVJ
      SUEAOVCPCVEDVFAVCCTAVCDTRUFUGUHUSUTUQNUIUJ $.
      $( [22-Oct-2011] $) $( [12-Nov-2005] $)
  $}

  ${
    $d y A $.  $d y C $.  $d y D $.  $d x y F $.
    $( Move class substitution in and out of an operation. $)
    csbov12g $p |- ( A e. D ->
                 [_ A / x ]_ ( B F C ) = ( [_ A / x ]_ B F [_ A / x ]_ C ) ) $=
      ( wcel co csb csbovg csbconstgfv oveqd eqtrd ) BEGZABCDFHIABCIZABDIZABFIZ
      HOPFHABCDEFJNQFOPABFEKLM $.
      $( [12-Nov-2005] $)
  $}

  ${
    $d y A $.  $d x y C $.  $d y D $.  $d x y F $.
    $( Move class substitution in and out of an operation. $)
    csbov1g $p |- ( A e. D ->
                  [_ A / x ]_ ( B F C ) = ( [_ A / x ]_ B F C ) ) $=
      ( wcel co csb csbov12g csbconstgfv oveq2d eqtrd ) BEGZABCDFHIABCIZABDIZFH
      ODFHABCDEFJNPDOFABDEKLM $.
      $( [12-Nov-2005] $)
  $}

  ${
    $d y A $.  $d x y B $.  $d y D $.  $d x y F $.
    $( Move class substitution in and out of an operation. $)
    csbov2g $p |- ( A e. D ->
                  [_ A / x ]_ ( B F C ) = ( B F [_ A / x ]_ C ) ) $=
      ( wcel co csb csbov12g csbconstgfv oveq1d eqtrd ) BEGZABCDFHIABCIZABDIZFH
      CPFHABCDEFJNOCPFABCEKLM $.
      $( [12-Nov-2005] $)
  $}

  ${
    $d x A $.  $d x y B $.  $d x y C $.  $d y D $.  $d x y F $.  $d x y S $.
    $( A frequently used special case of ~ rcla42ev for operation values. $)
    rcla4eov $p |- ( ( C e. A /\ D e. B /\ S = ( C F D ) ) ->
                 E. x e. A E. y e. B S = ( x F y ) ) $=
      ( cv co wceq oveq1 eqeq2d oveq2 rcla42ev ) GAIZBIZHJZKGEFHJZKGEQHJZKABEFC
      DPEKRTGPEQHLMQFKTSGQFEHNMO $.
      $( [21-Mar-2007] $)
  $}

  ${
    fnopovb.1 $e |- R e. _V $.
    $( Equivalence of operation value and ordered triple membership, analogous
       to ~ fnopfvb . $)
    fnopovb $p |- ( ( F Fn ( A X. B ) /\ C e. A /\ D e. B ) ->
                   ( ( C F D ) = R <-> <. <. C , D >. , R >. e. F ) ) $=
      ( cxp wfn wcel co wceq cop wb wa opelxpi cfv df-ov eqeq1i fnopfvb syl5bb
      sylan2 3impb ) FABHZIZCAJZDBJZCDFKZELZCDMZEMFJZNZUFUGOUEUJUDJZULCDABPUIUJ
      FQZELUEUMOUKUHUNECDFRSUDUJEFGTUAUBUC $.
      $( [17-Dec-2008] $)
  $}

  ${
    $d x z w v $.  $d y z w v $.  $d w ph v $.
    $( Class abstraction for operations in terms of class abstraction of
       ordered pairs. $)
    dfoprab2 $p |- { <. <. x , y >. , z >. | ph } =
                   { <. w , z >. | E. x E. y ( w = <. x , y >. /\ ph ) } $=
      ( vv cv cop wceq wa wex cab copab2 copab excom exrot4 19.42v opeq1 eqeq2d
      3bitr3i pm5.32ri anbi1i anass an32 exbii opex isseti biantru 3exbii bitri
      3bitr4i 19.42vv 2exbii abbii df-oprab df-opab 3eqtr4i ) FGZBGZCGZHZDGZHZI
      ZAJZDKCKBKZFLUREGZVBHZIZVGVAIZAJZCKBKZJZDKEKZFLABCDMVLEDNVFVNFVIVKJZCKBKZ
