    $( A mapping (first hypothesis) that is one-to-one (second hypothesis)
       implies its domain is dominated by its codomain. $)
    dom2d $p |- ( ph -> ( A e. R -> A ~<_ B ) ) $=
      ( cv wcel wceq wa copab wf1 cdom wbr dom2lem f1domg syl5com ) ADEBKDLCKFM
      NBCOZPDHLDEQRABCDEFGIJSDEHUBTUA $.
      $( [24-Jul-2004] $)

    $( A mapping (first hypothesis) that is one-to-one (second hypothesis)
       implies its domain is dominated by its codomain. $)
    dom2dOLD $p |- ( ph -> ( A e. R -> A ~<_ B ) ) $=
      ( vz cv wcel wceq wa cfv wi wral imp ax-17 copab wf1 cdom wbr wf ralrimiv
