    $( A supremum is a set. $)
    supex $p |- sup ( B , A , R ) e. _V $=
      ( vx vy vz csup cv wcel wbr wn wral wrex wi wa cab cuni cvv crab eqeltri
      df-sup df-rab unieqi eqtri wmo supmo moabex ax-mp uniex ) BACHZEIZAJULFIZ
