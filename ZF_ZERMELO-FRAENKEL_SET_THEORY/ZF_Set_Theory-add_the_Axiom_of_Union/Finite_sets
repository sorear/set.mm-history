  ${
    unxpdom2.1 $e |- A e. _V $.
    unxpdom2.2 $e |- B e. _V $.
    $( Corollary of ~ unxpdom . $)
    unxpdom2 $p |- ( ( 1o ~< A /\ B ~<_ A ) -> ( A u. B ) ~<_ ( A X. A ) ) $=
      ( cdom wbr c1o csdm cun cxp csn cen xpsnen ensymi ax-mp domentr mpan2 cvv
      c0 wa com 1onn elexi endom 0ex cin wceq wne xpsndisj snex xpex p0ex undom
      1n0 sylancr wcel sdomentr unxpdom syl2anc xpen mp2an sylancl domtr syl2an
