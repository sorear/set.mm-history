    hbreud.4 $e |- ( ph -> ( ps -> A. x ps ) ) $.
    $( Deduction version of ~ hbreu . $)
    hbreud $p |- ( ph -> ( E! y e. A ps -> A. x E! y e. A ps ) ) $=
