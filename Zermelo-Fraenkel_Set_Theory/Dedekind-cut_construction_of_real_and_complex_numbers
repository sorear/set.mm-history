  $( The empty set is not a signed real. $)
  0nsr $p |- -. (/) e. N_r $=
    ( c0 cnr wcel cnp cxp cer cqs df-nr eleq2i enrer dmenr 0nelqs mtbir ) ABCAD
    DEZFGZCBOAHINFJKLM $.
