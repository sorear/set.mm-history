  $( Technical trick to permit re-use of some equivalence class lemmas for
     operator laws. $)
  dfcnqs $p |- CC = ( ( N_r X. N_r ) /. `' E ) $=
      ( cc cnr cxp cep ccnv cqs df-c nrex xpex qsid eqtr4 ) ABBCZLDEFGLBBHHIJK
      $.
      $([13-Aug-95]$)

  $( Technical trick to permit re-use of some equivalence class lemmas for
     operator laws. $)
  addcnsrec $p |- ( ( ( A e. N_r /\ B e. N_r ) /\ ( C e. N_r /\ D e. N_r ) ) ->
                 ( ( `' E [ <. A , B >. ] ) + ( `' E [ <. C , D >. ] ) ) =
                   ( `' E [ <. ( A +_r C ) , ( B +_r D ) >. ] ) ) $=
      ( cnr wcel wa cop caddc co cplr cep ccnv cec addcnsr opex ecid opreq12i
      3eqtr4g ) AEFBEFGCEFDEFGGABHZCDHZIJACKJZBDKJZHZTLMZNZUAUENZIJUDUENABCDOUF
      TUGUAITABPQUACDPQRUDUBUCPQS $.
      $([13-Aug-95]$)

  $( Technical trick to permit re-use of some equivalence class lemmas for
     operator laws. $)
  mulcnsrec $p |- ( ( ( A e. N_r /\ B e. N_r ) /\ ( C e. N_r /\ D e. N_r ) ) ->
               ( ( `' E [ <. A , B >. ] ) x. ( `' E [ <. C , D >. ] ) ) =
                   ( `' E [ <. ( ( A ._r C ) +_r ( -1_r ._r ( B ._r D ) ) ) ,
                          ( ( B ._r C ) +_r ( A ._r D ) ) >. ] ) ) $=
      ( cnr wcel wa cop cmulc co cmr cm1r cplr cep ccnv cec mulcnsr opex ecid
      opreq12i 3eqtr4g ) AEFBEFGCEFDEFGGABHZCDHZIJACKJLBDKJKJMJZBCKJADKJMJZHZUB
      NOZPZUCUGPZIJUFUGPABCDQUHUBUIUCIUBABRSUCCDRSTUFUDUERSUA $.
      $([13-Aug-95]$)

