  $( Technical trick to permit reuse of some of our previous equivalence class
     lemmas to prove arithmetic operation laws in ` CC ` from those in ` R. ` .
     Note that converse epsilon is not an equivalence relation, but the lemmas
     we reuse do not require it. $)
  dfcnqs $p |- CC = ( ( R. X. R. ) /. `' E ) $=
    ( cc cnr cxp cep ccnv cqs df-c srex xpex qsid eqtr4 ) ABBCZLDEFGLBBHHIJK $.
    $( [13-Aug-95] $)

  $( Technical trick to permit re-use of some equivalence class lemmas for
     operation laws. $)
  addcnsrec $p |- ( ( ( A e. R. /\ B e. R. ) /\ ( C e. R. /\ D e. R. ) ) ->
                 ( [ <. A , B >. ] `' E + [ <. C , D >. ] `' E ) =
                   [ <. ( A +R C ) , ( B +R D ) >. ] `' E ) $=
    ( cnr wcel wa cop caddc co cplr cep ccnv cec addcnsr opex ecid opreq12i
    3eqtr4g ) AEFBEFGCEFDEFGGABHZCDHZIJACKJZBDKJZHZTLMZNZUAUENZIJUDUENABCDOUFTU
    GUAITABPQUACDPQRUDUBUCPQS $.
    $( [13-Aug-95] $)

  $( Technical trick to permit re-use of some equivalence class lemmas for
     operation laws. $)
  mulcnsrec $p |- ( ( ( A e. R. /\ B e. R. ) /\ ( C e. R. /\ D e. R. ) ) ->
               ( [ <. A , B >. ] `' E x. [ <. C , D >. ] `' E ) =
                    [ <. ( ( A .R C ) +R ( -1R .R ( B .R D ) ) ) ,
                          ( ( B .R C ) +R ( A .R D ) ) >. ] `' E ) $=
    ( cnr wcel wa cop cmulc co cmr cm1r cplr cep ccnv cec mulcnsr opex ecid
    opreq12i 3eqtr4g ) AEFBEFGCEFDEFGGABHZCDHZIJACKJLBDKJKJMJZBCKJADKJMJZHZUBNO
    ZPZUCUGPZIJUFUGPABCDQUHUBUIUCIUBABRSUCCDRSTUFUDUERSUA $.
    $( [13-Aug-95] $)

