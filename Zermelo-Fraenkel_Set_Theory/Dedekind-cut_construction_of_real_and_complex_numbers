  ${
    $v s $.
    $v a $.
    $v b $.
    $v c $.
    $v d $.
    pipq.vs $f set s $.
    pipq.va $f set a $.
    pipq.vb $f set b $.
    pipq.vc $f set c $.
    pipq.vd $f set d $.
    $d x y z w v u t s f g h a b c d A $.
    $d x y z w v u t s f g h a b c d B $.
    $d x y z w v u t s f g h a b c d C $.
    $d x y z w v u t s f g h a b c d D $.
                $( ` x y z w v u t s f g h a b c d ` are dummy. $)
    $( Addition of positive fractions in terms of positive integers. $)
    addpipq $p |- ( ( ( A e. N. /\ B e. N. ) /\
                     ( C e. N. /\ D e. N. ) ) ->
                ( [ <. A , B >. ] ~Q +Q [ <. C , D >. ] ~Q ) =
                   [ <. ( ( A .N D ) +N ( B .N C ) ) ,
                              ( B .N D ) >. ] ~Q ) $=
      ( vz vu vw vv pipq.va pipq.vd pipq.vb pipq.vc vg pipq.vs vh vt vx vy vf
      cv cmi co wceq ceq cnpi cplq cplpq cpli cop cnq opex enqex enqer dmenq
      df-enq weq wb wa opreq12 cleqan12d an41r3s df-plpq opeq12 opreqan12d
      adantl an4s sylan13 df-plq df-nq visset addcmpblnq oprec ) ETZFTZUAUBZGTZ
