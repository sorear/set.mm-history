      CUD $.
      $([3-Sep-95]$)
  $}

  $( The class of positive fractions exists. $)
  nqex $p |- N_q e. V $=
    ( cnq cnpi cxp ceq cqs cvv df-nq niex xpex qsex eqeltr ) ABBCZDEFGLDBBHHIJK
    $.
    $([16-Aug-95]$)

  $( The empty set is not a positive fraction. $)
  0npq $p |- -. (/) e. N_q $=
    ( c0 cnq wcel cnpi cxp ceq cqs df-nq eleq2i enqer dmenq 0nelqs mtbir ) ABCA
    DDEZFGZCBOAHINFJKLM $.
