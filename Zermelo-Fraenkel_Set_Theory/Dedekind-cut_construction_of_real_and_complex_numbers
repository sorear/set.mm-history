  ${
    $v t $.
    $v s $.
    $v a $.
    $v b $.
    $v c $.
    $v d $.
    pipq.vt $f var t $.
    pipq.vs $f var s $.
    pipq.va $f var a $.
    pipq.vb $f var b $.
    pipq.vc $f var c $.
    pipq.vd $f var d $.
    $d x y z w v u t s f g h a b c d A $.
    $d x y z w v u t s f g h a b c d B $.
    $d x y z w v u t s f g h a b c d C $.
    $d x y z w v u t s f g h a b c d D $.
                $( ` x y z w v u t s f g h a b c d ` are dummy. $)
    $( Addition of positive fractions in terms of positive integers. $)
    addpipq $p |- ( ( ( A e. N_i /\ B e. N_i ) /\
                     ( C e. N_i /\ D e. N_i ) ) ->
                ( ( ~_q [ <. A , B >. ] ) +_q ( ~_q [ <. C , D >. ] ) ) =
                   ( ~_q [ <. ( ( A ._i D ) +_i ( B ._i C ) ) ,
                              ( B ._i D ) >. ] ) ) $=
      ( vz vu vw vv pipq.va pipq.vd pipq.vb pipq.vc vg pipq.vs vh pipq.vt vx
      vy vf cv cmi co wceq ceq cnpi cplq cplpq cpli cop cnq opex enqex enqer
      dmenq df-enq weq wb wa opreq12 cleqan12d an41r3s df-plpq opreqan12d
      adantl an4s jca opeq12 syl df-plq df-nq visset addcmpblnq oprec ) ETZFTZU
