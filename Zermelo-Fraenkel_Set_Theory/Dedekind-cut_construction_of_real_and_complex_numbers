  ${
    $d x y z $.
    $( Define the class of positive reals.  Definition 9-3.1 of Gleason,
       p. 121. $)
    df-np  $a |- N_p = { x | ( ( (/) (. x /\ x (. N_q ) /\
         A. y e. x ( A. z ( z <_q y -> z e. x ) /\ E. z e. x y <_q z ) ) } $.
  $}

  $( Define the positive real constant `1`.  Gleason, p. 122. $)
  df-1p $a |- 1_p = { x | x <_q 1_q } $.

  ${
    $d x y z w v u $.
    $( Define addition on positive reals.  From Proposition 9-3.5 of Gleason,
       p.123. $)
    df-plp $a |- +_p = { <. <. x , y >. , z >. | ( ( x e. N_p /\ y e. N_p ) /\
                 z = { w | E. v e. x E. u e. y w = ( v +_q u ) } ) } $.

    $( Define multiplication on positive reals.  From Proposition 9-3.7 of
       Gleason, p. 124. $)
    df-mp $a |- ._p = { <. <. x , y >. , z >. | ( ( x e. N_p /\ y e. N_p ) /\
                 z = { w | E. v e. x E. u e. y w = ( v ._q u ) } ) } $.

    $( Define ordering on positive reals.  From 9-3.2 of Gleason, p. 122. $)
    df-ltp $a |- <_p = { <. x , y >. | ( ( x e. N_p /\ y e. N_p ) /\
                 x (. y ) } $.
  $}

  ${
    $d x y z $.  $( ` x y z ` are dummy. $)
    $( The class of positive reals is a set. $)
    npex $p |- N_p e. V $=
