  ${
    $d x y z w v u f $.

    $( Define pre-addition on positive fractions.  From Proposition 9-2.3
       of Gleason, p. 117. $)
    df-plpq $a |- +_pq = { <. <. x , y >. , z >. | ( ( x e. ( N_i X. N_i ) /\
         y e. ( N_i X. N_i ) ) /\ E. w E. v E. u E. f ( ( x = <. w , v >. /\
         y = <. u , f >. ) /\ z = <. ( ( w ._i f ) +_i ( v ._i u ) ) ,
           ( v ._i f ) >. ) ) } $.

    $( Define pre-multiplication on positive fractions.  From Proposition 9-2.4
       of Gleason, p. 119. $)
    df-mpq $a |- ._pq = { <. <. x , y >. , z >. | ( ( x e. ( N_i X. N_i ) /\
         y e. ( N_i X. N_i ) ) /\ E. w E. v E. u E. f ( ( x = <. w , v >. /\
         y = <. u , f >. ) /\ z = <. ( w ._i u ) , ( v ._i f ) >. ) ) } $.

    $( Define equivalence relation for positive fractions.  From Proposition
       9-2.1 of Gleason, p. 117. $)
    df-enq $a |- ~_q = { <. x , y >. | ( ( x e. ( N_i X. N_i ) /\
                   y e. ( N_i X. N_i ) ) /\ E. z E. w E. v E. u
                   ( ( x = <. z , w >. /\ y = <. v , u >. ) /\
                   ( z ._i u ) = ( w ._i v ) ) ) } $.

    $( Define class of positive fractions.  From 9-2.2 of Gleason, p. 117. $)
    df-nq $a |- N_q = ( ( N_i X. N_i ) /. ~_q ) $.

    $( Define addition on positive fractions.   From Proposition 9-2.3
       of Gleason, p. 117. $)
    df-plq $a |- +_q = { <. <. x , y >. , z >. | ( ( x e. N_q /\ y e. N_q ) /\
      E. w E. v E. u E. f (
         ( x = ( ~_q [ <. w , v >. ] ) /\ y = ( ~_q [ <. u , f >. ] ) ) /\
               z = ( ~_q [ ( <. w , v >. +_pq <. u , f >. ) ] ) ) ) } $.

    $( Define multiplication on positive fractions.   From Proposition 9-2.4
       of Gleason, p. 119. $)
    df-mq $a |- ._q = { <. <. x , y >. , z >. | ( ( x e. N_q /\ y e. N_q ) /\
      E. w E. v E. u E. f (
         ( x = ( ~_q [ <. w , v >. ] ) /\ y = ( ~_q [ <. u , f >. ] ) ) /\
               z = ( ~_q [ ( <. w , v >. ._pq <. u , f >. ) ] ) ) ) } $.

    $( Define reciprocal on positive fractions.  From 9-2.5 of Gleason,
       p. 119. $)
    df-rq $a |- *_q = { <. x , y >. | ( x e. N_q /\ ( x ._q y ) = 1_q ) } $.

    $( Define ordering relation on positive fractions.  Suppes, p. ??? $)
    df-ltq $a |- <_q = { <. x , y >. | ( ( x e. N_q /\ y e. N_q ) /\
       E. z E. w E. v E. u
       ( ( x = ( ~_q [ <. z , w >. ] ) /\ y = ( ~_q [ <. v , u >. ] ) ) /\
                ( z ._i u ) <_i ( w ._i v ) ) ) } $.

    $( Define positive fraction constant 1.  From 9-2.2 of Gleason, p. 117. $)
    df-1q $a |- 1_q = ( ~_q [ <. 1_o , 1_o >. ] ) $.

  $}

  ${
    $d x y z w v u A $.  $d x y z w v u B $.  $d x y z w v u C $.
    $d x y z w v u D $.  $( ` x y z w v u ` are dummy. $)
    $( Equivalence relation for positive fractions in terms of positive
       integers. $)
    enqbreq $p |- ( ( ( A e. N_i /\ B e. N_i ) /\ ( C e. N_i /\ D e. N_i ) ) ->
          ( <. A , B >. ~_q <. C , D >. <-> ( A ._i D ) = ( B ._i C ) ) ) $=
      ( vx vy vz vw vv vu ceq cnpi cmi df-enq ecopopreq ) EFGHIJABCDKLMEFGHIJNO
      $.
      $([27-Aug-95]$)
  $}

  ${
    $d x y z w v u $.  $( ` x y z w v u ` are dummy. $)
    $( Domain of equivalence relation for positive fractions. $)
    dmenq $p |- dom ~_q = ( N_i X. N_i ) $=
      ( vx vy vz vw vv vu ceq cnpi cmi df-enq cv visset mulcompi ecopoprdm ) AB
      CDEFGHIABCDEFJAKBKALBLMN $.
      $([27-Aug-95]$)
  $}

  ${
    $d x y z w v u $.  $( ` x y z w v u ` are dummy. $)
    $( The equivalence relation for positive fractions is an equivalence
       relation.  Proposition 9-2.1 of Gleason, p. 117. $)
    enqer $p |- Er ~_q $=
      ( vx vy vz vw vv vu ceq cnpi cmi df-enq cv visset mulcompi mulclpi
      mulasspi mulcanpi ecopoprer ) ABCDEFGHIABCDEFJAKZBKZALBLZMRSNRSCKZTCLZORS
      UAUBPQ $.
      $([27-Aug-95]$)
  $}

  $( Equivalence class equality of positive fractions in terms of positive
     integers. $)
  enqeceq $p |- ( ( ( A e. N_i /\ B e. N_i ) /\ ( C e. N_i /\ D e. N_i ) ) ->
                ( ( ~_q [ <. A , B >. ] ) = ( ~_q [ <. C , D >. ] ) <->
                ( A ._i D ) = ( B ._i C ) ) ) $=
    ( cnpi wcel wa cop ceq cec wceq wbr cmi co cdm wb pm3.26 cxp opelxpi dmenq
    eleq2i sylibr opex enqer erthdm 3syl enqbreq bitrd ) AEFBEFGZCEFDEFGZGZABHZ
    IJCDHZIJKZULUMILZADMNBCMNKUKUIULIOZFZUNUOPUIUJQUIULEERZFUQABEESUPURULTUAUBU
    LUMICDUCUDUEUFABCDUGUH $.
    $([29-Nov-95]$)

  ${
    $d x y z w v u $.  $( ` x y z w v u ` are dummy. $)
    $( The equivalence relation for positive fractions exists. $)
    enqex $p |- ~_q e. V $=
      ( vx vy vz vw vv vu ceq cnpi cxp niex xpex cv wcel wa cop wceq cmi co
      wex copab df-enq opabssxp eqsstr ssexi ) GHHIZUEIZUEUEHHJJKZUGKGALZUEMBLZ
