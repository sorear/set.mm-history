  ${
    $v t $.
    $v s $.
    $v a $.
    $v b $.
    $v c $.
    $v d $.
    addsrpr.vt $f var t $.
    addsrpr.vs $f var s $.
    addsrpr.va $f var a $.
    addsrpr.vb $f var b $.
    addsrpr.vc $f var c $.
    addsrpr.vd $f var d $.
    $d x y z w v u t s f g h a b c d A $.
    $d x y z w v u t s f g h a b c d B $.
    $d x y z w v u t s f g h a b c d C $.
    $d x y z w v u t s f g h a b c d D $.
                $( ` x y z w v u t s f g h a b c d ` are dummy. $)
    $( Addition of signed reals in terms of positive reals. $)
    addsrpr $p |- ( ( ( A e. N_p /\ B e. N_p ) /\
                     ( C e. N_p /\ D e. N_p ) ) ->
                ( ( ~_r [ <. A , B >. ] ) +_r ( ~_r [ <. C , D >. ] ) ) =
                   ( ~_r [ <. ( A +_p C ) , ( B +_p D ) >. ] ) ) $=
      ( vz vu vw vv addsrpr.va addsrpr.vd addsrpr.vb addsrpr.vc vg addsrpr.vs
      vh addsrpr.vt vx vy vf cv cpp co wceq cer cnp cplr cplpr cop cnr opex
