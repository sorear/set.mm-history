  ${
    $d x y z w v u f $.

    $( Define pre-addition on signed reals.  From Proposition 9-4.1
       of Gleason, p. 126. $)
    df-plpr $a |- +_pr = { <. <. x , y >. , z >. | ( ( x e. ( N_p X. N_p ) /\
         y e. ( N_p X. N_p ) ) /\ E. w E. v E. u E. f ( ( x = <. w , v >. /\
         y = <. u , f >. ) /\ z = <. ( w +_p u ) , ( v +_p f ) >. ) ) } $.

    $( Define pre-multiplication on signed reals.  From Proposition 9-4.1
       of Gleason, p. 126. $)
    df-mpr $a |- ._pr = { <. <. x , y >. , z >. | ( ( x e. ( N_p X. N_p ) /\
         y e. ( N_p X. N_p ) ) /\ E. w E. v E. u E. f ( ( x = <. w , v >. /\
         y = <. u , f >. ) /\ z = <. ( ( w ._p u ) +_p ( v ._p f ) ) ,
           ( ( w ._p f ) +_p ( v ._p u ) ) >. ) ) } $.

    $( Define equivalence relation for signed reals.  From Proposition
       9-4.1 of Gleason, p. 126. $)
    df-enr $a |- ~_r = { <. x , y >. | ( ( x e. ( N_p X. N_p ) /\
                   y e. ( N_p X. N_p ) ) /\ E. z E. w E. v E. u
                   ( ( x = <. z , w >. /\ y = <. v , u >. ) /\
                   ( z +_p u ) = ( w +_p v ) ) ) } $.

    $( Define class of signed reals.  From 9-4.2 of Gleason, p. 126. $)
    df-nr $a |- N_r = ( ( N_p X. N_p ) /. ~_r ) $.

    $( Define addition on signed reals.   From Proposition 9-4.3
       of Gleason, p. 126. $)
    df-plr $a |- +_r = { <. <. x , y >. , z >. | ( ( x e. N_r /\ y e. N_r ) /\
      E. w E. v E. u E. f (
         ( x = ( ~_r [ <. w , v >. ] ) /\ y = ( ~_r [ <. u , f >. ] ) ) /\
               z = ( ~_r [ ( <. w , v >. +_pr <. u , f >. ) ] ) ) ) } $.

    $( Define multiplication on signed reals.   From Proposition 9-4.3
       of Gleason, p. 126. $)
    df-mr $a |- ._r = { <. <. x , y >. , z >. | ( ( x e. N_r /\ y e. N_r ) /\
      E. w E. v E. u E. f (
         ( x = ( ~_r [ <. w , v >. ] ) /\ y = ( ~_r [ <. u , f >. ] ) ) /\
               z = ( ~_r [ ( <. w , v >. ._pr <. u , f >. ) ] ) ) ) } $.

    $( Define ordering relation on signed reals.  From Proposition 9-4.4 of
       Gleason, p. 127. $)
    df-ltr $a |- <_r = { <. x , y >. | ( ( x e. N_r /\ y e. N_r ) /\
       E. z E. w E. v E. u
       ( ( x = ( ~_r [ <. z , w >. ] ) /\ y = ( ~_r [ <. v , u >. ] ) ) /\
                ( z +_p u ) <_p ( w +_p v ) ) ) } $.

    $( Define signed real constant 0.  From 9-4.2 of Gleason, p. 126. $)
    df-0r $a |- 0_r = ( ~_r [ <. 1_p , 1_p >. ] ) $.

    $( Define signed real constant 1.  From 9-4.2 of Gleason, p. 126. $)
    df-1r $a |- 1_r = ( ~_r [ <. ( 1_p +_p 1_p ) , 1_p >. ] ) $.

    $( Define signed real constant -1. $)
    df-m1r $a |- -1_r = ( ~_r [ <. 1_p , ( 1_p +_p 1_p ) >. ] ) $.

  $}

  ${
    $d x y z w v u A $.  $d x y z w v u B $.  $d x y z w v u C $.
    $d x y z w v u D $.  $( ` x y z w v u ` are dummy. $)
    $( Equivalence relation for signed reals in terms of positive reals. $)
    enrbreq $p |- ( ( ( A e. N_p /\ B e. N_p ) /\
                     ( C e. N_p /\ D e. N_p ) ) ->
          ( <. A , B >. ~_r <. C , D >. <-> ( A +_p D ) = ( B +_p C ) ) ) $=
      ( vx vy vz vw vv vu cer cnp cpp df-enr ecopopreq ) EFGHIJABCDKLMEFGHIJNO
      $.
      $([3-Sep-95]$)
  $}

