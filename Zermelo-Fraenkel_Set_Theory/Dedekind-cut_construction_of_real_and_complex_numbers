      ( c0r cop cr wcel cnr wceq csn cxp wa df-r cleq4i 0r elisseti opelxp
      elsnc bilan 3bitr cleqid mpbiranr ) ABCZDEZAFEZBBGZUBUAFBHZIZEUCBUEEZJUCU
      DJDUFUAKLABFUEBFMNZOUGUDUCBBUHPQRBST $.
      $([22-Feb-96]$)

