  $( No positive integer is less than one. $)
  nlt1pi $p |- -. A <N 1o $=
    ( cnpi wcel c1o clti wbr wn c0 wceq com elni pm3.27bd wa noel wo wb 1pi
    ltpiord mpan2 csuc elsucg df-1o eleq2i syl5bb bitrd biimpa ord mpi exp
