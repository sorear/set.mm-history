  ${
    $d x y z w v u A $.  $( ` w v u ` are dummy. $)
    $( A non-empty, bounded set of signed reals has a supremum. $)
    supsr $p |- ( ( ( A (_ N_r /\ -. A = (/) ) /\ E. x ( x e. N_r /\
                     A. y ( y e. N_r -> ( y e. A -> y <_r x ) ) ) ) ->
       E. x ( x e. N_r /\ A. y ( y e. N_r -> ( ( y e. A -> -. x <_r y ) /\
