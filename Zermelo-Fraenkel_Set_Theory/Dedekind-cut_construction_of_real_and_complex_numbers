$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
         Dedekind-cut construction of real and complex numbers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Introduce new constant symbols. $)

  $c N. $. $( Set of positive integers $)

  $c +N $. $( Positive integer addition $)
  $c .N $. $( Positive integer multiplication $)
  $c <N $. $( Positive integer ordering relation $)

  $c +pQ $. $( Positive fraction pre-addition $)
  $c .pQ $. $( Positive fraction pre-multiplication $)

  $c ~Q $. $( Equivalence relation to construct positive fractions $)
  $c Q. $. $( Set of positive fractions $)
  $c 1Q $. $( Positive fraction constant 1 $)
  $c +Q $. $( Positive fraction addition $)
  $c .Q $. $( Positive fraction multiplication $)
  $c *Q $. $( Positive fraction reciprocal $)
  $c <Q $. $( Positive fraction ordering relation $)

  $c P. $. $( Set of positive reals $)
  $c 1P $. $( Positive real constant 1 $)
  $c +P. $. $( Positive real addition $)
  $c .P $. $( Positive real multiplication $)
  $c <P $. $( Positive real ordering relation $)

  $c +pR $. $( Signed real pre-addition $)
  $c .pR $. $( Signed real pre-multiplication $)

  $c ~R $. $( Equivalence relation to construct signed reals $)
  $c R. $. $( Set of signed reals $)
  $c 0R $. $( Signed real constant 0 $)
  $c 1R $. $( Signed real constant 1 $)
  $c -1R $. $( Signed real constant minus 1 $)
  $c +R $. $( Signed real addition $)
  $c .R $. $( Signed real multiplication $)
  $c <R $. $( Signed real ordering relation $)

  $c CC $. $( The set of complex numbers (blackboard bold C) $)
  $c RR $. $( The set of real numbers (blackboard bold R) $)
  $c 0 $. $( The real number 0 base 10. $)
  $c 1 $. $( The real number 1 base 10. $)
  $c i $. $( The constant i (square root of -1) $)
  $c + $. $( Plus (addition) symbol $)
  $c x. $. $( Multiplication symbol (center dot) $)
  $c < $. $( 'Less than' relation $)

  $( Extend class notation with new sets (constants) used for positive
     integers: $)

  $( Set of positive integers. $)
  cnpi $a class N. $.
  $( Positive integer addition. $)
  cpli $a class +N $.
  $( Positive integer multiplication. $)
  cmi $a class .N $.
  $( Positive integer ordering relation. $)
  clti $a class <N $.

  $( Extend class notation with new sets (constants) used in construction of
    positive fractions: $)

  $( Positive fraction pre-addition. $)
  cplpq $a class +pQ $.
  $( Positive fraction pre-multiplication. $)
  cmpq $a class .pQ $.
  $( Equivalence class used to construct positive fractions. $)
  ceq $a class ~Q $.
  $( Set of positive fractions. $)
  cnq $a class Q. $.
  $( The positive fraction constant 1. $)
  c1q $a class 1Q $.
  $( Positive fraction addition. $)
  cplq $a class +Q $.
  $( Positive fraction multiplication. $)
  cmq $a class .Q $.
  $( Positive fraction reciprocal operation. $)
  crq $a class *Q $.
  $( Positive fraction ordering relation. $)
  cltq $a class <Q $.

  $( Extend class notation with new sets (constants) used in construction of
    positive real numbers: $)

  $( Set of positive reals. $)
  cnp $a class P. $.
  $( Positive real constant 1. $)
  c1p $a class 1P $.
  $( Positive real addition. $)
  cpp $a class +P. $.
  $( Positive real multiplication. $)
  cmp $a class .P $.
  $( Positive real ordering relation. $)
  cltp $a class <P $.

  $( Extend class notation with new sets (constants) used in construction of
    signed real numbers: $)

  $( Signed real pre-addition. $)
  cplpr $a class +pR $.
  $( Signed real pre-multiplication. $)
  cmpr $a class .pR $.
  $( Equivalence class used to construct signed reals. $)
  cer $a class ~R $.
  $( Set of signed reals. $)
  cnr $a class R. $.
  $( The signed real constant 0. $)
  c0r $a class 0R $.
  $( The signed real constant 1. $)
  c1r $a class 1R $.
  $( The signed real constant -1. $)
  cm1r $a class -1R $.
  $( Signed real addition. $)
  cplr $a class +R $.
  $( Signed real multiplication. $)
  cmr $a class .R $.
  $( Signed real ordering relation. $)
  cltr $a class <R $.

  $( Define the class of positive integers.  This is a "temporary" set used
     in the construction of complex numbers ~ df-c , and is intended to be
     used only by the construction. $)
  df-ni $a |- N. = ( om \ { (/) } ) $.

  $( Define addition on positive integers.  This is a "temporary" set
     used in the construction of complex numbers ~ df-c , and is intended to
     be used only by the construction. $)
  df-pli $a |- +N = ( +o |` ( N. X. N. ) ) $.

  $( Define multiplication on positive integers. This is a "temporary" set
     used in the construction of complex numbers ~ df-c , and is intended to
     be used only by the construction. $)
  df-mi $a |- .N = ( .o |` ( N. X. N. ) ) $.

  $( Define 'less than' on positive integers. This is a "temporary" set
     used in the construction of complex numbers ~ df-c , and is intended to
     be used only by the construction. $)
  df-lti $a |- <N = ( E i^i ( N. X. N. ) ) $.

  $( Membership in the class of positive integers. $)
  elni $p |- ( A e. N. <-> ( A e. om /\ -. A = (/) ) ) $=
    ( cnpi wcel com c0 csn cdif wn wa wceq df-ni eleq2i eldif 0ex elsnc2 bineg
    bilan 3bitr ) ABCADEFZGZCADCZASCZHZIUAAEJZHZIBTAKLADSMUCUEUAUBUDAENOPQR $.
    $( [15-Aug-95] $)

  $( Membership in the class of positive integers. $)
  elni2 $p |- ( A e. N. <-> ( A e. om /\ (/) e. A ) ) $=
    ( cnpi wcel com c0 wceq wn wa elni word wb nnord ord0eln0 syl pm5.32i
    bitr4 ) ABCADCZAEFGZHQEACZHAIQSRQAJSRKALAMNOP $.
    $( [27-Nov-95] $)

  $( A positive integer is a natural number. $)
  pinn $p |- ( A e. N. -> A e. om ) $=
    ( cnpi com c0 csn cdif df-ni difss eqsstr sseli ) BCABCDEZFCGCKHIJ $.
    $( [15-Aug-95] $)

  $( A positive integer is an ordinal number. $)
  pion $p |- ( A e. N. -> A e. On ) $=
    ( cnpi wcel com con0 pinn nnont syl ) ABCADCAECAFAGH $.
    $( [23-Mar-96] $)

  $( A positive integer is ordinal. $)
  piord $p |- ( A e. N. -> Ord A ) $=
    ( cnpi wcel com word pinn nnord syl ) ABCADCAEAFAGH $.
    $( [29-Jan-96] $)

  $( The class of positive integers is a set. $)
  niex $p |- N. e. V $=
    ( cnpi com omex c0 csn cdif df-ni difss eqsstr ssexi ) ABCABDEZFBGBKHIJ $.
    $( [15-Aug-95] $)

  $( The empty set is not a positive integer. $)
  0npi $p |- -. (/) e. N. $=
    ( c0 cnpi wcel wceq cleqid com wn elni pm3.27bd mt2 ) ABCZAADZAEKAFCLGAHIJ
    $.
    $( [26-Aug-95] $)

  $( Ordinal 'one' is a positive integer. $)
  1pi $p |- 1o e. N. $=
    ( c1o cnpi wcel com c0 wceq wn wa 1onn csuc nsuceq0 df-1o cleq1i mtbir
    pm3.2i elni mpbir ) ABCADCZAEFZGZHRTISEJZEFEKAUAELMNOAPQ $.
    $( [29-Oct-95] $)

  $( Positive integer addition in terms of ordinal addition. $)
  addpiord $p |- ( ( A e. N. /\ B e. N. ) -> ( A +N B ) = ( A +o B ) ) $=
    ( cnpi wcel wa cop cxp cpli co coa wceq opelxpi cres cfv fvres df-opr
    df-pli fveq1i eqtr 3eqtr4g syl ) ACDBCDEABFZCCGZDZABHIZABJIZKABCCLUDUBJUCMZ
    NZUBJNUEUFUBUCJOUEUBHNUHABHPUBHUGQRSABJPTUA $.
    $( [27-Aug-95] $)

  $( Positive integer multiplication in terms of ordinal multiplication. $)
  mulpiord $p |- ( ( A e. N. /\ B e. N. ) -> ( A .N B ) = ( A .o B ) ) $=
    ( cnpi wcel wa cop cxp cmi co comu wceq opelxpi cres cfv fvres df-opr
    df-mi fveq1i eqtr 3eqtr4g syl ) ACDBCDEABFZCCGZDZABHIZABJIZKABCCLUDUBJUCMZN
    ZUBJNUEUFUBUCJOUEUBHNUHABHPUBHUGQRSABJPTUA $.
    $( [27-Aug-95] $)

  $( 1 is an identity element for multiplication on positive integers. $)
  mulidpi $p |- ( A e. N. -> ( A .N 1o ) = A ) $=
    ( cnpi wcel c1o cmi co comu wceq 1pi mulpiord mpan2 com con0 pinn nnont
    om1 3syl eqtrd ) ABCZADEFZADGFZASDBCTUAHIADJKSALCAMCUAAHANAOAPQR $.
    $( [4-Mar-96] $)

  ${
    $d x y A $.  $d x y B $.  $( ` x y ` are dummy. $)
    $( Positive integer 'less than' in terms of ordinal membership. $)
    ltpiord $p |- ( ( A e. N. /\ B e. N. ) -> ( A <N B <-> A e. B ) ) $=
      ( vx vy cv clti wbr wel wb wcel cnpi wceq breq1 eleq1 bi2bid breq2 eleq2
      wa cop cxp visset opelxp cep iba df-br epel bitr3 syl5bbr cin df-lti
      eleq2i elin 3bitr syl6rbbr sylbir vtocl2ga ) CEZDEZFGZCDHZIZAURFGZAURJZIA
      BFGZABJZICDABKKUQALUSVBUTVCUQAURFMUQAURNOURBLVBVDVCVEURBAFPURBAQOUQKJURKJ
      RUQURSZKKTZJZVAUQURKKDUAUBVHUTVFUCJZVHRZUSVHVIVJUTVHVIUDVIUQURUCGUTUQURUC
      UECDUFUGUHUSVFFJVFUCVGUIZJVJUQURFUEFVKVFUJUKVFUCVGULUMUNUOUP $.
      $( [6-Feb-96] $)
  $}

  ${
    $d x y z $.  $( ` x y z ` are dummy. $)
    $( Positive integer 'less than' is a strict ordering. $)
    ltsopi $p |- <N Or N. $=
      ( vx vy vz cnpi clti cv wcel w3a wbr weq wo wn wb wa wi wel word ordtri2
      piord syl2an ltpiord ancoms bilord binegd 3bitr4d 3adant3 con0 3simp3
      pion ontr 3syl 3adant1 bi2and 3adant2 3imtr4d jca so ) ABCDEAFZDGZBFZDGZC
      FZDGZHZURUTEIZABJZUTUREIZKZLZMZVEUTVBEIZNZURVBEIZOUSVAVJVCUSVANZABPZVFBAP
      ZKZLZVEVIURQUTQVOVRMUSVAURUTRURSUTSTURUTUAZVNVHVQVNVGVPVFVAUSVGVPMUTURUAU
      BUCUDUEUFVDVOBCPZNZACPZVLVMVDVCVBUGGWAWBOUSVAVCUHVBUIURUTVBUJUKVDVEVOVKVT
      USVAVEVOMVCVSUFVAVCVKVTMUSUTVBUAULUMUSVCVMWBMVAURVBUAUNUOUPUQ $.
      $( [8-Feb-96] $)
  $}

  $( Positive integer 'less than' is a relation on postive integers. $)
  ltrelpi $p |- <N (_ ( N. X. N. ) $=
    ( clti cep cnpi cxp cin df-lti inss2 eqsstr ) ABCCDZEIFBIGH $.
    $( [8-Feb-96] $)

  $( Domain of addition on positive integers. $)
  dmaddpi $p |- dom +N = ( N. X. N. ) $=
    ( coa cnpi cxp cres cdm con0 cin cpli dmres wfn wceq fnoa fndm ax-mp
    ineq2i eqtr df-pli dmeqi wss wa com c0 csn cdif df-ni difss eqsstr omsson
    sstri anidm mpbir ssxp dfss mpbi 3eqtr4 ) ABBCZDZEZUPFFCZGZHEUPURUPAEZGUTAU
    PIVAUSUPAUSJVAUSKLUSAMNOPHUQQRUPUSSZUPUTKBFSZVCTZVBVDVCBUAFBUAUBUCZUDUAUEUA
    VEUFUGUHUIVCUJUKBFBFULNUPUSUMUNUO $.
    $( [26-Aug-95] $)

  $( Domain of multiplication on positive integers. $)
  dmmulpi $p |- dom .N = ( N. X. N. ) $=
    ( comu cnpi cxp cres cdm con0 cin cmi dmres wfn wceq fnom fndm ax-mp
    ineq2i eqtr df-mi dmeqi wss wa com c0 csn cdif df-ni difss eqsstr omsson
    sstri anidm mpbir ssxp dfss mpbi 3eqtr4 ) ABBCZDZEZUPFFCZGZHEUPURUPAEZGUTAU
    PIVAUSUPAUSJVAUSKLUSAMNOPHUQQRUPUSSZUPUTKBFSZVCTZVBVDVCBUAFBUAUBUCZUDUAUEUA
    VEUFUGUHUIVCUJUKBFBFULNUPUSUMUNUO $.
    $( [26-Aug-95] $)

  $( Closure of addition of positive integers. $)
  addclpi $p |- ( ( A e. N. /\ B e. N. ) -> ( A +N B ) e. N. ) $=
    ( cnpi wcel wa cpli co coa addpiord com c0 wceq wn nnacl pinn sylan5 wi
    nnaordi noel2 syl6 exp com12 imp32 elni2 sylan5b jca elni sylibr sylan
    eqeltrd ) ACDZBCDZEABFGABHGZCABIAJDZULUMCDZUKUNULEZUMJDZUMKLMZEUOUPUQURUNBJ
    DZUQULABNBOPUNUSKBDZEURULUNUSUTURUSUNUTURQZUSUNVAUSUNEUTAKHGZUMDURKBARUMVBS
    TUAUBUCBUDUEUFUMUGUHAOUIUJ $.
    $( [18-Oct-95] $)

  $( Closure of multiplication of positive integers. $)
  mulclpi $p |- ( ( A e. N. /\ B e. N. ) -> ( A .N B ) e. N. ) $=
    ( cnpi wcel wa cmi co comu mulpiord com c0 wceq wn nnmcl pinn syl2an wi
    peano1 nnmordi mp3an1 imp an4s elni2 syl2anb ancoms noel2 syl jca elni
    sylibr eqeltrd ) ACDZBCDZEZABFGABHGZCABIUNUOJDZUOKLMZEUOCDUNUPUQAJDZBJDZUPU
    LUMABNAOBOPUNAKHGZUODZUQUMULVAUSKBDZEURKADZEVAUMULUSURVBVCVAUSUREVBVCEZVAKJ
    DUSURVDVAQRKBASTUAUBBUCAUCUDUEUOUTUFUGUHUOUIUJUK $.
    $( [18-Oct-95] $)

  ${
    addcompi.1 $e |- A e. V $.
    addcompi.2 $e |- B e. V $.
    $( Addition of positive integers is commutative. $)
    addcompi $p |- ( A +N B ) = ( B +N A ) $=
      ( cnpi wcel wa cpli co wceq coa com nnacom pinn syl2an addpiord ancoms
      3eqtr4d dmaddpi ndmoprcom pm2.61i ) AEFZBEFZGZABHIZBAHIZJUDABKIZBAKIZUEUF
      ALFBLFUGUHJUBUCABMANBNOABPUCUBUFUHJBAPQRABEHDSCTUA $.
      $( [27-Aug-95] $)
  $}

  ${
    addasspi.1 $e |- B e. V $.
    addasspi.2 $e |- C e. V $.
    $( Addition of positive integers is associative. $)
    addasspi $p |- ( ( A +N B ) +N C ) = ( A +N ( B +N C ) ) $=
      ( cnpi wcel w3a cpli co wceq coa com nnaass pinn syl3an wa addpiord
      addclpi sylan opreq1d adantr eqtrd 3impa sylan5 opreq2d adantl 3impb
      3eqtr4d dmaddpi 0npi ndmoprass pm2.61i ) AFGZBFGZCFGZHZABIJZCIJZABCIJZIJZ
      KUQABLJZCLJZABCLJZLJZUSVAAMGBMGCMGVCVEKUNABCNUOUPAOBOCOPUNUOUPUSVCKUNUOQZ
      UPQUSURCLJZVCURFGUPUSVGKVFURCRABSTVFVGVCKUPVFURVBCLABRUAUBUCUDUNUOUPVAVEK
      UNUOUPQZQVAAUTLJZVEUNUTFGVAVIKVHAUTRBCSUEVHVIVEKUNVHUTVDALBCRUFUGUCUHUIAB
      CFIDUJEUKULUM $.
      $( [27-Aug-95] $)
  $}

  ${
    mulcompi.1 $e |- A e. V $.
    mulcompi.2 $e |- B e. V $.
    $( Multiplication of positive integers is commutative. $)
    mulcompi $p |- ( A .N B ) = ( B .N A ) $=
      ( cnpi wcel wa cmi co wceq comu com nnmcom pinn syl2an mulpiord ancoms
      3eqtr4d dmmulpi ndmoprcom pm2.61i ) AEFZBEFZGZABHIZBAHIZJUDABKIZBAKIZUEUF
      ALFBLFUGUHJUBUCABMANBNOABPUCUBUFUHJBAPQRABEHDSCTUA $.
      $( [21-Sep-95] $)
  $}

  ${
    mulasspi.1 $e |- B e. V $.
    mulasspi.2 $e |- C e. V $.
    $( Multiplication of positive integers is associative. $)
    mulasspi $p |- ( ( A .N B ) .N C ) = ( A .N ( B .N C ) ) $=
      ( cnpi wcel w3a cmi co wceq comu com nnmass pinn syl3an wa mulpiord
      mulclpi sylan opreq1d adantr eqtrd 3impa sylan5 opreq2d adantl 3impb
      3eqtr4d dmmulpi 0npi ndmoprass pm2.61i ) AFGZBFGZCFGZHZABIJZCIJZABCIJZIJZ
      KUQABLJZCLJZABCLJZLJZUSVAAMGBMGCMGVCVEKUNABCNUOUPAOBOCOPUNUOUPUSVCKUNUOQZ
      UPQUSURCLJZVCURFGUPUSVGKVFURCRABSTVFVGVCKUPVFURVBCLABRUAUBUCUDUNUOUPVAVEK
      UNUOUPQZQVAAUTLJZVEUNUTFGVAVIKVHAUTRBCSUEVHVIVEKUNVHUTVDALBCRUFUGUCUHUIAB
      CFIDUJEUKULUM $.
      $( [21-Sep-95] $)
  $}

  ${
    distrpi.1 $e |- B e. V $.
    distrpi.2 $e |- C e. V $.
    $( Multiplication of positive integers is distributive. $)
    distrpi $p |- ( A .N ( B +N C ) ) = ( ( A .N B ) +N ( A .N C ) ) $=
      ( cnpi wcel w3a cpli co cmi wceq coa comu com nndi pinn syl3an wa
      mulpiord addclpi sylan5 addpiord opreq2d adantl eqtrd 3impb mulclpi
      syl2an opreqan12d 3impdi 3eqtr4d dmaddpi 0npi dmmulpi ndmoprdistr
      pm2.61i ) AFGZBFGZCFGZHZABCIJZKJZABKJZACKJZIJZLVAABCMJZNJZABNJZACNJZMJZVC
      VFAOGBOGCOGVHVKLURABCPUSUTAQBQCQRURUSUTVCVHLURUSUTSZSVCAVBNJZVHURVBFGVCVM
      LVLAVBTBCUAUBVLVMVHLURVLVBVGANBCUCUDUEUFUGURUSUTVFVKLURUSSZURUTSZSVFVDVEM
      JZVKVDFGVEFGVFVPLVNVOVDVEUCABUHACUHUIVNVOVDVIVEVJMABTACTUJUFUKULABCFIKDUM
      EUNUOUPUQ $.
      $( [21-Sep-95] $)
  $}

  ${
    mulcanpi.1 $e |- C e. V $.
    $( Multiplication cancellation law for positive integers. $)
    mulcanpi $p |- ( ( A e. N. /\ B e. N. ) ->
                   ( ( A .N B ) = ( A .N C ) -> B = C ) ) $=
      ( cnpi wcel wa cmi co wceq wi comu mulpiord adantr adantlr cleq12d com
      w3a c0 nnmcan biimpd elni2 pm3.27bd sylan5 exp pinn syl3an 3exp com4r
      pm2.43i imp31 sylbid eleq1 mulclpi syl5bi imp dmmulpi 0npi ndmoprrcl
      pm3.27 3syl exp32 imp4b ) AEFZBEFZGZABHIZACHIZJZBCJZVFVIGVJVFVIVFVIVJVFVI
      VFVIVJKZVFCEFZVKVIVFGZVFVLGZVIABLIZACLIZJZVJVNVGVOVHVPVFVGVOJVLABMNVDVLVH
      VPJVEACMOPVDVEVLVQVJKZVDVEVLVRKKVDVEVLVDVRVDVEVLVDVRKZAQFZBQFZCQFZVSVDVTW
      AWBRZVDVRWCSAFZVRVDWCWDGVQVJABCTUAVDVTWDAUBUCUDUEVEVLAUFBUFCUFUGUHUIUJUKU
      LVMVHEFZVDVLGVLVIVFWEVIVGEFWEVFVGVHEUMABUNUOUPACEHDUQURUSVDVLUTVAUDVBVCUJ
      UE $.
      $( [4-Feb-96] $)
  $}

  ${
    addnidpi.1 $e |- B e. V $.
    $( There is no identity element for addition on positive integers. $)
    addnidpi $p |- ( A e. N. -> -. ( A +N B ) = A ) $=
      ( cnpi wcel wa cpli co wceq wn wi coa com c0 nnaordi nna0 eleq1d eleq2
      binegd word nnord ordeirr syl syl5bir com12 con2d sylbid adantl syld exp
      imp32 elni2 sylan5b pinn sylan addpiord cleq1d mtbird a1d dmaddpi ndmopr
      0npi eleq1 mtbii syl6bi pm2.61i ) ADEZBDEZFZVGABGHZAIZJZKVIVLVGVIVKABLHZA
      IZAMEZVHVNJZVGVOBMEZNBEZFVPVHVOVQVRVPVQVOVRVPKZVQVOVSVQVOFVRANLHZVMEZVPNB
      AOVOWAVPKVQVOWAAVMEZVPVOVTAVMAPQVOVNWBVNVOWBJZVNWCAAEZJZVOVNWBWDVMAARSVOA
      TWEAUAAUBUCUDUEUFUGUHUIUJUEUKBULUMAUNUOVIVJVMAABUPUQURUSVIJZVKVGWFVKNAIZV
      GJWFVJNAABDGCUTVAUQWGNDEVGVBNADVCVDVEUFVF $.
      $( [28-Nov-95] $)
  $}

  ${
    $d x A $.  $d x B $.
    $( Ordering on positive integers in terms of existence of sum. $)
    ltexpi $p |- ( ( A e. N. /\ B e. N. ) ->
               ( A <N B <-> E. x ( x e. N. /\ ( A +N x ) = B ) ) ) $=
      ( cnpi wcel wa cv com c0 coa co wceq wex clti wbr cpli wb wrex nnordex
      df-rex syl6bb pinn syl2an ltpiord addpiord cleq1d exp pm5.32d elni2
      biran anass bitr biexdv adantr 3bitr4d ) BDEZCDEZFBCEZAGZHEZIUSEZBUSJKZCL
      ZFZFZAMZBCNOUSDEZBUSPKZCLZFZAMZBHEZCHEZURVFQUPUQVLVMFURVDAHRVFABCSVDAHTUA
      BUBCUBUCBCUDUPVKVFQUQUPVJVEAUPVJVGVCFZVEUPVGVIVCUPVGVIVCQUPVGFVHVBCBUSUEU
      FUGUHVNUTVAFZVCFVEVGVOVCUSUIUJUTVAVCUKULUAUMUNUO $.
      $( [15-Mar-96] $)
  $}

  ${
    ltapi.1 $e |- A e. V $.
    ltapi.2 $e |- B e. V $.
    $( Ordering property of multiplication for positive integers. $)
    ltapi $p |- ( C e. N. -> ( A <N B <-> ( C +N A ) <N ( C +N B ) ) ) $=
      ( clti cnpi cpli dmaddpi ltrelpi 0npi wcel wbr co wb wa coa com nnaord
      pinn syl3an 3expa ltpiord adantr addclpi syl2an wceq addpiord adantl
      eleq12d bitrd anandis ancoms 3bitr4d 3impa ndmord ) ABCFGHEIDJKAGLZBGLZCG
      LZABFMZCAHNZCBHNZFMZOUQURPZUSPABLZCAQNZCBQNZLZUTVCUQURUSVEVHOZARLBRLCRLVI
      UQABCSURUSATBTCTUAUBVDUTVEOUSABUCUDUSVDVCVHOZUSUQURVJUSUQPZUSURPZPZVCVAVB
      LZVHVAGLVBGLVCVNOVKVLVAVBUCCAUECBUEUFVMVAVFVBVGVKVAVFUGVLCAUHUDVLVBVGUGVK
      CBUHUIUJUKULUMUNUOUP $.
      $( [7-Mar-96] $)
  $}

  ${
    ltmpi.1 $e |- A e. V $.
    ltmpi.2 $e |- B e. V $.
    $( Ordering property of multiplication for positive integers. $)
    ltmpi $p |- ( C e. N. -> ( A <N B <-> ( C .N A ) <N ( C .N B ) ) ) $=
      ( clti cnpi cmi dmmulpi ltrelpi 0npi wcel wbr co wb wa comu com wi c0
      w3a iba nnmord sylan9bbr exp 3exp imp4b elni2 syl5ib pinn syl2an imp
