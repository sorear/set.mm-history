$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
         Dedekind-cut construction of real and complex numbers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Introduce new constant symbols. $)

  $c N_i $. $( Set of positive integers $)

  $c +_i $. $( Positive integer addition $)
  $c ._i $. $( Positive integer multiplication $)
  $c <_i $. $( Positive integer ordering relation $)

  $c +_pq $. $( Positive fraction pre-addition $)
  $c ._pq $. $( Positive fraction pre-multiplication $)

  $c ~_q $. $( Equivalence relation to construct positive fractions $)
  $c N_q $. $( Set of positive fractions $)
  $c 1_q $. $( Positive fraction constant ` 1 ` $)
  $c +_q $. $( Positive fraction addition $)
  $c ._q $. $( Positive fraction multiplication $)
  $c *_q $. $( Positive fraction reciprocal $)
  $c <_q $. $( Positive fraction ordering relation $)

  $c N_p $. $( Set of positive reals $)
  $c 1_p $. $( Positive real constant ` 1 ` $)
  $c +_p $. $( Positive real addition $)
  $c ._p $. $( Positive real multiplication $)
  $c <_p $. $( Positive real ordering relation $)

  $c +_pr $. $( Signed real pre-addition $)
  $c ._pr $. $( Signed real pre-multiplication $)

  $c ~_r $. $( Equivalence relation to construct signed reals $)
  $c N_r $. $( Set of signed reals $)
  $c 0_r $. $( Signed real constant ` 0 ` $)
  $c 1_r $. $( Signed real constant ` 1 ` $)
  $c -1_r $. $( Signed real constant ` -1 ` $)
  $c +_r $. $( Signed real addition $)
  $c ._r $. $( Signed real multiplication $)
  $c <_r $. $( Signed real ordering relation $)

  $c CC $. $( The set of complex numbers (blackboard bold C) $)
  $c RR $. $( The set of real numbers (blackboard bold R) $)
  $c _10 $. $( Subscript 10 - indicates end of a base-10 digit string $)
  $c 0 $. $( The digit 0.  Note:  The number 0 is ` 0 _10 `. $)
  $c 1 $. $( The digit 1.  Note:  The number 1 is ` 1 _10 `. $)
  $c i $. $( The constant i (square root of -1) $)
  $c + $. $( Plus (addition) symbol $)
  $c x. $. $( Multiplication symbol (center dot) $)
  $c < $. $( 'Less than' relation $)

  $( Extend class notation with new sets (constants) used for positive
     integers: $)

  $( Set of positive integers. $)
  cnpi $a class N_i $.
  $( Positive integer addition. $)
  cpli $a class +_i $.
  $( Positive integer multiplication. $)
  cmi $a class ._i $.
  $( Positive integer ordering relation. $)
  clti $a class <_i $.

  $( Extend class notation with new sets (constants) used in construction of
    positive fractions: $)

  $( Positive fraction pre-addition. $)
  cplpq $a class +_pq $.
  $( Positive fraction pre-multiplication. $)
  cmpq $a class ._pq $.
  $( Equivalence class used to construct positive fractions. $)
  ceq $a class ~_q $.
  $( Set of positive fractions. $)
  cnq $a class N_q $.
  $( The positive fraction constant 1. $)
  c1q $a class 1_q $.
  $( Positive fraction addition. $)
  cplq $a class +_q $.
  $( Positive fraction multiplication. $)
  cmq $a class ._q $.
  $( Positive fraction division. $)
  crq $a class *_q $.
  $( Positive fraction ordering relation. $)
  cltq $a class <_q $.

  $( Extend class notation with new sets (constants) used in construction of
    positive real numbers: $)

  $( Set of positive reals. $)
  cnp $a class N_p $.
  $( Positive real constant ` 1 `. $)
  c1p $a class 1_p $.
  $( Positive real addition. $)
  cpp $a class +_p $.
  $( Positive real multiplication. $)
  cmp $a class ._p $.
  $( Positive real ordering relation. $)
  cltp $a class <_p $.

  $( Extend class notation with new sets (constants) used in construction of
    signed real numbers: $)

  $( Signed real pre-addition. $)
  cplpr $a class +_pr $.
  $( Signed real pre-multiplication. $)
  cmpr $a class ._pr $.
  $( Equivalence class used to construct signed reals. $)
  cer $a class ~_r $.
  $( Set of signed reals. $)
  cnr $a class N_r $.
  $( The signed real constant 0. $)
  c0r $a class 0_r $.
  $( The signed real constant 1. $)
  c1r $a class 1_r $.
  $( The signed real constant -1. $)
  cm1r $a class -1_r $.
  $( Signed real addition. $)
  cplr $a class +_r $.
  $( Signed real multiplication. $)
  cmr $a class ._r $.
  $( Signed real ordering relation. $)
  cltr $a class <_r $.

  $( Define the class of positive integers. $)
  df-ni $a |- N_i = ( om \ { (/) } ) $.

  $( Define addition on positive integers. $)
  df-pli $a |- +_i = ( +_o |` ( N_i X. N_i ) ) $.

  $( Define multiplication on positive integers. $)
  df-mi $a |- ._i = ( ._o |` ( N_i X. N_i ) ) $.

  $( Define 'less than' on positive integers. $)
  df-lti $a |- <_i = ( E i^i ( N_i X. N_i ) ) $.

  $( Membership in the class of positive integers. $)
  elni $p |- ( A e. N_i <-> ( A e. om /\ -. A = (/) ) ) $=
    ( cnpi wcel com c0 csn cdif wn wa wceq df-ni eleq2i eldif zfnull elsnc2
    bineg bilan 3bitr ) ABCADEFZGZCADCZASCZHZIUAAEJZHZIBTAKLADSMUCUEUAUBUDAENOP
    QR $.
    $([15-Aug-95]$)

  $( Membership in the class of positive integers. $)
  elni2 $p |- ( A e. N_i <-> ( A e. om /\ (/) e. A ) ) $=
    ( cnpi wcel com c0 wceq wn wa elni word wb nnord ord0eln0 syl pm5.32i
    bitr4 ) ABCADCZAEFGZHQEACZHAIQSRQAJSRKALAMNOP $.
    $([27-Nov-95]$)

  $( A positive integer is a natural number. $)
  pinn $p |- ( A e. N_i -> A e. om ) $=
    ( cnpi com c0 csn cdif df-ni difss eqsstr ssi ) BCABCDEZFCGCKHIJ $.
    $([15-Aug-95]$)

  $( A positive integer is an ordinal number. $)
  pion $p |- ( A e. N_i -> A e. On ) $=
    ( cnpi wcel com con pinn nnont syl ) ABCADCAECAFAGH $.
    $([23-Mar-96]$)

  $( A positive integer is ordinal. $)
  piord $p |- ( A e. N_i -> Ord A ) $=
    ( cnpi wcel com word pinn nnord syl ) ABCADCAEAFAGH $.
    $([29-Jan-96]$)

  $( The class of positive integers is a set. $)
  niex $p |- N_i e. V $=
    ( cnpi com omex c0 csn cdif df-ni difss eqsstr ssexi ) ABCABDEZFBGBKHIJ $.
    $([15-Aug-95]$)

  $( The empty set is not a positive integer. $)
  0npi $p |- -. (/) e. N_i $=
    ( c0 cnpi wcel wceq com wn wa elni biimp pm3.27d cleqid mt2 ) ABCZAADZMAECZ
    NFZMOPGAHIJAKL $.
    $([26-Aug-95]$)

  $( Ordinal 'one' is a positive integer. $)
  1pi $p |- 1_o e. N_i $=
    ( c1o cnpi wcel com c0 wceq wn wa elni 1nn csuc df-1o cleq1i nsuceq0 mtbir
    pm3.2i mpbir ) ABCADCZAEFZGZHAIRTJSEKZEFAUAELMENOPQ $.
    $([29-Oct-95]$)

  $( Positive integer addition in terms of ordinal addition. $)
  addpiord $p |- ( ( A e. N_i /\ B e. N_i ) -> ( A +_i B ) = ( A +_o B ) ) $=
    ( cnpi wcel wa coa co cpli cop cxp wceq opelxpi cfv cres fvres df-opr
    df-pli fveq1 ax-mp eqtr 3eqtr4g syl cleqcomd ) ACDBCDEZABFGZABHGZUDABIZCCJZ
    DZUEUFKABCCLUIUGFMUGFUHNZMZUEUFUGUHFOABFPUFUGHMZUKABHPHUJKULUKKQUGHUJRSTUAU
    BUC $.
    $([27-Aug-95]$)

  $( Positive integer multiplication in terms of ordinal multiplication. $)
  mulpiord $p |- ( ( A e. N_i /\ B e. N_i ) -> ( A ._i B ) = ( A ._o B ) ) $=
    ( cnpi wcel wa comu co cmi cop cxp wceq opelxpi cfv cres fvres df-opr
    df-mi fveq1 ax-mp eqtr 3eqtr4g syl cleqcomd ) ACDBCDEZABFGZABHGZUDABIZCCJZD
    ZUEUFKABCCLUIUGFMUGFUHNZMZUEUFUGUHFOABFPUFUGHMZUKABHPHUJKULUKKQUGHUJRSTUAUB
    UC $.
    $([27-Aug-95]$)

  $( 1 is an identity element for multiplication on positive integers. $)
  mulidpi $p |- ( A e. N_i -> ( A ._i 1_o ) = A ) $=
    ( cnpi wcel c1o cmi co comu wceq mulpiord 1pi mpan2 com con pinn nnont 1id
    3syl eqtrd ) ABCZADEFZADGFZASDBCTUAHADIJKSALCAMCUAAHANAOAPQR $.
    $([4-Mar-96]$)

  ${
    $d x y A $.  $d x y B $.  $( ` x y ` are dummy. $)
    $( Positive integer 'less than' in terms of ordinal membership. $)
    ltpiord $p |- ( ( A e. N_i /\ B e. N_i ) -> ( A <_i B <-> A e. B ) ) $=
      ( vx vy cv clti wbr wel wb wcel cnpi wceq breq1 eleq1 bi2bid breq2 eleq2
      wa cop cxp visset opelxp cep iba df-br epel bitr3 syl5bbr cin df-lti
      eleq2i elin 3bitr syl6rbbr sylbir vtocl2ga ) CEZDEZFGZCDHZIZAURFGZAURJZIA
      BFGZABJZICDABKKUQALUSVBUTVCUQAURFMUQAURNOURBLVBVDVCVEURBAFPURBAQOUQKJURKJ
      RUQURSZKKTZJZVAUQURKKDUAUBVHUTVFUCJZVHRZUSVHVIVJUTVHVIUDVIUQURUCGUTUQURUC
      UECDUFUGUHUSVFFJVFUCVGUIZJVJUQURFUEFVKVFUJUKVFUCVGULUMUNUOUP $.
      $([6-Feb-96]$)
  $}

  ${
    $d x y z $.  $( ` x y z ` are dummy. $)
    $( Positive integer 'less than' is a strict ordering. $)
    ltsopi $p |- <_i Or N_i $=
      ( vx vy vz cnpi clti cv wcel w3a wbr weq wo wn wb wa wi wel word ordtri
      piord syl2an ltpiord ancoms bilord binegd 3bitr4d 3adant3 con 3simp3
      pion ontr1t 3syl 3adant1 bi2and 3adant2 3imtr4d jca so ) ABCDEAFZDGZBFZDG
      ZCFZDGZHZURUTEIZABJZUTUREIZKZLZMZVEUTVBEIZNZURVBEIZOUSVAVJVCUSVANZABPZVFB
      APZKZLZVEVIURQUTQVOVRMUSVAURUTRURSUTSTURUTUAZVNVHVQVNVGVPVFVAUSVGVPMUTURU
      AUBUCUDUEUFVDVOBCPZNZACPZVLVMVDVCVBUGGWAWBOUSVAVCUHVBUIVBURUTUJUKVDVEVOVK
      VTUSVAVEVOMVCVSUFVAVCVKVTMUSUTVBUAULUMUSVCVMWBMVAURVBUAUNUOUPUQ $.
      $([8-Feb-96]$)
  $}

  $( Positive integer 'less than' is a relation on postive integers. $)
  ltrelpi $p |- <_i (_ ( N_i X. N_i ) $=
    ( clti cep cnpi cxp cin df-lti inss2 eqsstr ) ABCCDZEIFBIGH $.
    $([8-Feb-96]$)

  $( Domain of addition on positive integers. $)
  dmaddpi $p |- dom +_i = ( N_i X. N_i ) $=
    ( coa cnpi cxp cres cdm con cin cpli dmres wfn wceq fnoa fndm ax-mp ineq2i
    eqtr df-pli dmeq wss sseqin1 wa anabs com sstr c0 csn cdif df-ni difss
    eqsstr omsson mp2 mpbi ssxp 3eqtr4 ) ABBCZDZEZUPFFCZGZHEZUPURUPAEZGUTAUPIVB
    USUPAUSJVBUSKLUSAMNOPHUQKVAURKQHUQRNUPUSSZUPUTKUPUSTBFSZVDUAZVCVDVEVDUBBUCS
    UCFSVDBUCFUDBUCUEUFZUGUCUHUCVFUIUJUKULUMBFBFUNNUMUO $.
    $([26-Aug-95]$)

  $( Domain of multiplication on positive integers. $)
  dmmulpi $p |- dom ._i = ( N_i X. N_i ) $=
    ( comu cnpi cxp cres cdm con cin cmi dmres wfn wceq fnom fndm ax-mp ineq2i
    eqtr df-mi dmeq wss sseqin1 wa anabs com sstr c0 csn cdif df-ni difss
    eqsstr omsson mp2 mpbi ssxp 3eqtr4 ) ABBCZDZEZUPFFCZGZHEZUPURUPAEZGUTAUPIVB
    USUPAUSJVBUSKLUSAMNOPHUQKVAURKQHUQRNUPUSSZUPUTKUPUSTBFSZVDUAZVCVDVEVDUBBUCS
    UCFSVDBUCFUDBUCUEUFZUGUCUHUCVFUIUJUKULUMBFBFUNNUMUO $.
    $([26-Aug-95]$)

  $( Closure of addition of positive integers. $)
  addclpi $p |- ( ( A e. N_i /\ B e. N_i ) -> ( A +_i B ) e. N_i ) $=
    ( cnpi wcel wa cpli co coa addpiord eleq1d com c0 wceq wn nnacl pinn
    sylan5 wi nnaordi peano1 mp3an1 noel2 syl6 exp comm imp32 elni2 sylan5b
    jca elni sylibr sylan mpbird ) ACDZBCDZEZABFGZCDABHGZCDZUPUQURCABIJAKDZUOUS
    UNUTUOEZURKDZURLMNZEUSVAVBVCUTBKDZVBUOABOBPQUTVDLBDZEVCUOUTVDVEVCVDUTVEVCRZ
    VDUTVFVDUTEVEALHGZURDZVCLKDVDUTVEVHRLBASTUAURVGUBUCUDUEUFBUGUHUIURUJUKAPULU
    M $.
    $([18-Oct-95]$)

  $( Closure of multiplication of positive integers. $)
  mulclpi $p |- ( ( A e. N_i /\ B e. N_i ) -> ( A ._i B ) e. N_i ) $=
    ( cnpi wcel wa cmi co comu mulpiord eleq1d com c0 wceq wn nnmcl pinn
    syl2an wi nnmordi peano1 mp3an1 imp an4s elni2 syl2anb ancoms noel2 syl
    jca elni sylibr mpbird ) ACDZBCDZEZABFGZCDABHGZCDZUOUPUQCABIJUOUQKDZUQLMNZE
    URUOUSUTAKDZBKDZUSUMUNABOAPBPQUOALHGZUQDZUTUNUMVDVBLBDZEVALADZEVDUNUMVBVAVE
    VFVDVBVAEVEVFEZVDLKDVBVAVGVDRLBASTUAUBUCBUDAUDUEUFUQVCUGUHUIUQUJUKUL $.
    $([18-Oct-95]$)

  ${
    addcompi.1 $e |- A e. V $.
    addcompi.2 $e |- B e. V $.
    $( Addition of positive integers is commutative. $)
    addcompi $p |- ( A +_i B ) = ( B +_i A ) $=
      ( cnpi wcel wa cpli co wceq coa com nnacom pinn syl2an addpiord ancoms
      3eqtr4d dmaddpi ndmoprcom pm2.61i ) AEFZBEFZGZABHIZBAHIZJUDABKIZBAKIZUEUF
      ALFBLFUGUHJUBUCABMANBNOABPUCUBUFUHJBAPQRABEHDSCTUA $.
      $([27-Aug-95]$)
  $}

  ${
    addasspi.1 $e |- B e. V $.
    addasspi.2 $e |- C e. V $.
    $( Addition of positive integers is associative. $)
    addasspi $p |- ( ( A +_i B ) +_i C ) = ( A +_i ( B +_i C ) ) $=
      ( cnpi wcel w3a cpli co wceq coa com pinn im3an nnaass syl wa addpiord
      addclpi sylan opreq1d adantr eqtrd 3impa sylan5 opreq2d adantl 3impb
      3eqtr4d dmaddpi 0npi ndmoprass pm2.61i ) AFGZBFGZCFGZHZABIJZCIJZABCIJZIJZ
      KURABLJZCLJZABCLJZLJZUTVBURAMGZBMGZCMGZHVDVFKUOVGUPVHUQVIANBNCNOABCPQUOUP
      UQUTVDKUOUPRZUQRUTUSCLJZVDUSFGUQUTVKKVJUSCSABTUAVJVKVDKUQVJUSVCCLABSUBUCU
      DUEUOUPUQVBVFKUOUPUQRZRVBAVALJZVFUOVAFGVBVMKVLAVASBCTUFVLVMVFKUOVLVAVEALB
      CSUGUHUDUIUJABCFIDUKEULUMUN $.
      $([27-Aug-95]$)
  $}

  ${
    mulcompi.1 $e |- A e. V $.
    mulcompi.2 $e |- B e. V $.
    $( Multiplication of positive integers is commutative. $)
    mulcompi $p |- ( A ._i B ) = ( B ._i A ) $=
      ( cnpi wcel wa cmi co wceq comu com nnmcom pinn syl2an mulpiord ancoms
      3eqtr4d dmmulpi ndmoprcom pm2.61i ) AEFZBEFZGZABHIZBAHIZJUDABKIZBAKIZUEUF
      ALFBLFUGUHJUBUCABMANBNOABPUCUBUFUHJBAPQRABEHDSCTUA $.
      $([21-Sep-95]$)
  $}

  ${
    mulasspi.1 $e |- B e. V $.
    mulasspi.2 $e |- C e. V $.
    $( Multiplication of positive integers is associative. $)
    mulasspi $p |- ( ( A ._i B ) ._i C ) = ( A ._i ( B ._i C ) ) $=
      ( cnpi wcel w3a cmi co wceq comu com pinn im3an nnmass syl wa mulpiord
      mulclpi sylan opreq1d adantr eqtrd 3impa sylan5 opreq2d adantl 3impb
      3eqtr4d dmmulpi 0npi ndmoprass pm2.61i ) AFGZBFGZCFGZHZABIJZCIJZABCIJZIJZ
      KURABLJZCLJZABCLJZLJZUTVBURAMGZBMGZCMGZHVDVFKUOVGUPVHUQVIANBNCNOABCPQUOUP
      UQUTVDKUOUPRZUQRUTUSCLJZVDUSFGUQUTVKKVJUSCSABTUAVJVKVDKUQVJUSVCCLABSUBUCU
      DUEUOUPUQVBVFKUOUPUQRZRVBAVALJZVFUOVAFGVBVMKVLAVASBCTUFVLVMVFKUOVLVAVEALB
      CSUGUHUDUIUJABCFIDUKEULUMUN $.
      $([21-Sep-95]$)
  $}

  ${
    distrpi.1 $e |- B e. V $.
    distrpi.2 $e |- C e. V $.
    $( Multiplication of positive integers is distributive. $)
    distrpi $p |- ( A ._i ( B +_i C ) ) = ( ( A ._i B ) +_i ( A ._i C ) ) $=
      ( cnpi wcel w3a cpli co cmi wceq coa comu com pinn im3an nndist syl wa
      mulpiord addclpi sylan5 addpiord opreq2d adantl eqtrd 3impb mulclpi
      syl2an opreqan12d 3impdi 3eqtr4d dmaddpi 0npi dmmulpi ndmoprdistr
      pm2.61i ) AFGZBFGZCFGZHZABCIJZKJZABKJZACKJZIJZLVBABCMJZNJZABNJZACNJZMJZVD
      VGVBAOGZBOGZCOGZHVIVLLUSVMUTVNVAVOAPBPCPQABCRSUSUTVAVDVILUSUTVATZTVDAVCNJ
      ZVIUSVCFGVDVQLVPAVCUABCUBUCVPVQVILUSVPVCVHANBCUDUEUFUGUHUSUTVAVGVLLUSUTTZ
      USVATZTVGVEVFMJZVLVEFGVFFGVGVTLVRVSVEVFUDABUIACUIUJVRVSVEVJVFVKMABUAACUAU
      KUGULUMABCFIKDUNEUOUPUQUR $.
      $([21-Sep-95]$)
  $}

  ${
    mulcanpi.1 $e |- C e. V $.
    $( Multiplication cancellation law for positive integers. $)
    mulcanpi $p |- ( ( A e. N_i /\ B e. N_i ) ->
                   ( ( A ._i B ) = ( A ._i C ) -> B = C ) ) $=
      ( cnpi wcel wa cmi co wceq wi comu mulpiord adantr adantalr cleq12d w3a
      com pinn im3an c0 nnmcan elni2 pm3.27bd sylan5 exp syl 3exp com4r
      pm2.43i imp31 sylbid eleq1 mulclpi syl5bi imp dmmulpi 0npi ndmoprrcl
      pm3.27 3syl exp32 imp4b ) AEFZBEFZGZABHIZACHIZJZBCJZVFVIGVJVFVIVFVIVJVFVI
      VFVIVJKZVFCEFZVKVIVFGZVFVLGZVIABLIZACLIZJZVJVNVGVOVHVPVFVGVOJVLABMNVDVLVH
      VPJVEACMOPVDVEVLVQVJKZVDVEVLVRKKVDVEVLVDVRVDVEVLVDVRKZVDVEVLQARFZBRFZCRFZ
      QZVSVDVTVEWAVLWBASBSCSTWCVDVRWCUAAFZVRVDABCUBVDVTWDAUCUDUEUFUGUHUIUJUKULV
      MVHEFZVDVLGVLVIVFWEVIVGEFWEVFVGVHEUMABUNUOUPACEHDUQURUSVDVLUTVAUEVBVCUJUF
      $.
      $([4-Feb-96]$)
  $}

  ${
    addnidpi.1 $e |- B e. V $.
    $( There is no identity element for addition on positive integers. $)
    addnidpi $p |- ( A e. N_i -> -. ( A +_i B ) = A ) $=
      ( cnpi wcel wa cpli co wceq wn wi coa addpiord cleq1d com c0 nnaordi
      peano1 mp3an1 nna0 eleq1d eleq2 binegd word nnord ordeirr syl syl5bir
      comm pm2.03d sylbid adantl syld exp imp32 elni2 sylan5b pinn sylan
      mtbird a1d dmaddpi ndmopr eleq1 0npi mtbii syl6bi pm2.61i ) ADEZBDEZFZVIA
      BGHZAIZJZKVKVNVIVKVMABLHZAIZVKVLVOAABMNAOEZVJVPJZVIVQBOEZPBEZFVRVJVQVSVTV
      RVSVQVTVRKZVSVQWAVSVQFVTAPLHZVOEZVRPOEVSVQVTWCKPBAQRSVQWCVRKVSVQWCAVOEZVR
      VQWBAVOATUAVQVPWDVPVQWDJZVPWEAAEZJZVQVPWDWFVOAAUBUCVQAUDWGAUEAUFUGUHUIUJU
      KULUMUNUIUOBUPUQAURUSUTVAVKJZVMVIWHVMPAIZVIJWHVLPAABDGCVBVCNWIPDEVIPADVDV
      EVFVGUJVH $.
      $([28-Nov-95]$)
  $}

  ${
    $d x A $.  $d x B $.
    $( Ordering on positive integers in terms of existence of sum. $)
    ltexpi $p |- ( ( A e. N_i /\ B e. N_i ) ->
               ( A <_i B <-> E. x ( x e. N_i /\ ( A +_i x ) = B ) ) ) $=
      ( cnpi wcel wa cv com c0 coa co wceq wex clti wbr cpli wb nnordex pinn
      syl2an ltpiord addpiord cleq1d exp pm5.32d elni2 biran syl6bb biexdv
      adantr 3bitr4d ) BDEZCDEZFBCEZAGZHEIUOEFZBUOJKZCLZFZAMZBCNOUODEZBUOPKZCLZ
      FZAMZBHECHEUNUTQULUMABCRBSCSTBCUAULVEUTQUMULVDUSAULVDVAURFUSULVAVCURULVAV
      CURQULVAFVBUQCBUOUBUCUDUEVAUPURUOUFUGUHUIUJUK $.
      $([15-Mar-96]$)
  $}

  ${
    ltapi.1 $e |- A e. V $.
    ltapi.2 $e |- B e. V $.
    $( Ordering property of multiplication for positive integers. $)
    ltapi $p |- ( C e. N_i -> ( A <_i B <-> ( C +_i A ) <_i ( C +_i B ) ) ) $=
      ( clti cnpi cpli dmaddpi ltrelpi 0npi wcel wbr co wb wa coa w3a com pinn
      im3an nnaord syl 3expa ltpiord adantr anandi addclpi syl2an wceq
      addpiord adantl eleq12d bitrd sylbi ancoms 3bitr4d 3impa ndmord ) ABCFGHE
      IDJKAGLZBGLZCGLZABFMZCAHNZCBHNZFMZOUTVAPZVBPABLZCAQNZCBQNZLZVCVFUTVAVBVHV
      KOZUTVAVBRASLZBSLZCSLZRVLUTVMVAVNVBVOATBTCTUAABCUBUCUDVGVCVHOVBABUEUFVBVG
      VFVKOZVBVGPVBUTPZVBVAPZPZVPVBUTVAUGVSVFVDVELZVKVDGLVEGLVFVTOVQVRVDVEUECAU
      HCBUHUIVSVDVIVEVJVQVDVIUJVRCAUKUFVRVEVJUJVQCBUKULUMUNUOUPUQURUS $.
      $([7-Mar-96]$)
  $}

  ${
    ltmpi.1 $e |- A e. V $.
    ltmpi.2 $e |- B e. V $.
    $( Ordering property of multiplication for positive integers. $)
    ltmpi $p |- ( C e. N_i -> ( A <_i B <-> ( C ._i A ) <_i ( C ._i B ) ) ) $=
      ( clti cnpi cmi dmmulpi ltrelpi 0npi wcel wbr co wb wa comu com wi c0
      w3a iba nnmord sylan9bbr exp 3exp imp4b elni2 syl5ib pinn syl2an imp
      ltpiord adantr anandi mulclpi wceq mulpiord adantl eleq12d bitrd sylbi
      ancoms 3bitr4d 3impa ndmord ) ABCFGHEIDJKAGLZBGLZCGLZABFMZCAHNZCBHNZFMZOV
      GVHPZVIPABLZCAQNZCBQNZLZVJVMVNVIVOVROZARLZBRLZVIVSSVGVHVTWAPCRLZTCLZPVSVI
      VTWAWBWCVSVTWAWBWCVSSVTWAWBUAZWCVSWCVOVOWCPWDVRWCVOUBABCUCUDUEUFUGCUHUIAU
      JBUJUKULVNVJVOOVIABUMUNVIVNVMVROZVIVNPVIVGPZVIVHPZPZWEVIVGVHUOWHVMVKVLLZV
      RVKGLVLGLVMWIOWFWGVKVLUMCAUPCBUPUKWHVKVPVLVQWFVKVPUQWGCAURUNWGVLVQUQWFCBU
      RUSUTVAVBVCVDVEVF $.
      $([8-Feb-96]$)
  $}

  $( One is less than two (one plus one). $)
  1lt2pi $p |- 1_o <_i ( 1_o +_i 1_o ) $=
    ( c1o cpli co clti wbr wcel cnpi wb ltpiord 1pi addclpi mp2an coa c0 com
    wceq 1nn nna0 ax-mp nnaord peano1 mp3an csuc zfnull sucid df-1o eleqtrr
    mpbi eqeltrr addpiord mpbir ) AAABCZDEZAULFZAGFZULGFZUMUNHAULIJUOUOUPAAKJJL
    LAAAMCZULANMCZAUQAOFZURAPQARSNAFZURUQFZNOFUSUSUTVAHNAATUAQQUBNNUCANUDUEUFUG
    UHUIUOUOULUQPAAUJJJLUGUK $.
    $([13-Mar-96]$)

  $( No positive integer is less than one. $)
  nlt1pi $p |- -. A <_i 1_o $=
    ( cnpi wcel c1o clti wbr wn c0 wceq wa wo wb ltpiord 1pi mpan2 csuc elsucg
    df-1o eleq2i syl5bb bitrd biimpa ord noel mpi exp com elni pm3.27bd mtod
    elisseti ltrelpi ndmbr pm3.26d con3 pm2.61i ) ABCZADEFZGUQURAHIZUQURUSUQURJ
    ZAHCZGUSUTVAUSUQURVAUSKZUQURADCZVBUQDBCZURVCLADMNOUQAHPZCVBVCAHBQDVEARSTUAU
    BUCAUDUEUFUQAUGCUSGAUHUIUJURUQURUQVDADBBEDBNUKULUMUNUOUP $.
    $([23-Mar-96]$)

  ${
    $d x y $.  $d x A $.  $d x ps $.  $d x ch $.  $d x th $.  $d x ta $.
    $d y ph $.
    $( Substitutions. $)
    indpi.1 $e |- ( x = 1_o -> ( ph <-> ps ) ) $.
    indpi.2 $e |- ( x = y -> ( ph <-> ch ) ) $.
    indpi.3 $e |- ( x = ( y +_i 1_o ) -> ( ph <-> th ) ) $.
    indpi.4 $e |- ( x = A -> ( ph <-> ta ) ) $.
    $( Basis. $)
    indpi.5 $e |- ps $.
    $( Induction hypothesis. $)
    indpi.6 $e |- ( y e. N_i -> ( ch -> th ) ) $.
    $( Principle of Finite Induction on positive integers. $)
    indpi $p |- ( A e. N_i -> ta ) $=
      ( cnpi wcel c1o wceq clti wbr wo wi cv 1pi elisseti eqvinc mpbiri gencl
      cleqcoms a1i com wss wa pinn c0 elni2 csuc word nnord ordsucssi syl
      df-1o sseq1i syl6ibr imp sylbi jca eleq1 breq2 bi2and bi2imd weq wn
      peano2b syl6bbr syl5bi adantrd eleq2 elsuci noel2 zfnull sucid eleqtrr
      mpbii jaoi syl6bi wb ltpiord mpan biimpa syl5 jcad elni pm3.27 cpli co
      coa addpiord mpan2 con pion oa1suc eqtrd cleq2d biimparc eleq1d addclpi
      syl5bir exp pm2.43d biimprd im2and impbid birimd syl6bir adantr comm
      pm5.74d bitrd pm3.2 pm5.32i syl4d jao mpi visset sucex ltrelpi ndmbr
      biimpd pm2.43i syl6 imp3a sylbir cvv onsucssir ax-mp sylan5 syl9r
      adantalr findsg 1nn mpan12 exp3a jaod wor sotric ltsopi nlt1pi mtbii
      nega mpd ) HOPZHQRZQHSTZUAZEUVLUVMEUVNUVMEUBUVLEQHAEFUCZQRZQHRFUVPHFQHQOU
      DUEZUFLUVQABIMUGZUHUIUJUVLUVNEUBUVLUVLUVNEUVLHUKPZQHULZUMUVLUVNUMZEUBZUVL
      UVTUWAHUNUVLUVTUOHPZUMUWAHUPUVTUWDUWAUVTUWDUOUQZHULZUWAUVTHURUWDUWFUBHUSU
