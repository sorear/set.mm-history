  $(
  Digit strings will usually be unambigous without whitespace (for better
  readability) although we will separate digits, at least initially in our
  development, to emphasize that they are separate Metamath tokens.
  $)

  $( Class of complex numbers. $)
  cc $a class CC $.
  $( Class of real numbers. $)
  cr $a class RR $.
  $( Extend class notation to include the complex number 0.  Note that
     the subscript ` _10 `, meaning base 10, indicates the end of a digit
     string to eliminate ambiguity. $)
  cn0 $a class 0 _10 $.
  $( Extend class notation to include the complex number 1. $)
  cn1 $a class 1 _10 $.
  $( Extend class notation to include the complex number i. $)
  cni $a class i $.
  $( Addition on complex numbers. $)
  caddc $a class + $.
  $( Multiplication on complex numbers.  The token ` x. ` is a center dot. $)
  cmulc $a class x. $.
  $( Less than predicate (defined over real subset of complex numbers). $)
  clt $a class < $.

  $( Define the class of complex numbers. $)
  df-c $a |- CC = ( N_r X. N_r ) $.

  $( Define the complex number 0 (base 10). $)
  df-0 $a |- 0 _10 = <. 0_r , 0_r >. $.

  $( Define the complex number 1 (base 10). $)
  df-1 $a |- 1 _10 = <. 1_r , 0_r >. $.

  $( Define the complex (imaginary) number i. $)
  df-i $a |- i = <. 0_r , 1_r >. $.

  ${
    $d x y $.
    $( Define the class of real numbers. $)
    df-r $a |- RR = ( N_r X. { 0_r } ) $.
  $}

  ${
    $d x y z w v u f $.

    $( Define addition over complex numbers. $)
    df-plus $a |- + = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. CC ) /\
                  E. w E. v E. u E. f ( ( x = <. w , v >. /\ y = <. u , f >. )
                  /\ z = <. ( w +_r u ) , ( v +_r f ) >. ) ) } $.

    $( Define multiplication over complex numbers. $)
    df-mul $a |- x. = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. CC ) /\
               E. w E. v E. u E. f ( ( x = <. w , v >. /\ y = <. u , f >. )
                  /\ z = <. ( ( w ._r u ) +_r ( -1_r ._r ( v ._r f ) ) ) ,
                  ( ( v ._r u ) +_r ( w ._r f ) ) >. ) ) } $.

    $( Define "less than" on real subset of compex numbers. $)
    df-lt $a |- < = { <. x , y >. | ( ( x e. RR /\ y e. RR ) /\
     E. z E. w ( ( x = <. z , 0_r >. /\ y = <. w , 0_r >. ) /\ z <_r w ) ) } $.

  $}

  ${
    opelcn.1 $e |- B e. V $.
    $( Ordered pair membership in class of complex numbers. $)
    opelcn $p |- ( <. A , B >. e. CC <-> ( A e. N_r /\ B e. N_r ) ) $=
      ( cop cc wcel cnr cxp wa df-c eleq2i opelxp bitr ) ABDZEFNGGHZFAGFBGFIEON
      JKABGGCLM $.
      $([14-May-96]$)
  $}

  $( Ordered pair membership in class of real subset of complex numbers. $)
  opelreal $p |- ( <. A , 0_r >. e. RR <-> A e. N_r ) $=
    ( c0r cop cr wcel cnr wceq csn cxp wa df-r eleq2i 0r elisseti opelxp elsnc
    bilan 3bitr cleqid mpbiranr ) ABCZDEZAFEZBBGZUBUAFBHZIZEUCBUEEZJUCUDJDUFUAK
    LABFUEBFMNZOUGUDUCBBUHPQRBST $.
    $([22-Feb-96]$)

  ${
    $d x y A $. $( ` y ` is dummy. $)
    $( Membership in class of real numbers. $)
    elreal $p |- ( A e. RR <-> E. x ( x e. N_r /\ <. x , 0_r >. = A ) ) $=
      ( vy cr wcel cnr c0r csn cxp cv cop wceq wa wex df-r eleq2i elxp ancom
      anass elsn cleqcom bi2an opeq2 cleq1d pm5.32i bitr bilan an1 3bitr biex
      19.41v 0r elisseti isseti mpbiran ) BDEBFGHZIZEBAJZCJZKZLZURFEZUSUPEZMZMZ
      CNZANVBURGKZBLZMZANDUQBOPACBFUPQVFVIAVFUSGLZCNZVIVFVJVIMZCNVKVIMVEVLCVEVD
      VAMZVLVAVDRVMVBVCVAMZMVBVJVHMZMVLVBVCVASVNVOVBVNVJUTBLZMVOVCVJVAVPCGTBUTU
      AUBVJVPVHVJUTVGBUSGURUCUDUEUFUGVBVJVHUHUIUFUJVJVICUKUFCGGFULUMUNUOUJUI $.
      $([31-Mar-96]$)
  $}

  $( The empty set is not a complex number. $)
  0ncn $p |- -. (/) e. CC $=
    ( c0 cc wcel cnr cxp df-c eleq2i 0nelxp mtbir ) ABCADDEZCBJAFGDDHI $.
    $([2-May-96]$)

