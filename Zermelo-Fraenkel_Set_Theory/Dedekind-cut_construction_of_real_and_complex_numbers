    $( The union of a non-empty, bounded set of positive reals has a supremum.
       Part of Proposition 9-3.3 of Gleason, p. 122. $)
    suppr $p |- ( ( ( A (_ N_p /\ -. A = (/) ) /\ E. x ( x e. N_p /\
                     A. y ( y e. N_p -> ( y e. A -> y <_p x ) ) ) ) ->
       E. x ( x e. N_p /\ A. y ( y e. N_p -> ( ( y e. A -> -. x <_p y ) /\
           ( y <_p x -> E. z ( z e. N_p /\ ( z e. A /\ y <_p z ) ) ) ) ) ) ) $=
