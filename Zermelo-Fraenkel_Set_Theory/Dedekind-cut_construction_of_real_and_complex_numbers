  $( Equivalence class equality of positive fractions in terms of positive
     integers. $)
  enreceq $p |- ( ( ( A e. N_p /\ B e. N_p ) /\ ( C e. N_p /\ D e. N_p ) ) ->
                ( ( ~_r [ <. A , B >. ] ) = ( ~_r [ <. C , D >. ] ) <->
                ( A +_p D ) = ( B +_p C ) ) ) $=
