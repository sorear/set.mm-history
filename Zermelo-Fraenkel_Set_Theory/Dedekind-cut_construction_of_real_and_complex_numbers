    $( Multiplication of positive fractions in terms of positive integers. $)
    mulpipq $p |- ( ( ( A e. N_i /\ B e. N_i ) /\
                     ( C e. N_i /\ D e. N_i ) ) ->
                ( ( ~_q [ <. A , B >. ] ) ._q ( ~_q [ <. C , D >. ] ) ) =
                   ( ~_q [ <. ( A ._i C ) , ( B ._i D ) >. ] ) ) $=
      ( vz vu vw vv pipq.va pipq.vd pipq.vb pipq.vc vg pipq.vs vh pipq.vt vx
      vy vf cv cmi co wceq ceq cnpi cmq cmpq cop cnq opex enqex enqer dmenq
