      $( Lemma for supremum theorem. $)
      supsrlem6 $p |- ( ( C e. A /\
         E. x ( x e. R. /\ A. y ( y e. R. -> ( y e. A -> y <R x ) ) ) ) ->
       E. x ( x e. R. /\ A. y ( y e. R. -> ( ( y e. A -> -. x <R y ) /\
