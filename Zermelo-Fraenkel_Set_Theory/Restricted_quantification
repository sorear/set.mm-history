  ${
    $d y z A $.  $d x z $.  $( ` z ` is dummy. $)
    rgen2.1 $e |- ( ( x e. A /\ y e. A ) -> ph ) $.
    $( Generalization rule for restricted quantification.  Note that ` x `
       and ` y ` needn't be distinct. $)
    rgen2   $p |- A. x e. A A. y e. A ph $=
      ( vz wral cv wcel wi wal weq exp ax-gen eq5 wb eleq1 a4s birimd biald
      mpbiri pm2.43 19.20i ax-1 3syl wn ax-17 ddelim syl6 pm2.61i df-ral
      sylibr rgen ) ACDGZBDBHZDIZCHZDIZAJZCKZUNCBLZCKZUPUTJZVBURUSJZCKZUTVCVBVE
      UPUSJZCKVFCUPURAEMZNVBVDVFCCBCOVBURUPUSVAURUPPCUQUODQRSTUAVDUSCURAUBUCUTU
      PUDUEVBUFUPUPCKUTFHZDIZUPCBFVHUODQVICUGUHUPUSCVGUCUIUJACDUKULUM $.
      $([23-Nov-94]$)
  $}

  $( Distribution of restricted quantification over implication. $)
  r19.20 $p |- ( A. x e. A ( ph -> ps ) -> ( A. x e. A ph -> A. x e. A ps ) ) $=
    ( wi wral cv wcel wal df-ral ax-2 19.20ii sylbi 3imtr4g ) ABEZCDFZCGDHZAEZC
    IZQBEZCIZACDFBCDFPQOEZCISUAEOCDJUBRTCQABKLMACDJBCDJN $.
    $([9-Feb-97]$)

  ${
    r19.20i.1 $e |- ( x e. A -> ( ph -> ps ) ) $.
