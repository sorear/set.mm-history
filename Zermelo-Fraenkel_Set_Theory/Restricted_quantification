  $( Restricted version of Theorem 19.43, p. 90, of Margaris. $)
  r19.43 $p |- ( E. x e. A ( ph \/ ps ) <->
               ( E. x e. A ph \/ E. x e. A ps ) ) $=
    ( cv wcel wo wa wex wrex andi biex 19.43 bitr df-rex bi2or 3bitr4 ) CEDFZAB
    GZHZCIZRAHZCIZRBHZCIZGZSCDJACDJZBCDJZGUAUBUDGZCIUFTUICRABKLUBUDCMNSCDOUGUCU
    HUEACDOBCDOPQ $.
    $([27-May-98]$)

  ${
    $d x A $.  $d x B $.
    $( Two ways to say " ` A ` belongs to ` B ` ". $)
    risset $p |- ( A e. B <-> E. x e. B x = A ) $=
      ( cv wcel wceq wa wex wrex exancom df-rex df-clel 3bitr4r ) ADZCEZNBFZGAH
      POGAHPACIBCEOPAJPACKABCLM $.
      $([22-Nov-94]$)
  $}

  ${
    birald.1 $e |- ( ph -> A. x ph ) $.
    birald.2 $e |- ( ph -> ( ps <-> ch ) ) $.

    $( Formula-building rule for restricted universal quantifier (deduction
       rule). $)
    birald $p |- ( ph -> ( A. x e. A ps <-> A. x e. A ch ) ) $=
      ( cv wcel wi wal wral bilimd biald df-ral 3bitr4g ) ADHEIZBJZDKQCJZDKBDEL
      CDELARSDFABCQGMNBDEOCDEOP $.
      $([27-Jun-98]$)

    $( Formula-building rule for restricted existential quantifier (deduction
       rule). $)
    birexd $p |- ( ph -> ( E. x e. A ps <-> E. x e. A ch ) ) $=
      ( wn wral wrex binegd birald dfrex2 3bitr4g ) ABHZDEIZHCHZDEIZHBDEJCDEJAP
      RAOQDEFABCGKLKBDEMCDEMN $.
      $([27-Jun-98]$)
  $}

  ${
    $d x ph $.
    biraldv.1 $e |- ( ph -> ( ps <-> ch ) ) $.

    $( Formula-building rule for restricted universal quantifier (deduction
