  ${
    bireu.1 $e |- ( ph <-> ps ) $.
    $( Formula-building rule for restricted existential quantifier (inference
       rule). $)
    bireu $p |- ( E! x e. A ph <-> E! x e. A ps ) $=
      ( cv wcel wa weu wreu bilan bieu df-reu 3bitr4 ) CFDGZAHZCIOBHZCIACDJBCDJ
      PQCABOEKLACDMBCDMN $.
      $([22-Oct-99]$)
  $}

  ${
    birabi.1 $e |- ( x e. A -> ( ps <-> ch ) ) $.
    $( Equivalent wff's yield equal restricted abstraction classes (inference
       rule). $)
    birabi $p |- { x e. A | ps } = { x e. A | ch } $=
      ( cv wcel wa cab crab pm5.32i biabi df-rab 3eqtr4 ) CFDGZAHZCIOBHZCIACDJB
      CDJPQCOABEKLACDMBCDMN $.
      $([22-May-99]$)
  $}

  ${
    $d x ph $.
    birabdv.1 $e |- ( ph -> ( x e. A -> ( ps <-> ch ) ) ) $.
    $( Equivalent wff's yield equal restricted abstraction classes (deduction
       rule). $)
    birabdv   $p |- ( ph -> { x e. A | ps } = { x e. A | ch } ) $=
      ( cv wcel wa cab crab pm5.32d biabdv df-rab 3eqtr4g ) ADGEHZBIZDJPCIZDJBD
      EKCDEKAQRDAPBCFLMBDENCDENO $.
      $([30-Nov-03]$)
  $}

  ${
    $d x ph $.
    birabsdv.1 $e |- ( ph -> ( ps <-> ch ) ) $.
    $( Equivalent wff's yield equal restricted abstraction classes (deduction
       rule). $)
    birabsdv   $p |- ( ph -> { x e. A | ps } = { x e. A | ch } ) $=
      ( wb cv wcel a1d birabdv ) ABCDEABCGDHEIFJK $.
      $([10-Feb-95]$)
  $}

  ${
    $d x A $.  $d x B $.
    $( Equality theorem for restricted abstraction classes. $)
    rabeq $p |- ( A = B -> { x e. A | ph } = { x e. B | ph } ) $=
      ( wceq cv wcel wa cab crab eleq2 birand biabdv df-rab 3eqtr4g ) CDEZBFZCG
      ZAHZBIQDGZAHZBIABCJABDJPSUABPRTACDQKLMABCNABDNO $.
      $([16-Oct-03]$)
