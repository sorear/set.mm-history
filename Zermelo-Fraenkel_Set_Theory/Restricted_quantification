  $( Theorem 19.21, p. 90, of Margaris.  (Restricted version.) $)
  r19.22 $p |- ( A. x e. A ( ph -> ps ) ->
               ( E. x e. A ph -> E. x e. A ps ) ) $=
    ( cv wcel wi wal wa wex wral wrex imdistan 19.20i 19.22 syl df-ral df-rex
    bi2im 3imtr4 ) CEDFZABGZGZCHZUAAIZCJZUABIZCJZGZUBCDKACDLZBCDLZGUDUEUGGZCHUI
    UCULCUAABMNUEUGCOPUBCDQUJUFUKUHACDRBCDRST $.
    $([22-Nov-94]$)

  ${
    r19.22ia.1 $e |- ( x e. A -> ( ph -> ps ) ) $.
