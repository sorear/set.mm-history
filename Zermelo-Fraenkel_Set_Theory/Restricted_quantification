$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Restricted quantification
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Extend wff notation to include restricted universal quantification. $)
  wral $a wff A. x e. A ph $.

  $( Extend wff notation to include restricted existential quantificaton. $)
  wrex $a wff E. x e. A ph $.

  $( Extend wff notation to include restricted existential uniqueness. $)
  wreu $a wff E! x e. A ph $.

  $( Extend class notation to include restricted abstraction class. $)
  crab $a class { x e. A | ph } $.

  $( Define restricted universal quantification.  Special case of Definition
     4.15(3) of [TakeutiZaring] p. 22. $)
  df-ral $a |- ( A. x e. A ph <-> A. x ( x e. A -> ph ) ) $.

  $( Define restricted existential quantification.  Special case of Definition
     4.15(4) of [TakeutiZaring] p. 22. $)
  df-rex $a |- ( E. x e. A ph <-> E. x ( x e. A /\ ph ) ) $.

  $( Define restricted existential uniqueness. $)
  df-reu $a |- ( E! x e. A ph <-> E! x ( x e. A /\ ph ) ) $.

  $( Define the restricted abstraction class, which is the class of all ` x `
     in ` A ` such that ` ph ` is true.  Definition of [TakeutiZaring]
     p. 20. $)
  df-rab $a |- { x e. A | ph } = { x | ( x e. A /\ ph ) } $.

  $( Relationship between restricted universal and existential quantifiers. $)
  ralnex $p |- ( A. x e. A -. ph <-> -. E. x e. A ph ) $=
    ( cv wcel wn wi wal wa wex wral wrex alnex df-ral df-rex bineg 3bitr4 ) BDC
    EZAFZGBHRAIBJZFSBCKABCLZFRABMSBCNUATABCOPQ $.
    $([21-Jan-97]$)

  $( Relationship between restricted universal and existential quantifiers. $)
  rexnal $p |- ( E. x e. A -. ph <-> -. A. x e. A ph ) $=
    ( cv wcel wn wa wex wi wal wrex wral exnal df-rex df-ral bineg 3bitr4 ) BDC
    EZAFZGBHRAIBJZFSBCKABCLZFRABMSBCNUATABCOPQ $.
    $([21-Jan-97]$)

  $( Relationship between restricted universal and existential quantifiers. $)
  dfral2 $p |- ( A. x e. A ph <-> -. E. x e. A -. ph ) $=
    ( wn wrex wral rexnal bicon2 ) ADBCEABCFABCGH $.
    $([21-Jan-97]$)

  $( Relationship between restricted universal and existential quantifiers. $)
  dfrex2 $p |- ( E. x e. A ph <-> -. A. x e. A -. ph ) $=
    ( wn wral wrex ralnex bicon2 ) ADBCEABCFABCGH $.
    $([21-Jan-97]$)


  ${
    $d y A $.  $d y B $.  $d x y $.
    rexeqf.1 $e |- ( y e. A -> A. x y e. A ) $.
    rexeqf.2 $e |- ( y e. B -> A. x y e. B ) $.
    $( Equality theorem for restricted existential quantifier, with "not free"
       hypotheses instead of distinct variable restrictions. $)
    rexeqf $p |- ( A = B -> ( E. x e. A ph <-> E. x e. B ph ) ) $=
      ( wceq cv wcel wa wex wrex hbeq eleq2 birand biexd df-rex 3bitr4g ) DEHZB
      IZDJZAKZBLUAEJZAKZBLABDMABEMTUCUEBBCCDEFGNTUBUDADEUAOPQABDRABERS $.
      $([9-Oct-03]$)
  $}

  ${
    $d x A $.  $d x B $.
    $( Equality theorem for restricted universal quantifier. $)
    raleq $p |- ( A = B -> ( A. x e. A ph <-> A. x e. B ph ) ) $=
      ( wceq cv wcel wi wal wral eleq2 birimd bialdv df-ral 3bitr4g ) CDEZBFZCG
      ZAHZBIQDGZAHZBIABCJABDJPSUABPRTACDQKLMABCNABDNO $.
      $([16-Nov-95]$)

    $( Equality theorem for restricted existential quantifier. $)
    rexeq $p |- ( A = B -> ( E. x e. A ph <-> E. x e. B ph ) ) $=
      ( wceq cv wcel wa wex wrex eleq2 birand biexdv df-rex 3bitr4g ) CDEZBFZCG
      ZAHZBIQDGZAHZBIABCJABDJPSUABPRTACDQKLMABCNABDNO $.
      $([29-Oct-95]$)
  $}

  ${
    biralda.1 $e |- ( ph -> A. x ph ) $.
    biralda.2 $e |- ( ( ph /\ x e. A ) -> ( ps <-> ch ) ) $.
    $( Formula-building rule for restricted universal quantifier (deduction
       rule). $)
    biralda   $p |- ( ph -> ( A. x e. A ps <-> A. x e. A ch ) ) $=
      ( cv wcel wi wal wral wb exp pm5.74d biald df-ral 3bitr4g ) ADHEIZBJZDKSC
      JZDKBDELCDELATUADFASBCASBCMGNOPBDEQCDEQR $.
      $([3-Nov-03]$)

    $( Formula-building rule for restricted existential quantifier (deduction
       rule). $)
    birexda   $p |- ( ph -> ( E. x e. A ps <-> E. x e. A ch ) ) $=
      ( cv wcel wa wex wrex wb exp pm5.32d biexd df-rex 3bitr4g ) ADHEIZBJZDKSC
      JZDKBDELCDELATUADFASBCASBCMGNOPBDEQCDEQR $.
      $([3-Nov-03]$)
  $}

  ${
    $d x ph $.
    biraldva.1 $e |- ( ( ph /\ x e. A ) -> ( ps <-> ch ) ) $.
    $( Formula-building rule for restricted universal quantifier (deduction
       rule). $)
    biraldva   $p |- ( ph -> ( A. x e. A ps <-> A. x e. A ch ) ) $=
      ( ax-17 biralda ) ABCDEADGFH $.
      $([4-Mar-97]$)

    $( Formula-building rule for restricted existential quantifier (deduction
       rule). $)
    birexdva   $p |- ( ph -> ( E. x e. A ps <-> E. x e. A ch ) ) $=
      ( ax-17 birexda ) ABCDEADGFH $.
      $([9-Mar-97]$)
  $}

  ${
    birald.1 $e |- ( ph -> A. x ph ) $.
    birald.2 $e |- ( ph -> ( ps <-> ch ) ) $.

    $( Formula-building rule for restricted universal quantifier (deduction
       rule). $)
    birald $p |- ( ph -> ( A. x e. A ps <-> A. x e. A ch ) ) $=
      ( wb cv wcel adantr biralda ) ABCDEFABCHDIEJGKL $.
      $([27-Jun-98]$)

    $( Formula-building rule for restricted existential quantifier (deduction
       rule). $)
    birexd $p |- ( ph -> ( E. x e. A ps <-> E. x e. A ch ) ) $=
      ( wb cv wcel adantr birexda ) ABCDEFABCHDIEJGKL $.
      $([27-Jun-98]$)
  $}

  ${
    $d x ph $.
    biraldv.1 $e |- ( ph -> ( ps <-> ch ) ) $.

    $( Formula-building rule for restricted universal quantifier (deduction
       rule). $)
    biraldv   $p |- ( ph -> ( A. x e. A ps <-> A. x e. A ch ) ) $=
      ( ax-17 birald ) ABCDEADGFH $.
      $([20-Nov-94]$)

    $( Formula-building rule for restricted existential quantifier (deduction
