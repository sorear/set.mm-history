$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Equinumerosity
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)


  $( Introduce new constant symbols. $)
  $c ~~ $. $( Equinumerosity relation ("approximately equals" symbol) $)
  $c ~<_ $. $( Dominance relation (curly less-than-or-equal) $)
  $c ~< $. $( Strict dominance relation (curly less-than) $)

  $( Extend class definition to include equinumerosity relation
     ("approximately equals" symbol) $)
  cen $a class ~~ $.
  $( Extend class definition to include dominance relation (curly
     less-than-or-equal) $)
  cdom $a class ~<_ $.
  $( Extend class definition to include strict dominance relation (curly
     less-than) $)
  csdm $a class ~< $.

  ${
    $d x y f $.  $d x y f $.
    $( Define equinumerosity relation.  Definition of [Enderton]
       p. 129, except we consider ` ~~ ` to be a (proper) class rather
       than a connective.  This is acceptable because equinumerosity is
       meaningful for sets only and not for proper classes.  We derive
       the usual definition as ~ bren . $)
    df-en $a |- ~~ = { <. x , y >. | E. f f : x -1-1-onto-> y } $.

    $( Define dominance relation.  For an alternate definition see
       ~ dfdom2 . Compare Definition of [Enderton] p. 145.  Typical
       textbook definitions are derived as ~ brdom and ~ domen . $)
    df-dom $a |- ~<_ = { <. x , y >. | E. f f : x -1-1-> y } $.

    $( Define strict dominance relation.  Alternate possible definitions are
       derived as ~ brsdom and ~ dfsdom2 .  Definition 3 of [Suppes] p. 97. $)
    df-sdom $a |- ~< = ( ~<_ \ ~~ ) $.

  $}

  ${
    $d x y f $.  $d x y f $.
    $( Equinumerosity is a relation. $)
    relen $p |- Rel ~~ $=
      ( vx vy vf cen wrel cv wf1o wex copab relopab wceq wb df-en releq ax-mp
      mpbir ) DEZAFBFCFGCHZABIZEZRABJDSKQTLABCMDSNOP $.
      $([28-Mar-98]$)

    $( Dominance is a relation. $)
    reldom $p |- Rel ~<_ $=
      ( vx vy vf cdom wrel cv wf1 wex copab relopab wceq wb df-dom releq ax-mp
      mpbir ) DEZAFBFCFGCHZABIZEZRABJDSKQTLABCMDSNOP $.
      $([28-Mar-98]$)
  $}

  $( Strict dominance is a relation. $)
  relsdom $p |- Rel ~< $=
    ( cdom wrel csdm reldom cen cdif reldif wceq wb df-sdom releq ax-mp sylibr
    ) ABZCBZDNAEFZBZOAEGCPHOQIJCPKLML $.
    $([31-Mar-98]$)

  ${
    $d f x y A $.  $d f x y B $.  $d y C $.  $( ` x y ` are dummy. $)
    $( Equinumerosity relation. $)
    breng $p |- ( B e. C -> ( A ~~ B <-> E. f f : A -1-1-onto-> B ) ) $=
      ( vx vy cvv wcel cen wbr cv wf1o wex wb wi wceq f1oeq2 biexdv f1oeq3
      df-en brabg exp wn relen elbrexi wfn f1ofn cdm visset dmexg ax-mp fndm
      eleq1d mpbii syl 19.23aiv pm5.21ni a1d pm2.61i ) AGHZBCHZABIJZABDKZLZDMZN
      ZOUTVAVFEKZFKZVCLZDMAVHVCLZDMVEEFABGCIVGAPVIVJDVGAVHVCQRVHBPVJVDDVHBAVCSR
      EFDTUAUBUTUCVFVAVBUTVEABIUDUEVDUTDVDVCAUFZUTABVCUGVKVCUHZGHZUTVCGHVMDUIVC
      GUJUKVKVLAGAVCULUMUNUOUPUQURUS $.
      $([15-Jun-98]$)

    $( Dominance relation. $)
    brdomg $p |- ( B e. C -> ( A ~<_ B <-> E. f f : A -1-1-> B ) ) $=
      ( vx vy cvv wcel cdom wbr cv wf1 wex wb wi wceq f1eq2 biexdv f1eq3
      df-dom brabg exp wn reldom elbrexi wf f1f cdm visset dmexg ax-mp fdm
      eleq1d mpbii syl 19.23aiv pm5.21ni a1d pm2.61i ) AGHZBCHZABIJZABDKZLZDMZN
      ZOUTVAVFEKZFKZVCLZDMAVHVCLZDMVEEFABGCIVGAPVIVJDVGAVHVCQRVHBPVJVDDVHBAVCSR
      EFDTUAUBUTUCVFVAVBUTVEABIUDUEVDUTDVDABVCUFZUTABVCUGVKVCUHZGHZUTVCGHVMDUIV
      CGUJUKVKVLAGABVCULUMUNUOUPUQURUS $.
      $([15-Jun-98]$)
  $}

  ${
    $d f x y A $.  $d f x y B $.  $( ` y ` is dummy. $)
    bren.1 $e |- B e. V $.

    $( Equinumerosity relation.  Compare Definition of [Enderton] p. 129. $)
    bren $p |- ( A ~~ B <-> E. f f : A -1-1-onto-> B ) $=
      ( cvv wcel cen wbr cv wf1o wex wb breng ax-mp ) BEFABGHABCIJCKLDABECMN $.
      $([15-Jun-98]$)

    $( Dominance relation. $)
    brdom $p |- ( A ~<_ B <-> E. f f : A -1-1-> B ) $=
      ( cvv wcel cdom wbr cv wf1 wex wb brdomg ax-mp ) BEFABGHABCIJCKLDABECMN
      $.
      $([15-Jun-98]$)

    $( Dominance in terms of equinumerosity.  Example 1 of [Enderton] p. 146. $)
    domen $p |- ( A ~<_ B <-> E. x ( A ~~ x /\ x (_ B ) ) $=
      ( vf cv wf1 wex wf1o wss wa cdom wbr cen visset f11o biex 19.11 bitr
      brdom bren biran 19.41v bitr4 3bitr4 ) BCEFZGZEHZBAFZUFIZUICJZKZEHZAHZBCL
      MBUINMZUKKZAHUHULAHZEHUNUGUQEABCUFEOPQULEARSBCEDTUPUMAUPUJEHZUKKUMUOURUKB
      UIEAOUAUBUJUKEUCUDQUE $.
      $([15-Jun-98]$)
  $}

  ${
    $d x y A $.  $d x y B $.
    $( Dominance in terms of equinumerosity, with the sethood requirement
       expressed as an antecedent.  Example 1 of [Enderton] p. 146. $)
    domeng $p |- ( B e. C -> ( A ~<_ B <-> E. x ( A ~~ x /\ x (_ B ) ) ) $=
      ( vy cv cdom wbr cen wss wa wex wb wceq breq2 sseq2 biland biexdv bi2bid
      visset domen vtoclg ) BEFZGHZBAFZIHZUEUCJZKZALZMBCGHZUFUECJZKZALZMECDUCCN
      ZUDUJUIUMUCCBGOUNUHULAUNUGUKUFUCCUEPQRSABUCETUAUB $.
      $([?]$)
  $}

    $( Strict dominance relation, meaning " ` B ` is strictly greater in size
       than ` A ` ."  Definition of [Mendelson] p. 255. $)
    brsdom $p |- ( A ~< B <-> ( A ~<_ B /\ -. A ~~ B ) ) $=
      ( cop csdm wcel cdom cen cdif wbr wn wa df-sdom eleq2i df-br bineg bi2an
      eldif bitr4 3bitr4 ) ABCZDETFGHZEZABDIABFIZABGIZJZKZDUATLMABDNUFTFEZTGEZJ
      ZKUBUCUGUEUIABFNUDUHABGNOPTFGQRS $.
      $([25-Jun-98]$)

  ${
    $d x y f $.  $( ` x y f ` are dummy. $)
    $( Equinumerosity implies dominance. $)
    enssdom $p |- ~~ (_ ~<_ $=
      ( vx vy vf cen cdom relen cv cop wf1o wex copab wcel wf1 f1of1 19.22i
      opabid 3imtr4 df-en eleq2i df-dom relssi ) ABDEFAGZBGZHZUBUCCGZIZCJZABKZL
      ZUDUBUCUEMZCJZABKZLZUDDLUDELUGUKUIUMUFUJCUBUCUENOUGABPUKABPQDUHUDABCRSEUL
      UDABCTSQUA $.
      $([31-Mar-98]$)
  $}

  $( Alternate definition of dominance. $)
  dfdom2 $p |- ~<_ = ( ~< u. ~~ ) $=
    ( cen csdm cun cdom cdif df-sdom uneq2i uncom wss wceq enssdom undif3 mpbi
    3eqtr4r ) ABCADAEZCZBACDBOAFGBAHADIDPJKADLMN $.
    $([17-Jun-98]$)

  $( Equinumerosity implies dominance.  Theorem 15 of [Suppes] p. 94. $)
  endom $p |- ( A ~~ B -> A ~<_ B ) $=
    ( cop cen wcel cdom wbr enssdom ssi df-br 3imtr4 ) ABCZDELFEABDGABFGDFLHIAB
    DJABFJK $.
    $([28-May-98]$)

  $( Strict dominance implies dominance. $)
  sdomdom $p |- ( A ~< B -> A ~<_ B ) $=
    ( csdm wbr cdom cen wn brsdom pm3.26bd ) ABCDABEDABFDGABHI $.
    $([10-Jun-98]$)

  $( Strict dominance implies non-equinumerosity. $)
  sdomnen $p |- ( A ~< B -> -. A ~~ B ) $=
    ( csdm wbr cdom cen wn brsdom pm3.27bd ) ABCDABEDABFDGABHI $.
    $([10-Jun-98]$)

  $( Dominance in terms of strict dominance and equinumerosity.  Theorem 22(iv)
     of [Suppes] p. 97. $)
  brdom2 $p |- ( A ~<_ B <-> ( A ~< B \/ A ~~ B ) ) $=
    ( cop cdom wcel csdm cen cun wbr wo dfdom2 eleq2i df-br bi2or elun bitr4
    3bitr4 ) ABCZDERFGHZEZABDIABFIZABGIZJZDSRKLABDMUCRFEZRGEZJTUAUDUBUEABFMABGM
    NRFGOPQ $.
    $([17-Jun-98]$)

  ${
    $d f A $.  $( ` f ` is dummy. $)
    $( Equinumerosity is reflexive.  Theorem 1 of [Suppes] p. 92. $)
    enrefg $p |- ( A e. B -> A ~~ A ) $=
      ( vf wcel cen wbr cv wf1o wex cid cres cvv wfun funi resfunexg mpi f1oi
      f1oeq1 cla4egv syl breng mpbird ) ABDZAAEFAACGZHZCIZUCJAKZLDZUFUCJMUHNJAB
      OPUHAAUGHZUFAQUEUICUGLAAUDUGRSPTAABCUAUB $.
      $([18-Jun-98]$)
  $}

  $( Equality implies equinumerosity. $)
  eqeng $p |- ( A e. C -> ( A = B -> A ~~ B ) ) $=
    ( wceq wcel cen wbr breq2 enrefg syl5bi comm ) ABDZACEZABFGZLAAFGNMABAFHACI
    JK $.
    $([8-Nov-03]$)

  $( Dominance is reflexive. $)
  domrefg $p |- ( A e. B -> A ~<_ A ) $=
    ( wcel cen wbr cdom enrefg endom syl ) ABCAADEAAFEABGAAHI $.
    $([18-Jun-98]$)

  ${
    $d f A $.  $d f B $.  $d f F $.  $( ` f ` is dummy. $)
    $( The domain and range of a one-to-one, onto function are equinumerous. $)
    f1oeng $p |- ( A e. C -> ( F : A -1-1-onto-> B -> A ~~ B ) ) $=
      ( vf wcel wf1o cv wex cen wbr cvv wi wfn fnex f1ofn syl5 f1oeq1 cla4egv
      syl6 pm2.43d wn brprc enrefg syl5bir a1d com3r breng biimprd pm2.61d2
      syld ) ACFZABDGZABEHZGZEIZABJKZULUMUPULUMDLFZUMUPMULDANURUMACDOABDPQUOUME
      DLABUNDRSTUAULBLFZUPUQMUSUBZUPULUQUTULUQMUPUTUQAAJKULABJUCACUDUEUFUGUSUQU
      PABLEUHUIUJUK $.
      $([19-Jun-98]$)

    $( The domain of a one-to-one function is dominated by its range. $)
    f1domg $p |- ( A e. C -> ( F : A -1-1-> B -> A ~<_ B ) ) $=
      ( vf wcel wf1 cv wex cdom wbr cvv wi wfn fnex wf f1f ffn syl syl5 f1eq1
      cla4egv syl6 pm2.43d wn brprc domrefg syl5bir a1d com3r brdomg biimprd
      pm2.61d2 syld ) ACFZABDGZABEHZGZEIZABJKZUOUPUSUOUPDLFZUPUSMUODANZVAUPACDO
      UPABDPVBABDQABDRSTURUPEDLABUQDUAUBUCUDUOBLFZUSUTMVCUEZUSUOUTVDUOUTMUSVDUT
      AAJKUOABJUFACUGUHUIUJVCUTUSABLEUKULUMUN $.
      $([18-Jun-98]$)
  $}

  ${
    f1oen.1 $e |- A e. V $.
    $( The domain and range of a one-to-one, onto function are equinumerous. $)
    f1oen $p |- ( F : A -1-1-onto-> B -> A ~~ B ) $=
      ( cvv wcel wf1o cen wbr wi f1oeng ax-mp ) AEFABCGABHIJDABECKL $.
      $([19-Jun-98]$)

    $( The domain of a one-to-one function is dominated by its range. $)
    f1dom $p |- ( F : A -1-1-> B -> A ~<_ B ) $=
      ( cvv wcel wf1 cdom wbr wi f1domg ax-mp ) AEFABCGABHIJDABECKL $.
      $([19-Jun-98]$)
  $}

  ${
    $d x y A $.  $d x y B $.  $d y C $.  $d x D $.
    en2.1 $e |- A e. V $.
    en2.2 $e |- ( x e. A -> C e. V ) $.
    en2.3 $e |- ( y e. B -> D e. V ) $.
    en2.4 $e |- ( ( x e. A /\ y = C ) <-> ( y e. B /\ x = D ) ) $.
    $( Equinumerosity inference from an implicit one-to-one onto function. $)
    en2 $p |- A ~~ B $=
      ( cv wcel wceq wa copab wf1o cen wbr wfn ccnv cvv weu eueq sylib bicom
      biopab fnopab cnvopab pm3.2i f1o4 mpbir f1oen ax-mp ) CDBKZDLZAKZFMZNZABO
      ZPZCDQRUTUSCSZUSTZDSZNVAVCUNEMZABCUSUPCLZEUALVDBUBHBEUCUDURVEVDNZABVFURJU
      EUFUGUQBADVBUOFUALUQAUBIAFUCUDURABUHUGUICDUSUJUKCDUSGULUM $.
      $([?]$)
  $}

  ${
    $d x y z A $.  $d x y z B $.  $d y z C $.  $d x z D $.
    dom2.1 $e |- ( x e. A -> C e. B ) $.
    dom2.2 $e |- ( ( x e. A /\ y e. A ) -> ( C = D <-> x = y ) ) $.
    $( A mapping (first hypothesis) that is one-to-one (second hypothesis)
       implies its domain is dominated by its range. $)
    dom2 $p |- ( A e. R -> A ~<_ B ) $=
      ( vz wcel cv wceq wa copab wf1 cdom wbr wf cfv weq wi wral rgen cleqid
