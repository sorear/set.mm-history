  $( Lemma for the Axiom of Regularity with no distinct variable
     conditions. $)
  axregndlem1 $p |- ( A. x x = z -> ( x e. y ->
               E. x ( x e. y /\ A. z ( z e. x -> -. z e. y ) ) ) ) $=
    ( weq wal wel wex wn wi wa eq5 eirrv a13b mtbii a4s pm2.21d 19.21ai imlan
    exp comm del42 19.8a syl5 ) ACDZAEZABFZAGUFCAFZCBFHZIZCEZJZAGUFUFUKACAUFUEU
    KUFUEUKUEUJUFUEUICACCKUEUGUHUDUGHAUDAAFUGALACAMNOPQRSTUAUFAUBUC $.
    $([3-Jan-02]$)

  ${
    $d x w $.  $d z y w $.
    $( Lemma for the Axiom of Regularity with no distinct variable
       conditions. $)
    axregndlem2 $p |- ( x e. y ->
                 E. x ( x e. y /\ A. z ( z e. x -> -. z e. y ) ) ) $=
