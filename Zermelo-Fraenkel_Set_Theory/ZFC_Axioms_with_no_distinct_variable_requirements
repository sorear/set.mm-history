  $( Lemma for proving conditionless ZFC axioms. $)
  nd1 $p |- ( A. x x = y -> -. A. x y e. z ) $=
    ( weq wal wel eirrv wsb stdpc4 pm2.21i a13b ddel4 sylib mto ax-10 mtoi ) AB
    DAEBCFZAEQBEZRCCFZCGZRQBCHSQBCIQSBCSSBETJBCCKLMNQABOP $.
    $([1-Jan-02]$)

  $( Lemma for proving conditionless ZFC axioms. $)
  nd2 $p |- ( A. x x = y -> -. A. x z e. y ) $=
    ( weq wal wel eirrv wsb stdpc4 pm2.21i a14b ddel4 sylib mto ax-10 mtoi ) AB
    DAECBFZAEQBEZRCCFZCGZRQBCHSQBCIQSBCSSBETJBCCKLMNQABOP $.
    $([1-Jan-02]$)

  $( Lemma for proving conditionless ZFC axioms. $)
  nd3 $p |- ( A. x x = y -> -. A. z x e. y ) $=
    ( weq wal wel wn ax-4 eirrv a14b mtbii con3 3syl ) ABDZAENABFZGOCEZGNAHNAAF
    OAIABAJKPOOCHLM $.
    $([2-Jan-02]$)

  $( Lemma for proving conditionless ZFC axioms. $)
  nd4 $p |- ( A. x x = y -> -. A. z y e. x ) $=
    ( weq wal wel wn ax-4 eirrv a13b mtbii con3 3syl ) ABDZAENBAFZGOCEZGNAHNAAF
    OAIABAJKPOOCHLM $.
    $([2-Jan-02]$)

