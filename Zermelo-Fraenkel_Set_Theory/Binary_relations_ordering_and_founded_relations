$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Binary relations, ordering, and founded relations
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

$(
We have not yet defined relations, but here but introduce a few related
notions we will use to develop ordinals.  The class variable ` R ` is no
different from other class variables, but it reminds us that normally it
represents what we will later call a "relation."
$)

  $( Declare new constant symbols. $)
  $c E $. $( Letter E (for epsilon relation) $)
  $c I $.  $( Letter I (for identity relation) $)
  $c Po $.  $( Partial ordering predicate symbol (read: 'partial ordering'). $)
  $c Or $.  $( Strict ordering predicate symbol (read: 'orders'). $)
  $c sup $.  $( Supremum. $)
  $c Fr $. $( Founded predicate symbol (read: 'founded'). $)
  $c We $. $( Well-ordering predicate symbol (read: 'well orders') $)

  $( Extend wff notation to include the general binary relation predicate.
     Note that this is simply three class symbols in a row. $)
  wbr $a wff A R B $.

  $( Extend class notation to include abstraction classes of ordered pairs. $)
  copab $a class { <. x , y >. | ph } $.

  $( Extend class notation to include the epsilon relation. $)
  cep $a class E $.

  $( Extend the definition of a class to include identity relation. $)
  cid $a class I $.

  $( Extend wff notation to include the partial ordering predicate.  Read:
     '` R ` is a partial order on ` A `.' $)
  wpo $a wff R Po A $.

  $( Extend wff notation to include the strict ordering predicate.  Read:
     '` R ` orders ` A `.' $)
  wor $a wff R Or A $.

  $( Extend class notation to include supremum of class ` A ` .  Here ` R `
     is ordinarily a relation that strictly orders class ` B ` .  For example,
     ` R ` could be "less than" and ` B ` could be the set of real numbers. $)
  csup $a class sup ( A , B , R ) $.

  $( Extend wff notation to include the founded predicate.  Read:  '` R ` is a
     founded relation on ` A `.' $)
  wfr $a wff R Fr A $.

  $( Extend wff notation to include the well-ordering predicate.  Read:  '`R `
     well-orders ` A `.' $)
  wwe $a wff R We A $.

  $( Define a notation for a general binary relation.  Definition 6.18, p. 29,
      of Takeuti and Zaring, generalized to arbitrary classes.  This definition
      is well-defined, although not very meaningful, when classes ` A ` and/or
      ` B ` are proper. $)
  df-br $a |- ( A R B <-> <. A , B >. e. R ) $.

  $( Equality theorem for a binary relation. $)
  breq $p |- ( R = S -> ( A R B <-> A S B ) ) $=
    ( wceq cop wcel wbr eleq2 df-br 3bitr4g ) CDEABFZCGLDGABCHABDHCDLIABCJABDJK
    $.
    $([4-Jun-95]$)

  $( Equality theorem for binary relation. $)
  breq1 $p |- ( A = B -> ( A R C <-> B R C ) ) $=
    ( wceq cop wcel wbr opeq1 eleq1d df-br 3bitr4g ) ABEZACFZDGBCFZDGACDHBCDHMN
    ODABCIJACDKBCDKL $.
    $([31-Dec-93]$)

  $( Equality theorem for binary relation. $)
  breq2 $p |- ( A = B -> ( C R A <-> C R B ) ) $=
    ( wceq cop wcel wbr opeq2 eleq1d df-br 3bitr4g ) ABEZCAFZDGCBFZDGCADHCBDHMN
    ODABCIJCADKCBDKL $.
    $([31-Dec-93]$)

  $( Equality theorem for binary relation. $)
  breq12 $p |- ( ( A = B /\ C = D ) -> ( A R C <-> B R D ) ) $=
    ( wceq wbr breq1 breq2 sylan9bb ) ABFACEGBCEGCDFBDEGABCEHCDBEIJ $.
    $([8-Feb-96]$)

  ${
    breq1i.1 $e |- A = B $.
    $( Equality inference for binary relation. $)
    breq1i   $p |- ( A R C <-> B R C ) $=
      ( wceq wbr wb breq1 ax-mp ) ABFACDGBCDGHEABCDIJ $.
      $([8-Feb-96]$)

    $( Equality inference for binary relation. $)
    breq2i   $p |- ( C R A <-> C R B ) $=
      ( wceq wbr wb breq2 ax-mp ) ABFCADGCBDGHEABCDIJ $.
      $([8-Feb-96]$)

    ${
      breq12i.2 $e |- C = D $.
      $( Equality inference for binary relation. $)
      breq12i   $p |- ( A R C <-> B R D ) $=
        ( wbr breq1i breq2i bitr ) ACEHBCEHBDEHABCEFICDBEGJK $.
        $([8-Feb-96]$)
    $}
  $}

  ${
    breq1d.1 $e |- ( ph -> A = B ) $.
    $( Equality inference for binary relation. $)
    breq1d   $p |- ( ph -> ( A R C <-> B R C ) ) $=
      ( wceq wbr wb breq1 syl ) ABCGBDEHCDEHIFBCDEJK $.
      $([8-Feb-96]$)

    $( Equality inference for binary relation. $)
    breq2d   $p |- ( ph -> ( C R A <-> C R B ) ) $=
      ( wceq wbr wb breq2 syl ) ABCGDBEHDCEHIFBCDEJK $.
      $([8-Feb-96]$)

    ${
      breq12d.2 $e |- ( ph -> C = D ) $.
      $( Equality inference for binary relation. $)
      breq12d   $p |- ( ph -> ( A R C <-> B R D ) ) $=
        ( wbr breq1d breq2d bitrd ) ABDFICDFICEFIABCDFGJADECFHKL $.
        $([8-Feb-96]$)
    $}

    ${
      breqan12i.2 $e |- ( ps -> C = D ) $.
      $( Equality inference for binary relation. $)
      breqan12d   $p |- ( ( ph /\ ps ) -> ( A R C <-> B R D ) ) $=
        ( wceq wbr wb breq12 syl2an ) CDJEFJCEGKDFGKLABCDEFGMHIN $.
        $([8-Feb-96]$)

      $( Equality inference for binary relation. $)
      breqan12rd   $p |- ( ( ps /\ ph ) -> ( A R C <-> B R D ) ) $=
        ( wbr wb breqan12d ancoms ) ABCEGJDFGJKABCDEFGHILM $.
        $([8-Feb-96]$)
    $}
  $}

  ${
    eqbrtr.1 $e |- A = B $.
    eqbrtr.2 $e |- B R C $.
    $( Substitution of equal classes into binary relation. $)
    eqbrtr   $p |- A R C $=
      ( wbr breq1i mpbir ) ACDGBCDGABCDEHFI $.
      $([5-Aug-93]$)
  $}

  ${
    eqbrtrr.1 $e |- A = B $.
    eqbrtrr.2 $e |- A R C $.
    $( Substitution of equal classes into binary relation. $)
    eqbrtrr   $p |- B R C $=
      ( cleqcomi eqbrtr ) BACDABEGFH $.
      $([5-Aug-93]$)
  $}

  ${
    breqtr.1 $e |- A R B $.
    breqtr.2 $e |- B = C $.
    $( Substitution of equal classes into binary relation. $)
    breqtr   $p |- A R C $=
      ( wbr breq2i mpbi ) ABDGACDGBCADFHEI $.
      $([5-Aug-93]$)
  $}

  ${
    breqtrr.1 $e |- A R B $.
    breqtrr.2 $e |- C = B $.
    $( Substitution of equal classes into binary relation. $)
    breqtrr   $p |- A R C $=
      ( cleqcomi breqtr ) ABCDECBFGH $.
      $([5-Aug-93]$)
  $}

  ${
    3brtr3.1 $e |- A R B $.
    3brtr3.2 $e |- A = C $.
    3brtr3.3 $e |- B = D $.
    $( Substitution of equality into both sides of a binary relation. $)
    3brtr3   $p |- C R D $=
      ( eqbrtrr breqtr ) CBDEACBEGFIHJ $.
      $([11-Aug-99]$)
  $}

  ${
    3brtr4.1 $e |- A R B $.
    3brtr4.2 $e |- C = A $.
    3brtr4.3 $e |- D = B $.
    $( Substitution of equality into both sides of a binary relation. $)
    3brtr4   $p |- C R D $=
      ( eqbrtr breqtrr ) CBDECABEGFIHJ $.
      $([11-Aug-99]$)
  $}

  ${
    3brtr3g.1 $e |- ( ph -> A R B ) $.
    3brtr3g.2 $e |- A = C $.
    3brtr3g.3 $e |- B = D $.
    $( Substitution of equality into both sides of a binary relation. $)
    3brtr3g   $p |- ( ph -> C R D ) $=
      ( wbr breq1i breq2i bitr sylib ) ABCFJZDEFJZGODCFJPBDCFHKCEDFILMN $.
      $([16-Jan-97]$)
  $}

  ${
    3brtr4g.1 $e |- ( ph -> A R B ) $.
    3brtr4g.2 $e |- C = A $.
    3brtr4g.3 $e |- D = B $.
    $( Substitution of equality into both sides of a binary relation. $)
    3brtr4g   $p |- ( ph -> C R D ) $=
      ( wbr breq1i breq2i bitr sylibr ) ABCFJZDEFJZGPBEFJODBEFHKECBFILMN $.
      $([16-Jan-97]$)
  $}

  ${
    $d x z $.  $d y z $.  $d R z $.
    $( Relationship between a binary relation and a class abstraction. $)
    brab1 $p |- ( x R y <-> x e. { z | z R y } ) $=
      ( cv wbr cab wcel visset breq1 abcl bicom ) AEZCEZBEZDFZCGHMODFZPQCMAINMO
      DJKL $.
      $([7-Apr-94]$)
  $}

  $( A property of proper class as the second argument of a binary relation. $)
  brprc $p |- ( -. B e. V -> ( A R B <-> A R A ) ) $=
    ( cvv wcel wn cop wbr opprc2 eleq1d df-br 3bitr4g ) BDEFZABGZCEAAGZCEABCHAA
    CHMNOCABIJABCKAACKL $.
    $([14-Jun-98]$)

  ${
    $d x y z $.  $d z ph $.
    $( Define abstraction classes of ordered pairs.  Definition 3.3 of
       Monk1, p. 34. $)
    df-opab $a |- { <. x , y >. | ph } =
                  { z | E. x E. y ( z = <. x , y >. /\ ph ) } $.
  $}

  ${
    $d x y z w v $.  $d ph z w v $.  $( ` z w v ` are dummy. $)
    $( The law of concretion.  Special case of Theorem 9.5 of Quine, p. 61. $)
    opabid $p |- ( ph <-> <. x , y >. e. { <. x , y >. | ph } ) $=
      ( vz vw vv cv cop wceq wa wex cab wcel copab clelab df-opab eleq2i wsb
      weq 19.41v anass biex cleqcom opex eqvinc visset opth 3bitr3 biran
      3bitr3r bi2ex sbel2x 19.11 exdistr2 3bitr4 ax-17 hbsbv hban hbex opeq1
      cleq2d sbequ12 bi2and biexdv cbvex opeq2 bitr bilan bitr4 3bitr4r ) BGZCG
      ZHZDGZVMIZAJZCKZBKZDLZMVOVRJZDKZVMABCNZMAVRDVMOWBVSVMABCDPQAVOVNEGZFGZHZI
      ZABERZCFRZJZFKZEKZJZDKZWAEBSFCSJZWHJZFKEKVOWIJZDKZFKZEKZAWMWOWQEFVOWFJZWH
      JZDKWTDKZWHJWQWOWTWHDTXAWPDVOWFWHUAUBXBWNWHVMWEIWEVMIXBWNVMWEUCDVMWEVKVLU
      DUEWCWDVKVLEUFFUFCUFUGUHUIUJUKAEFBCULWPFKZEKDKXCDKZEKWMWSXCDEUMVOWIDEFUNX
      DWREWPDFUMUBUHUOVTWLDVRWKVOVRVNWCVLHZIZWGJZCKZEKWKVQXHBEVQEUPXGBCXFWGBXFB
      UPABEUQURUSBESZVPXGCXIVOXFAWGXIVMXEVNVKWCVLUTVAABEVBVCVDVEXHWJEXGWICFXGFU
      PWFWHCWFCUPWGCFUQURCFSZXFWFWGWHXJXEWEVNVLWDWCVFVAWGCFVBVCVEUBVGVHUBVIVJ
      $.
      $([14-Apr-95]$)
  $}

  ${
    $d x y w $.  $d x z $.  $d w ph $.  $( ` w ` is dummy. $)
    $( The abstraction variables in an abstraction class of pairs are not
       free. $)
    hbopab1 $p |- ( z e. { <. x , y >. | ph } ->
                 A. x z e. { <. x , y >. | ph } ) $=
      ( vw cv cop wceq wa wex cab wcel wal copab hbe1 hbab df-opab eleq2i bial
      3imtr4 ) DFZEFBFCFGHAICJZBJZEKZLZUEBMUAABCNZLZUGBMUCBEDUBBOPUFUDUAABCEQRZ
      UGUEBUHST $.
      $([16-May-95]$)
  $}

  ${
    $d x y w $.  $d y z $.  $d w ph $.  $( ` w ` is dummy. $)
    $( The abstraction variables in an abstraction class of pairs are not
       free. $)
    hbopab2 $p |- ( z e. { <. x , y >. | ph } ->
                 A. y z e. { <. x , y >. | ph } ) $=
      ( vw cv cop wceq wa wex cab wcel wal copab hbe1 hbex hbab df-opab eleq2i
      bial 3imtr4 ) DFZEFBFCFGHAIZCJZBJZEKZLZUGCMUBABCNZLZUICMUECEDUDCBUCCOPQUH
      UFUBABCERSZUIUGCUJTUA $.
      $([16-May-95]$)
  $}

  ${
    $d x y z R $.  $( ` z ` is dummy. $)
    $( The collection of ordered pairs in a class is a subclass of it. $)
    opabss $p |- { <. x , y >. | x R y } (_ R $=
      ( vz cv cop wceq wbr wa wex cab wcel copab eleq1 biimpar df-br sylan5b
      19.23aivv ssab2i df-opab abid2 3sstr4 ) DEZAEZBEZFZGZUDUECHZIZBJAJZDKUCCL
      ZDKUHABMCUJUKDUIUKABUGUFCLZUKUHUGUKULUCUFCNOUDUECPQRSUHABDTDCUAUB $.
      $([27-Dec-96]$)
  $}

  ${
    $d x y z w $.  $d z w v ph $.  $d x y v ps $.  $( ` v ` is dummy. $)
    cbvopabv.1 $e |- ( ( x = z /\ y = w ) -> ( ph <-> ps ) ) $.
    $( Rule used to change bound variables with implicit substitution. $)
    cbvopabv   $p |- { <. x , y >. | ph } = { <. z , w >. | ps } $=
      ( vv cv cop wceq wa wex cab copab weq visset opth biimpr cleq2d bi2and
      cbvex2v biab df-opab 3eqtr4 ) HIZCIZDIZJZKZALZDMCMZHNUFEIZFIZJZKZBLZFMEMZ
      HNACDOBEFOULURHUKUQCDEFCEPDFPLZUJUPABUSUIUOUFUIUOKUSUGUHUMUNCQDQFQRSTGUAU
      BUCACDHUDBEFHUDUE $.
      $([15-Oct-96]$)
  $}

  ${
    $d x y z ph $.  $d z ps $.  $d z ch $.  $( ` z ` is dummy. $)
    biopabdv.1 $e |- ( ph -> ( ps <-> ch ) ) $.
    $( Equivalent wff's yield equal abstraction classes (deduction rule). $)
    biopabdv   $p |- ( ph -> { <. x , y >. | ps } = { <. x , y >. | ch } ) $=
      ( vz cv cop wceq wa wex cab copab biland bi2exdv biabdv df-opab 3eqtr4g
      ) AGHDHEHIJZBKZELDLZGMTCKZELDLZGMBDENCDENAUBUDGAUAUCDEABCTFOPQBDEGRCDEGRS
      $.
      $([15-May-95]$)
  $}

  ${
    $d x y z $.  $d z ph $.  $d z ps $.  $( ` z ` is dummy. $)
    biopab.1 $e |- ( ph <-> ps ) $.
    $( Equivalent wff's yield equal abstraction classes. $)
    biopab   $p |- { <. x , y >. | ph } = { <. x , y >. | ps } $=
      ( vz weq wi copab wceq eqcom wb a1i biopabdv ax-mp ) FFGZPHZACDIBCDIJFFKQ
      ABCDABLQEMNO $.
      $([15-May-95]$)
  $}

  ${
    $d x y z A $.  $d z ph $.  $( ` z ` is dummy. $)
    $( Membership in abstraction class of pairs. $)
    elopab $p |- ( A e. { <. x , y >. | ph } <->
                 E. x E. y ( A = <. x , y >. /\ ph ) ) $=
      ( vz copab wcel cvv cv cop wceq wa wex elisset eleq1 opex mpbiri adantr
      19.23aivv cleq1 birand bi2exdv cab df-opab eleq2i abid bitr4 vtoclgb
      pm5.21nii ) DABCFZGZDHGZDBIZCIZJZKZALZCMBMZDUJNUQULBCUPULAUPULUOHGDUOHOUM
      UNPQRSEIZUJGZUSUOKZALZCMBMZUKUREDHUSDUJOUSDKZVBUQBCVDVAUPAUSDUOTUAUBUTUSV
      CEUCZGVCUJVEUSABCEUDUEVCEUFUGUHUI $.
      $([24-Mar-98]$)
  $}

  ${
    $d x y z A $.  $d x y z B $.  $d x y ch $.  $d z ph $.
        $( ` z ` is dummy. $)
    opelopabg.1 $e |- ( x = A -> ( ph <-> ps ) ) $.
    opelopabg.2 $e |- ( y = B -> ( ps <-> ch ) ) $.
    $( The law of concretion.  Theorem 9.5 of Quine, p. 61. $)
    opelopabg   $p |- ( ( A e. C /\ B e. D ) ->
                    ( <. A , B >. e. { <. x , y >. | ph } <-> ch ) ) $=
      ( vz wcel wa cv wceq wex cop copab wb elex im2an eeanv sylibr ax-17
      hbopab1 hbel hbbi hbopab2 opeq12 eleq1d opabid syl5bb sylan9bb bitr3d
      19.23ai syl ) FHMZGIMZNZDOZFPZEOZGPZNZEQZDQZFGRZADESZMZCTZUTVBDQZVDEQZNVG
      URVLUSVMDFHUAEGIUAUBVBVDDEUCUDVFVKDVJCDDLLVHVILOVHMZDUEADELUFUGCDUEUHVEVK
      EVJCEELLVHVIVNEUEADELUIUGCEUEUHVEAVJCVEVAVCRZVIMVJAVEVOVHVIVAVCFGUJUKADEU
      LUMVBABVDCJKUNUOUPUPUQ $.
      $([28-May-95]$)

    ${
      brabg.5 $e |- R = { <. x , y >. | ph } $.
      $( The law of concretion for a binary relation. $)
      brabg $p |- ( ( A e. C /\ B e. D ) -> ( A R B <-> ch ) ) $=
        ( wcel wa cop copab wbr opelopabg df-br eleq2i bitr syl5bb ) FHNGINOFGP
        ZADEQZNZCFGJRZABCDEFGHIKLSUGUDJNUFFGJTJUEUDMUAUBUC $.
        $([16-Aug-99]$)
    $}
  $}

  ${
    $d x y A $.  $d x y B $.  $d x y ch $.
    opelopab.1 $e |- A e. V $.
    opelopab.2 $e |- B e. V $.
    opelopab.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    opelopab.4 $e |- ( y = B -> ( ps <-> ch ) ) $.
    $( The law of concretion.  Theorem 9.5 of Quine, p. 61. $)
    opelopab   $p |- ( <. A , B >. e. { <. x , y >. | ph } <-> ch ) $=
      ( cvv wcel cop copab wb opelopabg mp2an ) FLMGLMFGNADEOMCPABCDEFGLLJKQHIR
      $.
      $([16-May-95]$)

    ${
      brab.5 $e |- R = { <. x , y >. | ph } $.
      $( The law of concretion for a binary relation. $)
      brab $p |- ( A R B <-> ch ) $=
        ( cvv wcel wbr wb brabg mp2an ) FNOGNOFGHPCQABCDEFGNNHKLMRIJS $.
        $([16-Aug-99]$)
    $}
  $}

  ${
    $d ph z $.  $d ps z $.  $d x y z $.  $( ` z ` is dummy. $)
    $( Equivalence of abstraction subclass and implication. $)
    ssopab2 $p |- ( { <. x , y >. | ph } (_ { <. x , y >. | ps } <->
               A. x A. y ( ph -> ps ) ) $=
      ( vz copab wss wi wal hbopab1 hbss hbopab2 cv cop wceq wex opex isseti
      wa cab copsexg bi2imd ssab2 ax-4 sylbi syl5bir df-opab sseq12i syl5ib
      19.23aiv ax-mp 19.21ai hba1 imland 19.22d a4s 19.21aiv sylibr impbi ) ACD
      FZBCDFZGZABHZDIZCIZVBVDCCEUTVAACDEJBCDEJKVBVCDDEUTVAACDELBCDELKEMZCMZDMZN
      ZOZEPVBVCHZEVIVGVHQRVJVKEVJVJASZDPZCPZETZVJBSZDPZCPZETZGZVCVBVJVCVNVRHZVT
      VJAVNBVRACDVFUABCDVFUAUBVTWAEIZWAVNVREUCZWAEUDUEUFUTVOVAVSACDEUGBCDEUGUHZ
      UIUJUKULULVEVTVBVEWBVTVEWAEVEVMVQCVDCUMVDVMVQHCVDVLVPDVCDUMVDABVJVCDUDUNU
      OUPUOUQWCURWDURUS $.
      $([27-Dec-96]$)
  $}

  ${
    $d ph z $.  $d ps z $.  $d x y z $.  $( ` z ` is dummy. $)
    ssopab2i.1 $e |- ( ph -> ps ) $.
    $( Inference of abstraction subclass from implication. $)
    ssopab2i $p |- { <. x , y >. | ph } (_ { <. x , y >. | ps } $=
      ( copab wss wi wal ssopab2 ax-gen mpgbir ) ACDFBCDFGABHZDICABCDJMDEKL $.
      $([5-Apr-95]$)
  $}

  ${
    $d x y $.
    $( Define the epsilon relation.  Similar to Definition 6.22, p. 30, of
       Takeuti and Zaring. $)
    df-eprel $a |- E = { <. x , y >. | x e. y } $.
  $}

  ${
    $d x y A $.  $d x y B $.  $( ` x y ` are dummy. $)
    epelc.1 $e |- A e. V $.
    epelc.2 $e |- B e. V $.
    $( The epsilon relation and the membership relation are the same. $)
    epelc $p |- ( A E B <-> A e. B ) $=
      ( vx vy cep wbr cop wcel wel copab df-br df-eprel eleq2i cv eleq1 eleq2
      opelopab 3bitr ) ABGHABIZGJUAEFKZEFLZJABJZABGMGUCUAEFNOUBAFPZJUDEFABCDEPA
      UEQUEBARST $.
      $([13-Aug-95]$)
  $}

  ${
    $( The epsilon relation and the membership relation are the same. $)
    epel $p |- ( x E y <-> x e. y ) $=
      ( cv visset epelc ) ACBCADBDE $.
      $([13-Aug-95]$)
  $}

  ${
    $d x y $.
    $( Define the identity relation.  Definition 9.15, p. 64, of Quine. $)
    df-id $a |- I = { <. x , y >. | x = y } $.
  $}

  ${
    $d x y A $.  $d x y B $.  $( ` x y ` are dummy. $)
    ideq.1 $e |- A e. V $.
    ideq.2 $e |- B e. V $.
    $( For sets, the identity relation is the same as equality. $)
    ideq   $p |- ( A I B <-> A = B ) $=
      ( vx vy cid wbr cop wcel weq copab wceq df-br df-id eleq2i cv cleq1
      cleq2 opelopab 3bitr ) ABGHABIZGJUBEFKZEFLZJABMZABGNGUDUBEFOPUCAFQZMUEEFA
      BCDEQAUFRUFBASTUA $.
      $([13-Aug-95]$)
  $}

  ${
    $d x y z R $.  $d x y z A $.
    $( Define a partial order relation.  Definition ? of Enderton, p. ?. $)
    df-po $a |- ( R Po A <-> A. x e. A A. y e. A A. z e. A
                ( -. x R x /\ ( ( x R y /\ y R z ) -> x R z ) ) ) $.
  $}

  ${
    $d x y z R $.  $d x y z A $.  $d x y z B $.  $( ` x y z ` are dummy. $)
    $( Subset theorem for the partial ordering predicate. $)
    poss $p |- ( A (_ B -> ( R Po B -> R Po A ) ) $=
      ( vx vy vz wss cv wbr wn wa wi wral wpo wcel w3a wal ssel im2and df-3an
      3imtr4g syl4d 19.20dv r3al df-po ) ABGZDHZUGCIJUGEHZCIUHFHZCIKUGUICILKZFB
      MEBMDBMZUJFAMEAMDAMZBCNACNUFUGBOZUHBOZUIBOZPZUJLZFQZEQZDQUGAOZUHAOZUIAOZP
      ZUJLZFQZEQZDQUKULUFUSVFDUFURVEEUFUQVDFUFVCUPUJUFUTVAKZVBKUMUNKZUOKVCUPUFV
