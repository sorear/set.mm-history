  epne3 $p |- ( ( E Fr A /\ ( x e. A /\ y e. A /\ z e. A ) ) ->
                -. ( x e. y /\ y e. z /\ z e. x ) ) $=
      ( cep wfr cv wcel w3a wa wbr wn wel fr3nr epel bi3an bineg sylib ) DEFAGZ
      DHBGZDHCGZDHIJSTEKZTUAEKZUASEKZIZLABMZBCMZCAMZIZLABCDENUEUIUBUFUCUGUDUHAB
      OBCOCAOPQR $.
      $([19-Apr-94]$)

  $( Similar to Theorem 7.2, p. 37, of Takeuti and Zaring, except that the
     Axiom of Regularity is not required due to antecedent ` E Fr A `. $)
  tz7.2 $p |- (  ( ( Tr A /\ E Fr A ) /\ B e. A ) ->
             ( B (_ A /\ -. B = A ) ) $=
      ( wtr cep wfr wa wcel wss wceq wn wi trss cleq3 binegd epnec syl5bir
      comm pm2.03d im2an jcab sylibr imp ) ACZADEZFZBAGZBAHZBAIZJZFZUEUFUGKZUFU
      IKZFUFUJKUCUKUDULABLUDUHUFUHUDUFJZUHUMAAGZJUDUHUFUNBAAMNAOPQRSUFUGUITUAUB
