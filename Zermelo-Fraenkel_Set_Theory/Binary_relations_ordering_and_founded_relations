$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Binary relations, ordering, and founded relations
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

$(
We have not yet defined relations, but here but introduce a few related
notions we will use to develop ordinals.  The class variable ` R ` is no
different from other class variables, but it reminds us that normally it
represents what we will later call a "relation."
$)

  $( Declare new constant symbols. $)
  $c E $. $( Letter E (for epsilon relation) $)
  $c I $.  $( Letter I (for identity relation) $)
  $c Po $.  $( Partial ordering predicate symbol (read: 'partial ordering'). $)
  $c Or $.  $( Strict ordering predicate symbol (read: 'orders'). $)
  $c Fr $. $( Founded predicate symbol (read: 'founded'). $)
  $c We $. $( Well-ordering predicate symbol (read: 'well orders') $)

  $( Extend wff notation to include the general binary relation predicate.
     Note that this is simply three class symbols in a row. $)
  wbr $a wff A R B $.

  $( Extend class notation to include abstraction classes of ordered pairs. $)
  copab $a class { <. x , y >. | ph } $.

  $( Extend class notation to include the epsilon relation. $)
  cep $a class E $.

  $( Extend the definition of a class to include identity relation. $)
  cid $a class I $.

  $( Extend wff notation to include the partial ordering predicate.  Read:
     '` R ` is a partial order on ` A `.' $)
  wpo $a wff R Po A $.

  $( Extend wff notation to include the strict ordering predicate.  Read:
     '` R ` orders ` A `.' $)
  wor $a wff R Or A $.

  $( Extend wff notation to include the founded predicate.  Read:  '` R ` is a
     founded relation on ` A `.' $)
  wfr $a wff R Fr A $.

  $( Extend wff notation to include the well-ordering predicate.  Read:  '`R `
     well-orders ` A `.' $)
  wwe $a wff R We A $.

  $( Define a notation for a general binary relation.  Definition 6.18, p. 29,
      of Takeuti and Zaring, generalized to arbitrary classes.  This definition
      is well-defined, although not very meaningful, when classes ` A ` and/or
      ` B ` are proper. $)
  df-br $a |- ( A R B <-> <. A , B >. e. R ) $.

  $( Equality theorem for a binary relation. $)
  breq $p |- ( R = S -> ( A R B <-> A S B ) ) $=
      ( wceq cop wcel wbr cleq4 df-br 3bitr4g ) CDEABFZCGLDGABCHABDHCDLIABCJABD
      JK $.
      $([4-Jun-95]$)

  $( Equality theorem for binary relation. $)
  breq1 $p |- ( A = B -> ( A R C <-> B R C ) ) $=
      ( wceq cop wcel wbr opeq1 cleq3d df-br 3bitr4g ) ABEZACFZDGBCFZDGACDHBCD
      HMNODABCIJACDKBCDKL $.
      $([31-Dec-93]$)

  $( Equality theorem for binary relation. $)
  breq2 $p |- ( A = B -> ( C R A <-> C R B ) ) $=
      ( wceq cop wcel wbr opeq2 cleq3d df-br 3bitr4g ) ABEZCAFZDGCBFZDGCADHCBD
      HMNODABCIJCADKCBDKL $.
      $([31-Dec-93]$)

  $( Equality theorem for binary relation. $)
  breq12 $p |- ( ( A = B /\ C = D ) -> ( A R C <-> B R D ) ) $=
      ( wceq wbr breq1 breq2 sylan9bb ) ABFACEGBCEGCDFBDEGABCEHCDBEIJ $.
      $([8-Feb-96]$)

  ${
    breq1i.1 $e |- A = B $.
    $( Equality inference for binary relation. $)
    breq1i   $p |- ( A R C <-> B R C ) $=
      ( wceq wbr wb breq1 ax-mp ) ABFACDGBCDGHABCDIEJ $.
      $([8-Feb-96]$)

    $( Equality inference for binary relation. $)
    breq2i   $p |- ( C R A <-> C R B ) $=
      ( wceq wbr wb breq2 ax-mp ) ABFCADGCBDGHABCDIEJ $.
      $([8-Feb-96]$)

    ${
      breq12i.2 $e |- C = D $.
      $( Equality inference for binary relation. $)
      breq12i   $p |- ( A R C <-> B R D ) $=
      ( wbr breq1i breq2i bitr ) ACEHBCEHBDEHABCEFICDBEGJK $.
      $([8-Feb-96]$)
    $}
  $}

  ${
    breq1d.1 $e |- ( ph -> A = B ) $.
    $( Equality inference for binary relation. $)
    breq1d   $p |- ( ph -> ( A R C <-> B R C ) ) $=
      ( wceq wbr wb breq1 syl ) ABCGBDEHCDEHIFBCDEJK $.
      $([8-Feb-96]$)

    $( Equality inference for binary relation. $)
    breq2d   $p |- ( ph -> ( C R A <-> C R B ) ) $=
      ( wceq wbr wb breq2 syl ) ABCGDBEHDCEHIFBCDEJK $.
      $([8-Feb-96]$)

    ${
      breq12d.2 $e |- ( ph -> C = D ) $.
      $( Equality inference for binary relation. $)
      breq12d   $p |- ( ph -> ( A R C <-> B R D ) ) $=
      ( wbr breq1d breq2d bitrd ) ABDFICDFICEFIABCDFGJADECFHKL $.
      $([8-Feb-96]$)
    $}

    ${
      breqan12i.2 $e |- ( ps -> C = D ) $.
      $( Equality inference for binary relation. $)
      breqan12d   $p |- ( ( ph /\ ps ) -> ( A R C <-> B R D ) ) $=
      ( wceq wbr wb breq12 syl2an ) CDJEFJCEGKDFGKLABCDEFGMHIN $.
      $([8-Feb-96]$)

      $( Equality inference for binary relation. $)
      breqan12rd   $p |- ( ( ps /\ ph ) -> ( A R C <-> B R D ) ) $=
      ( wbr wb breqan12d ancoms ) ABCEGJDFGJKABCDEFGHILM $.
      $([8-Feb-96]$)
    $}
  $}

  ${
    3brtr3g.1 $e |- ( ph -> A R B ) $.
    3brtr3g.2 $e |- A = C $.
    3brtr3g.3 $e |- B = D $.
    $( Substitution of equality into both sides of a binary relation. $)
    3brtr3g   $p |- ( ph -> C R D ) $=
      ( wbr breq1i breq2i bitr sylib ) ABCFJZDEFJZGODCFJPBDCFHKCEDFILMN $.
      $([16-Jan-97]$)
  $}

  ${
    3brtr4g.1 $e |- ( ph -> A R B ) $.
    3brtr4g.2 $e |- C = A $.
    3brtr4g.3 $e |- D = B $.
    $( Substitution of equality into both sides of a binary relation. $)
    3brtr4g   $p |- ( ph -> C R D ) $=
      ( wbr breq1i breq2i bitr sylibr ) ABCFJZDEFJZGPBEFJODBEFHKECBFILMN $.
      $([16-Jan-97]$)
  $}

  ${
    $d x z $.  $d y z $.  $d R z $.
    $( Relationship between a binary relation and a class abstraction. $)
    brab $p |- ( x R y <-> x e. { z | z R y } ) $=
      ( cv wbr cab wcel visset breq1 abcl bicom ) AEZCEZBEZDFZCGHMODFZPQCMAINMO
      DJKL $.
      $([7-Apr-94]$)
  $}

  ${
    $d x y z $.  $d z ph $.
    $( Define abstraction classes of ordered pairs.  Definition 3.3 of
       Monk1, p. 34. $)
    df-opab $a |- { <. x , y >. | ph } =
                  { z | E. x E. y ( z = <. x , y >. /\ ph ) } $.
  $}

  ${
    $d x y z w v $.  $d ph z w v $.  $( ` z w v ` are dummy. $)
    $( The law of concretion.  Special case of Theorem 9.5 of Quine, p. 61. $)
    opabid $p |- ( ph <-> <. x , y >. e. { <. x , y >. | ph } ) $=
      ( vz vw vv cv cop wceq wa wex cab wcel copab clelab df-opab cleq4i wsb
      weq 19.41v anass biex cleqcom opex eqvinc visset opth 3bitr3 biran
      3bitr3r bi2ex sbel2x 19.11 exdistr2 3bitr4 ax-17 hbsbv hban hbex opeq1
      cleq2d sbequ12 bi2and biexdv cbvex opeq2 bitr bilan bitr4 3bitr4r ) BG
      ZCGZHZDGZVMIZAJZCKZBKZDLZMVOVRJZDKZVMABCNZMAVRDVMOWBVSVMABCDPQAVOVNEGZFGZ
      HZIZABERZCFRZJZFKZEKZJZDKZWAEBSFCSJZWHJZFKEKVOWIJZDKZFKZEKZAWMWOWQEFVOWFJ
      ZWHJZDKWTDKZWHJWQWOWTWHDTXAWPDVOWFWHUAUBXBWNWHVMWEIWEVMIXBWNVMWEUCDVMWEVK
      VLUDUEWCWDVKVLEUFFUFCUFUGUHUIUJUKAEFBCULWPFKZEKDKXCDKZEKWMWSXCDEUMVOWIDEF
      UNXDWREWPDFUMUBUHUOVTWLDVRWKVOVRVNWCVLHZIZWGJZCKZEKWKVQXHBEVQEUPXGBCXFWGB
      XFBUPABEUQURUSBESZVPXGCXIVOXFAWGXIVMXEVNVKWCVLUTVAABEVBVCVDVEXHWJEXGWICFX
      GFUPWFWHCWFCUPWGCFUQURCFSZXFWFWGWHXJXEWEVNVLWDWCVFVAWGCFVBVCVEUBVGVHUBVIV
      J $.
      $([14-Apr-95]$)
  $}

  ${
    $d x y w $.  $d x z $.  $d w ph $.  $( ` w ` is dummy. $)
    $( The abstraction variables in an abstraction class of pairs are not
       free. $)
    hbopab1 $p |- ( z e. { <. x , y >. | ph } ->
                 A. x z e. { <. x , y >. | ph } ) $=
      ( vw cv cop wceq wa wex cab wcel wal copab hbe1 hbab df-opab cleq4i
      bial 3imtr4 ) DFZEFBFCFGHAICJZBJZEKZLZUEBMUAABCNZLZUGBMUCBEDUBBOPUFUDUAAB
      CEQRZUGUEBUHST $.
      $([16-May-95]$)
  $}

  ${
    $d x y w $.  $d y z $.  $d w ph $.  $( ` w ` is dummy. $)
    $( The abstraction variables in an abstraction class of pairs are not
       free. $)
    hbopab2 $p |- ( z e. { <. x , y >. | ph } ->
                 A. y z e. { <. x , y >. | ph } ) $=
      ( vw cv cop wceq wa wex cab wcel wal copab hbe1 hbex hbab df-opab
      cleq4i bial 3imtr4 ) DFZEFBFCFGHAIZCJZBJZEKZLZUGCMUBABCNZLZUICMUECEDUDCBU
      CCOPQUHUFUBABCERSZUIUGCUJTUA $.
      $([16-May-95]$)
  $}

  ${
    $d x y z R $.  $( ` z ` is dummy. $)
    $( The collection of ordered pairs in a class is a subclass of it. $)
    opabss $p |- { <. x , y >. | x R y } (_ R $=
      ( vz cv cop wceq wbr wa wex cab wcel copab cleq3 biimpar df-br sylan5b
      19.23aiaiv ssab2i df-opab abid2 3sstr4 ) DEZAEZBEZFZGZUDUECHZIZBJAJZDKUCC
      LZDKUHABMCUJUKDUIUKABUGUFCLZUKUHUGUKULUCUFCNOUDUECPQRSUHABDTDCUAUB $.
      $([27-Dec-96]$)
  $}

  ${
    $d x y z w $.  $d z w v ph $.  $d x y v ps $.  $( ` v ` is dummy. $)
    cbvopabv.1 $e |- ( ( x = z /\ y = w ) -> ( ph <-> ps ) ) $.
    $( Rule used to change bound variables with implicit substitution. $)
    cbvopabv   $p |- { <. x , y >. | ph } = { <. z , w >. | ps } $=
      ( vv cv cop wceq wa wex cab copab weq visset opth biimpr cleq2d bi2and
      cbvex2v biab df-opab 3eqtr4 ) HIZCIZDIZJZKZALZDMCMZHNUFEIZFIZJZKZBLZFMEMZ
      HNACDOBEFOULURHUKUQCDEFCEPDFPLZUJUPABUSUIUOUFUIUOKUSUGUHUMUNCQDQFQRSTGUAU
      BUCACDHUDBEFHUDUE $.
      $([15-Oct-96]$)
  $}

  ${
    $d x y z ph $.  $d z ps $.  $d z ch $.  $( ` z ` is dummy. $)
    biopabdv.1 $e |- ( ph -> ( ps <-> ch ) ) $.
    $( Equivalent wff's yield equal abstraction classes (deduction rule). $)
    biopabdv   $p |- ( ph -> { <. x , y >. | ps } = { <. x , y >. | ch } ) $=
      ( vz cv cop wceq wa wex cab copab biland bi2exdv biabdv df-opab
      3eqtr4g ) AGHDHEHIJZBKZELDLZGMTCKZELDLZGMBDENCDENAUBUDGAUAUCDEABCTFOPQBDE
      GRCDEGRS $.
      $([15-May-95]$)
  $}

  ${
    $d x y z $.  $d z ph $.  $d z ps $.  $( ` z ` is dummy. $)
    biopab.1 $e |- ( ph <-> ps ) $.
    $( Equivalent wff's yield equal abstraction classes. $)
    biopab   $p |- { <. x , y >. | ph } = { <. x , y >. | ps } $=
      ( vz weq wi copab wceq wb a1i biopabdv eqcom ax-mp ) FFGZPHZACDIBCDIJQAB
      CDABKQELMFFNO $.
      $([15-May-95]$)
  $}

  ${
    $d x y z A $.  $d x y z B $.  $d x y ch $.  $d z ph $.  $( ` z ` is dummy. $)
    elopabg.1 $e |- ( x = A -> ( ph <-> ps ) ) $.
    elopabg.2 $e |- ( y = B -> ( ps <-> ch ) ) $.
    $( The law of concretion.  Theorem 9.5 of Quine, p. 61. $)
    elopabg   $p |- ( ( A e. C /\ B e. D ) ->
                    ( <. A , B >. e. { <. x , y >. | ph } <-> ch ) ) $=
      ( vz wcel wa cv wceq wex cop copab wb elex im2an eeanv sylibr ax-17
      hbopab1 hbel hbbi hbopab2 opeq12 cleq3d opabid syl5bb sylan9bb bitr3d
      19.23ai syl ) FHMZGIMZNZDOZFPZEOZGPZNZEQZDQZFGRZADESZMZCTZUTVBDQZVDEQZNVG
      URVLUSVMDFHUAEGIUAUBVBVDDEUCUDVFVKDVJCDDLLVHVILOVHMZDUEADELUFUGCDUEUHVEVK
      EVJCEELLVHVIVNEUEADELUIUGCEUEUHVEAVJCVEVAVCRZVIMVJAVEVOVHVIVAVCFGUJUKADEU
      LUMVBABVDCJKUNUOUPUPUQ $.
      $([28-May-95]$)
  $}

  ${
    $d x y A $.  $d x y B $.  $d x y ch $.
    elopab.1 $e |- A e. V $.
    elopab.2 $e |- B e. V $.
    elopab.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    elopab.4 $e |- ( y = B -> ( ps <-> ch ) ) $.
    $( The law of concretion.  Theorem 9.5 of Quine, p. 61. $)
    elopab   $p |- ( <. A , B >. e. { <. x , y >. | ph } <-> ch ) $=
      ( cvv wcel cop copab wb elopabg mp2an ) FLMGLMFGNADEOMCPABCDEFGLLJKQHIR
      $.
      $([16-May-95]$)
  $}

  ${
    $d ph z $.  $d ps z $.  $d x y z $.  $( ` z ` is dummy. $)
    $( Equivalence of abstraction subclass and implication. $)
    ssopab2 $p |- ( { <. x , y >. | ph } (_ { <. x , y >. | ps } <->
               A. x A. y ( ph -> ps ) ) $=
      ( vz copab wss wi wal hbopab1 hbss hbopab2 cv cop wceq wex wa cab
      copsexg bi2imd ssab2 ax-4 sylbi syl5bir df-opab sseq1i sseq2i bitr
      syl5ib 19.23aiv opex isseti ax-mp 19.21ai hba1 imland 19.22d a4s
      19.21aiv sylibr impbi ) ACDFZBCDFZGZABHZDIZCIZVDVFCCEVBVCACDEJBCDEJKVDVED
      DEVBVCACDELBCDELKEMZCMZDMZNZOZEPVDVEHZVLVMEVLVLAQZDPZCPZERZVLBQZDPZCPZERZ
      GZVEVDVLVEVPVTHZWBVLAVPBVTACDVHSBCDVHSTWBWCEIZWCVPVTEUAZWCEUBUCUDVDVQVCGW
      BVBVQVCACDEUEUFVCWAVQBCDEUEUGUHZUIUJEVKVIVJUKULUMUNUNVGWBVDVGWDWBVGWCEVGV
      OVSCVFCUOVFVOVSHCVFVNVRDVEDUOVFABVLVEDUBUPUQURUQUSWEUTWFUTVA $.
      $([27-Dec-96]$)
  $}

  ${
    $d ph z $.  $d ps z $.  $d x y z $.  $( ` z ` is dummy. $)
    ssopab2i.1 $e |- ( ph -> ps ) $.
    $( Inference of abstraction subclass from implication. $)
    ssopab2i $p |- { <. x , y >. | ph } (_ { <. x , y >. | ps } $=
      ( copab wss wi wal ssopab2 ax-gen mpgbir ) ACDFBCDFGABHZDICABCDJMDEKL $.
      $([5-Apr-95]$)
  $}

  ${
    $d x y $.
    $( Define the epsilon relation.  Similar to Definition 6.22, p. 30, of
       Takeuti and Zaring. $)
    df-eprel $a |- E = { <. x , y >. | x e. y } $.
  $}

  ${
    $d x y A $.  $d x y B $.  $( ` x y ` are dummy. $)
    epelc.1 $e |- A e. V $.
    epelc.2 $e |- B e. V $.
    $( The epsilon relation and the membership relation are the same. $)
    epelc $p |- ( A E B <-> A e. B ) $=
      ( vx vy cep wbr cop wcel wel copab df-br df-eprel cleq4i cv cleq3 cleq4
      elopab 3bitr ) ABGHABIZGJUAEFKZEFLZJABJZABGMGUCUAEFNOUBAFPZJUDEFABCDEPAUE
      QUEBARST $.
      $([13-Aug-95]$)
  $}

  ${
    $( The epsilon relation and the membership relation are the same. $)
    epel $p |- ( x E y <-> x e. y ) $=
      ( cv visset epelc ) ACBCADBDE $.
      $([13-Aug-95]$)
  $}

  ${
    $d x y $.
    $( Define the identity relation.  Definition 9.15, p. 64, of Quine. $)
    df-id $a |- I = { <. x , y >. | x = y } $.
  $}

  ${
    $d x y A $.  $d x y B $.  $( ` x y ` are dummy. $)
    ideq.1 $e |- A e. V $.
    ideq.2 $e |- B e. V $.
    $( For sets, the identity relation is the same as equality. $)
    ideq   $p |- ( A I B <-> A = B ) $=
      ( vx vy cid wbr cop wcel weq copab wceq df-br df-id cleq4i cv cleq1
      cleq2 elopab 3bitr ) ABGHABIZGJUBEFKZEFLZJABMZABGNGUDUBEFOPUCAFQZMUEEFABC
      DEQAUFRUFBASTUA $.
      $([13-Aug-95]$)
  $}

  ${
    $d x y z R $.  $d x y z A $.
    $( Define a partial order relation.  Definition ? of Enderton, p. ?. $)
    df-po $a |- ( R Po A <-> A. x e. A A. y e. A A. z e. A
                ( -. x R x /\ ( ( x R y /\ y R z ) -> x R z ) ) ) $.
  $}

  ${
    $d x y z R $.  $d x y z A $.  $d x y z B $.  $( ` x y z ` are dummy. $)
    $( Subset theorem for the partial ordering predicate. $)
    poss $p |- ( A (_ B -> ( R Po B -> R Po A ) ) $=
      ( vx vy vz wss cv wbr wn wa wi wral wpo wcel w3a wal ssel im2and df-3an
      3imtr4g syl4d 19.21aiiv r3al df-po ) ABGZDHZUGCIJUGEHZCIUHFHZCIKUGUICILKZ
      FBMEBMDBMZUJFAMEAMDAMZBCNACNUFUGBOZUHBOZUIBOZPZUJLZFQZEQZDQUGAOZUHAOZUIAO
      ZPZUJLZFQZEQZDQUKULUFUSVFDUFURVEEUFUQVDFUFVCUPUJUFUTVAKZVBKUMUNKZUOKVCUPU
      FVGVHVBUOUFUTUMVAUNABUGRABUHRSABUIRSUTVAVBTUMUNUOTUAUBUCUCUCUJDEFBBBUDUJD
      EFAAAUDUADEFBCUEDEFACUEUA $.
      $([27-Mar-97]$)
  $}

  ${
    $d x y z R $.  $d x y z S $.  $d x y z A $.  $( ` x y z ` are dummy. $)
    $( Equality theorem for partial ordering predicate. $)
    poeq1 $p |- ( R = S -> ( R Po A <-> S Po A ) ) $=
      ( vx vy vz wceq cv wbr wn wa wi wral wpo breq binegd bi2and bi2imd
      biraldv df-po 3bitr4g ) BCGZDHZUCBIZJZUCEHZBIZUFFHZBIZKZUCUHBIZLZKZFAMZEA
      MZDAMUCUCCIZJZUCUFCIZUFUHCIZKZUCUHCIZLZKZFAMZEAMZDAMABNACNUBUOVEDAUBUNVDE
      AUBUMVCFAUBUEUQULVBUBUDUPUCUCBCOPUBUJUTUKVAUBUGURUIUSUCUFBCOUFUHBCOQUCUHB
      CORQSSSDEFABTDEFACTUA $.
      $([27-Mar-97]$)
  $}

  $( Equality theorem for partial ordering predicate. $)
  poeq2 $p |- ( A = B -> ( R Po A <-> R Po B ) ) $=
      ( wceq wpo wss wa wi wb poss im2an eqss bi 3imtr4 bicomd ) ABDZBCEZACEZAB
      FZBAFZGQRHZRQHZGPQRISUATUBABCJBACJKABLQRMNO $.
      $([27-Mar-97]$)

  ${
    $d x y z R $. $d x y z A $.
    $d x y z B $.  $d x y z C $.  $d x y z D $.  $( ` x y z ` are dummy. $)
    $( Properties of partial order relation in class notation. $)
    pocl $p |- ( R Po A -> ( ( B e. A /\ C e. A /\ D e. A ) ->
               ( -. B R B /\ ( ( B R C /\ C R D ) -> B R D ) ) ) ) $=
      ( vx vy vz wcel w3a wpo wbr wn wa wi cv wceq id breq12d binegd breq1
      birand bi2imd bi2and bilimd breq2 birimd biland wal wral df-po r3al bitr
      biimp 19.21bbi 19.21bi comm vtocl3ga ) BAICAIDAIJAEKZBBELZMZBCELZCDELZNZB
      DELZOZNZUSFPZVHELZMZVHGPZELZVKHPZELZNZVHVMELZOZNZOUSVABVKELZVNNZBVMELZOZN
      ZOUSVAVBCVMELZNZWAOZNZOUSVGOFGHBCDAAAVHBQZVRWCUSWHVJVAVQWBWHVIUTWHVHBVHBE
      WHRZWISTWHVOVTVPWAWHVLVSVNVHBVKEUAUBVHBVMEUAUCUDUEVKCQZWCWGUSWJWBWFVAWJVT
      WEWAWJVSVBVNWDVKCBEUFVKCVMEUAUDUGUHUEVMDQZWGVGUSWKWFVFVAWKWEVDWAVEWKWDVCV
      BVMDCEUFUHVMDBEUFUCUHUEUSVHAIVKAIVMAIJZVRUSWLVROZHUSWMHUIZFGUSWNGUIFUIZUS
      VRHAUJGAUJFAUJWOFGHAEUKVRFGHAAAULUMUNUOUPUQURUQ $.
      $([27-Mar-97]$)
  $}

  $( A partial order relation is nonreflexive. $)
  ponr $p |- ( ( R Po A /\ B e. A ) -> -. B R B ) $=
      ( wpo wcel w3a wbr wn wa wi pocl imp pm3.26d anabs biran df-3an 3bitr4
      sylan5b ) ACDZBAEZTTFZBBCGZHZTSUAIUCUBUBIUBJZSUAUCUDIABBBCKLMTTIZUETITUAT
      UETTNZOUFTTTPQR $.
      $([27-Mar-97]$)

  $( A partial order relation is a transitive relation. $)
  potr $p |- ( ( R Po A /\ ( B e. A /\ C e. A /\ D e. A ) ) ->
             ( ( B R C /\ C R D ) -> B R D ) ) $=
      ( wpo wcel w3a wa wbr wn wi pocl imp pm3.27d ) AEFZBAGCAGDAGHZIBBEJKZBCEJ
      CDEJIBDEJLZPQRSIABCDEMNO $.
      $([27-Mar-97]$)

  $( A partial order relation has no 2-cycle loops. $)
  po2nr $p |- ( ( R Po A /\ ( B e. A /\ C e. A ) ) ->
              -. ( B R C /\ C R B ) ) $=
      ( wpo wcel wa wbr wi w3a potr df-3an sylan5br exp44 com34 pm2.43d imp32
      wn ponr adantarr mtod ) ADEZBAFZCAFZGZGBCDHCBDHGZBBDHZUBUCUDUFUGIZUBUCUDU
