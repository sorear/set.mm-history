      $.
      $([23-May-99]$)

    $( The supremum is the least upper bound. $)
    suplub $p |- ( ( C e. A /\ C R sup ( B , A , R ) ) -> E. z e. B C R z ) $=
      ( wcel csup wbr cv wrex wi wral wn wa crab cuni wceq wreu wb breq1
