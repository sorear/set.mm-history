  $( A well-ordering is founded. $)
  wefr $p |- ( R We A -> R Fr A ) $=
      ( wwe wfr wor wa df-we pm3.26 sylbi ) ABCABDZABEZFJABGJKHI $.
      $([22-Apr-94]$)

  $( A well-ordering is a strict ordering. $)
  weso $p |- ( R We A -> R Or A ) $=
      ( wwe wfr wor wa df-we pm3.27 sylbi ) ABCABDZABEZFKABGJKHI $.
      $([16-Mar-97]$)

  $( The elements of an epsilon well-ordering are comparable. $)
  wecmpep $p |- ( ( E We A /\ ( x e. A /\ y e. A ) ) ->
                 ( x e. y \/ x = y \/ y e. x ) ) $=
      ( cep wor cv wcel wa wel weq w3o wwe wbr solin epel pm4.2 bi3or sylib
      weso sylan ) CDEZAFZCGBFZCGHZABIZABJZBAIZKZCDLUAUDHUBUCDMZUFUCUBDMZKUHCUB
      UCDNUIUEUFUFUJUGABOUFPBAOQRCDST $.
      $([17-May-94]$)

  $( An epsilon well-ordering is a transitive relation. $)
  wetrep $p |- ( ( E We A /\ ( x e. A /\ y e. A /\ z e. A ) ) ->
             ( ( x e. y /\ y e. z ) -> x e. z ) ) $=
      ( cep wwe cv wcel w3a wa wbr wel wor wi sotr weso sylan epel bi2an
      3imtr3g ) DEFZAGZDHBGZDHCGZDHIZJUBUCEKZUCUDEKZJZUBUDEKZABLZBCLZJACLDEMUEU
      HUINUADUBUCUDEODEPQUFUJUGUKABRBCRSACRT $.
