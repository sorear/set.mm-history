  $( A class belongs to omega iff its successor does. $)
  peano2b $p |- ( A e. om <-> suc A e. om ) $=
    ( com wcel csuc c0 cded wceq suceq eleq1d peano1 elimel peano2 dedth cvv
    wi elnn sucidg sylan exp wn sucprc biimprd pm2.61i impbi ) ABCZADZBCZUEUGUE
    AEFZDZBCAEAUHGUFUIBAUHHIUHAEBJKLMANCZUGUEOUJUGUEAUFCUGUEUJAUFPANQRSUJTZUEUG
    UKAUFBAUAIUBUCUD $.
    $([3-Dec-95]$)

  ${
    $d x A $.
    $( A natural number is either 0 or a successor. $)
    nn0suc $p |- ( A e. om -> ( A = (/) \/ E. x e. om A = suc x ) ) $=
      ( com wcel c0 wceq cv csuc wex wo wrex wa eleq1 biimpcd peano2b syl6ibr
      ancrd 19.22dv df-rex imlord wn wi nnsuc exp df-or sylibr mpd ) BCDZBEFZBA
      GZHZFZAIZJZUIULACKZJUHUMUOUIUHUMUJCDZULLZAIUOUHULUQAUHULUPUHULUKCDZUPULUH
      URBUKCMNUJOPQRULACSPTUHUIUAZUMUBUNUHUSUMABUCUDUIUMUEUFUG $.
      $([27-May-98]$)
  $}

  ${
    $d x A $.
    find.1 $e |- ( A (_ om /\ (/) e. A /\
                 A. x ( x e. om -> ( x e. A -> suc x e. A ) ) ) $.
    $( The Principle of Finite Induction (mathematical induction).  Corollary
       7.31 of Takeuti and Zaring, p. 43. $)
    find   $p |- A = om $=
      ( com wss c0 wcel cv csuc wi wal w3a 3simp1 ax-mp peano5 3adant1 eqssi )
      BDBDEZFBGZAHZDGTBGTIBGJJAKZLZRCRSUAMNUBDBEZCSUAUCRABOPNQ $.
      $([4-Aug-94]$)
  $}

  ${
    $d x y $.  $d x A $.  $d x ps $.  $d x ch $.  $d x th $.  $d x ta $.
    $d y ph $.
    $( Substitutions. $)
    finds.1 $e |- ( x = (/) -> ( ph <-> ps ) ) $.
    finds.2 $e |- ( x = y -> ( ph <-> ch ) ) $.
    finds.3 $e |- ( x = suc y -> ( ph <-> th ) ) $.
    finds.4 $e |- ( x = A -> ( ph <-> ta ) ) $.
    $( Basis. $)
    finds.5 $e |- ps $.
    $( Induction hypothesis. $)
    finds.6 $e |- ( y e. om -> ( ch -> th ) ) $.
    $( Principle of Finite Induction (inference schema). $)
    finds $p |- ( A e. om -> ta ) $=
      ( com wcel cab abclg c0 cv csuc wi wal wss peano5 zfnull abcl mpbir
      visset sucex 3imtr4g ax-gen mp2an ssi mpbid ) HOPHAFQZPEAEFHOLROUPHSUPPZG
      TZOPZURUPPZURUAZUPPZUBUBZGUCOUPUDGUPUEUQBABFSUFIUGMUHVCGUSCDUTVBNACFURGUI
      ZJUGADFVAURVDUJKUGUKULUMUNUO $.
      $([14-Apr-95]$)
  $}

  ${
    $d x A $.  $d x y B $.
    $d x ps $.  $d x ch $.  $d x th $.  $d x ta $. $d y ph $.
    $( Substitutions. $)
    findsgNEW.1 $e |- ( x = B -> ( ph <-> ps ) ) $.
    findsgNEW.2 $e |- ( x = y -> ( ph <-> ch ) ) $.
    findsgNEW.3 $e |- ( ( x = suc y /\ B (_ x ) -> ( ph <-> th ) ) $.
    findsgNEW.4 $e |- ( x = A -> ( ph <-> ta ) ) $.
    $( Basis. $)
    findsgNEW.5 $e |- ps $.
    $( Induction hypothesis. $)
    findsgNEW.6 $e |- ( ( ( y e. om /\ B e. om ) /\ B (_ y ) -> ( ch -> th ) ) $.
