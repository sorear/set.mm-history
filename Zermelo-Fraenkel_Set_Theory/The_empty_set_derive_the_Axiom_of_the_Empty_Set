    $( Restricted version of Theorem 19.45, p. 90, of Margaris. $)
    r19.45zv $p |- ( -. A = (/) ->
                   ( E. x e. A ( ph \/ ps ) <-> ( ph \/ E. x e. A ps ) ) ) $=
      ( c0 wceq wn wrex wo r19.9rzv birord r19.43 syl6rbbr ) DEFGZABCDHZIACDHZO
      IABICDHNAPOACDJKABCDLM $.
      $([27-May-98]$)
  $}

  ${
    $d x A $.
    $( Any wff is true when quantified over the empty set. $)
    rzal $p |- ( A = (/) -> A. x e. A ph ) $=
      ( c0 wceq cv wcel eleq2 noel pm2.21i syl6bi r19.21aiv ) CDEZABCMBFZCGNDGZ
      ACDNHOANIJKL $.
      $([11-Mar-97]$)

    $( Absorption law for restricted quantifier. $)
    ralabs $p |- ( A. x e. A ph <-> A. x e. A A. x e. A ph ) $=
      ( c0 wceq wral wb pm5.1 rzal sylan13 wn cv wcel wex nnull wi biimt wal
