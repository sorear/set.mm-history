  $( Ordinal 1 is an ordinal number. $)
  1o $p |- 1o e. On $=
    ( c1o c0 csuc con0 df-1o 0elon onsuc eqeltr ) ABCDEBFGH $.
    $( [29-Oct-95] $)

  $( Ordinal 2 is an ordinal number. $)
  2o $p |- 2o e. On $=
    ( c1o con0 wcel c2o 1o csuc sucelon df-2o eleq1i bitr4 mpbi ) ABCZDBCZELAFZ
    BCMAGDNBHIJK $.
    $( [19-Feb-04] $) $( [18-Feb-04] $)

  $( 1 + 1 = 2 for ordinal numbers. $)
  o1p1e2 $p |- ( 1o +o 1o ) = 2o $=
    ( c1o coa co csuc c2o con0 wcel wceq 1o oa1suc ax-mp df-2o eqtr4 ) AABCZADZ
    EAFGNOHIAJKLM $.
    $( [19-Feb-04] $) $( [18-Feb-04] $)

  $( Ordinal multiplication with 1.  Proposition 8.18(2) of [TakeutiZaring]
     p. 63. $)
  om1 $p |- ( A e. On -> ( A .o 1o ) = A ) $=
    ( con0 wcel c1o comu co c0 coa csuc wceq 0elon omsuc mpan2 df-1o opreq2i
    syl5eq om0 opreq1d oa0r 3eqtrd ) ABCZADEFZAGEFZAHFZGAHFAUAAGIZEFZUDUBUAGBCU
    FUDJKAGLMDUEAENOPUAUCGAHAQRAST $.
    $( [29-Oct-95] $)

  ${
    $d x y $.  $d x A $.
    $( Ordinal multiplication with 1.  Proposition 8.18(2) of [TakeutiZaring]
       p. 63. $)
    om1r $p |- ( A e. On -> ( 1o .o A ) = A ) $=
      ( vx vy c1o cv comu co wceq c0 csuc opreq2 id cleq12d weq om0x con0 wcel
      wa coa 1o omsuc mpan opreq1 sylan9eq oa1suc adantr eqtrd exp wlim ciun
      cuni wral cvv visset omlim limuni iuneq2 uniiun syl6eqr syl5bir tfinds )
      DBEZFGZVBHZDIFGZIHDCEZFGZVFHZDVFJZFGZVIHZDAFGZAHBCAVBIHZVCVEVBIVBIDFKVMLM
