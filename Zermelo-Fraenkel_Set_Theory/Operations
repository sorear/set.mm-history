$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Operations
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Extend the definition of a class to include operations. $)
  co $a class ( A F B ) $.

  $( Extend class notation to include abstraction classes of nested
     ordered pairs. $)
  copab2 $a class { <. <. x , y >. , z >. | ph } $.

  $( Define the result of an operation.  Here, ` F ` is an operation on two
     values (such as ` + ` for real numbers).   This is defined for proper
     classes ` A ` and ` B ` even though not meaningful in that case.  However,
     the definition can be meaningful when ` F ` is a proper class. $)
  df-opr $a |- ( A F B ) = ( F ` <. A , B >. ) $.

  ${
    $d x y z w $.  $d w ph $.
    $( Define abstraction class of nested ordered pairs (for use in defining
       operations).  A special case of Definition 4.16, p. 14, of Takeuti and
       Zaring. $)
    df-oprab $a |- { <. <. x , y >. , z >. | ph } =
                  { w | E. x E. y E. z ( w = <. <. x , y >. , z >. /\ ph ) } $.
  $}

  $( Equality theorem for operators. $)
  opreq $p |- ( F = G -> ( A F B ) = ( A G B ) ) $=
      ( wceq cop cfv co fveq1 df-opr 3eqtr4g ) CDEABFZCGLDGABCHABDHLCDIABCJABDJ
      K $.
      $([28-Feb-95]$)

  $( Equality theorem for operators. $)
  opreq1 $p |- ( A = B -> ( A F C ) = ( B F C ) ) $=
      ( wceq cop cfv co opeq1 fveq2 syl df-opr 3eqtr4g ) ABEZACFZDGZBCFZDGZACDH
      BCDHNOQEPREABCIOQDJKACDLBCDLM $.
      $([28-Feb-95]$)

  $( Equality theorem for operators. $)
  opreq2 $p |- ( A = B -> ( C F A ) = ( C F B ) ) $=
      ( wceq cop cfv co opeq2 fveq2 syl df-opr 3eqtr4g ) ABEZCAFZDGZCBFZDGZCADH
      CBDHNOQEPREABCIOQDJKCADLCBDLM $.
      $([28-Feb-95]$)

  $( Equality theorem for operators. $)
  opreq12 $p |- ( ( A = B /\ C = D ) -> ( A F C ) = ( B F D ) ) $=
      ( wceq co opreq1 opreq2 sylan9eq ) ABFCDFACEGBCEGBDEGABCEHCDBEIJ $.
      $([16-Jul-95]$)

  ${
    opreq1i.1 $e |- A = B $.
    $( Equality inference for operators. $)
    opreq1i   $p |- ( A F C ) = ( B F C ) $=
      ( wceq co opreq1 ax-mp ) ABFACDGBCDGFABCDHEI $.
      $([28-Feb-95]$)

    $( Equality inference for operators. $)
    opreq2i   $p |- ( C F A ) = ( C F B ) $=
      ( wceq co opreq2 ax-mp ) ABFCADGCBDGFABCDHEI $.
      $([28-Feb-95]$)

    ${
      opreq12i.2 $e |- C = D $.
      $( Equality inference for operators. $)
      opreq12i   $p |- ( A F C ) = ( B F D ) $=
      ( co opreq1i opreq2i eqtr ) ACEHBCEHBDEHABCEFICDBEGJK $.
      $([28-Feb-95]$)
    $}
  $}

  ${
    opreq1d.1 $e |- ( ph -> A = B ) $.
    $( Equality inference for operators. $)
    opreq1d   $p |- ( ph -> ( A F C ) = ( B F C ) ) $=
      ( wceq co opreq1 syl ) ABCGBDEHCDEHGFBCDEIJ $.
      $([13-Mar-95]$)

    $( Equality inference for operators. $)
    opreq2d   $p |- ( ph -> ( C F A ) = ( C F B ) ) $=
      ( wceq co opreq2 syl ) ABCGDBEHDCEHGFBCDEIJ $.
      $([13-Mar-95]$)

    ${
      opreq12d.2 $e |- ( ph -> C = D ) $.
      $( Equality inference for operators. $)
      opreq12d   $p |- ( ph -> ( A F C ) = ( B F D ) ) $=
      ( co opreq1d opreq2d eqtrd ) ABDFICDFICEFIABCDFGJADECFHKL $.
      $([13-Mar-95]$)
    $}

    ${
      opreqan12i.2 $e |- ( ps -> C = D ) $.
      $( Equality inference for operators. $)
      opreqan12d   $p |- ( ( ph /\ ps ) -> ( A F C ) = ( B F D ) ) $=
      ( wceq co opreq12 syl2an ) CDJEFJCEGKDFGKJABCDEFGLHIM $.
      $([10-Aug-95]$)

      $( Equality inference for operators. $)
      opreqan12rd   $p |- ( ( ps /\ ph ) -> ( A F C ) = ( B F D ) ) $=
      ( co wceq opreqan12d ancoms ) ABCEGJDFGJKABCDEFGHILM $.
      $([10-Aug-95]$)
    $}

  $}

  $( The result of an operation is a set. $)
  oprex $p |- ( A F B ) e. V $=
      ( co cop cfv cvv df-opr fvex eqeltr ) ABCDABEZCFGABCHKCIJ $.
      $([13-Mar-95]$)

  ${
    $d x y z w v $.  $d w ph v $.  $( ` v ` is dummy. $)
    $( Abstraction class for operators in terms of abstraction class of
       ordered pairs. $)
    dfoprab2 $p |- { <. <. x , y >. , z >. | ph } =
                   { <. w , z >. | E. x E. y ( w = <. x , y >. /\ ph ) } $=
      ( vv cv cop wceq wa wex cab copab2 copab 19.11 exrot4 19.42v opeq1
      cleq2d pm5.32ri biran anass an1r 3bitr3 biex opex isseti biantru 3bitr4
      bi3ex bitr 19.42vv bi2ex biab df-oprab df-opab 3eqtr4 ) FGZBGZCGZHZDGZHZI
      ZAJZDKCKBKZFLUREGZVBHZIZVGVAIZAJZCKBKZJZDKEKZFLABCDMVLEDNVFVNFVIVKJZCKBKZ
      EKDKZVPDKEKVFVNVPDEOVQVOEKZDKCKBKVFVODEBCPVRVEBCDVEVJJZEKVEVJEKZJVRVEVEVJ
      EQVOVSEVIVJJZAJVDVJJZAJVOVSWAWBAVJVIVDVJVHVCURVGVAVBRSTUAVIVJAUBVDVJAUCUD
      UEVTVEEVAUSUTUFUGUHUIUJUKVPVMEDVIVKBCULUMUDUNABCDFUOVLEDFUPUQ $.
      $([12-Mar-95]$)
  $}

  ${
    $d x y z v $.  $d x w $.  $d ph v $.  $( ` v ` is dummy. $)
    $( The abstraction variables in an operator abstraction are not free. $)
    hboprab1 $p |- ( w e. { <. <. x , y >. , z >. | ph } ->
                   A. x w e. { <. <. x , y >. , z >. | ph } ) $=
      ( vv cv cop wceq wa wex cab wcel wal copab2 hbe1 hbab df-oprab cleq4i
      bial 3imtr4 ) EGZFGBGCGHDGHIAJDKCKZBKZFLZMZUFBNUBABCDOZMZUHBNUDBFEUCBPQUG
      UEUBABCDFRSZUHUFBUITUA $.
      $([25-Apr-95]$)
  $}

  ${
    $d x y z v $.  $d y w $.  $d ph v $.  $( ` v ` is dummy. $)
    $( The abstraction variables in an operator abstraction are not free. $)
    hboprab2 $p |- ( w e. { <. <. x , y >. , z >. | ph } ->
                   A. y w e. { <. <. x , y >. , z >. | ph } ) $=
      ( vv cv cop wceq wa wex cab wcel wal copab2 hbe1 hbex hbab df-oprab
      cleq4i bial 3imtr4 ) EGZFGBGCGHDGHIAJDKZCKZBKZFLZMZUHCNUCABCDOZMZUJCNUFCF
      EUECBUDCPQRUIUGUCABCDFSTZUJUHCUKUAUB $.
