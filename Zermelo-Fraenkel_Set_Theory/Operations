  $}

  ${
    $d x y z w $.  $d w ph $.  $d w ps $.  $( ` w ` is dummy. $)
    bioprab.1 $e |- ( ph <-> ps ) $.
    $( Equivalent wff's yield equal abstraction classes. $)
    bioprab   $p |- { <. <. x , y >. , z >. | ph } =
                    { <. <. x , y >. , z >. | ps } $=
      ( vw cv cop wceq wa wex copab copab2 bilan bi2ex biopab dfoprab2 3eqtr4
      ) GHCHDHIJZAKZDLCLZGEMTBKZDLCLZGEMACDENBCDENUBUDGEUAUCCDABTFOPQACDEGRBCDE
      GRS $.
      $([28-May-95]$)
  $}

  ${
    $d x y z w $.  $d w ph $.  $( ` w ` is dummy. $)
    $( The domain of an operator abstraction. $)
    dmoprab $p |- dom { <. <. x , y >. , z >. | ph } =
                  { <. x , y >. | E. z ph } $=
      ( vw copab2 cdm cv cop wceq wa wex copab cab dmeq dfoprab2 ax-mp dmopab
      exrot3 19.42v bi2ex bitr biab df-opab eqtr4 3eqtr ) ABCDFZGZEHBHCHIJZAKZC
      LBLZEDMZGZUKDLZENZADLZBCMZUGULJUHUMJUGULOABCDEPQUKEDRUOUIUPKZCLBLZENUQUNU
      SEUNUJDLZCLBLUSUJDBCSUTURBCUIADTUAUBUCUPBCEUDUEUF $.
      $([17-Mar-95]$)
  $}

  ${
    $d x y z A $.  $d x y z B $.
    $( The domain of an operator abstraction. $)
    dmoprabss $p |- dom { <. <. x , y >. , z >. |
           ( ( x e. A /\ y e. B ) /\ ph ) } (_ ( A X. B ) $=
      ( cv wcel wa copab2 cdm wex copab cxp dmoprab 19.42v biopab opabssxp
      eqsstr ) BGEHCGFHIZAIZBCDJKUADLZBCMZEFNZUABCDOUCTADLZIZBCMUDUBUFBCTADPQUE
      BCEFRSS $.
      $([24-Aug-95]$)
  $}

  ${
    $d x y z $.
    $( The domain of an operator abstraction class is a relation. $)
    reldmoprab $p |- Rel dom { <. <. x , y >. , z >. | ph } $=
      ( copab2 cdm wrel wex copab wceq wb releq dmoprab ax-mp relopab mpbir )
      ABCDEFZGZADHZBCIZGZQTJRUAKQTLABCDMNSBCOP $.
      $([17-Mar-95]$)
  $}

  ${
    $d x y z w A $.  $d x y z w B $.  $d x y z w C $.
    $d x y z w R $.  $d w D $.
    $d w ph $.  $d x ps $.  $d x y ch $.  $d x y z w th $.  $( ` w ` is dummy. $)
    eloprabg.1 $e |- C e. V $.
    eloprabg.2 $e |- ( x = A -> ( ph <-> ps ) ) $.
    eloprabg.3 $e |- ( y = B -> ( ps <-> ch ) ) $.
    eloprabg.4 $e |- ( z = C -> ( ch <-> th ) ) $.
