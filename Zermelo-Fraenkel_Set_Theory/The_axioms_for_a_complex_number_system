  $( 1 and 0 are distinct.  One of the 28 axioms for real and complex numbers,
     derived from ZF set theory. $)
  ax1ne0 $p |- 1 =/= 0 $=
    ( c1 cc0 wne wceq wn c1r c0r cop 1ne0sr cnr 1r elisseti eqresr mtbir df-1
    df-0 cleq12i df-ne mpbir ) ABCABDZETFGHZGGHZDZUCFGDIFGFJKLMNAUABUBOPQNABRS
    $.
    $([19-Mar-96]$)

  ${
    $d x y A $.  $( ` x y ` are dummy. $)
    $( 0 is an identity element for addition.  One of the 28 axioms for real
       and complex numbers, derived from ZF set theory. $)
    ax0id $p |- ( A e. CC -> ( A + 0 ) = A ) $=
      ( vx vy cv cop cc0 caddc co wceq cnr cc df-c opreq1 id cleq12d wcel wa
      c0r cplr 0r pm3.2i addcnsr mpan2 opeq12 0idsr syl2an eqtrd df-0 opreq2i
      syl5eq optocl ) BDZCDZEZFGHZUNIAFGHZAIBCAJJKLUNAIZUOUPUNAUNAFGMUQNOULJPZU
