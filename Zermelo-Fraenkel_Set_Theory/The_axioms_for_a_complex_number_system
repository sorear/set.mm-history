$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        The axioms for a complex number system
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( The class of complex numbers is a set. $)
  axcnex $p |- CC e. V $=
      ( cc cnr cxp cvv df-c nrex xpex eqeltr ) ABBCDEBBFFGH $.
      $([13-Aug-95]$)

  $( The real numbers are a subset of the complex numbers. $)
  axresscn $p |- RR (_ CC $=
      ( cnr c0r csn cxp cr cc wss ssxp ssid wcel snssi 0r ax-mp mp2an df-r
      df-c 3sstr4 ) ABCZDZAADZEFAAGRAGZSTGAARAHAIBAJUABAKLMNOPQ $.
      $([1-Mar-95]$)

  $( 0 is a real number.  Note that the subscript ` _10 `, meaning base 10,
     indicates the end of a digit string to eliminate ambiguity $)
  ax0re $p |- 0 _10 e. RR $=
      ( cn0 c0r cop cr df-0 wcel cnr opelreal 0r mpbir eqeltr ) ABBCZDELDFBGFBH
      IJK $.
      $([1-Mar-95]$)

  $( 1 is a real number. $)
  ax1re $p |- 1 _10 e. RR $=
      ( cn1 c1r c0r cop cr df-1 wcel cnr opelreal 1r mpbir eqeltr ) ABCDZEFMEGB
      HGBIJKL $.
      $([1-Mar-95]$)

  $( ` i ` is a complex number. $)
  axicn $p |- i e. CC $=
      ( cni cc wcel c0r cnr c1r cop wa df-i cleq3i 1r elisseti opelcn bitr
      mpbiranr 0r mpbir ) ABCZDECZRSFECZRDFGZBCSTHAUABIJDFFEKLMNKOPQ $.
      $([23-Feb-96]$)

  ${
    $d x y z w A $.  $d x y z w B $.  $( ` x y z w ` are dummy. $)
    $( Closure law for addition of complex numbers. $)
    axaddcl $p |- ( ( A e. CC /\ B e. CC ) -> ( A + B ) e. CC ) $=
      ( vx vy vz vw cc wcel wa caddc co cnr cxp cv cop df-c wceq opreq1 cleq3d
      opreq2 cplr addcnsr addclsr im2an an4s opelxpi syl mpbird 2optocl cleq4i
      sylibr ) AGHBGHIABJKZLLMZHZULGHCNZDNZOZENZFNZOZJKZUMHZAUTJKZUMHUNCDEFABLL
      GPUQAQVAVCUMUQAUTJRSUTBQVCULUMUTBAJTSUOLHZUPLHZIURLHZUSLHZIIZVBUOURUAKZUP
      USUAKZOZUMHZVHVAVKUMUOUPURUSUBSVHVILHZVJLHZIZVLVDVFVEVGVOVDVFIVMVEVGIVNUO
      URUCUPUSUCUDUEVIVJLLUFUGUHUIGUMULPUJUK $.
      $([14-Jun-95]$)
  $}

  ${
    $d x y A $.  $d x y B $.  $( ` x y ` are dummy. $)
    $( Closure law for addition of real subfield of complex numbers. $)
    axaddrcl $p |- ( ( A e. RR /\ B e. RR ) -> ( A + B ) e. RR ) $=
      ( vx vy cv c0r cop caddc co cr wcel cnr elreal wceq opreq1 cleq3d
      opreq2 wa cplr addresr addclsr opelreal sylibr mpbird 2gencl ) CEZFGZDEZ
      FGZHIZJKZAUIHIZJKABHIZJKCDUGUIABLJCAMDBMUGANUJULJUGAUIHOPUIBNULUMJUIBAHQP
      UFLKUHLKRZUKUFUHSIZFGZJKZUNUJUPJUFUHTPUNUOLKUQUFUHUAUOUBUCUDUE $.
      $([31-Mar-96]$)
  $}

  ${
    $d x y z w A $.  $d x y z w B $.  $( ` x y z w ` are dummy. $)
    $( Closure law for multiplication of complex numbers. $)
    axmulcl $p |- ( ( A e. CC /\ B e. CC ) -> ( A x. B ) e. CC ) $=
      ( vx vy vz vw cc wcel wa cmulc co cnr cxp cv cop df-c wceq opreq1 cleq3d
      opreq2 cmr cm1r cplr mulcnsr addclsr mulclsr m1r mpan syl syl2an an4s
      ancoms an41r3s jca opelxpi mpbird 2optocl cleq4i sylibr ) AGHBGHIABJKZLLM
      ZHZUTGHCNZDNZOZENZFNZOZJKZVAHZAVHJKZVAHVBCDEFABLLGPVEAQVIVKVAVEAVHJRSVHBQ
      VKUTVAVHBAJTSVCLHZVDLHZIVFLHZVGLHZIIZVJVCVFUAKZUBVDVGUAKZUAKZUCKZVDVFUAKZ
      VCVGUAKZUCKZOZVAHZVPVIWDVAVCVDVFVGUDSVPVTLHZWCLHZIWEVPWFWGVLVNVMVOWFVQLHV
      SLHZWFVLVNIVMVOIZVQVSUEVCVFUFWIVRLHZWHVDVGUFUBLHWJWHUBVRUFUGUHUIUJUKVLVOV
      MVNWGVMVNIZVLVOIZWGWALHWBLHWGWKWLWAWBUEVDVFUFVCVGUFUJULUMUNVTWCLLUOUIUPUQ
      GVAUTPURUS $.
      $([10-Aug-95]$)
  $}

  ${
    $d x y A $.  $d x y B $.  $( ` x y ` are dummy. $)
    $( Closure law for multiplication of real subfield of complex numbers. $)
    axmulrcl $p |- ( ( A e. RR /\ B e. RR ) -> ( A x. B ) e. RR ) $=
      ( vx vy cv c0r cop cmulc co cr wcel cnr elreal wceq opreq1 cleq3d
      opreq2 wa cmr visset mulresr mulclsr opelreal sylibr mpbird 2gencl ) CEZ
      FGZDEZFGZHIZJKZAUJHIZJKABHIZJKCDUHUJABLJCAMDBMUHANUKUMJUHAUJHOPUJBNUMUNJU
      JBAHQPUGLKUILKRZULUGUISIZFGZJKZUOUKUQJUGUIDTUAPUOUPLKURUGUIUBUPUCUDUEUF
      $.
      $([31-Mar-96]$)
  $}

  ${
    $d x y z w A $.  $d x y z w B $.  $( ` x y z w ` are dummy. $)
    $( Addition of complex numbers is commutative. $)
    axaddcom $p |- ( ( A e. CC /\ B e. CC ) -> ( A + B ) = ( B + A ) ) $=
      ( vx vy vz vw cc cv cplr co cep ccnv cnr caddc dfcnqs addcnsrec visset
      addcomsr ecoprcom ) CDEFABGCHZEHZIJKLMNDHZFHZIJUATIJUCUBIJOTUBUAUCPUAUCTU
      BPTUACQEQRUBUCDQFQRS $.
      $([31-Aug-95]$)
  $}

  ${
    $d x y z w A $.  $d x y z w B $.  $( ` x y z w ` are dummy. $)
    $( Multiplication of complex numbers is commutative. $)
    axmulcom $p |- ( ( A e. CC /\ B e. CC ) -> ( A x. B ) = ( B x. A ) ) $=
      ( vx vy vz vw cc cv cmr co cm1r cplr cep ccnv cnr cmulc dfcnqs mulcnsrec
      visset mulcomsr opreq2i opreq12i oprex addcomsr eqtr ecoprcom ) CDEFABGCH
      ZEHZIJZKDHZFHZIJZIJZLJMNOPUJUHIJZUGUKIJZLJZUHUGIJZKUKUJIJZIJZLJUKUGIJZUHU
      JIJZLJZQUGUJUHUKRUHUKUGUJRUIUQUMUSLUGUHCSZESZTULURKIUJUKDSZFSZTUAUBUPVAUT
      LJVBUNVAUOUTLUJUHVEVDTUGUKVCVFTUBVAUTUHUJIUCUKUGIUCUDUEUF $.
      $([31-Aug-95]$)
  $}

  ${
    $d x y z w v u A $.  $d x y z w v u B $.  $d x y z w v u C $.
      $( ` x y z w v u ` are dummy. $)
    $( Addition of complex numbers is associative. $)
    axaddass $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
                 ( ( A + B ) + C ) = ( A + ( B + C ) ) ) $=
      ( vx vy vz vw vv vu cc cep ccnv cnr caddc cv cplr co dfcnqs addcnsrec
      wcel wa addclsr im2an an4s visset addasssr ecoprass ) DEFGHIABCJKLMNDOZFO
      ZPQZEOZGOZPQZUJHOZPQUMIOZPQUHUIUNPQZPQUKULUOPQZPQUPUQRUHUKUIULSUIULUNUOSU
      JUMUNUOSUHUKUPUQSUHMTZUIMTZUKMTZULMTZUJMTZUMMTZUAURUSUAVBUTVAUAVCUHUIUBUK
      ULUBUCUDUSUNMTZVAUOMTZUPMTZUQMTZUAUSVDUAVFVAVEUAVGUIUNUBULUOUBUCUDUHUIUNF
      UEHUEUFUKULUOGUEIUEUFUG $.
      $([2-Sep-95]$)
  $}

  ${
    $d x y z w v u A $.  $d x y z w v u B $.  $d x y z w v u C $.
    $d x y z w v u f g h $.  $( ` x y z w v u f g h ` are dummy. $)
    $( Multiplication of complex numbers is associative. $)
    axmulass $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
                 ( ( A x. B ) x. C ) = ( A x. ( B x. C ) ) ) $=
      ( vx vy vz vw vv vu vf vg vh cc cep ccnv cnr cmulc cv cmr co cm1r cplr
      dfcnqs mulcnsrec wcel wa addclsr mulclsr m1r mpan syl syl2an an4s ancoms
      an41r3s jca oprex visset addcomsr addasssr caopr41r3 distrsr opreq2i
      eqtr opreq12i elisseti mulcomsr mulasssr caoprdilem caopr12 3eqtr
      3eqtr4r ecoprass ) DEFGHIABCMNOPQDRZFRZSTZUAERZGRZSTZSTZUBTZVQVOSTZVNVRST
      ZUBTZWAHRZSTZUAWDIRZSTZSTZUBTZWDWESTZWAWGSTZUBTZVNVOWESTZUAVRWGSTZSTZUBTZ
