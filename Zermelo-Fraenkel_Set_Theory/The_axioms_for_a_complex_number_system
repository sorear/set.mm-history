  $( 1 and 0 are distinct. $)
  ax1ne0 $p |- 1 _10 =/= 0 _10 $=
      ( cn1 cn0 wne wceq wn df-ne c1r c0r cop df-1 df-0 cleq12i cnr 1r
      elisseti eqresr 1ne0sr mtbir mpbir ) ABCABDZEABFTGHIZHHIZDZAUABUBJKLUCGHD
      GHGMNOPQRRS $.
      $([19-Mar-96]$)

  ${
    $d x y A $.  $( ` x y ` are dummy. $)
    $( 0 is an identity element for addition. $)
    ax0id $p |- ( A e. CC -> ( A + 0 _10 ) = A ) $=
      ( vx vy cv cop cn0 caddc co wceq cnr cc df-c opreq1 id cleq12d wcel wa
      c0r cplr addcnsr 0r pm3.2i mpan2 opeq12 0idsr syl2an eqtrd df-0 opreq2i
      syl5eq optocl ) BDZCDZEZFGHZUNIAFGHZAIBCAJJKLUNAIZUOUPUNAUNAFGMUQNOULJPZU
      MJPZQZUNRREZGHZUNUOUTVBULRSHZUMRSHZEZUNUTRJPZVFQVBVEIULUMRRTVFVFUAUAUBUCV
      CULIVDUMIVEUNIURUSVCVDULUMUDULUEUMUEUFUGFVAUNGUHUIUJUK $.
      $([31-Mar-96]$)
  $}

  ${
    $d x y A $.  $( ` y ` is dummy. $)
    $( 1 is an identity element for multiplication. $)
    ax1id $p |- ( A e. CC -> ( A x. 1 _10 ) = A ) $=
      ( vx vy cv cop cn1 cmulc co wceq cnr cc df-c opreq1 id cleq12d wcel wa
      c1r c0r cmr cm1r cplr mulcnsr 1r 0r pm3.2i mpan2 00sr opreq2d m1r ax-mp
      syl6eq 1idsr opreq1d 0idsr eqtrd sylan9req sylan9eq jca opeq12 syl
      df-1 opreq2i syl5eq optocl ) BDZCDZEZFGHZVHIAFGHZAIBCAJJKLVHAIZVIVJVHAVHA
      FGMVKNOVFJPZVGJPZQZVHRSEZGHZVHVIVNVPVFRTHZUAVGSTHZTHZUBHZVGRTHZVFSTHZUBHZ
      EZVHVNRJPZSJPZQVPWDIVFVGRSUCWEWFUDUEUFUGVNVTVFIZWCVGIZQWDVHIVNWGWHVMVLVTV
