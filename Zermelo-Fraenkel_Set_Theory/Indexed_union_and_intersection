  ${
    iuneq2i.1 $e |- ( x e. A -> B = C ) $.
    $( Equality inference for indexed union. $)
    iuneq2i $p |- U. x e. A B = U. x e. A C $=
      ( wceq wral ciun rgen iuneq2 ax-mp ) CDFZABGABCHABDHFLABEIABCDJK $.
      $( [3-Nov-03] $) $( [22-Oct-03] $)

    $( Equality inference for indexed intersection. $)
    iineq2i $p |- |^| x e. A B = |^| x e. A C $=
      ( wceq wral ciin rgen iineq2 ax-mp ) CDFZABGABCHABDHFLABEIABCDJK $.
      $( [4-Nov-03] $) $( [22-Oct-03] $)
  $}

  ${
    $d x ph $.
    iuneq2dv.1 $e |- ( ph -> ( x e. A -> B = C ) ) $.
    $( Equality deduction for indexed union. $)
    iuneq2dv $p |- ( ph -> U. x e. A B = U. x e. A C ) $=
      ( wceq wral ciun r19.21aiv iuneq2 syl ) ADEGZBCHBCDIBCEIGAMBCFJBCDEKL $.
      $( [5-Aug-04] $) $( [3-Aug-04] $)

    $( Equality deduction for indexed intersection. $)
    iineq2dv $p |- ( ph -> |^| x e. A B = |^| x e. A C ) $=
