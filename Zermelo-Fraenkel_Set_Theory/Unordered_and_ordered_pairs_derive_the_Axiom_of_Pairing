  ${
    tpi3.1 $e |- C e. V $.
    $( One of the three elements of an unordered triple. $)
    tpi3 $p |- C e. { A , B , C } $=
      ( csn wcel ctp snid cpr cun elun2 df-tp eleq2i sylibr ax-mp ) CCEZFZCABCG
      ZFZCDHQCABIZPJZFSCPTKRUACABCLMNO $.
      $( [7-Apr-94] $)
