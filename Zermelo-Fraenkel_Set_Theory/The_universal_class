    vtocl2gf   $p |- ( ( A e. C /\ B e. D ) -> ch ) $=
      ( wcel wi ax-17 hbim cv wceq bilimd vtoclgf comm imp ) FHOZGIOZCUFUECUEBP
      UECPEGIUECEUEEQKRESGTBCUEMUAABDFHJLNUBUBUCUD $.
