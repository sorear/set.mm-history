    $( Implicit substitution of 3 classes for 3 set variables. $)
    vtocl3ga   $p |- ( ( A e. D /\ B e. R /\ C e. S ) -> th ) $=
      ( wcel wi cv wceq bilimd wa 3exp imp3a vtoclga comm vtocl2ga exp com3r
      3imp ) HKRZILRZJMRZDUMUNULDUMUNULDSZULBSULCSUOFGIJLMFTZIUABCULOUBGTZJUACD
      ULPUBULUPLRZUQMRZUCZBUTASUTBSEHKETZHUAABUTNUBVAKRZURUSAVBURUSAQUDUEUFUGUH
      UIUJUK $.
      $([20-Aug-95]$)
  $}

  ${
