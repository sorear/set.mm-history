$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        The universal class
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Declare the symbol for the universal class. $)
  $c V $. $( The letter V $)

  $( Extend class notation to include the universal class symbol. $)
  cvv $a class V $.

  $( Define the universal class.  Definition 5.20, p. 21, of Takeuti and
     Zaring. $)
  df-v    $a |- V = { x | x = x } $.

  $( All set variables are sets (see ~ isset ).  Theorem 6.8 of Quine, p. 43. $)
  visset $p |- x e. V $=
    ( cv cvv wcel weq eqid df-v cleqabi mpbir ) ABCDAAEZAFJACAGHI $.
    $([5-Aug-93]$)

  ${
    $d x A $.
    $( Two ways to say " ` A ` is a set":  A class ` A ` is a member of
       the universal class ` V ` (see ~ df-v ) if and only if the class ` A `
       exists (i.e. there exists some set ` x ` equal to class ` A ` ).  We
       will use the notational device " ` A e. V ` " to mean "` A ` is a set"
       very frequently.  Note the when ` A ` is not a set, it is called a
       proper class.  Theorem 6.9 of Quine, p. 43. $)
    isset $p |- ( A e. V <-> E. x x = A ) $=
      ( cvv wcel cv wceq wa wex df-clel visset biantru biex bitr4 ) BCDAEZBFZNC
      DZGZAHOAHABCIOQAPOAJKLM $.
      $([5-Aug-93]$)
  $}

  ${
    $d x A $.
    isseti.1 $e |- A e. V $.
    $( A way to say "` A ` is a set" (inference rule). $)
    isseti   $p |- E. x x = A $=
      ( cvv wcel cv wceq wex isset mpbi ) BDEAFBGAHCABIJ $.
      $([5-Aug-93]$)
  $}

  ${
    $d x A $.
    issetri.1 $e |- E. x x = A $.
    $( A way to say "` A ` is a set" (inference rule). $)
    issetri   $p |- A e. V $=
      ( cvv wcel cv wceq wex isset mpbir ) BDEAFBGAHCABIJ $.
      $([5-Aug-93]$)
  $}

  ${
    $d x A $.  $d x B $.  $( ` x ` is dummy. $)
    $( If a class is a member of another class, it is a set.  Theorem 6.12
       of Quine, p. 44. $)
    elisset $p |- ( A e. B -> A e. V ) $=
      ( vx wcel cv wceq wex cvv wa df-clel 19.40 sylbi pm3.26d isset sylibr )
      ABDZCEZAFZCGZAHDPSQBDZCGZPRTICGSUAICABJRTCKLMCANO $.
      $([5-Aug-93]$)
  $}

  ${
    elisseti.1 $e |- A e. B $.
    $( If a class is a member of another class, it is a set. $)
    elisseti $p |- A e. V $=
      ( wcel cvv elisset ax-mp ) ABDAEDCABFG $.
      $([11-Jun-94]$)
  $}

  ${
    $d x A $.
    $( An element of a class exists. $)
    elex $p |- ( A e. B -> E. x x = A ) $=
      ( wcel cvv cv wceq wex elisset isset sylib ) BCDBEDAFBGAHBCIABJK $.
      $([1-May-95]$)
  $}

  ${
    $d x A $.
    ceqsal.1 $e |- ( ps -> A. x ps ) $.
    ceqsal.2 $e |- A e. V $.
    ceqsal.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( An inference with implicit substitution. $)
    ceqsal $p |- ( A. x ( x = A -> ph ) <-> ps ) $=
      ( cv wceq wi wal wex isseti biimpd a2i 19.20i 19.23 sylib mpi biimprcd
      19.21ai impbi ) CHDIZAJZCKZBUEUCCLZBCDFMUEUCBJZCKUFBJUDUGCUCABUCABGNOPUCB
      CEQRSBUDCEUCABGTUAUB $.
      $([18-Aug-93]$)
  $}

  ${
    $d x A $.  $d x ps $.
    ceqsalv.1 $e |- A e. V $.
    ceqsalv.2 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( An inference with implicit substitution. $)
    ceqsalv $p |- ( A. x ( x = A -> ph ) <-> ps ) $=
      ( ax-17 ceqsal ) ABCDBCGEFH $.
      $([18-Aug-93]$)
  $}

  ${
    $d x ps $.
    gencl.1 $e |- ( th <-> E. x ( ch /\ A = B ) ) $.
    gencl.2 $e |- ( A = B -> ( ph <-> ps ) ) $.
    gencl.3 $e |- ( ch -> ph ) $.
    $( Implicit substitution for class with embedded variable. $)
    gencl   $p |- ( th -> ps ) $=
      ( wceq wa wex syl5bi comm imp 19.23aiv sylbi ) DCFGKZLZEMBHTBECSBSCBSABCI
      JNOPQR $.
      $([17-May-96]$)
  $}

  ${
    $d x y $.  $d x R $.  $d x ps $.  $d y C $.  $d y S $.  $d y ch $.
    2gencl.1 $e |- ( C e. S <-> E. x ( x e. R /\ A = C ) ) $.
    2gencl.2 $e |- ( D e. S <-> E. y ( y e. R /\ B = D ) ) $.
    2gencl.3 $e |- ( A = C -> ( ph <-> ps ) ) $.
    2gencl.4 $e |- ( B = D -> ( ps <-> ch ) ) $.
    2gencl.5 $e |- ( ( x e. R /\ y e. R ) -> ph ) $.
    $( Implicit substitution for class with embedded variable. $)
    2gencl   $p |- ( ( C e. S /\ D e. S ) -> ch ) $=
      ( wcel wi cv wceq bilimd exp gencl comm imp ) HKQZIKQZCUGUFCUFBRUFCRESJQZ
      UGEGIMGITBCUFOUAUFUHBUHARUHBRDSJQZUFDFHLFHTABUHNUAUIUHAPUBUCUDUCUDUE $.
      $([17-May-96]$)
  $}

  ${
    $v F $.
    $v G $.
    3gencl.F $f class F $.
    3gencl.G $f class G $.
    $d x y z $.  $d y z D $.  $d z F $.  $d x y R $.  $d y z S $.
    $d x ps $.  $d y ch $.  $d z th $.
    3gencl.1 $e |- ( D e. S <-> E. x ( x e. R /\ A = D ) ) $.
    3gencl.2 $e |- ( F e. S <-> E. y ( y e. R /\ B = F ) ) $.
    3gencl.3 $e |- ( G e. S <-> E. z ( z e. R /\ C = G ) ) $.
    3gencl.4 $e |- ( A = D -> ( ph <-> ps ) ) $.
    3gencl.5 $e |- ( B = F -> ( ps <-> ch ) ) $.
    3gencl.6 $e |- ( C = G -> ( ch <-> th ) ) $.
    3gencl.7 $e |- ( ( x e. R /\ y e. R /\ z e. R ) -> ph ) $.
    $( Implicit substitution for class with embedded variable. $)
    3gencl   $p |- ( ( D e. S /\ F e. S /\ G e. S ) -> th ) $=
      ( wcel wi wa cv wceq bilimd 3exp imp 2gencl comm gencl exp 3imp ) KMUCZNM
      UCZOMUCZDUPUQURDUDURUPUQUEZDUSCUDUSDUDGUFLUCZURGJORJOUGCDUSUAUHUSUTCUTAUD
      ZUTBUDUTCUDEFHIKNLMPQHKUGABUTSUHINUGBCUTTUHEUFLUCZFUFLUCZVAVBVCUTAUBUIUJU
      KULUMULUNUO $.
      $([17-May-96]$)
  $}

  ${
    $d x y ps $.  $d x y A $.  $d x y B $.
    cgsex2g.1 $e |- ( ( x = A /\ y = B ) -> ch ) $.
    cgsex2g.2 $e |- ( ch -> ( ph <-> ps ) ) $.
    $( Implicit substitution inference for general classes. $)
    cgsex2g   $p |- ( ( A e. C /\ B e. D ) ->
                     ( E. x E. y ( ch /\ ph ) <-> ps ) ) $=
      ( wcel wa wex wi biimpa 19.23aivv a1i biimprcd ancld 19.22dvv cv wceq
      elex im2an eeanv sylibr 19.22i syl syl5 comm impbid ) FHLZGILZMZCAMZENDNZ
      BUQBOUOUPBDECABKPQRBUOUQBCENZDNZUQUOBCUPDEBCACABKSTUAUODUBFUCZEUBGUCZMZEN
      ZDNZUSUOUTDNZVAENZMVDUMVEUNVFDFHUDEGIUDUEUTVADEUFUGVCURDVBCEJUHUHUIUJUKUL
      $.
      $([26-Jul-95]$)
  $}

  ${
    $d x y z w A $.  $d x y z w B $.  $d x y z w C $.  $d x y z w D $.
    $d x y z w ps $.
    cgsex4g.1 $e |- ( ( ( x = A /\ y = B ) /\ ( z = C /\ w = D ) ) -> ch ) $.
    cgsex4g.2 $e |- ( ch -> ( ph <-> ps ) ) $.
    $( An implicit substitution inference for 4 general classes. $)
    cgsex4g   $p |- ( ( ( A e. R /\ B e. S ) /\ ( C e. R /\ D e. S ) ) ->
                    ( E. x E. y E. z E. w ( ch /\ ph ) <-> ps ) ) $=
      ( wcel wa wex wi biimpa 19.23aivv a1i biimprcd ancld 19.22dvv cv wceq
      elex im2an eeanv sylibr ee4anv 19.22i syl syl5 comm impbid ) HLPZIMPZQZJL
      PZKMPZQZQZCAQZGRFRZERDRZBVGBSVDVFBDEVEBFGCABOTUAUAUBBVDVGBCGRZFRZERZDRZVG
      VDBVIVFDEBCVEFGBCACABOUCUDUEUEVDDUFHUGZEUFIUGZQZFUFJUGZGUFKUGZQZQZGRZFRZE
      RZDRZVKVDVNERDRZVQGRFRZQWBUTWCVCWDUTVLDRZVMERZQWCURWEUSWFDHLUHEIMUHUIVLVM
      DEUJUKVCVOFRZVPGRZQWDVAWGVBWHFJLUHGKMUHUIVOVPFGUJUKUIVNVQDEFGULUKWAVJDVTV
      IEVSVHFVRCGNUMUMUMUMUNUOUPUQ $.
      $([5-Aug-95]$)
  $}

  ${
    $d x A $.
    ceqsex.1 $e |- ( ps -> A. x ps ) $.
    ceqsex.2 $e |- A e. V $.
    ceqsex.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( An inference with implicit substitution. $)
    ceqsex   $p |- ( E. x ( x = A /\ ph ) <-> ps ) $=
      ( cv wceq wa wex biimpa 19.23ai isseti wi wal biimprcd 19.21ai exintr
      syl mpi impbi ) CHDIZAJZCKZBUDBCEUCABGLMBUCCKZUECDFNBUCAOZCPUFUEOBUGCEUCA
      BGQRUCACSTUAUB $.
      $([2-Mar-95]$)
  $}

  ${
    $d x A $.  $d x ps $.
    ceqsexv.1 $e |- A e. V $.
    ceqsexv.2 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( An inference with implicit substitution. $)
    ceqsexv   $p |- ( E. x ( x = A /\ ph ) <-> ps ) $=
      ( ax-17 ceqsex ) ABCDBCGEFH $.
      $([2-Mar-95]$)
  $}
  ${
    $d x ps $.  $d y ph $.  $d x th $.  $d y ch $.  $d y A $.
    gencbvex.1 $e |- A e. V $.
    gencbvex.2 $e |- ( A = y -> ( ph <-> ps ) ) $.
    gencbvex.3 $e |- ( A = y -> ( ch <-> th ) ) $.
    gencbvex.4 $e |- ( th <-> E. x ( ch /\ A = y ) ) $.
    $( Change of bound variable using implicit substitution. $)
    gencbvex   $p |- ( E. x ( ch /\ ph ) <-> E. y ( th /\ ps ) ) $=
      ( cv wceq wa wex 19.11 wb bi2and bicomd cleqcoms ceqsexv biex anass
      pm5.32i ancom cleqcom biran 3bitr3 19.41v 3bitr 3bitr4r ) FLZGMZDBNZNZFOZ
      EOUOEOZFOCANZEOUNFOUOEFPUPUREUNURFGHUNURQGULGULMZURUNUSCDABJIRSTUAUBUQUNF
      UMEOZDNZBNUTUNNUNUQUTDBUCDVABDCUSNZEOUMDNZEOVAKVBVCEUSCNUSDNVBVCUSCDJUDUS
      CUEUSUMDGULUFUGUHUBUMDEUIUJUGUMUNEUIUKUBUH $.
      $([17-May-96]$)
  $}

  ${
    $d x ps $.  $d y ph $.  $d x th $.  $d y ch $.  $d y A $.
    gencbval.1 $e |- A e. V $.
    gencbval.2 $e |- ( A = y -> ( ph <-> ps ) ) $.
    gencbval.3 $e |- ( A = y -> ( ch <-> th ) ) $.
    gencbval.4 $e |- ( th <-> E. x ( ch /\ A = y ) ) $.
    $( Change of bound variable using implicit substitution. $)
    gencbval   $p |- ( A. x ( ch -> ph ) <-> A. y ( th -> ps ) ) $=
      ( wi wal wn wa wex cv wceq binegd gencbvex exnal 3bitr3 bicon4 ) CALEMZDB
      LFMZCANZOEPDBNZOFPUDNUENUFUGCDEFGHGFQRABISJKTCAEUADBFUAUBUC $.
      $([17-May-96]$)
  $}

  ${
    $d x A $.  $d x B $.
    clel2.1 $e |- A e. V $.
    $( An alternate definition of class membership when the class is a set. $)
    clel2   $p |- ( A e. B <-> A. x ( x = A -> x e. B ) ) $=
      ( cv wceq wcel wi wal eleq1 ceqsalv bicom ) AEZBFMCGZHAIBCGZNOABDMBCJKL
      $.
      $([18-Aug-93]$)
  $}

  ${
    $d x A $.  $d x B $.
    clel3.1 $e |- B e. V $.
    $( An alternate definition of class membership when the class is a set. $)
    clel3   $p |- ( A e. B <-> E. x ( x = B /\ A e. x ) ) $=
      ( cv wceq wcel wa wex eleq2 ceqsexv bicom ) AEZCFBMGZHAIBCGZNOACDMCBJKL
      $.
      $([18-Aug-93]$)
  $}

  ${
    $d x A $.  $d x B $.
    clel4.1 $e |- B e. V $.
    $( An alternate definition of class membership when the class is a set. $)
    clel4   $p |- ( A e. B <-> A. x ( x = B -> A e. x ) ) $=
      ( cv wceq wcel wi wal eleq2 ceqsalv bicom ) AEZCFBMGZHAIBCGZNOACDMCBJKL
      $.
      $([18-Aug-93]$)
  $}

  ${
    $d x A $.
    vtocl.1 $e |- ( ps -> A. x ps ) $.
    vtocl.2 $e |- A e. V $.
    vtocl.3 $e |- ( x = A -> ( ph -> ps ) ) $.
    vtocl.4 $e |- ph $.
    $( Implicit substitution of a class for a set variable. $)
    vtocl   $p |- ps $=
      ( cv wceq wex wi isseti 19.22i ax-mp 19.36i mpg ) ABCABCECIDJZCKABLZCKCDF
      MRSCGNOPHQ $.
      $([25-Jun-98]$)
  $}

  ${
    $d x A $.
    vtoclb.1 $e |- ( ps -> A. x ps ) $.
    vtoclb.2 $e |- A e. V $.
    vtoclb.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    vtoclb.4 $e |- ph $.
    $( Implicit substitution of a class for a set variable. $)
    vtoclb   $p |- ps $=
      ( cv wceq biimpd vtocl ) ABCDEFCIDJABGKHL $.
      $([30-Aug-93]$)
  $}

  ${
    $d x A $.  $d x ps $.
    vtoclbv.1 $e |- A e. V $.
    vtoclbv.2 $e |- ( x = A -> ( ph <-> ps ) ) $.
    vtoclbv.3 $e |- ph $.
