    vtocl2ga   $p |- ( ( A e. C /\ B e. D ) -> ch ) $=
      ( wcel wi cv wceq bilimd exp vtoclga comm imp ) FHMZGIMZCUCUBCUBBNUBCNEG
      IEOZGPBCUBKQUBUDIMZBUEANUEBNDFHDOZFPABUEJQUFHMUEALRSTSTUA $.
