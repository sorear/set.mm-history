$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        The universal class
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Declare the symbol for the universal class. $)
  $c V $. $( The letter V $)

  $( Extend class notation to include the universal class symbol. $)
  cvv $a class V $.

  $( Define the universal class.  Definition 5.20 of [TakeutiZaring] p. 21. $)
  df-v $a |- V = { x | x = x } $.

  $( All set variables are sets (see ~ isset ).  Theorem 6.8 of [Quine]
     p. 43. $)
  visset $p |- x e. V $=
    ( cv cvv wcel weq cleqid df-v cleqabi mpbir ) ABZCDAAEZJFKACAGHI $.
    $( [5-Aug-93] $)

  ${
    $d x A $.
    $( Two ways to say " ` A ` is a set":  A class ` A ` is a member of the
       universal class ` V ` (see ~ df-v ) if and only if the class ` A `
       exists (i.e. there exists some set ` x ` equal to class ` A ` ). Theorem
       6.9 of [Quine] p. 43.  _Notational convention_:  We will use the
       notational device " ` A e. V ` " to mean " ` A ` is a set" very
       frequently, for example in ~ uniex . Note the when ` A ` is not a set,
       it is called a proper class.  In some theorems, such as ~ uniexg , in
       order to shorten certain proofs we use the antecedent ` A e. B ` instead
       of ` A e. V ` to mean " ` A ` is a set".  $)
    isset $p |- ( A e. V <-> E. x x = A ) $=
      ( cvv wcel cv wceq wa wex df-clel visset biantru biex bitr4 ) BCDAEZBFZNC
      DZGZAHOAHABCIOQAPOAJKLM $.
      $( [5-Aug-93] $)
  $}

  ${
    $d x A $.
    isseti.1 $e |- A e. V $.
    $( A way to say " ` A ` is a set" (inference rule). $)
    isseti $p |- E. x x = A $=
      ( cvv wcel cv wceq wex isset mpbi ) BDEAFBGAHCABIJ $.
      $( [5-Aug-93] $)
  $}

  ${
    $d x A $.
    issetri.1 $e |- E. x x = A $.
    $( A way to say " ` A ` is a set" (inference rule). $)
    issetri $p |- A e. V $=
      ( cvv wcel cv wceq wex isset mpbir ) BDEAFBGAHCABIJ $.
      $( [5-Aug-93] $)
  $}

  ${
    $d x A $.  $d x B $.  $( ` x ` is dummy. $)
    $( If a class is a member of another class, it is a set.  Theorem 6.12
       of [Quine] p. 44. $)
    elisset $p |- ( A e. B -> A e. V ) $=
      ( vx wcel cv wceq wex cvv wa df-clel 19.40 sylbi pm3.26d isset sylibr )
      ABDZCEZAFZCGZAHDPSQBDZCGZPRTICGSUAICABJRTCKLMCANO $.
      $( [5-Aug-93] $)
  $}

  ${
    elisseti.1 $e |- A e. B $.
    $( If a class is a member of another class, it is a set. $)
    elisseti $p |- A e. V $=
      ( wcel cvv elisset ax-mp ) ABDAEDCABFG $.
      $( [11-Jun-94] $)
  $}

  ${
    $d x A $.
    $( An element of a class exists. $)
    elex $p |- ( A e. B -> E. x x = A ) $=
      ( wcel cvv cv wceq wex elisset isset sylib ) BCDBEDAFBGAHBCIABJK $.
      $( [1-May-95] $)
  $}

  $( A universal quantifier restricted to the universe is unrestricted. $)
  ralv $p |- ( A. x e. V ph <-> A. x ph ) $=
    ( cvv wral cv wcel wi wal df-ral visset a1bi bial bitr4 ) ABCDBECFZAGZBHABH
    ABCIAOBNABJKLM $.
    $( [31-Mar-04] $) $( [26-Mar-04] $)

  $( An existential quantifier restricted to the universe is unrestricted. $)
  rexv $p |- ( E. x e. V ph <-> E. x ph ) $=
    ( cvv wrex cv wcel wa wex df-rex visset biantrur biex bitr4 ) ABCDBECFZAGZB
    HABHABCIAOBNABJKLM $.
    $( [4-Apr-04] $) $( [26-Mar-04] $)

  ${
    $d x y $. $d y A $.
    $( Commutation of restricted and unrestricted universal quantifiers. $)
    ralcom4 $p |- ( A. x e. A A. y ph <-> A. y A. x e. A ph ) $=
      ( wral cvv wal ralcom ralv biral 3bitr3r ) ABDEZCFEACFEZBDELCGACGZBDEACBF
      DHLCIMNBDACIJK $.
      $( [31-Mar-04] $) $( [26-Mar-04] $)

    $( Commutation of restricted and unrestricted existential quantifiers. $)
    rexcom4 $p |- ( E. x e. A E. y ph <-> E. y E. x e. A ph ) $=
      ( wrex cvv wex rexcom rexv birex 3bitr3r ) ABDEZCFEACFEZBDELCGACGZBDEACBF
      DHLCIMNBDACIJK $.
      $( [13-Apr-04] $) $( [12-Apr-04] $)
  $}

  ${
    $d x A $.
    ceqsalg.1 $e |- ( ps -> A. x ps ) $.
    ceqsalg.2 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( A representation of explicit substitution of a class for a variable,
       inferred from an implicit substitution hypothesis. $)
    ceqsalg $p |- ( A e. B -> ( A. x ( x = A -> ph ) <-> ps ) ) $=
      ( wcel cv wceq wi wal wex biimpd a2i 19.20i 19.23 sylib elex syl5 com12
      biimprcd 19.21ai a1i impbid ) DEHZCIDJZAKZCLZBUIUFBUIUGCMZBUFUIUGBKZCLUJB
      KUHUKCUGABUGABGNOPUGBCFQRCDESTUABUIKUFBUHCFUGABGUBUCUDUE $.
      $( [29-Oct-03] $) $( [29-Oct-03] $)
  $}

  ${
    $d x A $.
    ceqsal.1 $e |- ( ps -> A. x ps ) $.
    ceqsal.2 $e |- A e. V $.
    ceqsal.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( A representation of explicit substitution of a class for a variable,
       inferred from an implicit substitution hypothesis. $)
    ceqsal $p |- ( A. x ( x = A -> ph ) <-> ps ) $=
      ( cvv wcel cv wceq wi wal wb ceqsalg ax-mp ) DHICJDKALCMBNFABCDHEGOP $.
      $( [18-Aug-93] $)
  $}

  ${
    $d x A $.  $d x ps $.
    ceqsalv.1 $e |- A e. V $.
    ceqsalv.2 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( A representation of explicit substitution of a class for a variable,
       inferred from an implicit substitution hypothesis. $)
    ceqsalv $p |- ( A. x ( x = A -> ph ) <-> ps ) $=
      ( ax-17 ceqsal ) ABCDBCGEFH $.
      $( [18-Aug-93] $)
  $}

  ${
    $d x ps $.
    gencl.1 $e |- ( th <-> E. x ( ch /\ A = B ) ) $.
    gencl.2 $e |- ( A = B -> ( ph <-> ps ) ) $.
    gencl.3 $e |- ( ch -> ph ) $.
    $( Implicit substitution for class with embedded variable. $)
    gencl $p |- ( th -> ps ) $=
      ( wceq wa wex syl5bi com12 imp 19.23aiv sylbi ) DCFGKZLZEMBHTBECSBSCBSABC
      IJNOPQR $.
      $( [17-May-96] $)
  $}

  ${
    $d x y $.  $d x R $.  $d x ps $.  $d y C $.  $d y S $.  $d y ch $.
    2gencl.1 $e |- ( C e. S <-> E. x ( x e. R /\ A = C ) ) $.
    2gencl.2 $e |- ( D e. S <-> E. y ( y e. R /\ B = D ) ) $.
    2gencl.3 $e |- ( A = C -> ( ph <-> ps ) ) $.
    2gencl.4 $e |- ( B = D -> ( ps <-> ch ) ) $.
    2gencl.5 $e |- ( ( x e. R /\ y e. R ) -> ph ) $.
    $( Implicit substitution for class with embedded variable. $)
    2gencl $p |- ( ( C e. S /\ D e. S ) -> ch ) $=
      ( wcel wi cv wceq bilimd exp gencl com12 imp ) HKQZIKQZCUGUFCUFBRUFCRESJQ
      ZUGEGIMGITBCUFOUAUFUHBUHARUHBRDSJQZUFDFHLFHTABUHNUAUIUHAPUBUCUDUCUDUE $.
      $( [17-May-96] $)
  $}

  ${
    $v F $.
    $v G $.
    3gencl.F $f class F $.
    3gencl.G $f class G $.
    $d x y z $.  $d y z D $.  $d z F $.  $d x y R $.  $d y z S $.
    $d x ps $.  $d y ch $.  $d z th $.
    3gencl.1 $e |- ( D e. S <-> E. x ( x e. R /\ A = D ) ) $.
    3gencl.2 $e |- ( F e. S <-> E. y ( y e. R /\ B = F ) ) $.
    3gencl.3 $e |- ( G e. S <-> E. z ( z e. R /\ C = G ) ) $.
    3gencl.4 $e |- ( A = D -> ( ph <-> ps ) ) $.
    3gencl.5 $e |- ( B = F -> ( ps <-> ch ) ) $.
    3gencl.6 $e |- ( C = G -> ( ch <-> th ) ) $.
    3gencl.7 $e |- ( ( x e. R /\ y e. R /\ z e. R ) -> ph ) $.
    $( Implicit substitution for class with embedded variable. $)
    3gencl $p |- ( ( D e. S /\ F e. S /\ G e. S ) -> th ) $=
      ( wcel wi wa cv wceq bilimd 3exp imp 2gencl com12 gencl exp 3imp ) KMUCZN
      MUCZOMUCZDUPUQURDUDURUPUQUEZDUSCUDUSDUDGUFLUCZURGJORJOUGCDUSUAUHUSUTCUTAU
      DZUTBUDUTCUDEFHIKNLMPQHKUGABUTSUHINUGBCUTTUHEUFLUCZFUFLUCZVAVBVCUTAUBUIUJ
      UKULUMULUNUO $.
      $( [17-May-96] $)
  $}

  ${
    $d x y ps $.  $d x y A $.  $d x y B $.
    cgsex2g.1 $e |- ( ( x = A /\ y = B ) -> ch ) $.
    cgsex2g.2 $e |- ( ch -> ( ph <-> ps ) ) $.
    $( Implicit substitution inference for general classes. $)
    cgsex2g $p |- ( ( A e. C /\ B e. D ) ->
                     ( E. x E. y ( ch /\ ph ) <-> ps ) ) $=
      ( wcel wa wex wi biimpa 19.23aivv a1i biimprcd ancld 19.22dvv cv wceq
      elex im2an eeanv sylibr 19.22i syl syl5 com12 impbid ) FHLZGILZMZCAMZENDN
      ZBUQBOUOUPBDECABKPQRBUOUQBCENZDNZUQUOBCUPDEBCACABKSTUAUODUBFUCZEUBGUCZMZE
      NZDNZUSUOUTDNZVAENZMVDUMVEUNVFDFHUDEGIUDUEUTVADEUFUGVCURDVBCEJUHUHUIUJUKU
      L $.
      $( [26-Jul-95] $)
  $}

  ${
    $d x y z w A $.  $d x y z w B $.  $d x y z w C $.  $d x y z w D $.
    $d x y z w ps $.
    cgsex4g.1 $e |- ( ( ( x = A /\ y = B ) /\ ( z = C /\ w = D ) ) -> ch ) $.
    cgsex4g.2 $e |- ( ch -> ( ph <-> ps ) ) $.
    $( An implicit substitution inference for 4 general classes. $)
    cgsex4g $p |- ( ( ( A e. R /\ B e. S ) /\ ( C e. R /\ D e. S ) ) ->
                    ( E. x E. y E. z E. w ( ch /\ ph ) <-> ps ) ) $=
      ( wcel wa wex wi biimpa 19.23aivv a1i biimprcd ancld 19.22dvv cv wceq
      elex im2an eeanv sylibr ee4anv 19.22i syl syl5 com12 impbid ) HLPZIMPZQZJ
      LPZKMPZQZQZCAQZGRFRZERDRZBVGBSVDVFBDEVEBFGCABOTUAUAUBBVDVGBCGRZFRZERZDRZV
      GVDBVIVFDEBCVEFGBCACABOUCUDUEUEVDDUFHUGZEUFIUGZQZFUFJUGZGUFKUGZQZQZGRZFRZ
      ERZDRZVKVDVNERDRZVQGRFRZQWBUTWCVCWDUTVLDRZVMERZQWCURWEUSWFDHLUHEIMUHUIVLV
      MDEUJUKVCVOFRZVPGRZQWDVAWGVBWHFJLUHGKMUHUIVOVPFGUJUKUIVNVQDEFGULUKWAVJDVT
      VIEVSVHFVRCGNUMUMUMUMUNUOUPUQ $.
      $( [5-Aug-95] $)
  $}

  ${
    $d x A $.
    ceqsex.1 $e |- ( ps -> A. x ps ) $.
    ceqsex.2 $e |- A e. V $.
    ceqsex.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( Elimination of an existential quantifier, using implicit
       substitution. $)
    ceqsex $p |- ( E. x ( x = A /\ ph ) <-> ps ) $=
      ( cv wceq wa wex biimpa 19.23ai isseti wi wal biimprcd 19.21ai exintr
      syl mpi impbi ) CHDIZAJZCKZBUDBCEUCABGLMBUCCKZUECDFNBUCAOZCPUFUEOBUGCEUCA
      BGQRUCACSTUAUB $.
      $( [2-Mar-95] $)
  $}

  ${
    $d x A $.  $d x ps $.
    ceqsexv.1 $e |- A e. V $.
    ceqsexv.2 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( Elimination of an existential quantifier, using implicit
       substitution. $)
    ceqsexv $p |- ( E. x ( x = A /\ ph ) <-> ps ) $=
      ( ax-17 ceqsex ) ABCDBCGEFH $.
      $( [2-Mar-95] $)
  $}

  ${
    $d x ps $.  $d y ph $.  $d x th $.  $d y ch $.  $d y A $.
    gencbvex.1 $e |- A e. V $.
    gencbvex.2 $e |- ( A = y -> ( ph <-> ps ) ) $.
    gencbvex.3 $e |- ( A = y -> ( ch <-> th ) ) $.
    gencbvex.4 $e |- ( th <-> E. x ( ch /\ A = y ) ) $.
    $( Change of bound variable using implicit substitution. $)
    gencbvex $p |- ( E. x ( ch /\ ph ) <-> E. y ( th /\ ps ) ) $=
      ( cv wceq wa wex excom wb bi2and bicomd cleqcoms ceqsexv biex anass
      pm5.32i ancom cleqcom biran 3bitr3 19.41v 3bitr 3bitr4r ) FLZGMZDBNZNZFOZ
      EOUOEOZFOCANZEOUNFOUOEFPUPUREUNURFGHUNURQGULGULMZURUNUSCDABJIRSTUAUBUQUNF
      UMEOZDNZBNUTUNNUNUQUTDBUCDVABDCUSNZEOUMDNZEOVAKVBVCEUSCNUSDNVBVCUSCDJUDUS
      CUEUSUMDGULUFUGUHUBUMDEUIUJUGUMUNEUIUKUBUH $.
      $( [17-May-96] $)
  $}

  ${
    $d x ps $.  $d y ph $.  $d x th $.  $d y ch $.  $d y A $.
    gencbval.1 $e |- A e. V $.
    gencbval.2 $e |- ( A = y -> ( ph <-> ps ) ) $.
    gencbval.3 $e |- ( A = y -> ( ch <-> th ) ) $.
    gencbval.4 $e |- ( th <-> E. x ( ch /\ A = y ) ) $.
    $( Change of bound variable using implicit substitution. $)
    gencbval $p |- ( A. x ( ch -> ph ) <-> A. y ( th -> ps ) ) $=
      ( wi wal wn wa wex cv wceq binegd gencbvex exanali 3bitr3 bicon4 ) CALEMZDB
      LFMZCANZOEPDBNZOFPUDNUENUFUGCDEFGHGFQRABISJKTCAEUADBFUAUBUC $.
      $( [17-May-96] $)
  $}

  ${
    $d x A $.  $d x B $.
    clel2.1 $e |- A e. V $.
    $( An alternate definition of class membership when the class is a set. $)
    clel2 $p |- ( A e. B <-> A. x ( x = A -> x e. B ) ) $=
      ( cv wceq wcel wi wal eleq1 ceqsalv bicom ) AEZBFMCGZHAIBCGZNOABDMBCJKL
      $.
      $( [18-Aug-93] $)
  $}

  ${
    $d x A $.  $d x B $.
    clel3.1 $e |- B e. V $.
    $( An alternate definition of class membership when the class is a set. $)
    clel3 $p |- ( A e. B <-> E. x ( x = B /\ A e. x ) ) $=
      ( cv wceq wcel wa wex eleq2 ceqsexv bicom ) AEZCFBMGZHAIBCGZNOACDMCBJKL
      $.
      $( [18-Aug-93] $)
  $}

  ${
    $d x A $.  $d x B $.
    clel4.1 $e |- B e. V $.
    $( An alternate definition of class membership when the class is a set. $)
    clel4 $p |- ( A e. B <-> A. x ( x = B -> A e. x ) ) $=
      ( cv wceq wcel wi wal eleq2 ceqsalv bicom ) AEZCFBMGZHAIBCGZNOACDMCBJKL
      $.
      $( [18-Aug-93] $)
  $}

  ${
    $d x A $.
    vtocl.1 $e |- ( ps -> A. x ps ) $.
    vtocl.2 $e |- A e. V $.
    vtocl.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    vtocl.4 $e |- ph $.
    $( Implicit substitution of a class for a set variable.  This is a
       generalization of ~ chv2 . $)
    vtoclf $p |- ps $=
      ( cv wceq wex wi isseti biimpd 19.22i ax-mp 19.36i mpg ) ABCABCECIDJZCKAB
      LZCKCDFMSTCSABGNOPQHR $.
      $( [30-Aug-93] $)
  $}

  ${
    $d x A $.  $d x ps $.
    vtocl.1 $e |- A e. V $.
    vtocl.2 $e |- ( x = A -> ( ph <-> ps ) ) $.
    vtocl.3 $e |- ph $.
    $( Implicit substitution of a class for a set variable. $)
    vtocl $p |- ps $=
      ( ax-17 vtoclf ) ABCDBCHEFGI $.
      $( [30-Aug-93] $)
  $}

  ${
    $d x y A $.  $d x y B $.  $d x y ps $.
    vtocl2.1 $e |- A e. V $.
    vtocl2.2 $e |- B e. V $.
    vtocl2.3 $e |- ( ( x = A /\ y = B ) -> ( ph <-> ps ) ) $.
    vtocl2.4 $e |- ph $.
    $( Implicit substitution of classes for set variables. $)
    vtocl2 $p |- ps $=
      ( wal wi wex cv wceq isseti wa eeanv biimpd 19.22i sylbir mp2an 19.36v
      biex bitr mpbi ax-gen mpg ) ADKZBCABLZDMZCMZUICKBLZCNEOZCMZDNFOZDMZULCEGP
      DFHPUOUQQUNUPQZDMZCMULUNUPCDRUSUKCURUJDURABISTTUAUBULUIBLZCMUMUKUTCABDUCU
      DUIBCUCUEUFADJUGUH $.
      $( [26-Jul-95] $)
  $}

  ${
    $d x y z A $.  $d x y z B $.  $d x y z C $.  $d x y z ps $.
    vtocl3.1 $e |- A e. V $.
    vtocl3.2 $e |- B e. V $.
    vtocl3.3 $e |- C e. V $.
    vtocl3.4 $e |- ( ( x = A /\ y = B /\ z = C ) -> ( ph <-> ps ) ) $.
    vtocl3.5 $e |- ph $.
    $( Implicit substitution of classes for set variables. $)
    vtocl3 $p |- ps $=
      ( wal wi wex cv wceq isseti w3a eeeanv biimpd 19.22i sylbir mp3an 19.36v
      biex bitr mpbi gen2 mpg ) AENZDNZBCABOZEPZDPZCPZUMCNBOZCQFRZCPZDQGRZDPZEQ
      HRZEPZUQCFISDGJSEHKSUTVBVDTUSVAVCTZEPZDPZCPUQUSVAVCCDEUAVGUPCVFUODVEUNEVE
      ABLUBUCUCUCUDUEUQUMBOZCPURUPVHCUPULBOZDPVHUOVIDABEUFUGULBDUFUHUGUMBCUFUHU
      IADEMUJUK $.
      $( [3-Jun-95] $)
  $}

  ${
    $d x A $.  $d x ch $. $d x th $.
    vtoclb.1 $e |- A e. V $.
    vtoclb.2 $e |- ( x = A -> ( ph <-> ch ) ) $.
    vtoclb.3 $e |- ( x = A -> ( ps <-> th ) ) $.
    vtoclb.4 $e |- ( ph <-> ps ) $.
    $( Implicit substitution of a class for a set variable. $)
    vtoclb $p |- ( ch <-> th ) $=
      ( wb cv wceq bi2bid vtocl ) ABKCDKEFGELFMACBDHINJO $.
      $( [23-Dec-93] $)
  $}

  ${
    $d y A $. $d x y $.
    vtoclgf.1 $e |- ( y e. A -> A. x y e. A ) $.
    vtoclgf.2 $e |- ( ps -> A. x ps ) $.
    vtoclgf.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    vtoclgf.4 $e |- ph $.
    $( Implicit substitution of a class for a set variable, with bound variable
       hypotheses in place of distinct variable restrictions. $)
    vtoclgf $p |- ( A e. B -> ps ) $=
      ( wcel cvv elisset cv wceq wex isset hbeleq ax-17 cleq1 cbvex bitr mpbii
      19.23ai sylbi syl ) EFKELKZBEFMUGCNZEOZCPZBUGDNZEOZDPUJDEQULUIDCCDEGRUIDS
      UKUHETUAUBUIBCHUIABJIUCUDUEUF $.
      $( [21-Sep-03] $)
  $}

  ${
    $d x y A $.  $d x ps $.
    vtoclg.1 $e |- ( x = A -> ( ph <-> ps ) ) $.
    vtoclg.2 $e |- ph $.
    $( Implicit substitution of a class for a set variable. $)
    vtoclg $p |- ( A e. B -> ps ) $=
      ( vy cv wcel ax-17 vtoclgf ) ABCHDEHIDJCKBCKFGL $.
      $( [17-Apr-95] $)
  $}

  ${
    $d x A $.  $d x ch $. $d x th $.
    vtoclbg.1 $e |- ( x = A -> ( ph <-> ch ) ) $.
    vtoclbg.2 $e |- ( x = A -> ( ps <-> th ) ) $.
    vtoclbg.3 $e |- ( ph <-> ps ) $.
    $( Implicit substitution of a class for a set variable. $)
    vtoclbg $p |- ( A e. B -> ( ch <-> th ) ) $=
      ( wb cv wceq bi2bid vtoclg ) ABKCDKEFGELFMACBDHINJO $.
      $( [29-Apr-94] $)
  $}

  ${
    $d x A $.  $d y z A $.  $d y z B $.  $d y C $.  $d z x $.
    vtocl2gf.1 $e |- ( ps -> A. x ps ) $.
    vtocl2gf.2 $e |- ( ch -> A. y ch ) $.
    vtocl2gf.3 $e |- ( x = A -> ( ph <-> ps ) ) $.
    vtocl2gf.4 $e |- ( y = B -> ( ps <-> ch ) ) $.
    vtocl2gf.5 $e |- ph $.
    $( Implicit substitution of a class for a set variable. $)
    vtocl2gf $p |- ( ( A e. C /\ B e. D ) -> ch ) $=
      ( vz wcel wi cv ax-17 hbim wceq bilimd vtoclgf com12 imp ) FHPZGIPZCUGUFC
      UFBQUFCQEOGIORZGPESUFCEUFESKTERGUABCUFMUBABDOFHUHFPDSJLNUCUCUDUE $.
      $( [25-Apr-95] $)
  $}

  ${
    $d x A $.  $d y A $.  $d y B $.  $d y C $.  $d x ps $.  $d y ch $.
    vtocl2g.1 $e |- ( x = A -> ( ph <-> ps ) ) $.
    vtocl2g.2 $e |- ( y = B -> ( ps <-> ch ) ) $.
    vtocl2g.3 $e |- ph $.
    $( Implicit substitution of 2 classes for 2 set variables. $)
    vtocl2g $p |- ( ( A e. C /\ B e. D ) -> ch ) $=
      ( ax-17 vtocl2gf ) ABCDEFGHIBDMCEMJKLN $.
      $( [25-Apr-95] $)
  $}

  ${
    $d x A $.  $d x B $.  $d x ps $.
    vtoclga.1 $e |- ( x = A -> ( ph <-> ps ) ) $.
    vtoclga.2 $e |- ( x e. B -> ph ) $.
    $( Implicit substitution of a class for a set variable. $)
    vtoclga $p |- ( A e. B -> ps ) $=
      ( wcel cv wi wceq eleq1 bi2imd vtoclg pm2.43i ) DEHZBCIZEHZAJPBJCDEQDKRPA
      BQDELFMGNO $.
      $( [20-Aug-95] $)
  $}

  ${
    $d x y A $.  $d y B $.  $d x y C $.  $d x y D $.  $d x ps $.  $d y ch $.
    vtocl2ga.1 $e |- ( x = A -> ( ph <-> ps ) ) $.
