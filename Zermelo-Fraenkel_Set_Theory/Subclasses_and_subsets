$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Subclasses and subsets
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x A $.  $d x B $.
    $( Alternate definition of the subclass relationship between two classes.
       Definition 5.9 of [TakeutiZaring] p. 17. $)
    dfss2 $p |- ( A (_ B <-> A. x ( x e. A -> x e. B ) ) $=
      ( wss cv wcel wa wb wal wi cin wceq cab dfss df-in cleq2i cleqab 3bitr
      pm4.71 bial bitr4 ) BCDZAEZBFZUDUCCFZGZHZAIZUDUEJZAIUBBBCKZLBUFAMZLUHBCNU
      JUKBABCOPUFABQRUIUGAUDUESTUA $.
      $( [8-Jan-02] $)

    $( Alternate definition of subclass relationship. $)
    dfss3 $p |- ( A (_ B <-> A. x e. A x e. B ) $=
      ( wss cv wcel wi wal wral dfss2 df-ral bitr4 ) BCDAEZBFMCFZGAHNABIABCJNAB
      KL $.
      $( [14-Oct-99] $)
  $}

  ${
    $d y A $.  $d y B $.  $d x y $.
    dfss2f.1 $e |- ( y e. A -> A. x y e. A ) $.
    dfss2f.2 $e |- ( y e. B -> A. x y e. B ) $.
    $( Equivalence for subclass relation requiring only that ` x ` not be
       free in ` A ` and ` B ` (but not necessarily absent from them). $)
    dfss2f $p |- ( A (_ B <-> A. x ( x e. A -> x e. B ) ) $=
      ( wss cv wcel wi wal dfss2 ax-17 hbim weq eleq1 bi2imd cbval bitr4 ) CDGB
      HZCIZTDIZJZBKAHZCIZUDDIZJZAKBCDLUGUCABUGBMUAUBAEFNABOUEUAUFUBUDTCPUDTDPQR
      S $.
      $( [3-Jul-94] $)

    $( Equivalence for subclass relation requiring only that ` x ` not be
       free in ` A ` and ` B ` (but not necessarily absent from them). $)
    dfss3f $p |- ( A (_ B <-> A. x e. A x e. B ) $=
      ( wss cv wcel wi wal wral dfss2f df-ral bitr4 ) CDGAHZCIPDIZJAKQACLABCDEF
      MQACNO $.
      $( [21-Mar-04] $) $( [20-Mar-04] $)

    $( If ` x ` is not free in ` A ` and ` B ` , it is not free in
       ` A (_ B ` . $)
    hbss $p |- ( A (_ B -> A. x A (_ B ) $=
      ( cv wcel wi wal wss hba1 dfss2f bial 3imtr4 ) AGZCHPDHIZAJZRAJCDKZSAJQAL
      ABCDEFMZSRATNO $.
      $( [27-Dec-96] $)
  $}

  ${
    $d x A $.  $d x B $.  $d x C $.  $( ` x ` is dummy. $)
    $( Membership relationships follow from a subclass relationship. $)
    ssel $p |- ( A (_ B -> ( C e. A -> C e. B ) ) $=
      ( vx wss cv wceq wcel wa wex wi wal dfss2 biimp 19.21bi imland 19.22dv
      df-clel 3imtr4g ) ABEZDFZCGZUAAHZIZDJUBUABHZIZDJCAHCBHTUDUFDTUCUEUBTUCUEK
      ZDTUGDLDABMNOPQDCARDCBRS $.
      $( [5-Aug-93] $)
  $}

  $( Membership relationships follow from a subclass relationship. $)
  ssel2 $p |- ( ( A (_ B /\ C e. A ) -> C e. B ) $=
    ( wss wcel ssel imp ) ABDCAECBEABCFG $.
    $( [8-Jun-04] $) $( [7-Jun-04] $)

  ${
    ssi.1 $e |- A (_ B $.
    $( Membership inference from subclass relationship. $)
    sseli $p |- ( C e. A -> C e. B ) $=
      ( wss wcel wi ssel ax-mp ) ABECAFCBFGDABCHI $.
      $( [5-Aug-93] $)
    ${
      ssii.2 $e |- C e. A $.
      $( Membership inference from subclass relationship. $)
      ssii $p |- C e. B $=
        ( wcel sseli ax-mp ) CAFCBFEABCDGH $.
        $( [31-May-99] $)
    $}
  $}

  ${
    ssd.1 $e |- ( ph -> A (_ B ) $.
    $( Membership deduction from subclass relationship. $)
    sseld $p |- ( ph -> ( C e. A -> C e. B ) ) $=
      ( wss wcel wi ssel syl ) ABCFDBGDCGHEBCDIJ $.
      $( [15-Nov-95] $)
  $}

  ${
    $d x A $.  $d x B $.
    ssriv.1 $e |- ( x e. A -> x e. B ) $.
    $( Inference rule based on subclass definition. $)
    ssriv $p |- A (_ B $=
      ( wss cv wcel wi dfss2 mpgbir ) BCEAFZBGKCGHAABCIDJ $.
      $( [5-Aug-93] $)
  $}

  ${
    $d x A $.  $d x B $.  $d x ph $.
    ssrdv.1 $e |- ( ph -> ( x e. A -> x e. B ) ) $.
    $( Deduction rule based on subclass definition. $)
    ssrdv $p |- ( ph -> A (_ B ) $=
      ( cv wcel wi wal wss 19.21aiv dfss2 sylibr ) ABFZCGNDGHZBICDJAOBEKBCDLM
      $.
      $( [15-Nov-95] $)
  $}

  ${
    $d x A $.  $d x B $.  $d x C $.  $( ` x ` is dummy. $)
    $( Transitivity of subclasses.  Exercise 5 of [TakeutiZaring] p. 17. $)
    sstr $p |- ( A (_ B -> ( B (_ C -> A (_ C ) ) $=
      ( vx wss cv wcel wi wal dfss2 syl2 19.20ii 3imtr4g sylbi ) ABEDFZAGZOBGZH
      ZDIZBCEZACEZHDABJSQOCGZHZDIPUBHZDITUARUCUDDPQUBKLDBCJDACJMN $.
      $( [5-Aug-93] $)
  $}

  $( Transitivity of subclasses.  Theorem 6 of [Suppes] p. 23. $)
  sstr2 $p |- ( ( A (_ B /\ B (_ C ) -> A (_ C ) $=
    ( wss sstr imp ) ABDBCDACDABCEF $.
    $( [5-Sep-03] $)

  ${
    sstri.1 $e |- A (_ B $.
    sstri.2 $e |- B (_ C $.
    $( Subclass transitivity inference. $)
    sstri $p |- A (_ C $=
      ( wss sstr mp2 ) ABFBCFACFDEABCGH $.
      $( [5-May-00] $)
  $}

  ${
    sstrd.1 $e |- ( ph -> A (_ B ) $.
    sstrd.2 $e |- ( ph -> B (_ C ) $.
    $( Subclass transitivity deduction. $)
    sstrd $p |- ( ph -> A (_ C ) $=
      ( wss sstr2 sylan13 ) BCGCDGBDGABCDHEFI $.
      $( [4-Jun-04] $) $( [2-Jun-04] $)
  $}

  ${
    $d x A $.  $d x B $.  $( ` x ` is dummy. $)
    $( The subclass relationship is antisymmetric.  Compare Theorem 4 of
       [Suppes] p. 22. $)
    eqss $p |- ( A = B <-> ( A (_ B /\ B (_ A ) ) $=
      ( vx cv wcel wb wal wi wa wceq wss albi dfcleq dfss2 bi2an 3bitr4 ) CDZAE
      ZQBEZFCGRSHCGZSRHCGZIABJABKZBAKZIRSCLCABMUBTUCUACABNCBANOP $.
      $( [5-Aug-93] $)
  $}

  ${
    eqssi.1 $e |- A (_ B $.
    eqssi.2 $e |- B (_ A $.
    $( Infer equality from two subclass relationships.  Compare Theorem 4 of
       [Suppes] p. 22.  $)
    eqssi $p |- A = B $=
      ( wceq wss wa pm3.2i eqss mpbir ) ABEABFZBAFZGKLCDHABIJ $.
      $( [9-Sep-93] $)
  $}

  ${
    eqssd.1 $e |- ( ph -> A (_ B ) $.
    eqssd.2 $e |- ( ph -> B (_ A ) $.
    $( Equality deduction from two subclass relationships.  Compare Theorem 4
       of [Suppes] p. 22.  $)
    eqssd $p |- ( ph -> A = B ) $=
      ( wss wa wceq jca eqss sylibr ) ABCFZCBFZGBCHALMDEIBCJK $.
      $( [28-Jun-04] $) $( [27-Jun-04] $)
  $}

  $( Any class is a subclass of itself.  Exercise 10 of [TakeutiZaring]
     p. 18. $)
  ssid $p |- A (_ A $=
    ( wss wceq wa cleqid eqss mpbi pm3.26i ) AABZIAACIIDAEAAFGH $.
    $( [5-Aug-93] $)

  ${
    $d A x $.
    $( Any class is a subclass of the universal class. $)
    ssv $p |- A (_ V $=
      ( vx cvv cv elisset ssriv ) BACBDAEF $.
      $( [31-Oct-95] $)
  $}

  $( Equality theorem for subclasses. $)
  sseq1 $p |- ( A = B -> ( A (_ C <-> B (_ C ) ) $=
    ( wss wb wa wi wceq sstr im2an eqss bi 3imtr4 cleqcoms ) ACDZBCDZEZBABADZAB
    DZFOPGZPOGZFBAHQRTSUABACIABCIJBAKOPLMN $.
    $( [5-Aug-93] $)

  $( Equality theorem for the subclass relationship. $)
  sseq2 $p |- ( A = B -> ( C (_ A <-> C (_ B ) ) $=
    ( wss wa wi wceq wb sstr com12 im2an eqss bi 3imtr4 ) ABDZBADZECADZCBDZFZRQ
    FZEABGQRHOSPTQORCABIJRPQCBAIJKABLQRMN $.
    $( [25-Jun-98] $)

  $( Equality theorem for the subclass relationship. $)
  sseq12 $p |- ( ( A = B /\ C = D ) -> ( A (_ C <-> B (_ D ) ) $=
    ( wceq wss sseq1 sseq2 sylan9bb ) ABEACFBCFCDEBDFABCGCDBHI $.
    $( [31-May-99] $)

  ${
    sseq1i.1 $e |- A = B $.
    $( An equality inference for the subclass relationship. $)
    sseq1i $p |- ( A (_ C <-> B (_ C ) $=
      ( wceq wss wb sseq1 ax-mp ) ABEACFBCFGDABCHI $.
      $( [18-Aug-93] $)

    $( An equality inference for the subclass relationship. $)
    sseq2i $p |- ( C (_ A <-> C (_ B ) $=
      ( wceq wss wb sseq2 ax-mp ) ABECAFCBFGDABCHI $.
      $( [30-Aug-93] $)

    ${
      sseq12i.2 $e |- C = D $.
