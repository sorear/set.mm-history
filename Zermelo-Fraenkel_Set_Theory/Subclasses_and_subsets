    psseq2d $p |- ( ph -> ( C (. A <-> C (. B ) ) $=
      ( wceq wpss wb psseq2 syl ) ABCFDBGDCGHEBCDIJ $.
      $( [11-Jun-04] $) $( [9-Jun-04] $)

    ${
      psseq12d.2 $e |- ( ph -> C = D ) $.
      $( An equality deduction for the proper subclass relationship. $)
      psseq12d $p |- ( ph -> ( A (. C <-> B (. D ) ) $=
        ( wpss psseq1d psseq2d bitrd ) ABDHCDHCEHABCDFIADECGJK $.
        $( [15-Jun-04] $) $( [9-Jun-04] $)
    $}
  $}

  $( A proper subclass is a subclass.  Theorem 10 of [Suppes] p. 23. $)
  pssss $p |- ( A (. B -> A (_ B ) $=
    ( wpss wss wne df-pss pm3.26bd ) ABCABDABEABFG $.
    $( [7-Feb-96] $)

  ${
    pssssd.1 $e |- ( ph -> A (. B ) $.
    $( Deduce subclass from proper subclass. $)
    pssssd $p |- ( ph -> A (_ B ) $=
      ( wpss wss pssss syl ) ABCEBCFDBCGH $.
      $( [29-Feb-96] $)
  $}

  $( Subclass in terms of proper subclass. $)
  sspss $p |- ( A (_ B <-> ( A (. B \/ A = B ) ) $=
    ( wss wpss wceq wo wn wa dfpss2 biimpr exp con1d orrd pssss eqimss jaoi
    impbi ) ABCZABDZABEZFRSTRTSRTGZSSRUAHABIJKLMSRTABNABOPQ $.
    $( [25-Feb-96] $)

  $( Proper subclass is irreflexive.  Theorem 7 of [Suppes] p. 23. $)
  pssirr $p |- -. A (. A $=
    ( wpss wss wn wa pm3.24 dfpss3 mtbir ) AABAACZIDEIFAAGH $.
    $( [7-Feb-96] $)

  $( Proper subclass has no 2-cycle loops.  Compare Theorem 8 of [Suppes]
     p. 23. $)
  pssn2lp $p |- -. ( A (. B /\ B (. A ) $=
    ( wpss wa wss wn pm3.24 dfpss3 bi2an an41r3 bitr wo orc adantr ianor
    sylibr imlan sylbi mto ) ABCZBACZDZABEZBAEZDZUEFZDZUEGUBUEUCFZUDFZDZDZUGUBU
    CUIDZUDUHDZDUKTULUAUMABHBAHIUCUIUDUHJKUJUFUEUJUHUILZUFUHUNUIUHUIMNUCUDOPQRS
    $.
    $( [7-Feb-96] $)

  $( Two ways of stating trichotomy with respect to inclusion. $)
  sspsstri $p |- ( ( A (_ B \/ B (_ A ) <-> ( A (. B \/ A = B \/ B (. A ) ) $=
    ( wss wo wpss wceq w3o sspss cleqcom bilor bitr bi2or orordir or23 df-3or
    bitr4 bitr3 ) ABCZBACZDABEZABFZDZBAEZUADZDZTUAUCGZRUBSUDABHSUCBAFZDUDBAHUGU
    AUCBAIJKLUETUCDUADZUFTUCUAMUHUBUCDUFTUCUANTUAUCOPQK $.
    $( [13-Aug-04] $) $( [12-Aug-04] $)

  $( Partial trichotomy law for subclasses. $)
  ssnpss $p |- ( A (_ B -> -. B (. A ) $=
    ( wss wpss wceq wo wn sspss wi wa pssn2lp imnan mpbir pssirr psseq1 mtbii
    jaoi sylbi ) ABCABDZABEZFBADZGZABHSUBTSUBISUAJGABKSUALMTAADUAANABAOPQR $.
    $( [16-May-96] $)

  $( Transitive law for proper subclass.  Theorem 9 of [Suppes] p. 23. $)
  psstr $p |- ( ( A (. B /\ B (. C ) -> A (. C ) $=
    ( wpss wa wss wceq wn sstr2 pssss syl2an pssn2lp psseq1 birand mtbiri
    con2i jca dfpss2 sylibr ) ABDZBCDZEZACFZACGZHZEACDUBUCUEABFBCFUCTUAABCIABJB
    CJKUDUBUDUBCBDZUAECBLUDTUFUAACBMNOPQACRS $.
    $( [7-Feb-96] $)

  $( Transitive law for subclass and proper subclass. $)
  sspsstr $p |- ( ( A (_ B /\ B (. C ) -> A (. C ) $=
    ( wpss wceq wo wss wi psstr exp psseq1 biimprd jaoi imp sspss sylanb ) ABDZ
    ABEZFZBCDZACDZABGSTUAQTUAHRQTUAABCIJRUATABCKLMNABOP $.
    $( [3-Apr-96] $)


  $( Transitive law for subclass and proper subclass. $)
  psssstr $p |- ( ( A (. B /\ B (_ C ) -> A (. C ) $=
    ( wpss wceq wo wss psstr exp psseq2 biimpcd jaod imp sspss sylan5b ) ABDZBC
    DZBCEZFZACDZBCGPSTPQTRPQTABCHIRPTBCAJKLMBCNO $.
    $( [3-Apr-96] $)

  ${
    $d x A $.  $d x B $.
    $( Restriction of uniqueness to a smaller subclass. $)
    reuss $p |- ( ( A (_ B /\ ( E. x e. A ph /\ E! x e. B ph ) ) ->
                E! x e. A ph ) $=
      ( wss cv wcel wa wex weu wreu wrex wmo wi wal ssel imrand 19.21aiv
      euimmo syl eu5 biimpr exp syl9 imp32 df-reu sylibr df-rex bi2an sylan5b
      ) CDEZBFZCGZAHZBIZULDGZAHZBJZHZABCKZABCLZABDKZHUKUSHUNBJZUTUKUOURVCUKURUN
      BMZUOVCUKUNUQNZBOURVDNUKVEBUKUMUPACDULPQRUNUQBSTUOVDVCVCUOVDHUNBUAUBUCUDU
      EABCUFUGVAUOVBURABCUHABDUFUIUJ $.
      $( [21-Aug-99] $)


    $( Restricted uniqueness "picks" a member of a subclass. $)
    reupick $p |- ( ( ( A (_ B /\ ( E. x e. A ph /\ E! x e. B ph ) ) /\ ph ) ->
                  ( x e. A <-> x e. B ) ) $=
      ( wss wrex wreu wa cv wcel wi ssel ad2antll wex weu ancrd imrand an23
