  $( Equality implies the subclass relation. $)
  eqimss $p |- ( A = B -> A (_ B ) $=
    ( wceq wss ssid sseq2 mpbii ) ABCAADABDAEABAFG $.
    $( [5-Aug-93] $)

  $( Equality implies the subclass relation. $)
  eqimss2 $p |- ( B = A -> A (_ B ) $=
