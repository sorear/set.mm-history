  ${
    $d x y A $.  $d x y B $.
    $( An abstraction relation is a subset of a related cross product. $)
    opabssxp $p |- { <. x , y >. | ( ( x e. A /\ y e. B ) /\ ph ) }
                   (_ ( A X. B ) $=
      ( cv wcel wa copab cxp pm3.26 ssopab2i df-xp sseqtr4 ) BFDGCFEGHZAHZBCIOB
      CIDEJPOBCOAKLBCDEMN $.
      $([16-Jul-95]$)
  $}

  ${
    $d x y A $.  $d x y B $.  $d x y C $.  $d x y ps $.
    optocl.1 $e |- D = ( B X. C ) $.
    optocl.2 $e |- ( <. x , y >. = A -> ( ph <-> ps ) ) $.
    optocl.3 $e |- ( ( x e. B /\ y e. C ) -> ph ) $.
    $( Implicit substitution of class for ordered pair. $)
    optocl   $p |- ( A e. D -> ps ) $=
      ( wcel cxp eleq2i cv cop wceq wa wex elxp2 visset opelxp sylbi syl5bi
      imp 19.23aivv ) EHLEFGMZLZBHUGEINUHCOZDOZPZEQZUKUGLZRZDSCSBCDEFGTUNBCDULU
      MBULABUMJUMUIFLUJGLRAUIUJFGDUAUBKUCUDUEUFUCUC $.
      $([5-Mar-95]$)
  $}

  ${
    $d x y z w A $.  $d z w B $.  $d x y z w C $.  $d x y z w D $.
    $d x y ps $.  $d z w ch $.  $d z w R $.
    2optocl.1 $e |- R = ( C X. D ) $.
    2optocl.2 $e |- ( <. x , y >. = A -> ( ph <-> ps ) ) $.
    2optocl.3 $e |- ( <. z , w >. = B -> ( ps <-> ch ) ) $.
    2optocl.4 $e |- ( ( ( x e. C /\ y e. D ) /\ ( z e. C /\ w e. D ) ) ->
                   ph ) $.
    $( Implicit substitution of classes for ordered pairs. $)
    2optocl   $p |- ( ( A e. R /\ B e. R ) -> ch ) $=
      ( wcel wi cv cop wceq bilimd wa exp optocl comm imp ) HLQZILQZCUIUHCUHBRU
      HCRFGIJKLMFSZGSZTIUABCUHOUBUHUJJQUKKQUCZBULARULBRDEHJKLMDSZESZTHUAABULNUB
      UMJQUNKQUCULAPUDUEUFUEUFUG $.
      $([12-Mar-95]$)
  $}

  ${
    $d x y z w v u A $.  $d z w v u B $.  $d v u C $.
    $d x y z w v u D $.  $d x y z w v u F $.  $d z w v u R $.
    $d x y ps $.  $d z w ch $.  $d v u th $.
    3optocl.1 $e |- R = ( D X. F ) $.
    3optocl.2 $e |- ( <. x , y >. = A -> ( ph <-> ps ) ) $.
    3optocl.3 $e |- ( <. z , w >. = B -> ( ps <-> ch ) ) $.
    3optocl.4 $e |- ( <. v , u >. = C -> ( ch <-> th ) ) $.
    3optocl.5 $e |- ( ( ( x e. D /\ y e. F ) /\ ( z e. D /\ w e. F )
                    /\ ( v e. D /\ u e. F ) ) -> ph ) $.
    $( Implicit substitution of classes for ordered pairs. $)
    3optocl   $p |- ( ( A e. R /\ B e. R /\ C e. R ) -> th ) $=
      ( wcel wa wi cv cop wceq bilimd 3expa exp 2optocl comm optocl imp 3impa
      ) KOUBZLOUBZMOUBZDUPUQUCZURDURUSDUSCUDUSDUDIJMNPOQIUEZJUEZUFMUGCDUSTUHUSU
      TNUBVAPUBUCZCVBAUDVBBUDVBCUDEFGHKLNPOQEUEZFUEZUFKUGABVBRUHGUEZHUEZUFLUGBC
      VBSUHVCNUBVDPUBUCZVENUBVFPUBUCZUCVBAVGVHVBAUAUIUJUKULUMULUNUO $.
      $([12-Mar-95]$)
  $}

  ${
    $d x y z w v u A $.  $d x y z w v u B $.  $d x y z w v u C $.
    $d x y z w v u D $.  $d x y z w v u S $.
    $d x y ph $.  $d z w v u ps $.
    opbrop.1 $e |- ( ( ( z = A /\ w = B ) /\ ( v = C /\ u = D ) ) ->
                     ( ph <-> ps ) ) $.
    opbrop.2 $e |- R = { <. x , y >. | ( ( x e. ( S X. S ) /\
                      y e. ( S X. S ) ) /\
                      E. z E. w E. v E. u ( ( x = <. z , w >. /\
                      y = <. v , u >. ) /\ ph ) ) } $.
    $( Ordered pair membership in a relation.  Special case. $)
    opbrop   $p |- ( ( ( A e. S /\ B e. S ) /\ ( C e. S /\ D e. S ) ) ->
                   ( <. A , B >. R <. C , D >. <-> ps ) ) $=
      ( wcel wa cop wbr cxp cv wceq wex copsex4g biland copab df-br eleq2i
      opex eleq1 birand cleq1 bi4exdv bi2and opelopab 3bitr syl5bb opelxpi
