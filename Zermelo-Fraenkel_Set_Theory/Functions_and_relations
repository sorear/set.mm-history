      ( vz wfr cv wss c0 wceq wn wa wbr cab cin wrex wi wal ccnv csn cima
      dffr2 cvv wcel visset iniseg ax-mp ineq2i cleq1i birex bilim bial bitr4
      ) CDFAGZCHUNIJKLZUNEGBGZDMENZOZIJZBUNPZQZARUOUNDSUPTUAZOZIJZBUNPZQZARABEC
      DUBVFVAAVEUTUOVDUSBUNVCURIVBUQUNUPUCUDVBUQJBUEEDUPUCUFUGUHUIUJUKULUM $.
      $( [15-May-04] $) $( [23-Apr-04] $)
  $}

  $( Subset theorem for image. $)
  imass1 $p |- ( A (_ B -> ( A " C ) (_ ( B " C ) ) $=
    ( wss cres crn cima ssres rnss syl df-ima 3sstr4g ) ABDZACEZFZBCEZFZACGBCGM
    NPDOQDABCHNPIJACKBCKL $.
    $( [17-Mar-04] $) $( [16-Mar-04] $)

  $( Subset theorem for image.  Exercise 22(a) of [Enderton] p. 53. $)
  imass2 $p |- ( A (_ B -> ( C " A ) (_ ( C " B ) ) $=
    ( wss cres crn cima ssres2 rnss syl df-ima 3sstr4g ) ABDZCAEZFZCBEZFZCAGCBG
    MNPDOQDABCHNPIJCAKCBKL $.
    $( [22-Mar-98] $)

  $( The image of a singleton outside the domain is empty. $)
  ndmima $p |- ( -. A e. dom B -> ( B " { A } ) = (/) ) $=
    ( cdm wcel wn csn cres crn c0 cima wceq cin disjsn biimpr dmres incom eqtr
    syl5eq dm0rn0 sylib df-ima ) ABCZDEZBAFZGZHZIBUDJUCUECZIKUFIKUCUBUDLZIUGUHI
    KUCUBAMNUGUDUBLUHBUDOUDUBPQRUESTBUDUAR $.
    $( [22-May-98] $)

  ${
    $d x y A $.  $( ` x y ` are dummy. $)
    $( A converse is a relation.  Theorem 12 of [Suppes] p. 62. $)
    relcnv $p |- Rel `' A $=
      ( vy vx ccnv wrel cv wbr copab relopab wceq wb df-cnv releq ax-mp mpbir
      ) ADZEZBFCFAGZCBHZEZRCBIPSJQTKCBALPSMNO $.
      $( [29-Oct-96] $)
