    VAFZUTVDUTVTVFUTVTUTVDGVGVAVDVSUSVCCZVDVFUTWAVDGVNVFVCVBUNVOUIBNOUOUPUQURUA
    $.
    $([22-Jun-98]$)

  $( Ordinal derived from its successor. $)
  orddif $p |- ( Ord A -> A = ( suc A \ { A } ) ) $=
    ( word csn cin c0 wceq csuc cdif orddisj disj3 cun df-suc difeq1 ax-mp
    undif2 eqtr4 cleq2i bitr4 sylib ) ABAACZDEFZAAGZTHZFZAIUAAATHZFUDATJUCUEAUC
    ATKZTHZUEUBUFFUCUGFALUBUFTMNATOPQRS $.
    $([20-May-98]$)

  $( An ordinal number is an element of ` On `.  From now on, "ordinal" will
     usually mean "ordinal number".  Here we derive some basic facts about
     ordinals.  Note that any inference ` ph ` with a hypothesis ` A e. On ` can
     be converted to a theorem of the form ` A e. On -> ph ` by using
     ~ dedth , ~ elimel , and ~ 0elon .  See, for example, ~ limsuc below. $)

  ${
    on.1 $e |- A e. On $.

    $( An ordinal is an ordinal class. $)
    onord $p |- Ord A $=
      ( con wcel word eloni ax-mp ) ACDAEBAFG $.
      $([11-Jun-94]$)

    $( An ordinal is a transitive class. $)
    ontrc $p |- Tr A $=
      ( word wtr onord ordtr ax-mp ) ACADABEAFG $.
      $([11-Jun-94]$)

    $( An ordinal is not a member of itself.  Theorem 7M(c) of Enderton,
       p. 192. $)
    oneirr $p |- -. A e. A $=
      ( word wcel wn onord ordeirr ax-mp ) ACAADEABFAGH $.
      $([11-Jun-94]$)

    $( An ordinal has no membership loop. $)
    onn2lp $p |- -. ( A e. B /\ B e. A ) $=
      ( word wcel wa wn onord ordn2lp ax-mp ) ADABEBAEFGACHABIJ $.
      $([11-Jun-94]$)

    $( A member of an ordinal is an ordinal.  Theorem 7M(a) of Enderton,
       p. 192. $)
    onel $p |- ( B e. A -> B e. On ) $=
      ( wcel con word onord wa cvv wi elisset elong biimprd syl tz7.6 syl5
      mpani pm2.43i ) BADZBEDZSAFZSTACGSBFZTUASHSBIDZUBTJBAKUCTUBBILMNABOPQR $.
      $([11-Jun-94]$)

    $( An ordinal is a subset of ` On `. $)
    onss $p |- A (_ On $=
      ( con wcel wss onsst ax-mp ) ACDACEBAFG $.
      $([11-Aug-94]$)

    $( A member of an ordinal is a subset of it. $)
    onelss $p |- ( B e. A -> B (_ A ) $=
      ( con wcel wss wi onelsst ax-mp ) ADEBAEBAFGCABHI $.
      $([11-Aug-94]$)

    $( Transitive law for ordinals.  Theorem 7M(b) of Enderton, p. 192. $)
    ontr1 $p |- ( ( B e. C /\ C e. A ) -> B e. A ) $=
      ( con wcel wa wi ontr1t ax-mp ) AEFBCFCAFGBAFHDABCIJ $.
      $([11-Aug-94]$)

    $( Transitive law for ordinals.  Exercise 4 of Takeuti and Zaring,
       p. 40. $)
    ontr2 $p |- ( ( B e. C /\ C (_ A ) -> B e. A ) $=
      ( wcel wss ssel comm imp ) BCEZCAFZBAEZKJLCABGHI $.
      $([11-Jun-94]$)

    $( See below for ~ ontr3 . $)

    $( Transitive law for ordinals. $)
    ontr4 $p |- ( ( B (_ C /\ C (_ A ) -> B (_ A ) $=
      ( wss sstr imp ) BCECAEBAEBCAFG $.
      $([11-Jun-94]$)

    $( An ordering law for ordinals. $)
    onssnel $p |- ( A (_ B -> -. B e. A ) $=
      ( wcel wss con word wn onel eloni ordeirr 3syl ssel comm mtod con2 ) BADZ
      ABEZQRBBDZQBFDBGSHABCIBJBKLRQSABBMNOP $.
      $([13-Jun-94]$)

    $( An ordering law for ordinals. $)
    onssnel2 $p |- ( B (_ A -> -. A e. B ) $=
      ( wss wcel oneirr ssel mtoi ) BADABEAAEACFBAAGH $.
      $([13-Jun-94]$)

    $( An element of an ordinal equals its intersection with the ordinal. $)
    onelin $p |- ( B e. A -> B = ( B i^i A ) ) $=
      ( wcel wss cin wceq onelss sseqin1 sylib ) BADBAEBBAFGABCHBAIJ $.
      $([11-Jun-94]$)

    $( An ordinal equals its union with any element. $)
    onelun $p |- ( B e. A -> A = ( A u. B ) ) $=
      ( wcel wss cun wceq onelss ssequn2 sylib ) BADBAEAABFGABCHBAIJ $.
      $([13-Jun-94]$)

    $( An ordinal includes its union. $)
    onuniss $p |- U. A (_ A $=
      ( wtr cuni wss ontrc df-tr mpbi ) ACADAEABFAGH $.
      $([11-Jun-94]$)

    $( The union of an ordinal is an ordinal.  Corollary 7N(d) of Enderton,
       p. 193. $)
    onuni $p |- U. A e. On $=
      ( cuni con wcel word wss onss tz7.19 ax-mp elisseti zfun2 elon mpbir ) AC
      ZDEOFZADGPABHAIJOAADBKLMN $.
      $([11-Jun-94]$)

    $( Ordinal membership in successor of a class. $)
    onsuci $p |- ( A e. suc B <-> ( A e. B \/ A = B ) ) $=
      ( con wcel csuc wceq wo wb elsucg ax-mp ) ADEABFEABEABGHICABDJK $.
      $([15-Sep-95]$)

    $( Membership in successor of an ordinal. $)
    onsucd2 $p |- ( B e. suc A <-> ( B e. A \/ B = A ) ) $=
      ( csuc wcel csn cun wo wceq df-suc eleq2i elun con elisseti elsnc2 bilor
      3bitr ) BADZEBAAFZGZEBAEZBSEZHUABAIZHRTBAJKBASLUBUCUABAAMCNOPQ $.
      $([12-Jun-94]$)

    $( An ordinal is an element of its successor. $)
    onelsuc $p |- A e. suc A $=
      ( con wcel csuc sucidg ax-mp ) ACDAAEDBACFG $.
      $([1-Sep-95]$)

    $( The successor of an ordinal is an ordinal.  Corollary 7N(c) of
       Enderton, p. 193. $)
    onsuc $p |- suc A e. On $=
      ( con wcel csuc tz7.24 ax-mp ) ACDAECDBAFG $.
      $([12-Jun-94]$)

    $( An ordinal is equal to the union of its successor. $)
    onunisuc $p |- A = U. suc A $=
      ( wtr csuc cuni wceq ontrc con elisseti unisuc mpbi ) ACAADEFABGAAHBIJK
      $.
      $([12-Jun-94]$)

    $( There is no set between an ordinal and its successor. $)
    onbtwn $p |- -. ( A e. B /\ B e. suc A ) $=
      ( con wcel csuc wa wn tz7.25 ax-mp ) ADEABEBAFEGHCABIJ $.
      $([16-Sep-95]$)

    ${
      on.2 $e |- B e. On $.

      $( Membership and proper subset relations are equivalent for ordinals. $)
      onelpss $p |- ( A e. B <-> ( A (_ B /\ -. A = B ) ) $=
        ( con wcel wss wceq wn wa wb onelpsst mp2an ) AEFBEFABFABGABHIJKCDABLM
        $.
        $([15-Sep-95]$)

      $( Subset is equivalent to membership or equality for ordinals. $)
      onssel $p |- ( A (_ B <-> ( A e. B \/ A = B ) ) $=
        ( con wcel wss wceq wo wb onsselt mp2an ) AEFBEFABGABFABHIJCDABKL $.
        $([15-Sep-95]$)

      $( Ordering law for ordinals. $)
      onelorss $p |- ( A e. B \/ B (_ A ) $=
        ( wcel wss wo wceq w3o word onord tz7.10 mp2an 3orass mpbi onssel
        orcom cleqcom biror 3bitr bilor mpbir ) ABEZBAFZGUCABHZBAEZGZGZUCUEUFIZ
        UHAJBJUIACKBDKABLMUCUEUFNOUDUGUCUDUFBAHZGUJUFGUGBADCPUFUJQUJUEUFBARSTUA
        UB $.
        $([12-Jun-94]$)

      $( Transitive law for ordinals.  Exercise 3 of Takeuti and Zaring,
         p. 40$)
      ontr3 $p |- ( ( B (_ C /\ C e. A ) -> B e. A ) $=
        ( wss wcel wi wa wceq wo word wb con onel eloni syl onord jctil
        ordssel biimpac ontr1 exp eleq1 biimprd jaoi pm2.43d imp ) BCFZCAGZBAGZ
        UIUJUKUIUJUJUKHZUIUJIBCGZBCJZKZULUJUIUOUJBLZCLZIUIUOMUJUQUPUJCNGUQACDOC
        PQBERSBCTQUAUMULUNUMUJUKABCDUBUCUNUKUJBCAUDUEUFQUCUGUH $.
        $([12-Jun-94]$)

      $( The union of two ordinals is an ordinal. $)
      onun $p |- ( A u. B ) e. On $=
        ( wcel wss wo cun con onelorss wceq onelun eleq1 mpbii syl ssequn1
        sylbi jaoi ax-mp ) BAEZABFZGABHZIEZBADCJTUCUATAUBKZUCABCLUDAIEUCCAUBIMN
        OUABUBKZUCABPUEBIEUCDBUBIMNQRS $.
        $([13-Jun-94]$)

      $( A  subset of an ordinal is a member of its successor. $)
      onsssuc $p |- ( A (_ B <-> A e. suc B ) $=
        ( wss wcel wceq wo csuc onssel onsuci bitr4 ) ABEABFABGHABIFABCDJABCKL
        $.
        $([16-Sep-95]$)

      $( The successor of a member of an ordinal is a subset of the ordinal.
         Generalization of Exercise 8 of Takeuti and Zaring, p. 40. $)
      onsucss $p |- ( A e. B <-> suc A (_ B ) $=
        ( wcel csuc wss word wi onord ordsucssi ax-mp con onsucssir impbi ) ABE
        ZAFBGZBHPQIBDJABKLAMEQPICABMNLO $.
        $([16-Sep-95]$)

      $( The successor of a member of a limit ordinal is also a member. $)
      onlimsuc $p |- ( ( Lim A /\ B e. A ) -> suc B e. A ) $=
        ( wlim wcel csuc wn wi wceq wss wo onsucss onsuc onssel df-or 3bitr
        biimp wa cuni limuni unieq onunisuc syl5req sylan9eqr eqimss onssnel
        3syl exp syl6 com3r ax-3 pm2.43d imp ) AEZBAFZBGZAFZUOUPURUOUPURHZUPHZI
        UPURIUPUSUOUTUPUSUQAJZUOUTIUPUSVAIZUPUQAKURVALVBBADCMUQABDNCOURVAPQRVAU
        OUTVAUOSABJABKUTUOVAAATZBAUAVAUQTVCBUQAUBBDUCUDUEABUFABCUGUHUIUJUKURUPU
        LUJUMUN $.
        $([14-Jun-94]$)

    $}

    $( The successor of a member of a limit ordinal is also a member. $)
    limsuc $p |- ( ( Lim x /\ A e. x ) -> suc A e. x ) $=
      ( cv wlim wcel csuc word c0 wceq wn cuni wa wi w3a df-lim 3anass bitr
