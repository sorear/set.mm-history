  $( Corollary 7.8 of [TakeutiZaring] p. 37. $)
  ordelssne $p |- ( ( Ord A /\ Ord B ) ->
              ( A e. B <-> ( A (_ B /\ -. A = B ) ) ) $=
    ( word wcel wss wceq wn wa wb wtr tz7.7 ordtr sylan5 ancoms ) BCZACZABDABEA
