  $( Strict dominance of natural numbers is the same as dominance over the
     successor of the smaller. $)
  omomsdomdom $p |- ( ( A e. om /\ B e. om ) -> ( A ~< B <-> suc A ~<_ B ) ) $=
    ( com wcel wa wpss csuc wss csdm wbr cdom word wb ordelpss nnord sylan
    ordelsuc bitr3d sylan5 nnsdomo nndomo peano2b sylanb 3bitr4d ) ACDZBCDZEABF
    ZAGZBHZABIJUHBKJZUEBLZUGUIMUFUEUKEABDZUGUIALUKULUGMUEABNAOPABCQRBOSABTUHCDU
    FUJUIMUEUHBUAAUBUCUD $.
    $( [?] $) $( [25-Jul-04] $)

  $( Dominance of a set over a successor of a natural number implies strict
     dominance over the number.   For the converse, see ~ omsdomdom . $)
  omdomsdom $p |- ( ( A e. om /\ B e. C ) ->
                   ( suc A ~<_ B -> A ~< B ) ) $=
    ( csuc csdm wbr wcel cdom wi com sdomdomtr exp3a com12 imp php4 sylan ) AAD
    ZEFZBCGZQBHFZABEFZIZAJGRSUBSRUBSRTUAAQBCKLMNAOP $.
    $( [?] $) $( [26-Jul-04] $)

