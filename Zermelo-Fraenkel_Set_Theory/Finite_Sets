  $( Lemma for Pigeonhole Principle.  This just says that if we remove an
     element of a set then put it back in, we end up with the original set. $)
  phplem1 $p |- ( B e. A -> ( { B } u. ( A \ { B } ) ) = A ) $=
    ( wcel csn wss cdif cun wceq snssi ssundif sylib ) BACBDZAELALFGAHBAILAJK
    $.
    $( [25-May-98] $)

  $( Lemma for Pigeonhole Principle.  If we join a natural number to itself
     minus an element, we end up with its successor minus the same element. $)
  phplem2 $p |- ( ( A e. om /\ B e. A ) ->
             ( { A } u. ( A \ { B } ) ) = ( suc A \ { B } ) ) $=
    ( com wcel wa csn cin c0 wceq cdif cun csuc word wn nordeq disjsn2 syl
    nnord sylan undif4 df-suc uncom eqtr difeq1i syl6eqr ) ACDZBADZEAFZBFZGHIZU
    HAUIJKZALZUIJZIAMZUGUJUFUNUGEABINUJABOABPQARSUJUKUHAKZUIJUMUHAUITULUOUIULAU
    HKUOAUAAUHUBUCUDUEQ $.
    $( [25-May-98] $)

