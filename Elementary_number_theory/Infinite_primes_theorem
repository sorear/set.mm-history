  ${
    $d m n x y A $.
    $( An unbounded set of natural numbers is infinite. $)
    unben $p |- ( ( A C_ NN /\ A. m e. NN E. n e. A m < n ) -> A ~~ NN ) $=
      ( vx cn wss cv clt wbr wrex wral wa com cen cvv c1 caddc co cmpt crdg
      cres eqid unbenlem omex nnenom ensymi entr sylancl ) AEFBGCGHICAJBEKLAMNI
      MENIAENIDABCDODGPQRSPTMUAZUIUBUCEMUDUEUFAMEUGUH $.
      $( [15-Sep-2013] $) $( [5-May-2005] $)
  $}

  ${
    $d j k N $.  $d j M $.  $d j k K $.
    infpnlem.1 $e |- K = ( ( ! ` N ) + 1 ) $.
    $( Lemma for ~ infpn .  The smallest divisor (greater than 1) ` M ` of
       ` N ! + 1 ` is a prime greater than ` N ` . $)
    infpnlem1 $p |- ( ( N e. NN /\ M e. NN ) ->
                        ( ( ( 1 < M /\ ( K / M ) e. NN ) /\
                  A. j e. NN ( ( 1 < j /\ ( K / j ) e. NN ) -> M <_ j ) ) ->
       ( N < M /\ A. j e. NN ( ( M / j ) e. NN -> ( j = 1 \/ j = M ) ) ) ) ) $=
      ( cn wcel wa c1 clt wbr cdiv co cv cle wi wral wceq wo wn wb cr nnre
      lenlt syl2an ancoms adantr cn0 nnnn0 cfa cfv caddc cz facndiv opreq1i
      nnz syl5eqelr nsyl sylanl1 expr sylbird con4d expimpd adantrd w3a cc
      faccl syl peano2nn syl5eqel nncn nndivtr ex 3com13 3expa adantrl letri3
      biimprd exp4b com3l imp32 adantll imim2d com23 sylan2d exp4d com24 exp32
      imp31 com14 3imp ralimdva adantld imp3a prime adantl sylibrd jcad ) DFGZC
      FGZHZICJKZBCLMZFGZHZIANZJKZBXFLMFGZHZCXFOKZPZAFQZHZDCJKZCXFLMFGZXFIRXFCRZ
