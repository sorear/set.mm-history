  $( Lemma for ~ mulgcd . $)
  mulgcdlem1 $p |- ( A = B ->
                ( ( ( 1st ` ( R ` A ) ) = ( Z x. ( 1st ` ( Q ` A ) ) ) /\
                  ( 2nd ` ( R ` A ) ) = ( Z x. ( 2nd ` ( Q ` A ) ) ) ) <->
                ( ( 1st ` ( R ` B ) ) = ( Z x. ( 1st ` ( Q ` B ) ) ) /\
                  ( 2nd ` ( R ` B ) ) = ( Z x. ( 2nd ` ( Q ` B ) ) ) ) ) ) $=
    ( wceq cfv c1st cmul co c2nd fveq2 fveq2d opreq2d eqeq12d anbi12d ) ABFZADG
