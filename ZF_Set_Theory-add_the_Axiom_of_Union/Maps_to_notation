  ${
    $d w z A $.  $d w z B $.  $d w x y $.  $d w x z $.
    hbmpt.1 $e |- ( z e. A -> A. x z e. A ) $.
    hbmpt.2 $e |- ( z e. B -> A. x z e. B ) $.
    $( Bound-variable hypothesis builder for the maps-to notation. $)
    hbmpt $p |- ( z e. ( y e. A |-> B ) -> A. x z e. ( y e. A |-> B ) ) $=
      ( vw cmpt cv wcel wceq wa copab df-mpt hblem hbeleq hban hbopab hbxfreq )
      ACBDEIBJDKZHJELZMZBHNBHDEOUCBHACUAUBAACBDFPAHEACHEGPQRST $.
      $( [20-Feb-2013] $)
  $}

  ${
    $d A z $.  $d B z $.  $d x y $.  $d x z $.
    $( Bound-variable hypothesis builder for the maps-to notation.
       (Contributed by FL, 17-Feb-2008.) $)
    hbmpt1 $p |- ( y e. ( x e. A |-> B ) -> A. x y e. ( x e. A |-> B ) ) $=
      ( vz cmpt cv wcel wceq wa copab df-mpt hbopab1 hbxfreq ) ABACDFAGCHEGDIJZ
      AEKAECDLOAEBMN $.
      $( [17-Feb-2008] $)
  $}

  ${
    $d w A $.  $d w B $.  $d w C $.  $d w x $.  $d w y $.  $d x z $.
    $( Bound-variable hypothesis builder for an operation in maps-to
       notation. $)
    hbmpt21 $p |- ( z e. ( x e. A , y e. B |-> C )
         -> A. x z e. ( x e. A , y e. B |-> C ) ) $=
      ( vw cmpt2 cv wcel wa wceq copab2 df-mpt2 hboprab1 hbxfreq ) ACABDEFHAIDJ
      BIEJKGIFLKZABGMABGDEFNQABGCOP $.
      $( [27-Aug-2013] $)
  $}

  ${
    $d w A $.  $d w B $.  $d w C $.  $d w x $.  $d w y $.  $d y z $.
    $( Bound-variable hypothesis builder for an operation in maps-to
       notation. $)
    hbmpt22 $p |- ( z e. ( x e. A , y e. B |-> C )
         -> A. y z e. ( x e. A , y e. B |-> C ) ) $=
      ( vw cmpt2 cv wcel wa wceq copab2 df-mpt2 hboprab2 hbxfreq ) BCABDEFHAIDJ
      BIEJKGIFLKZABGMABGDEFNQABGCOP $.
