  ${
    unxpdom2.1 $e |- A e. _V $.
    unxpdom2.2 $e |- B e. _V $.
    $( Corollary of ~ unxpdom . $)
    unxpdom2 $p |- ( ( 1o ~< A /\ B ~<_ A ) -> ( A u. B ) ~<_ ( A X. A ) ) $=
      ( cdom wbr c1o csdm cun cxp csn c0 cen com 1onn elisseti xpsnen ensymi
