  $( Strict dominance of a set over a natural number is the same as dominance
     over its successor.  (Contributed by Mario Carneiro, 12-Jan-2013.) $)
  sucdom $p |- ( ( A e. om /\ B e. C ) -> ( A ~< B <-> suc A ~<_ B ) ) $=
    ( com wcel wa csdm wbr csuc cdom wi sucdom2 adantl php4 sdomdomtr exp3a
    mpan9 impbid ) ADEZBCEZFABGHZAIZBJHZTUAUCKSABCLMSAUBGHZTUCUAKANTUDUCUAAUBBC
    OPQR $.
    $( [12-Jan-2013] $)

  $( Dominance of a set over a successor of a natural number implies strict
     dominance over the number.  For the converse, see ~ sucdom . $)
  sucdomiOLD $p |- ( ( A e. om /\ B e. C ) ->
                   ( suc A ~<_ B -> A ~< B ) ) $=
    ( com wcel csuc csdm wbr cdom wi php4 sdomdomtr exp3a impcom sylan ) ADEAAF
    ZGHZBCEZPBIHZABGHZJZAKRQUARQSTAPBCLMNO $.
    $( [26-Jul-2004] $)

  ${
    $d x A $.
    0sdom1dom.1 $e |- A e. _V $.
    $( Strict dominance over zero is the same as dominance over one. $)
    0sdom1dom $p |- ( (/) ~< A <-> 1o ~<_ A ) $=
      ( c0 csdm wbr csuc cdom c1o com wcel cvv wb peano1 sucdom mp2an df-1o
      breq1i bitr4i ) CADEZCFZAGEZHAGECIJAKJSUALMBCAKNOHTAGPQR $.
      $( [28-Sep-2004] $)
  $}

  $( Ordinal 1 is strictly dominated by ordinal 2. $)
  1sdom2 $p |- 1o ~< 2o $=
    ( c0 csn cpw c1o c2o csdm p0ex canth2 df1o2 cpr df2o2 pwpw0 eqtr4i 3brtr4i
    ) ABZOCZDEFOGHIEAOJPKLMN $.
    $( [4-Apr-2007] $)

