    unxpdom2 $p |- ( ( 1o ~< A /\ B ~<_ A ) -> ( A u. B ) ~<_ ( A X. A ) ) $=
      ( cdom wbr c1o csdm cun cxp csn cen xpsnen ensymi ax-mp domentr mpan2 cvv
