  $( Addition with 1 is same as successor.  Proposition 4.34(a) of [Mendelson]
     p. 266. $)
  oa1suc $p |- ( A e. On ->
                ( A +o 1o ) = suc A ) $=
    ( con0 wcel c1o coa co c0 csuc df-1o opreq2i wceq 0elon oasuc mpan2 syl5eq
    oa0 suceq syl eqtrd ) ABCZADEFZAGEFZHZAHZTUAAGHZEFZUCDUEAEIJTGBCUFUCKLAGMNO
    TUBAKUCUDKAPUBAQRS $.
    $( [29-Oct-1995] $)

  ${
    $d x y A $.  $d x y B $.
    $( Multiplication with successor.  Definition 8.15 of [TakeutiZaring]
       p. 62. $)
    omsuc $p |- ( ( A e. On /\ B e. On ) ->
                ( A .o suc B ) = ( ( A .o B ) +o A ) ) $=
      ( vy vx con0 wcel wa csuc cv coa co wceq copab c0 crdg cfv comu rdgsuc
      adantl suceloni omv sylan2 oprex opreq1 fvopab fveq2d syl5eqr 3eqtr4d )
      AEFZBEFZGZBHZCIDIZAJKZLDCMZNOZPZBUPPZUOPZAULQKZABQKZAJKZUJUQUSLUINBUORSUJ
