      UIUJUHUHCPZFZUKUIUJUNBUHCQRUMUJBBSTUAUBUCUDUB $.
      $( [18-Apr-1994] $)
  $}

  $( A set founded by epsilon contains no 2-cycle loops. $)
  efrn2lp $p |- ( ( E Fr A /\ ( x e. A /\ y e. A ) ) ->
                -. ( x e. y /\ y e. x ) ) $=
    ( cep wfr cv wcel wa wbr wn fr2nr epel anbi12i negbii sylib ) CDEAFZCGBFZCG
    HHPQDIZQPDIZHZJPQGZQPGZHZJABCDKTUCRUASUBABLBALMNO $.
    $( [19-Apr-1994] $)

  $( A set founded by epsilon contains no 3-cycle loops. $)
  epne3 $p |- ( ( E Fr A /\ ( x e. A /\ y e. A /\ z e. A ) ) ->
                -. ( x e. y /\ y e. z /\ z e. x ) ) $=
    ( cep wfr cv wcel w3a wa wbr wn fr3nr epel 3anbi123i negbii sylib ) DEFAGZD
    HBGZDHCGZDHIJRSEKZSTEKZTREKZIZLRSHZSTHZTRHZIZLABCDEMUDUHUAUEUBUFUCUGABNBCNC
    ANOPQ $.
    $( [19-Apr-1994] $)

  $( Similar to Theorem 7.2 of [TakeutiZaring] p. 35, of  except that the
     Axiom of Regularity is not required due to antecedent ` E Fr A ` . $)
  tz7.2 $p |- ( ( Tr A /\ E Fr A /\ B e. A ) -> ( B (_ A /\ B =/= A ) ) $=
    ( wtr cep wfr wcel wss wne wa trss wceq wn eleq1 negbid efrirr syl5cbir
