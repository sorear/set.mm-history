$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                Founded and well-ordering relations
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Declare new constant symbols. $)
  $c Fr $. $( Founded predicate symbol (read: 'founded'). $)
  $c We $. $( Well-ordering predicate symbol (read: 'well orders') $)

  $( Extend wff notation to include the founded predicate.  Read:  '` R ` is a
     founded relation on ` A ` .' $)
  wfr $a wff R Fr A $.

  $( Extend wff notation to include the well-ordering predicate.  Read:  '`R `
     well-orders ` A ` .' $)
  wwe $a wff R We A $.

  ${
    $d x y z R $.  $d x y z A $.
    $( Define a founded relation.  For alternate definitions, see ~ dffr2 and
       ~ dffr3 . $)
    df-fr $a |- ( R Fr A <-> A. x ( ( x (_ A /\ x =/= (/) ) ->
                E. y e. x A. z e. x -. z R y ) ) $.
  $}

  ${
    $d x y z R $.  $d x y z A $.  $d x y z B $.
    $( Property of founded relation (one direction of definition). $)
    fri $p |- ( ( ( B e. C /\ R Fr A ) /\ ( B (_ A /\ B =/= (/) ) ) ->
                E. x e. B A. y e. B -. y R x ) $=
      ( vz wcel wfr wss c0 wne wa cv wbr wn wral wrex wi wal wceq sseq1 neeq1
      anbi12d raleq1 rexeqd imbi12d cla4gv df-fr syl5ib imp31 ) DEHZCFIZDCJZDKL
      ZMZBNANFOPZBDQZADRZULGNZCJZUTKLZMZUQBUTQZAUTRZSZGTUPUSSZUMVFVGGDEUTDUAZVC
      UPVEUSVHVAUNVBUOUTDCUBUTDKUCUDVDURAUTDUQBUTDUEUFUGUHGABCFUIUJUK $.
      $( [3-Apr-2007] $) $( [18-Mar-1997] $)
  $}

  ${
    $d x y w v R $.  $d y z w v R $.  $d x w v A $.
    $( Alternate definition of founded relation.  Similar to Definition 6.21 of
       [TakeutiZaring] p. 30. $)
    dffr2 $p |- ( R Fr A <-> A. x ( ( x (_ A /\ x =/= (/) ) ->
                E. y e. x ( x i^i { z | z R y } ) = (/) ) ) $=
      ( vv vw wfr cv wss c0 wne wa wbr wn wral wrex wi wal cab cin wceq df-fr
      wcel disj visset breq1 elab negbii ralbii bitr rexbii breq2 negbid
      ralbidv cbvrexv imbi2i albii bitr4 ) DEHAIZDJUTKLMZFIZGIZENZOZFUTPZGUTQZR
      ZASVAUTCIZBIZENZCTZUAKUBZBUTQZRZASAGFDEUCVOVHAVNVGVAVNVBVJENZOZFUTPZBUTQV
      GVMVRBUTVMVBVLUDZOZFUTPVRFUTVLUEVTVQFUTVSVPVKVPCVBFUFVIVBVJEUGUHUIUJUKULV
      RVFBGUTVJVCUBZVQVEFUTWAVPVDVJVCVBEUMUNUOUPUKUQURUS $.
      $( [1-Feb-2007] $) $( [17-Feb-2004] $)
  $}

  ${
    $d x y z R $.  $d z A $.  $d x z B $.
    frc.1 $e |- B e. V $.
    $( Property of founded relation (one direction of definition using class
       variables). $)
    frc $p |- ( ( R Fr A /\ B (_ A /\ B =/= (/) ) ->
                E. x e. B ( B i^i { y | y R x } ) = (/) ) $=
      ( vz wfr wss c0 wne cv wbr cab cin wceq wrex wa wi wal dffr2 sseq1 neeq1
      anbi12d ineq1 eqeq1d rexeqd imbi12d cla4v sylbi 3impib ) CEHZDCIZDJKZDBLA
      LEMBNZOZJPZADQZULGLZCIZUSJKZRZUSUOOZJPZAUSQZSZGTUMUNRZURSZGABCEUAVFVHGDFU
