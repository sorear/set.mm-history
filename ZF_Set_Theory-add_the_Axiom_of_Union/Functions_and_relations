        $( [10-Feb-1996] $)
    $}

    ${
      son2lpi.4 $e |- B e. V $.
      $( A strict order relation has no 2-cycle loops. $)
      son2lpi $p |- -. ( A R B /\ B R A ) $=
        ( wbr wa soirri sotri mto ) ABCIBACIJAACIACDEFGKABACDEFGHELM $.
        $( [10-Feb-1996] $)
