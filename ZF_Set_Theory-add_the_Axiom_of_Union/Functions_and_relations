      $( [26-Apr-1998] $)

    $( The membership relation is a relation. $)
    rele $p |- Rel _E $=
      ( vx vy cep wrel cv wcel copab relopab df-eprel releqi mpbir ) CDAEBEFZAB
      GZDLABHCMABIJK $.
