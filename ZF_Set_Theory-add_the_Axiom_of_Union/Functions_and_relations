  $( The range of a cross product.  Part of Theorem 3.13(x) of [Monk1]
     p. 37. $)
  rnxpOLD $p |- ( -. A = (/) -> ran ( A X. B ) = B ) $=
    ( c0 wceq wn cxp cdm crn dmxpOLD ccnv df-rn cnvxp dmeqi eqtr syl5eq ) ACDEB
    AFZGZBABFZHZBAISRJZGQRKTPABLMNO $.
    $( [17-Apr-2004] $) $( [12-Apr-2004] $)

  $( The domain of a cross product is a subclass of the first factor. $)
  dmxpss $p |- dom ( A X. B ) (_ A $=
    ( cxp cdm wss c0 wceq 0ss xpeq2 xp0 syl6eq dmeqd dm0 sseq1d mpbiri wne
    dmxp eqimss syl pm2.61ine ) ABCZDZAEZBFBFGZUCFAEAHUDUBFAUDUBFDFUDUAFUDUAAFC
    FBFAIAJKLMKNOBFPUBAGUCABQUBARST $.
    $( [20-Mar-2007] $) $( [19-Mar-2007] $)

  $( The range of a cross product is a subclass of the second factor. $)
  rnxpss $p |- ran ( A X. B ) (_ B $=
    ( cxp crn wss c0 wceq 0ss xpeq1 xp0r syl6eq rneqd rn0 sseq1d mpbiri wne
    rnxp eqimss syl pm2.61ine ) ABCZDZBEZAFAFGZUCFBEBHUDUBFBUDUBFDFUDUAFUDUAFBC
    FAFBIBJKLMKNOAFPUBBGUCABQUBBRST $.
    $( [17-Jan-2006] $) $( [16-Jan-2006] $)

  $( If a cross product is a set, one of its components must be a set. $)
  xpexr $p |- ( ( A X. B ) e. C -> ( A e. V \/ B e. V ) ) $=
    ( cxp wcel cvv wn wi c0 wceq 0ex eleq1 mpbiri pm2.21nd a1d wne crn rnxp
    eleq1d rnexg syl5bi a1dd pm2.61ine orrd ) ABDZCEZAFEZBFEZUFUGGZUHHZHAIAIJZU
    JUFUKUGUHUKUGIFEKAIFLMNOAIPZUFUHUIULUEQZFEUHUFULUMBFABRSUECTUAUBUCUD $.
    $( [27-Aug-2006] $) $( [27-Aug-2006] $)

  $( If a nonempty cross product is a set, so are both of its components. $)
  xpexr2 $p |- ( ( ( A X. B ) e. C /\ ( A X. B ) =/= (/) ) ->
               ( A e. V /\ B e. V ) ) $=
    ( cxp wcel c0 wne wa cvv cdm wceq dmxp adantl dmexg adantr eqeltrrd
    adantrr crn rnxp rnexg adantrl jca ancom2s xpnz sylan2br ) ABDZCEZAFGZBFGZH
    AIEZBIEZHZUFFGUGUIUHULUGUIUHHHUJUKUGUIUJUHUGUIHUFJZAIUIUMAKUGABLMUGUMIEUIUF
    CNOPQUGUHUKUIUGUHHUFRZBIUHUNBKUGABSMUGUNIEUHUFCTOPUAUBUCABUDUE $.
    $( [?] $) $( [27-Aug-2006] $)

  ${
