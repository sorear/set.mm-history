      UJNUFUKUJUEGUCUDOEUEUJPQSRTTCDEABUAUB $.
      $( [2-May-1996] $)

    $( A member of a cross product (ordered pair) doesn't contain the empty
       set. $)
    0nelelxp $p |- ( C e. ( A X. B ) -> -. (/) e. C ) $=
