  ${
    $d A x y z $.
    $d B x y z $.
    $( The cross product of nonempty classes is nonempty.  (Variation of a
       theorem contributed by Raph Levien, 30-Jun-2006.) $)
    xpnz $p |- ( ( A =/= (/) /\ B =/= (/) ) <-> ( A X. B ) =/= (/) ) $=
      ( vx vy vz c0 wne wa cxp cv wcel wex ne0 anbi12i eeanv bitr4 cop opex
      wceq eleq1 visset opelxp syl6bb cla4ev sylibr 19.23aivv sylbi xpeq1 xp0r
      syl6eq necon3i xpeq2 xp0 jca impbi ) AFGZBFGZHZABIZFGZURCJZAKZDJZBKZHZDLC
      LZUTURVBCLZVDDLZHVFUPVGUQVHCAMDBMNVBVDCDOPVEUTCDVEEJZUSKZELUTVJVEEVAVCQZV
      AVCRVIVKSVJVKUSKVEVIVKUSTVAVCABDUAUBUCUDEUSMUEUFUGUTUPUQAFUSFAFSUSFBIFAFB
      UHBUIUJUKBFUSFBFSUSAFIFBFAULAUMUJUKUNUO $.
      $( [19-Apr-2007] $) $( [30-Jun-2006] $)
  $}

  $( At least one member of an empty cross product is empty. $)
  xpeq0 $p |- ( ( A X. B ) = (/) <-> ( A = (/) \/ B = (/) ) ) $=
    ( cxp c0 wceq wne wa wn wo xpnz necon2bbii ianor nne orbi12i 3bitr ) ABCZDE
    ADFZBDFZGZHQHZRHZIADEZBDEZISPDABJKQRLTUBUAUCADMBDMNO $.
    $( [21-Sep-2006] $) $( [27-Aug-2006] $)

  $( Cross products with disjoint sets are disjoint. $)
  xpdisj1 $p |- ( ( A i^i B ) = (/) -> ( ( A X. C ) i^i ( B X. D ) ) = (/) ) $=
    ( cin c0 wceq cxp xpeq1 xp0r syl6eq inxp syl5eq ) ABEZFGZNCDEZHZFACHBDHEOQF
    PHFNFPIPJKACBDLM $.
    $( [15-Sep-2004] $) $( [13-Sep-2004] $)

  $( Cross products with disjoint sets are disjoint. $)
  xpdisj2 $p |- ( ( A i^i B ) = (/) -> ( ( C X. A ) i^i ( D X. B ) ) = (/) ) $=
    ( cin c0 wceq cxp xpeq2 xp0 syl6eq inxp syl5eq ) ABEZFGZCDEZNHZFCAHDBHEOQPF
    HFNFPIPJKCADBLM $.
    $( [15-Sep-2004] $) $( [13-Sep-2004] $)

  $( Cross products with two different singletons are disjoint. $)
  xpsndisj $p |- ( -. B = D -> ( ( A X. { B } ) i^i ( C X. { D } ) ) = (/) ) $=
    ( wceq wn csn cin c0 cxp disjsn2 xpdisj2 syl ) BDEFBGZDGZHIEANJCOJHIEBDKNOA
    CLM $.
    $( [29-Jul-2004] $) $( [28-Jul-2004] $)

  $( A double restriction to disjoint classes is the empty set. $)
  resdisj $p |- ( ( A i^i B ) = (/) -> ( ( C |` A ) |` B ) = (/) ) $=
    ( cin c0 wceq cvv cxp cres xpdisj1 ineq2d in0 syl6eq df-res ineq1i inass
    3eqtr syl5eq ) ABDEFZCAGHZBGHZDZDZECAIZBIZSUCCEDESUBECABGGJKCLMUEUDUADCTDZU
    ADUCUDBNUDUFUACANOCTUAPQR $.
    $( [17-Oct-2004] $) $( [7-Oct-2004] $)

  $( The range of a cross product.  Part of Theorem 3.13(x) of [Monk1]
     p. 37. $)
  rnxp $p |- ( A =/= (/) -> ran ( A X. B ) = B ) $=
    ( c0 wne cxp cdm crn dmxp ccnv df-rn cnvxp dmeqi eqtr syl5eq ) ACDBAEZFZBAB
