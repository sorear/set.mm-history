      $( The domain of a morphism is an object. $)
      dmo $p |- ( ( T e. Cat /\ F e. M ) -> ( D ` F ) e. O ) $=
        ( wf wcel cfv ccat ffvelrn cid_ cdom_ cdm eqcomi dmeqi eqtr eqid domc
