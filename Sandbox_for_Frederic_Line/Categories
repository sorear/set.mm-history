  ${
    cat.1 $e |- M = dom D $.
    cat.3 $e |- D = ( dom_ ` T ) $.
    ${
      cat.2 $e |- O = dom J $.
      cat.5 $e |- J = ( id_ ` T ) $.
      ${
        $( The 1st "axiom" of a category - ` ( dom_ `` T ) ` is a mapping
           from the morphisms of ` T ` to the objects of ` T ` . $)
        domc $p |- ( T e. Cat -> D : M --> O ) $=
          ( ccat wcel cded calg wf catded dedalg doma 3syl ) BJKBLKBMKDEANBOBPA
          BCDEFGHIQR $.
          $( [3-Jan-2008] $) $( [2-Jan-2008] $)
      $}

      ${
        cat.4 $e |- C = ( cod_ ` T ) $.
        $( The 2nd "axiom" of a category - ` ( cod_ `` T ) ` is a mapping
        from the morphisms of ` T ` to  the objects of ` T ` . $)
        codc $p |- ( T e. Cat -> C : M --> O ) $=
          ( ccat wcel cded calg wf catded dedalg coda 3syl ) CLMCNMCOMEFAPCQCRA
          BCDEFGHIJKST $.
          $( [3-Jan-2008] $) $( [2-Jan-2008] $)
      $}

      ${
        $( The 3rd "axiom" of a category - ` id_ ` is a mapping from the
        objects of ` T ` to  the morphisms of ` T ` . $)
        idc $p |- ( T e. Cat -> J : O --> M ) $=
          ( ccat wcel cded calg wf catded dedalg ida 3syl ) BJKBLKBMKEDCNBOBPAB
          CDEFGHIQR $.
          $( [5-Dec-2007] $) $( [5-Dec-2007] $)
      $}
   $}
   ${
        cat.6 $e |- R = ( o_ ` T ) $.
        $( The 4th "axiom" of a category - ` ( o_ `` T )  ` is a partial
        operation from the morphisms of ` T ` to the morphisms of ` T ` .
        $)
        cmppfc $p |- ( T e. Cat ->
        ( Fun R /\ dom R (_ ( M X. M ) /\ ran R (_ M ) ) $=
          ( ccat wcel cded calg wfun cdm cxp wss crn w3a catded dedalg cmppfa
          3syl ) CHICJICKIBLBMDDNOBPDOQCRCSABCDEFGTUA $.
          $( [10-Mar-2008] $) $( [10-Mar-2008] $)
       $}
   $}

  ${
    idosc.1 $e |- O = dom J $.
    idosc.2 $e |- D = ( dom_ ` T ) $.
    idosc.3 $e |- J = ( id_ ` T ) $.
    idosc.4 $e |- C = ( cod_ ` T ) $.
    $( The 5th "axiom" of a category - The identity is a morphism whose
    domain and codomain are equal. $)
    idosc $p |- ( ( T e. Cat /\ A e. O ) ->  ( ( D ` ( J ` A ) ) = A /\
        ( C ` ( J ` A ) ) = A ) ) $=
