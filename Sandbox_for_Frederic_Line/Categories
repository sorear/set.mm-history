      COMFBPEAPQRDSMABCDEFGHIJKTDUAUB $.
      $( [10-Mar-2008] $) $( [10-Mar-2008] $)


    $( The 7th "axiom" of a category - When ` ( G ( o_ `` T ) F ) ` is defined
    its domain is the domain of ` F ` . $)
    domcmpc $p |- ( ( T e. Cat /\ F e. M /\ G e. M ) ->
        ( ( D ` G ) = ( C ` F ) -> ( D ` ( G R F ) ) = ( D ` F ) ) ) $=
      ( cded wcel cfv wceq co wi ccat domcmpd catded syl3an1 ) DLMEGMFGMFBNEANO
      FECPBNEBNOQDRMABCDEFGHIJKSDTUA $.
      $( [10-Mar-2008] $) $( [10-Mar-2008] $)


    $( The 8th "axiom" of a category - When ` ( G ( o_ `` T ) F ) ` is defined
    its codomain is the codomain of ` G ` . $)
    codcmpc $p |- ( ( T e. Cat /\ F e. M /\ G e. M ) ->
              ( ( D ` G ) = ( C ` F )
              -> ( C ` ( G R F ) ) = ( C ` G ) ) ) $=
      ( cded wcel cfv wceq co wi ccat codcmpd catded syl3an1 ) DLMEGMFGMFBNEANO
      FECPANFANOQDRMABCDEFGHIJKSDTUA $.
      $( [10-Jan-2008] $) $( [10-Jan-2008] $)
