  $( A single axiom for propositional calculus offered by Wajsberg.
     (Contributed by Anthony Hart, 13-Aug-2011.) $)
  waj-ax $p |- ( ( ph -/\ ( ps -/\ ch ) ) -/\ ( ( ( th -/\ ch ) -/\ ( ( ph
         -/\ th ) -/\ ( ph -/\ th ) ) ) -/\ ( ph -/\ ( ph -/\ ps ) ) ) ) $=
    ( wnand wa wi nic-justlem simpr imim2i pm2.27 expdimp syl5com con3d df-nand
    wn anim2d 3imtr4g nic-justim sylib pm3.21 adantr com12 a2i sylibr jca sylbi
    mpbir ) ABCEEZDCEZADEZUKEEZAABEEZEEUIULUMFZGUIABCFZGZUNACBHUPULUMUPUJUKGULU
    PDCFZPADFZPUJUKUPURUQUPACGZURUQUOCABCIJADUSUQAUSCDACKQLMNDCOADORUJUKSTUPAAB
    FZGZUMAUOUTUOAUTBVACBAUAUBUCUDABAHUEUFUGUIUMULHUH $.
    $( [17-Aug-2011] $) $( [13-Aug-2011] $)

  $( A single axiom for propositional calculus offered by Lukasiewicz.
     (Contributed by Anthony Hart, 14-Aug-2011.) $)
  lukshef-ax2 $p |- ( ( ph -/\ ( ps -/\ ch ) ) -/\ ( ( ph -/\ ( ch -/\ ph ) )
          -/\ ( ( th -/\ ps ) -/\ ( ( ph -/\ th ) -/\ ( ph -/\ th ) ) ) ) ) $=
    ( wnand wa wi nic-justlem biimpi simpr imim2i pm2.27 anim2d expdimp syl5com
    simpl ancr anim1i wn df-nand syl11anc 3imtr4g anim2i syl biimpri nic-justim
    con3 anim12i 3syl mpbir ) ABCEEZACAEEZDBEZADEZUNEEZEEUKULUOFZGUKABCFZGZACAF
    GZUMUNGZFZUPUKURACBHIURUSADFZDBFZGZFZVAURACGZVDVEUQCABCJKURABGZVBVCUQBABCPK
    ADVGVCAVGBDABLMNOVFUSVDACQRUAVDUTUSVDVCSVBSUMUNVBVCUGDBTADTUBUCUDUSULUTUOUL
    USAACHUEUTUOUMUNUFIUHUIUKUOULHUJ $.
    $( [17-Aug-2011] $) $( [14-Aug-2011] $)

