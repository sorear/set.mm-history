  ${
    nvabl.1 $e |- G = ( +v ` U ) $.
    $( The vector addition operation of a normed complex vector space is an
       Abelian group. $)
    nvabl $p |- ( U e. NrmCVec -> G e. AbelOp ) $=
      ( cnv wcel c1st cfv cvc cabl eqid nvvc vafval vcabl syl ) ADEAFGZHEBIEAOO
      JKBOABCLMN $.
      $( [15-Feb-2008] $)

    $( The vector addition operation of a normed complex vector space is a
       group. $)
    nvgrp $p |- ( U e. NrmCVec -> G e. GrpOp ) $=
      ( cnv wcel cabl cgr nvabl ablogrpo syl ) ADEBFEBGEABCHBIJ $.
      $( [15-Feb-2008] $)
  $}

  ${
    nvgf.1 $e |- X = ( BaseSet ` U ) $.
    nvgf.2 $e |- G = ( +v ` U ) $.
    $( Mapping for the vector addition operation. $)
    nvgf $p |- ( U e. NrmCVec -> G : ( X X. X ) --> X ) $=
      ( cnv wcel cgr cxp wfo wf nvgrp bafval grpfo fof 3syl ) AFGBHGCCIZCBJQCBK
      ABELBCABCDEMNQCBOP $.
