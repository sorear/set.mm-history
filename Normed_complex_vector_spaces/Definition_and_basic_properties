      ECDEFGMNO $.
      $( [23-Apr-2007] $)

    $( The vector addition (group) operation is commutative. $)
    nvcom $p |- ( ( U e. NrmCVec /\ A e. X /\ B e. X ) ->
                ( A G B ) = ( B G A ) ) $=
