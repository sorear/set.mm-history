$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                     Definition and basic properties
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c NrmCVec $.

  $c +v $.
  $c BaseSet $.
  $c .s $.
  $c 0vec $.
  $c -v $.
  $c norm $.

  $c IndMet $.

  $( Extend class notation with the class of all normed complex vector
     spaces. $)
  cnv $a class NrmCVec $.

  $( Extend class notation with vector addition in a normed complex vector
     space.  In the literature, the subscript "v" is omitted, but we need it to
     avoid ambiguity with complex number addition ` + ` ~ caddc . $)
  cpv $a class +v $.

  $( Extend class notation with the base set of a normed complex vector space.
     (Note that ` BaseSet ` is capitalized because, once it is fixed for a
     particular vector space ` U ` , it is not a function, unlike e.g.
     ` norm ` .  This is our typical convention.) $)
  cba $a class BaseSet $.

  $( Extend class notation with scalar multiplication in a normed complex
     vector space.  In the literature scalar multiplication is usually
     indicated by juxtaposition, but we need an explicit symbol to prevent
     ambiguity. $)
  cns $a class .s $.

  $( Extend class notation with zero vector in a normed complex vector
     space. $)
  cn0v $a class 0vec $.

  $( Extend class notation with vector subtraction in a normed complex vector
     space. $)
  cnsb $a class -v $.

  $( Extend class notation with the norm function in a normed complex vector
     space.  In the literature, the norm of ` A ` is usually written "|| ` A `
     ||", but we use function notation to take advantage of our existing
     theorems about functions. $)
  cnm $a class norm $.

  $( Extend class notation with the class of the induced metrics on normed
     complex vector spaces. $)
  cims $a class IndMet $.

  ${
    $d g s m n o t u w x y z $.  $d y G $.  $d y N $.  $d y S $.
    $( Define the class of all normed complex vector spaces. $)
    df-nv $a |- NrmCVec = { <. <. g , s >. , n >. |
      ( <. g , s >. e. CVec /\ n : ran g --> RR /\ A. x e. ran g
        ( ( ( n ` x ) = 0 -> x = ( Id ` g ) ) /\
          A. y e. CC ( n ` ( y s x ) ) = ( ( abs ` y ) x. ( n ` x ) ) /\
          A. y e. ran g ( n ` ( x g y ) ) <_ ( ( n ` x ) + ( n ` y ) ) ) ) } $.

    $( Structure of the class of all normed complex vectors spaces. $)
    nvss $p |- NrmCVec C_ ( ( _V X. _V ) X. _V ) $=
      ( vg vs vn vx vy cnv cv cop cvc wcel crn cr wf cfv wceq wral w3a cvv cxp
      co cc0 cgi wi cabs cmul cc caddc cle wbr copab2 df-nv oprabss eqsstri )
      FAGZBGZHIJUNKZLCGZMDGZUQNZUAOURUNUBNOUCEGZURUOTUQNUTUDNUSUETOEUFPURUTUNTU
      QNUSUTUQNUGTUHUIEUPPQDUPPQZABCUJRRSRSDEACBUKVAABCULUM $.
      $( [28-Nov-2006] $)

    $( A normed complex vector space is a vector space. $)
    nvvcop $p |- ( <. <. G , S >. , N >. e. NrmCVec -> <. G , S >. e. CVec ) $=
      ( vy vg vs vn vx cop cnv wcel cv c1st cfv cvc wceq co wral wa wex cab crn
      cr wf cc0 cgi wi cabs cmul caddc cle wbr w3a copab2 df-nv simp1 ssoprab2i
      cc copab dfoprab2 pm5.32i 2exbii 19.41vv simprbi ssopab2i eqsstri df-opab
      eleq1 bitr3i fveq2 visset op1st syl6eq eleq1d sstri sseli opex elab sylib
      ss2abi ) BAIZCIZJKWBDLZMNZOKZDUAZKWAOKZJWFWBJELZFLZIZOKZWHUBZUCGLZUDZHLZW
      MNZUEPWOWHUFNPUGWCWOWIQWMNWCUHNWPUIQPDURRWOWCWHQWMNWPWCWMNUJQUKULDWLRUMHW
      LRZUMZEFGUNZWFHDEGFUOWSWKEFGUNZWFWRWKEFGWKWNWQUPUQWTWOOKZHGUSZWFWTWOWJPZW
      KSZFTETZHGUSXBWKEFGHUTXEXAHGXEXCFTETZXAXEXCXASZFTETXFXASXGXDEFXCXAWKWOWJO
      VHVAVBXCXAEFVCVIVDVEVFXBWCWOWMIZPZXASZGTHTZDUAWFXAHGDVGXKWEDXKXIGTHTZWEXK
      XIWESZGTHTXLWESXMXJHGXIWEXAXIWDWOOXIWDXHMNWOWCXHMVJWOWMHVKVLVMVNVAVBXIWEH
      GVCVIVDVTVFVOVOVFVPWEWGDWBWACVQWCWBPZWDWAOXNWDWBMNWAWCWBMVJWACBAVQVLVMVNV
      RVS $.
      $( [5-Jun-2008] $)

    $( Define vector addition on a normed complex vector space. $)
    df-va $a |- +v = ( 1st o. 1st ) $.

    $( Define the base set of a normed complex vector space. $)
    df-ba $a |- BaseSet = ( x e. _V |-> ran ( +v ` x ) ) $.

    $( Define scalar multiplication on a normed complex vector space. $)
    df-sm $a |- .s = ( 2nd o. 1st ) $.

    $( Define the zero vector in a normed complex vector space. $)
    df-0v $a |- 0vec = ( Id o. +v ) $.

    $( Define vector subtraction on a normed complex vector space. $)
    df-vs $a |- -v = ( /g o. +v ) $.

    $( Define the norm function in a normed complex vector space. $)
    df-nm $a |- norm = 2nd $.

    $( Define the induced metric on a normed complex vector space. $)
    df-ims $a |- IndMet = ( u e. NrmCVec |->
                            ( ( norm ` u ) o. ( -v ` u ) ) ) $.
  $}

  $( The class of all normed complex vectors spaces is a relation. $)
  nvrel $p |- Rel NrmCVec $=
    ( cnv wrel cvv cxp wss nvss xpss sstri df-rel mpbir ) ABACCDZEAKCDKFKCGHAIJ
    $.
    $( [14-Nov-2006] $)

  ${
    $d x y U $.
    vafval.2 $e |- G = ( +v ` U ) $.
    $( Value of the function for the vector addition (group) operation on a
       normed complex vector space. $)
    vafval $p |- G = ( 1st ` ( 1st ` U ) ) $=
      ( cpv cfv c1st cvv wcel wceq ccom df-va fveq1i wfun wfo fo1st fofun ax-mp
      wf c0 fvprc fof fvco3 mp3an12 syl5eq wn fveq2d 1st0 syl6req eqtrd pm2.61i
      eqtri ) BADEZAFEZFEZCAGHZULUNIUOULAFFJZEZUNADUPKLFMZGGFRZUOUQUNIGGFNZUROG
      GFPQUTUSOGGFUAQGGAFFUBUCUDUOUEZULSUNADTVAUNSFESVAUMSFAFTUFUGUHUIUJUK $.
      $( [23-Apr-2007] $)
  $}

  ${
    $d u U $.
    bafval.1 $e |- X = ( BaseSet ` U ) $.
    bafval.2 $e |- G = ( +v ` U ) $.
    $( Value of the function for the base set of a normed complex vector
       space. $)
    bafval $p |- X = ran G $=
      ( vu cba cfv cpv crn cvv wcel wceq cv fveq2 rneqd df-ba fvex c0 fvprc rn0
      rnex fvmpt wn eqcomi 3eqtr4a pm2.61i rneqi 3eqtr4i ) AGHZAIHZJZCBJAKLZUJU
