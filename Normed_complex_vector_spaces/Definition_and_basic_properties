      DKLCDGMABDECDEFGNOP $.
      $( [4-Dec-2007] $)

    $( The vector addition (group) operation is associative. $)
    nvass $p |- ( ( U e. NrmCVec /\ ( A e. X /\ B e. X /\ C e. X ) ) ->
              ( ( A G B ) G C ) = ( A G ( B G C ) ) ) $=
