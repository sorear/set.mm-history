$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                     Definition and basic properties
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c NrmCVec $.

  $c +v $.
  $c BaseSet $.
  $c .s $.
  $c 0vec $.
  $c -v $.
  $c norm $.

  $c IndMet $.

  $( Extend class notation with the class of all normed complex vector
     spaces. $)
  cnv $a class NrmCVec $.

  $( Extend class notation with vector addition in a normed complex vector
     space.  In the literature, the subscript "v" is omitted, but we need it to
     avoid ambiguity with complex number addition ` + ` ~ caddc . $)
  cpv $a class +v $.

  $( Extend class notation with the base set of a normed complex vector space.
     (Note that ` BaseSet ` is capitalized because, once it is fixed for a
     particular vector space ` U ` , it is not a function, unlike e.g.
     ` norm ` .  This is our typical convention.) $)
  cba $a class BaseSet $.

  $( Extend class notation with scalar multiplication in a normed complex
     vector space.  In the literature scalar multiplication is usually
     indicated by juxtaposition, but we need an explicit symbol to prevent
     ambiguity. $)
  cns $a class .s $.

  $( Extend class notation with zero vector in a normed complex vector
     space. $)
  cn0v $a class 0vec $.

  $( Extend class notation with vector subtraction in a normed complex vector
     space. $)
  cnsb $a class -v $.

  $( Extend class notation with the norm function in a normed complex vector
     space.  In the literature, the norm of ` A ` is usually written "|| ` A `
     ||", but we use function notation to take advantage of our existing
     theorems about functions. $)
  cnm $a class norm $.

  $( Extend class notation with the class of the induced metrics on normed
     complex vector spaces. $)
  cims $a class IndMet $.

  ${
    $d g s m n o t u w x y z $.  $d y G $.  $d y N $.  $d y S $.
    $( Define the class of all normed complex vector spaces. $)
    df-nv $a |- NrmCVec = { <. <. g , s >. , n >. | ( <. g , s >. e. CVec /\
   n : ran g --> RR /\ A. x e. ran g ( ( ( n ` x ) = 0 -> x = ( Id ` g ) )
/\ A. y e. CC ( n ` ( y s x ) ) = ( ( abs ` y ) x. ( n ` x ) ) /\
A. y e. ran g ( n ` ( x g y ) ) <_ ( ( n ` x ) + ( n ` y ) ) ) ) } $.

    $( Structure of the class of all normed complex vectors spaces. $)
    nvss $p |- NrmCVec C_ ( ( _V X. _V ) X. _V ) $=
      ( vg vs vn vx vy cnv cv cop cvc wcel crn cr wf cfv cc0 wceq cgi wi co
      cabs cmul cc wral caddc cle wbr w3a copab2 cvv cxp df-nv oprabss eqsstri
      ) FAGZBGZHIJUNKZLCGZMDGZUQNZOPURUNQNPREGZURUOSUQNUTTNUSUASPEUBUCURUTUNSUQ
      NUSUTUQNUDSUEUFEUPUCUGDUPUCUGZABCUHUIUIUJUIUJDEACBUKVAABCULUM $.
      $( [28-Nov-2006] $)

    $( A normed complex vector space is a vector space. $)
    nvvcop $p |- ( <. <. G , S >. , N >. e. NrmCVec -> <. G , S >. e. CVec ) $=
      ( vy vg vs vn vx cop cnv wcel cv c1st cfv cvc cab crn cr wf cc0 wceq cgi
      wi co cabs cmul cc wral caddc cle wbr w3a copab2 df-nv simp1 ssoprab2i
      copab wa wex dfoprab2 eleq1 pm5.32i 2exbii 19.41vv bitr3i simprbi
      ssopab2i eqsstri df-opab fveq2 visset op1st syl6eq eleq1d ss2abi sstri
      sseli opex elab sylib ) BAIZCIZJKWBDLZMNZOKZDPZKWAOKZJWFWBJELZFLZIZOKZWHQ
      ZRGLZSZHLZWMNZTUAWOWHUBNUAUCWCWOWIUDWMNWCUENWPUFUDUADUGUHWOWCWHUDWMNWPWCW
      MNUIUDUJUKDWLUHULHWLUHZULZEFGUMZWFHDEGFUNWSWKEFGUMZWFWRWKEFGWKWNWQUOUPWTW
      OOKZHGUQZWFWTWOWJUAZWKURZFUSEUSZHGUQXBWKEFGHUTXEXAHGXEXCFUSEUSZXAXEXCXAUR
      ZFUSEUSXFXAURXGXDEFXCXAWKWOWJOVAVBVCXCXAEFVDVEVFVGVHXBWCWOWMIZUAZXAURZGUS
      HUSZDPWFXAHGDVIXKWEDXKXIGUSHUSZWEXKXIWEURZGUSHUSXLWEURXMXJHGXIWEXAXIWDWOO
      XIWDXHMNWOWCXHMVJWOWMHVKVLVMVNVBVCXIWEHGVDVEVFVOVHVPVPVHVQWEWGDWBWACVRWCW
      BUAZWDWAOXNWDWBMNWAWCWBMVJWACBAVRVLVMVNVSVT $.
      $( [5-Jun-2008] $)

    $( Define vector addition on a normed complex vector space. $)
    df-va $a |- +v = ( 1st o. 1st ) $.

    $( Define the base set of a normed complex vector space. $)
    df-ba $a |- BaseSet = { <. x , y >. | y = ran ( +v ` x ) } $.

    $( Define scalar multiplication on a normed complex vector space. $)
    df-sm $a |- .s = ( 2nd o. 1st ) $.

    $( Define the zero vector in a normed complex vector space. $)
    df-0v $a |- 0vec = ( Id o. +v ) $.

    $( Define vector subtraction on a normed complex vector space. $)
    df-vs $a |- -v = ( /g o. +v ) $.

    $( Define the norm function in a normed complex vector space. $)
    df-nm $a |- norm = 2nd $.

    $( Define the induced metric on a normed complex vector space. $)
    df-ims $a |- IndMet = { <. u , d >. | ( u e. NrmCVec
                 /\ d = ( ( norm ` u ) o. ( -v ` u ) ) ) } $.
  $}

  $( The class of all normed complex vectors spaces is a relation. $)
  nvrel $p |- Rel NrmCVec $=
    ( cnv wrel cvv cxp wss nvss xpss sstri df-rel mpbir ) ABACCDZEAKCDKFKCGHAIJ
    $.
    $( [14-Nov-2006] $)

