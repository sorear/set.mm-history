  ${
    $d x y A $.
    $( Any set is strictly dominated by some ordinal. $)
    numthcor $p |- ( A e. V -> E. x e. On A ~< x ) $=
      ( vy cv csdm wbr con0 wrex wceq rexbidv cpw cen numth2 visset canth2 pwex
      breq1 cvv ax-mp ensym wcel wa wi sdomentr sylancr reximi vtoclg ) DEZAEZF
