    $( Lemma for ~ inawina . $)
    inawinalem $p |- ( A e. On ->
        ( A. x e. A ~P x ~< A -> A. x e. A E. y e. A x ~< y ) ) $=
      ( con0 wcel cv cpw csdm wbr wrex wa cen cdom sdomdom breq2 rcla4ev
