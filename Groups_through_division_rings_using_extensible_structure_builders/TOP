      EVRHUKUHULUMUNVLVGCSVLVGBTNCVABTUGGUHUNUOUPDVQUBIUQURJUSUT $.
      $( [18-Oct-2012] $)
  $}

  $( A division ring is a ring. $)
  divrngring $p |- ( R e. DivRingNEW -> R e. RingNEW ) $=
    ( cdivringNEW wcel crgNEW c1 cbs cfv c0g csn cdif cop c2 cmulr cpr cgrpNEW
    eqid isdivrngNEW simplbi ) ABCADCEAFGZAHGZIJKLAMGZKNZOCSAUAUBTSPUAPTPUBPQR
    $.
    $( [8-Sep-2011] $)

  $( A division ring is a group. $)
  divrnggrp $p |- ( R e. DivRingNEW -> R e. GrpNEW ) $=
    ( cdivringNEW wcel crgNEW cgrpNEW divrngring ringgrpNEW syl ) ABCADCAECAFAG
    H $.
    $( [8-Sep-2011] $)

  ${
    divrngmgrp.b $e |- B = ( Base ` R ) $.
