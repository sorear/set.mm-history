    $( The operation of a constructed group. $)
    grpplusgg $p |- ( ( B e. V /\ P e. W ) -> P = ( +g ` G ) ) $=
      ( vp vb cv cnx cbs cfv cop cplusg cpr wceq opeq2 preq1d fveq2d eqeq2d id
      preq2d syl6eqr eqeq12d visset eqid grpplusg vtocl2g ) GIZJKLZHIZMZJNLZUIM
      ZOZNLZPUIUJAMZUNOZNLZPBCNLZPHGABDEUKAPZUPUSUIVAUOURNVAULUQUNUKAUJQRSTUIBP
      ZUIBUSUTVBUAVBURCNVBURUQUMBMZOCVBUNVCUQUIBUMQUBFUCSUDUKUIUOHUEGUEUOUFUGUH
