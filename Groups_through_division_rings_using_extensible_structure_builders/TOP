      GUHUNVJVRVQUQUPUMURDVSUBIUSUTJVAVB $.
      $( [7-Sep-2011] $)
  $}

  $( A division ring is a ring. $)
  divrngring $p |- ( R e. DivRingNEW -> R e. RingNEW ) $=
    ( cdivring wcel crg c1 cbs cfv c0g csn cdif cop c2 cmulr cpr cgrp eqid
    isdivrngNEW simplbi ) ABCADCEAFGZAHGZIJKLAMGZKNZOCSAUAUBTSPUAPTPUBPQR $.
    $( [8-Sep-2011] $)

  $( A division ring is a group. $)
  divrnggrp $p |- ( R e. DivRingNEW -> R e. GrpNEW ) $=
    ( cdivring wcel crg cgrp divrngring ringgrpNEW syl ) ABCADCAECAFAGH $.
    $( [8-Sep-2011] $)

  ${
    divrngmgrp.b $e |- B = ( Base ` R ) $.
    divrngmgrp.t $e |- T = ( .r ` R ) $.
    divrngmgrp.z $e |- Z = ( 0g ` R ) $.
    divrngmgrp.6 $e |- G = { <. 1 , ( B \ { Z } ) >. , <. 2 , T >. } $.
    $( A division ring contains a multiplicative group. $)
    divrngmgrpNEW $p |- ( R e. DivRingNEW -> G e. GrpNEW ) $=
      ( cdivring wcel crg cgrp isdivrngNEW simprbi ) BJKBLKDMKABCDEFGHINO $.
      $( [8-Sep-2011] $)
  $}

  ${
    divrngmcl.1NEW $e |- B = ( Base ` R ) $.
    divrngmcl.3NEW $e |- T = ( .r ` R ) $.
