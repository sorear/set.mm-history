    isdivrng.z $e |- Z = ( 0g ` R ) $.
    isdivrng.g $e |- G = { <. 1 , ( B \ { Z } ) >. , <. 2 , T >. } $.
    $( The predicate "is a division ring". $)
    isdivrngNEW $p |- ( R e. DivRingNEW <-> ( R e. RingNEW /\ G e. GrpNEW ) )
        $=
      ( vr c1 cv cbs cfv c0g csn cdif cop c2 cmulr cpr cgrpNEW wcel crgNEW
      cdivringNEW wceq fveq2 syl6eqr difeq1d sneqd sneqi difeq2d eqtrd opeq2d
      preq12d eleq1d eleq1i syl6bbr df-drngNEW elrab2 ) KJLZMNZVAONZPZQZRZSVATN
