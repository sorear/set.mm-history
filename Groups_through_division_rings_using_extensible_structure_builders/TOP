    $( The unit element of a ring is a left multiplicative identity. $)
    ringlidmNEW $p |- ( ( R e. RingNEW /\ X e. B ) -> ( U T X ) = X ) $=
      ( crgNEW wcel wa co wceq ringidmlemNEW simpld ) BIJEAJKDECLEMEDCLEMABCDEF
      GHNO $.
