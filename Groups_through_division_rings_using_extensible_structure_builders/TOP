  $( A ring is a group. $)
  ringgrpNEW $p |- ( R e. RingNEW -> R e. GrpNEW ) $=
    ( crgNEW wcel cabelNEW cgrpNEW ringablNEW ablgrpNEW syl ) ABCADCAECAFAGH $.
