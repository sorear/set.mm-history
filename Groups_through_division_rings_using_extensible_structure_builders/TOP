$(
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
       Groups through division rings using extensible structure builders
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
$)

  $c Base $.
  $c +g $.
  $c Grp $.
  $c 0g $.
  $c -g $.
  $c Abel $.
  $c .r $.
  $c Ring $.
  $c 1r $.

  $( Extend class notation with the class of all base set extractors. $)
  cbs $a class Base $.

  $( Extend class notation with group operation. $)
  cplusg $a class +g $.

  $( Extend class notation with class of all groups. $)
  cgrp $a class Grp $.

  $( Extend class notation with group identity element. $)
  c0g $a class 0g $.

  $( Extend class notation with inverse of group element. $)
  cminusg $a class -g $.

  $( Extend class notation with class of all Abelian groups. $)
  cabel $a class Abel $.

  $( Extend class notation with ring multiplication. $)
  cmulr $a class .r $.

  $( Extend class notation with class of all rings. $)
  crg $a class Ring $.

  $( Extend class notation with ring unit. $)
  cur $a class 1r $.

  $( Define the class of base set extractors for posets and related
     structures. $)
  df-base $a |- Base = ( p e. _V |-> ( p ` 1 ) ) $.

  ${
    $d p K $.
    baseval.k $e |- K e. _V $.
    $( Value of the base set extractor. $)
    baseval $p |- ( Base ` K ) = ( K ` 1 ) $=
      ( vp cbs c1 df-base strcval ) DCAEBCFG $.
      $( [4-Sep-2011] $)
  $}

  $( Utility theorem: index-independent form of ~ df-base . $)
  baseid $p |- Base = ( f e. _V |-> ( f ` ( Base ` ndx ) ) ) $=
    ( cbs c1 df-base 1nn ndxid ) BACADEF $.
    $( [20-Oct-2012] $)

  $( Define group operation. $)
  df-plusg $a |- +g = ( g e. _V |-> ( g ` 2 ) ) $.

  ${
    $d g G $.
    plusgval.g $e |- G e. _V $.
    $( Value of the group operation extractor. $)
    plusgval $p |- ( +g ` G ) = ( G ` 2 ) $=
      ( vg cplusg c2 df-plusg strcval ) DCAEBCFG $.
      $( [4-Sep-2011] $)
  $}

  $( Utility theorem: index-independent form of ~ df-plusg . $)
  plusgid $p |- +g = ( f e. _V |-> ( f ` ( +g ` ndx ) ) ) $=
    ( cplusg c2 df-plusg 2nn ndxid ) BACADEF $.
    $( [20-Oct-2012] $)

  $( Utility theorem for constructing a group structure without direct
     reference to component indices. $)
  grphyp $p |- ( Base ` ndx ) =/= ( +g ` ndx ) $=
    ( vf cnx cbs cfv c2 cplusg c1 cn wcel wceq 1nn df-base ndxarg ax-mp 1ne2
    eqnetri 2nn df-plusg neeqtrri ) BCDZEBFDZTGEGHITGJKCAGALMNOPEHIUAEJQFAEARMN
    S $.
    $( [20-Oct-2012] $)

  ${
    $d a b c e f g h i k m p q r s t u v w x y z $.
    $( Define class of all groups. $)
    df-grp $a |- Grp = { f | E. g E. p ( g = ( Base ` f )
       /\ p = ( +g ` f ) /\ ( A. a e. g A. b e. g A. c e. g ( ( a p b ) e. g
            /\ ( ( a p b ) p c ) = ( a p ( b p c ) ) ) /\
          E. e e. g A. a e. g ( ( e p a ) = a /\
            E. m e. g ( m p a ) = e ) ) ) } $.

    $( Define group identity element. $)
    df-0g $a |- 0g = ( g e. _V |-> ( iota e ( e e. ( Base ` g ) /\
      A. x e. ( Base ` g ) ( e ( +g ` g ) x ) = x ) ) ) $.

    $( Define inverse of group element. $)
    df-minusg $a |- -g = ( g e. _V |-> ( x e. ( Base ` g ) |-> ( iota w
          ( w e. ( Base ` g ) /\ ( w ( +g ` g ) x ) = ( 0g ` g ) ) ) ) ) $.

    $( Define class of all Abelian groups. $)
    df-abl $a |- Abel = { g e. Grp | A. a e. ( Base ` g )
      A. b e. ( Base ` g ) ( a ( +g ` g ) b ) = ( b ( +g ` g ) a ) } $.

    $( Define ring multiplication. $)
    df-mulr $a |- .r = ( r e. _V |-> ( r ` 3 ) ) $.

    $( Define class of all rings (with unit). $)
    df-ring $a |- Ring = { f e. Abel |
        E. r E. p E. t ( ( r = ( Base ` f ) /\ p = ( +g ` f )
           /\ t = ( .r ` f ) )
       /\ A. x e. r A. y e. r A. z e. r
           ( ( x t y ) e. r /\ ( ( ( x t y ) t z ) = ( x t ( y t z ) )
            /\ ( x t ( y p z ) ) = ( ( x t y ) p ( x t z ) )
            /\ ( ( x p y ) t z ) = ( ( x t z ) p ( y t z ) ) ) )
        /\ E. u e. r A. x e. r ( ( u t x ) = x /\ ( x t u ) = x ) ) } $.

    $( Define ring unit. $)
    df-ur $a |- 1r = ( r e. _V |-> ( iota u ( u e. ( Base ` r ) /\
      A. x e. ( Base ` r ) ( ( u ( .r ` r ) x ) = x
           /\ ( x ( .r ` r ) u ) = x ) ) ) ) $.
  $}

  ${
    $d g B $.  $d g P $.
    grpbase.b $e |- B e. _V $.
    grpbase.p $e |- P e. _V $.
    grpbase.g $e |- G
               = { <. ( Base ` ndx ) , B >. , <. ( +g ` ndx ) , P >. } $.
    $( The base set of a constructed group. $)
    grpbase $p |- B = ( Base ` G ) $=
      ( vf cbs cplusg baseid plusgid grphyp str2v1 ) HIGCABGJGKFDELM $.
      $( [19-Oct-2012] $)

    $( The operation of a constructed group. $)
    grpplusg $p |- P = ( +g ` G ) $=
      ( vf cbs cplusg baseid plusgid grphyp str2v2 ) HIGCABGJGKFDELM $.
      $( [19-Oct-2012] $)
  $}

  ${
    grpstrx.b $e |- B e. _V $.
    grpstrx.p $e |- P e. _V $.
    grpstrx.g $e |- G = { <. 1 , B >. , <. 2 , P >. } $.
    $( Structure scaffold property of an explicitly given group. $)
    $( The base of an explicitly given group. $)
    grpbasex $p |- B = ( Base ` G ) $=
      ( vf cbs c1 c2 cn 1nn elisseti 2nn 1ne2 df-base str2v1x ) HGCIJABDEIKLMJK
      NMOFGPQ $.
      $( [17-Oct-2012] $)

    $( The operation of an explicitly given group. $)
    grpplusgx $p |- P = ( +g ` G ) $=
      ( vf cplusg c1 c2 cn 1nn elisseti 2nn 1ne2 df-plusg str2v2x ) HGCIJABDEIK
      LMJKNMOFGPQ $.
      $( [17-Oct-2012] $)
  $}

  ${
    $d a b c e f g m p B $.  $d f g p G $.  $d a b c e f g m p P $.
    isgrp.b $e |- B = ( Base ` G ) $.
    isgrp.p $e |- P = ( +g ` G ) $.
    $( The predicate "is a group." $)
    isgrp $p |- ( G e. Grp <-> ( G e. _V
            /\ A. a e. B A. b e. B A. c e. B
            ( ( a P b ) e. B /\ ( ( a P b ) P c ) = ( a P ( b P c ) ) )
   /\ E. e e. B A. a e. B ( ( e P a ) = a /\ E. m e. B ( m P a ) = e ) ) ) $=
