    $( Closure of the operation of a group. $)
    grpclNEW $p |- ( ( G e. GrpNEW /\ X e. B /\ Y e. B ) -> ( X P Y ) e. B ) $=
      ( cgrp wcel co w3a wa wceq grplem1 simpld 3anass anidm anbi2i bitri
      sylan2br 3impb ) CHIZDAIZEAIZDEBJZAIZUBUCUDUDKZUFUCUDLZUBUGLUFUEEBJDEEBJB
      JMABCDEEFGNOUGUCUDUDLZLUHUCUDUDPUIUDUCUDQRSTUA $.
      $( [14-Aug-2011] $)

    $( A group operation is associative. $)
    grpassNEW $p |- ( ( G e. GrpNEW /\ ( X e. B /\ Y e. B /\ Z e. B ) ) ->
              ( ( X P Y ) P Z ) = ( X P ( Y P Z ) ) ) $=
      ( cgrp wcel w3a wa co wceq grplem1 simprd ) CIJDAJEAJFAJKLDEBMZAJQFBMDEFB
      MBMNABCDEFGHOP $.
      $( [14-Aug-2011] $)

    $d u x y B $.  $d u x y P $.  $d a b c f u x y $.
    $( A group has a left identity element, and every member has a left
       inverse. $)
    grplidinvNEW $p |- ( G e. GrpNEW ->
         E. u e. B A. x e. B ( ( u P x ) = x /\ E. y e. B ( y P x ) = u ) ) $=
      ( vf vb vc cgrp wcel wtru c2 cstru cv co wceq wa wral wrex eqid isgrpNEW
      simp3bi ) FLMFNIOPZMAQZJQZERZDMUIKQZERUGUHUJERERSTKDUAJDUAADUACQZUGERUGSB
      QUGERUKSBDUBTADUACDUBDEUFCIBFAJKUFUCGHUDUE $.
