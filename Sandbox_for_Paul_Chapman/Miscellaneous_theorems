  ${
    abs2dif.1 $e |- A e. CC $.
    abs2dif.2 $e |- B e. CC $.
    $( Difference of absolute values. $)
    abs2dif $p |- ( ( abs ` A ) - ( abs ` B ) ) <_ ( abs ` ( A - B ) ) $=
      ( cc wcel cabs cfv cmin co cle wbr abs2dift mp2an ) AEFBEFAGHBGHIJABIJGHK
      LCDABMN $.
      $( [7-Sep-2007] $) $( [7-Sep-2007] $)
  $}

  ${
    abs2difabs.1 $e |- A e. CC $.
    abs2difabs.2 $e |- B e. CC $.
    $( Absolute value of difference of absolute values. $)
    abs2difabs $p |- ( abs ` ( ( abs ` A ) - ( abs ` B ) ) ) <_
                     ( abs ` ( A - B ) ) $=
      ( cc wcel cabs cfv cmin co cle wbr abs2difabst mp2an ) AEFBEFAGHBGHIJGHAB
      IJGHKLCDABMN $.
      $( [7-Sep-2007] $) $( [7-Sep-2007] $)
  $}

