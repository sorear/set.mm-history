      $( [6-Sep-2011] $)

    $( Value of a 2-member constructed structure. $)
    stb2val2 $p |- ( F ` 2 ) = B $=
      ( vm c2 cn wcel cle wbr cfv wceq 2nn 2re leidi cv c1 cif ifex wa cfz co
      wral fzprval bitr4i wn wne clt 1lt2 1re ltnei ax-mp pm13.181 mpan2 df-ne
      sylib iffalse syl stbval mp3an ) KLMZVFKKNOKEPCQRRKSTAJUAZUBQZBCUCZCDJEKK
      VHBCFGUDAUBDUAZPBQKVJPCQUEVGVJPVIQJUBKUFUGUHHJBCVJUIUJIVGKQZVHUKZVICQVKVG
      UBULZVLVKKUBULZVMUBKUMOVNUNUBKUOSUPUQVGKUBURUSVGUBUTVAVHBCVBVCVDVE $.
      $( [6-Sep-2011] $)
