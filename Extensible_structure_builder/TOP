$(
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
                    Extensible structure builder
#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#
$)

  $c Struct $.

  $( Extend class notation with extensible structure builders. $)
  cstru $a class Struct ( N , f , ph ) $.

  ${
    $d m N $.  $d m f $.  $d m ph $.
    $( Define an extensible structure builder.  This device provides the
       ability to succesively specialize structures.  For example, a ring can
       be a specialization of a group, and all theorems for groups will
       automatically be applicable to rings.  In the intended applications,
       ` N ` is a positive integer and and ` ph ` is a wff containing ` f `
       free in the form of occurrences of ` ( f `` 1 ) ` ,...,
       ` ( f `` N ) ` .  Note that ` f ` is a bound variable in
       ` Struct ( N , f , ph ) ` . $)
    df-struct $a |- Struct ( N , f , ph ) = { f | ( E. m e. NN ( N <_ m /\
          f Fn ( 1 ... m ) ) /\ ph ) } $.

  $}

  ${
    $d m n N $.  $d m n f $.  $d n ph $.
    $( Version of ~ df-struct with distinct variable requirement of ` m ` and
       ` ph ` removed. $)
    dfstruct2 $p |- Struct ( N , f , ph ) = { f | ( E. m e. NN ( N <_ m /\
          f Fn ( 1 ... m ) ) /\ ph ) } $=
      ( vn cstru cv cle wbr c1 cfz co wfn wa cn wrex cab df-struct wceq breq2
      opreq2 fneq2d anbi12d cbvrexv anbi1i abbii eqtri ) ABDFDEGZHIZBGZJUHKLZMZ
      NZEOPZANZBQDCGZHIZUJJUPKLZMZNZCOPZANZBQABEDRUOVBBUNVAAUMUTECOUHUPSZUIUQUL
      USUHUPDHTVCUKURUJUHUPJKUAUBUCUDUEUFUG $.
      $( [30-Aug-2011] $)

  $}

  ${
    $d m N $.  $d m f x $.
    $( Bound-variable hypothesis builder for an extensible structure
       builder. $)
    hbstr1 $p |- ( x e. Struct ( N , f , ph )
           -> A. f x e. Struct ( N , f , ph ) ) $=
      ( vm cstru cv cle wbr c1 cfz co wfn wa cn wrex cab dfstruct2 hbab1 hbxfr
      ) CBACDFDEGZHICGJUAKLMNEOPANZCQACEDRUBCBST $.
      $( [31-Aug-2011] $)
  $}

  ${
    $d m N $.  $d m f $.
    strss.1 $e |- ( ph -> ps ) $.
    $( Subclass relation for extensible structure builder. $)
    strss $p |- Struct ( N , f , ph ) C_ Struct ( N , f , ps ) $=
      ( vm cv cle wbr c1 cfz co wfn wa cn wrex cab cstru anim2i ss2abi
      dfstruct2 3sstr4i ) DFGZHICGJUCKLMNFOPZANZCQUDBNZCQACDRBCDRUEUFCABUDESTAC
      FDUABCFDUAUB $.
      $( [5-Sep-2011] $)
  $}

  ${
    strbi.1 $e |- ( ph <-> ps ) $.
    $( Equality theorem for extensible structure builder. $)
    strbi $p |- Struct ( N , f , ph ) = Struct ( N , f , ps ) $=
      ( cstru biimpi strss biimpri eqssi ) ACDFBCDFABCDABEGHBACDABEIHJ $.
      $( [6-Sep-2011] $)
  $}

  ${
    $d m f N $.  $d m ph $.
    $( Th. 8.3.77 p. 201 $)
    $( The specialization of an extensible structure builder generates a
       subclass. $)
    strssp1 $p |- ( N e. NN -> Struct ( ( N + 1 ) , f , ( ph /\ ps ) )
                    C_ Struct ( N , f , ph ) ) $=
      ( vm cn wcel c1 caddc co cv cle wbr cfz wfn wa wrex cab cstru cr wi w3a
      lep1 3ad2ant1 letr mpand 3expa nnre peano2re ancli syl syl2an anim1d
      reximdva simpl a1i anim12d ss2abdv dfstruct2 3sstr4g ) DFGZDHIJZEKZLMZCKH
      VCNJOZPZEFQZABPZPZCRDVCLMZVEPZEFQZAPZCRVHCVBSACDSVAVIVMCVAVGVLVHAVAVFVKEF
      VAVCFGZPVDVJVEDTGZVBTGZPZVCTGZVDVJUAZVAVNVOVPVRVSVOVPVRUBDVBLMZVDVJVOVPVT
      VRDUCUDDVBVCUEUFUGVAVOVQDUHVOVPDUIUJUKVCUHULUMUNVHAUAVAABUOUPUQURVHCEVBUS
      ACEDUSUT $.
      $( [29-Jul-2011] $)
  $}

  ${
    $d f m F $.  $d f m N $.  $d f ps $.
    elstr.1 $e |- ( f = F -> ( ph <-> ps ) ) $.
    $( Membership in an extensible structure builder. $)
    elstr $p |- ( F e. Struct ( N , f , ph ) <->
                      ( E. m e. NN ( N <_ m /\ F Fn ( 1 ... m ) ) /\ ps ) ) $=
      ( cstru wcel cv cle wbr c1 cfz co wfn wa cn wrex cab dfstruct2 eleq2i
      cvv oprex fnex mpan2 ad2antll r19.23aiva adantr wceq fneq1 anbi2d
      rexbidv anbi12d elab3 bitri ) EACFHZIEFDJZKLZCJZMURNOZPZQZDRSZAQZCTZIUSEV
      APZQZDRSZBQZUQVFEACDFUAUBVEVJCEVIEUCIZBVHVKDRVGVKURRIUSVGVAUCIVKMURNUDVAU
      CEUEUFUGUHUIUTEUJZVDVIABVLVCVHDRVLVBVGUSVAUTEUKULUMGUNUOUP $.
      $( [12-Aug-2011] $)

    $( Implication of membership in an extensible structure builder. $)
    elstri $p |- ( F e. Struct ( N , f , ph ) -> ps ) $=
      ( vm cstru wcel cv cle wbr c1 cfz co wfn wa cn wrex elstr simprbi ) DACEH
      IEGJZKLDMUBNOPQGRSBABCGDEFTUA $.
      $( [12-Aug-2011] $)
  $}

  ${
    $d f m F $.  $d f m N $.
    elstrscaf.1 $e |- S = Struct ( N , f , T. ) $.
    $( Membership in the "scaffold" of a structure. $)
    elstrscaf $p |- ( F e. S <-> E. m e. NN ( N <_ m /\ F Fn ( 1 ... m ) ) ) $=
      ( wtru cstru wcel cv cle wbr c1 cfz co wfn wa cn wrex wceq biidd elstr
      eleq2i tru biantru 3bitr4i ) DGBEHZIECJZKLDMUHNOPQCRSZGQDAIUIGGBCDEBJDTGU
      AUBAUGDFUCGUIUDUEUF $.
      $( [1-Sep-2011] $)
  $}

  ${
    $d f m F $.  $d f m N $.
    $( A finite sequence is a member of the "scaffold" of a structure. $)
    fnelstr $p |- ( ( N e. NN /\ F Fn ( 1 ... N ) )
                     -> F e. Struct ( N , f , T. ) ) $=
      ( vm cn wcel c1 cfz co wfn wa cv cle wbr wrex wtru cstru cr nnre leid
      syl wceq breq2 opreq2 fneq2d anbi12d rcla4ev ex mpand imp eqid elstrscaf
      sylibr ) CEFZBGCHIZJZKCDLZMNZBGUQHIZJZKZDEOZBPACQZFUNUPVBUNCCMNZUPVBUNCRF
      VDCSCTUAUNVDUPKZVBVAVEDCEUQCUBZURVDUTUPUQCCMUCVFUSUOBUQCGHUDUEUFUGUHUIUJV
      CADBCVCUKULUM $.
      $( [3-Sep-2011] $)
  $}

  ${
    $d f m F $.  $d f m N $.  $d f ps $.
    elstr2.1 $e |- S = Struct ( N , f , T. ) $.
    elstr2.2 $e |- ( f = F -> ( ph <-> ps ) ) $.
    $( Membership in an extensible structure builder that does not reference
       its explicit scaffold. $)
    elstr2 $p |- ( F e. Struct ( N , f , ph ) <-> ( F e. S /\ ps ) ) $=
      ( vm cstru wcel cv cle wbr c1 cfz co wfn wa cn wrex elstr elstrscaf
      anbi1i bitr4i ) EADFJKFILZMNEOUFPQRSITUAZBSECKZBSABDIEFHUBUHUGBCDIEFGUCUD
      UE $.
      $( [30-Aug-2011] $)
  $}

  ${
    $d f m x N $.  $d x ph $.
    $( Removing structures built for for ` N + 1 ` leaves only functions on
       ` ( 1 ... N ) ` . $)
    strdif $p |- ( N e. NN
          -> ( Struct ( N , f , ph ) \ Struct ( ( N + 1 ) , f , ph ) )
                      = { f | ( f Fn ( 1 ... N ) /\ ph ) } ) $=
      ( vm vx cn wcel cv cstru c1 caddc co cdif cfz wfn wa cab wb wal wceq cle
      wbr wrex wn wi wral cr letri3 nnre syl2an clt lenlt ancoms nnltp1le
      notbid bitrd anbi2d eqcom syl5bb anbi1d con2b anbi2i anass an12 abai
