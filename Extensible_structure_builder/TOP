  $}

  ${
    $d x F $.  $d f x N $.
    elstrdiff.1 $e |- ( x e. F -> A. f x e. F ) $.
    elstrdiff.2 $e |- ( ps -> A. f ps ) $.
    elstrdiff.3 $e |- ( f = F -> ( ph <-> ps ) ) $.
    $( Membership in an extensible structure builder for ` N ` but not
       ` N + 1 ` . $)
    elstrdiff $p |- ( N e. NN -> ( F e. ( Struct ( N , f , ph )
       \ Struct ( ( N + 1 ) , f , ph ) ) <-> ( F Fn ( 1 ... N ) /\ ps ) ) ) $=
      ( cn wcel cstru c1 caddc co cdif cv cfz wfn wa cab strdif eleq2d cvv wi
      wb oprex fnex mpan2 adantr ax-17 hbfn hban wceq fneq1 anbi12d elab3gf
      ax-mp syl6bb ) FJKZEADFLADFMNOLPZKEDQZMFROZSZATZDUAZKZEVCSZBTZUTVAVFEADFU
      BUCVIEUDKZUEVGVIUFVHVJBVHVCUDKVJMFRUGVCUDEUHUIUJVEVIDCEUDGVHBDDCVCEGCQVCK
      DUKULHUMVBEUNVDVHABVCVBEUOIUPUQURUS $.
      $( [6-Sep-2011] $)
  $}

  ${
    $d f x F $.  $d f x N $.  $d f ps $.
    elstrdif.1 $e |- ( f = F -> ( ph <-> ps ) ) $.
    $( Membership in an extensible structure builder for ` N ` but not
       ` N + 1 ` . $)
    elstrdif $p |- ( N e. NN -> ( F e. ( Struct ( N , f , ph )
       \ Struct ( ( N + 1 ) , f , ph ) ) <-> ( F Fn ( 1 ... N ) /\ ps ) ) ) $=
      ( vx cv wcel ax-17 elstrdiff ) ABGCDEGHDICJBCJFK $.
      $( [31-Aug-2011] $)
  $}

  ${
    $d f g A $.  $d f g m N $.  $d g ph $.
    eustrdif.x $e |- A e. _V $.
    eustrdif.1 $e |- ( ph <-> A. m e. ( 1 ... N ) ( f ` m ) = A ) $.
    $( Condition implying a unique member in an extensible structure builder
       difference. $)
    eustrdif $p |- ( N e. NN -> E! f f e. ( Struct ( N , f , ph )
       \ Struct ( ( N + 1 ) , f , ph ) ) ) $=
      ( vg cn wcel cv cstru c1 caddc co cdif weu cfz wfn cfv wceq wral wa
      oprex eufnfv fveq1 eqeq1d ralbidv syl5bb elstrdif eubidv mpbiri hbstr1
      hbdif ax-17 eleq1 cbveu sylib ) EIJZHKZACELZACEMNOZLZPZJZHQZCKZVDJZCQUSVF
      UTMEROZSDKZUTTZBUAZDVIUBZUCZHQDVIBHMERUDFUEUSVEVNHAVMCUTEVGUTUAZVJVGTZBUA
      ZDVIUBVMAVOVQVLDVIVOVPVKBVJVGUTUFUGUHGUIUJUKULVEVHHCCHVAVCAHCEUMAHCVBUMUN
      VHHUOUTVGVDUPUQUR $.
      $( [31-Aug-2011] $)
  $}

  ${
    $d f x A $.  $d f x B $.
    eustrdif2.x $e |- A e. _V $.
    eustrdif2.y $e |- B e. _V $.
    eustrdif2.1 $e |- ( ph <-> ( ( f ` 1 ) = A /\ ( f ` 2 ) = B ) ) $.
    $( Condition implying a unique member in a 2-member extensible structure
       builder difference. $)
    eustrdif2 $p |- E! f f e. ( Struct ( 2 , f , ph )
       \ Struct ( ( 2 + 1 ) , f , ph ) ) $=
      ( vx c2 cn wcel cv cstru c1 caddc co cdif weu 2nn wceq cif ifex cfv wa
      cfz wral fzprval bitr4i eustrdif ax-mp ) IJKDLZADIMADINOPMQKDRSAHLZNTZBCU
      AZDHIUMBCEFUBANUKUCBTIUKUCCTUDULUKUCUNTHNIUEPUFGHBCUKUGUHUIUJ $.
      $( [6-Sep-2011] $)
  $}

  $c StrBldr $.

  $( Extend class notation with constructed structure. $)
  ccstr $a class StrBldr ( N , f , ph ) $.

  ${
    $( Define a constructed structure, which is a specific member of a
       ` Struct ` class ( ~ df-struct ). $)
    df-strbldr $a |- StrBldr ( N , f , ph ) = ( iota f
      f e. ( Struct ( N , f , ph ) \ Struct ( ( N + 1 ) , f , ph ) ) ) $.

  $}

  ${
    stbbi.1 $e |- ( ph <-> ps ) $.
    $( Equality theorem for a constructed structure. $)
    stbbi $p |- StrBldr ( N , f , ph ) = StrBldr ( N , f , ps ) $=
      ( cv cstru c1 caddc co cdif wcel cio ccstr wb wceq iotabi strbi difeq12i
      eleq2i mpg df-strbldr 3eqtr4i ) CFZACDGZACDHIJZGZKZLZCMZUDBCDGZBCUFGZKZLZ
      CMZACDNBCDNUIUNOUJUOPCUIUNCQUHUMUDUEUKUGULABCDERABCUFERSTUAACDUBBCDUBUC
      $.
      $( [6-Sep-2011] $)
  $}

  ${
    $d f x $.
    $( Bound-variable hypothesis builder for a constructed structure. $)
    hbstb1 $p |- ( x e. StrBldr ( N , f , ph )
           -> A. f x e. StrBldr ( N , f , ph ) ) $=
      ( ccstr cv cstru c1 caddc co cdif wcel cio df-strbldr hbiota1 hbxfr ) CBA
      CDECFACDGACDHIJGKLZCMACDNQCBOP $.
      $( [6-Sep-2011] $)
  $}

  $( A constructed structure is a set. $)
  stbex $p |- StrBldr ( N , f , ph ) e. _V $=
    ( ccstr cv cstru c1 caddc co cdif wcel cio cvv df-strbldr iotaex eqeltri )
    ABCDBEABCFABCGHIFJKZBLMABCNQBOP $.
    $( [6-Sep-2011] $)

  ${
    $d f A $.  $d f m x N $.  $d x ph $.
    stbcl.1 $e |- A e. _V $.
    stbcl.2 $e |- ( ph <-> A. m e. ( 1 ... N ) ( f ` m ) = A ) $.
    stbcl.3 $e |- F = StrBldr ( N , f , ph ) $.
    $( A constructed structure belongs to its structure class. $)
    stbel $p |- ( N e. NN -> F e. ( Struct ( N , f , ph )
                      \ Struct ( ( N + 1 ) , f , ph ) ) ) $=
      ( vx cn wcel cv cstru c1 caddc co cdif cio weu eustrdif cab iotacl
      hbstr1 hbdif abid2f syl6eleq syl ccstr df-strbldr eqtri syl5eqel ) FKLZCM
      ACFNZACFOPQZNZRZLZCSZUQEUMURCTZUSUQLABCDFGHUAUTUSURCUBUQURCUCCJUQCJUNUPAJ
      CFUDAJCUOUDUEUFUGUHEACFUIUSIACFUJUKUL $.
      $( [6-Sep-2011] $)

    $( A constructed structure belongs to its structure scaffold. $)
    stbcl $p |- ( N e. NN -> F e. Struct ( N , f , T. ) ) $=
      ( cn wcel cstru c1 caddc co cdif wtru stbel difss tru a1i strss sstri
      sseli syl ) FJKEACFLZACFMNOLZPZKEQCFLZKABCDEFGHIRUHUIEUHUFUIUFUGSAQCFQATU
      AUBUCUDUE $.
      $( [6-Sep-2011] $)
  $}

  ${
    $d f x A $.  $d m B $.  $d m x F $.  $d m M $.  $d f m x N $.
    stbval.1 $e |- A e. _V $.
    stbval.2 $e |- ( ph <-> A. m e. ( 1 ... N ) ( f ` m ) = A ) $.
    stbval.3 $e |- F = StrBldr ( N , f , ph ) $.
    stbval.4 $e |- ( m = M -> A = B ) $.
    $( Value of a constructed structure. $)
    stbval $p |- ( ( N e. NN /\ M e. NN /\ M <_ N ) -> ( F ` M ) = B ) $=
      ( vx cn wcel cle wbr w3a c1 cfz co cv cfv wceq wral wa 3simpc wb fznn
      3ad2ant1 mpbird wfn cstru caddc cdif ccstr biid stbbi stbel syl5eqel
      hbstb1 hbxfr ax-17 hbfv hbeq hbral fveq1 eqeq1d ralbidv elstrdiff mpbid
      simprd fveq2 eqeq12d rcla4v sylc ) HNOZGNOZGHPQZRZGSHTUAZOZEUBZFUCZBUDZEW
      AUEZGFUCZCUDZVTWBVRVSUFZVQVRVSUGVQVRWBWIUHVSGHUIUJUKVQVRWFVSVQFWAULZWFVQF
      WCDUBZUCZBUDZEWAUEZDHUMWNDHSUNUAUMUOZOWJWFUFVQADHUPZWOFWNBDEWPHIWNUQAWNDH
