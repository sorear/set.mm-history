    pleval2.l $e |- L = ( le ` K ) $.
    pleval2.s $e |- S = ( lt ` K ) $.
    $( One direction of ~ pleval2 . $)
    pleval2i $p |- ( ( X e. B /\ Y e. B ) ->
      ( X L Y -> ( X S Y \/ X = Y ) ) ) $=
