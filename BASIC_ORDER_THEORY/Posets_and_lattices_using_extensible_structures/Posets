      DEEFGRSUAUHUIUOUNNUJUHUILZUOULUMUTUPUOULABCDFGUBZDEDCUCTUTUPUOUMVADEDCUDT
      UEUFUG $.
      $( [21-Oct-2011] $)

    $( A poset ordering is transitive. $)
    postr $p |- ( ( K e. Poset /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X L Y /\ Y L Z ) -> X L Z ) ) $=
      ( cpo wcel w3a wa wbr wceq wi posi simp3d ) BIJDAJEAJFAJKLDDCMDECMZEDCMLD
      ENOREFCMLDFCMOABCDEFGHPQ $.
      $( [11-Sep-2011] $)
  $}

  ${
    $d x y z B $.  $d x y z K $.  $d x y z ph $.
    isposd.k $e |- ( ph -> K e. _V ) $.
    isposd.b $e |- ( ph -> B = ( Base ` K ) ) $.
    isposd.l $e |- ( ph -> L = ( le ` K ) ) $.
    isposd.1 $e |- ( ( ph /\ x e. B ) -> x L x ) $.
    isposd.2 $e |- ( ( ph /\ x e. B /\ y e. B ) ->
                   ( ( x L y /\ y L x ) -> x = y ) ) $.
    isposd.3 $e |- ( ( ph /\ ( x e. B /\ y e. B /\ z e. B ) ) ->
                   ( ( x L y /\ y L z ) -> x L z ) ) $.
    $( Properties that determine a poset (implicit structure version). $)
    isposd $p |- ( ph -> K e. Poset ) $=
      ( wcel cv wbr wa wi wral breqd cvv cple cfv weq w3a cbs cpo adantrr 3expb
      adantr 3exp2 3jca ralrimiva ralrimivva anbi12d imbi1d 3anbi123d raleqbidv
      imp42 imbi12d anbi2d mpbi2and eqid ispos sylibr ) AFUANZBOZVGFUBUCZPZVGCO
      ZVHPZVJVGVHPZQZBCUDZRZVKVJDOZVHPZQZVGVPVHPZRZUEZDFUFUCZSZCWBSZBWBSZQZFUGN
      AVFVGVGGPZVGVJGPZVJVGGPZQZVNRZWHVJVPGPZQZVGVPGPZRZUEZDESZCESZBESZWFHAWQBC
      EEAVGENZVJENZQQZWPDEXBVPENZQWGWKWOXBWGXCAWTWGXAKUHUJXBWKXCAWTXAWKLUIUJAWT
      XAXCWOAWTXAXCWOMUKUSULUMUNAWSWEVFAWRWDBEWBIAWQWCCEWBIAWPWADEWBIAWGVIWKVOW
      OVTAGVHVGVGJTAWJVMVNAWHVKWIVLAGVHVGVJJTZAGVHVJVGJTUOUPAWMVRWNVSAWHVKWLVQX
      DAGVHVJVPJTUOAGVHVGVPJTUTUQURURURVAVBBCDWBFVHWBVCVHVCVDVE $.
      $( [29-Apr-2014] $)
  $}

  ${
    $d x y z B $.  $d x y z L $.
    isposi.k $e |- K e. _V $.
    isposi.b $e |- B = ( Base ` K ) $.
    isposi.l $e |- L = ( le ` K ) $.
    isposi.1 $e |- ( x e. B -> x L x ) $.
    isposi.2 $e |- ( ( x e. B /\ y e. B ) ->
                   ( ( x L y /\ y L x ) -> x = y ) ) $.
    isposi.3 $e |- ( ( x e. B /\ y e. B /\ z e. B ) ->
                   ( ( x L y /\ y L z ) -> x L z ) ) $.
    $( Properties that determine a poset (implicit structure version). $)
    isposi $p |- K e. Poset $=
      ( cpo wcel cv wbr wa wi w3a wral cvv wceq 3ad2ant1 3adant3 rgen3 mpbir2an
      3jca ispos ) EMNEUANAOZUIFPZUIBOZFPZUKUIFPQUIUKUBRZULUKCOZFPQUIUNFPRZSZCD
      TBDTADTGUPABCDDDUIDNZUKDNZUNDNZSUJUMUOUQURUJUSJUCUQURUMUSKUDLUGUEABCDEFHI
      UHUF $.
      $( [11-Sep-2011] $)
  $}

  ${
    $d p x y z B $.  $d p K $.  $d p x y z L $.
    isposix.a $e |- B e. _V $.
    isposix.b $e |- L e. _V $.
    isposix.k $e |- K = { <. ( Base ` ndx ) , B >. ,
         <. ( le ` ndx ) , L >. } $.
    isposix.1 $e |- ( x e. B -> x L x ) $.
    isposix.2 $e |- ( ( x e. B /\ y e. B ) ->
                   ( ( x L y /\ y L x ) -> x = y ) ) $.
    isposix.3 $e |- ( ( x e. B /\ y e. B /\ z e. B ) ->
                   ( ( x L y /\ y L z ) -> x L z ) ) $.
    $( Properties that determine a poset (explicit structure version).  Note
       that the numeric indices of the structure components are not mentioned
       explicitly in either the theorem or its proof. $)
    isposix $p |- K e. Poset $=
      ( cnx cbs cfv cop cple cvv wcel wceq cpr prex eqeltri posbase ax-mp posle
      isposi ) ABCDEFEMNODPZMQOFPZUARIUHUIUBUCDRSDENOTGDEFRIUDUEFRSFEQOTHDEFRIU
      FUEJKLUG $.
      $( [9-Nov-2012] $)
  $}

  $( Def. 8.4.3 p. 204. $)
  $( Define less-than ordering for posets and related structures.  Unlike
     ~ df-base and ~ df-ple , this is a derived component extractor and not an
     extensible structure component extractor that defines the poset. $)
  df-plt $a |- lt = ( p e. _V |-> ( ( le ` p ) \ _I ) ) $.

  ${
    $d p K $.  $d p L $.
    pltval.l $e |- L = ( le ` K ) $.
    pltval.s $e |- S = ( lt ` K ) $.
    $( Value of the less-than relation. $)
    pltfval $p |- ( K e. A -> S = ( L \ _I ) ) $=
      ( vp wcel cplt cfv cid cdif cvv wceq elex cv cple fveq2 syl6eqr difeq1d
      df-plt fvex eqeltri difexg ax-mp fvmpt syl syl5eq ) CAHZBCIJZDKLZFUICMHUJ
      UKNCAOGCGPZQJZKLUKMIULCNZUMDKUNUMCQJZDULCQRESTGUADMHUKMHDUOMECQUBUCDKMUDU
      EUFUGUH $.
      $( [8-Feb-2015] $)

    $( Less-than relation.  ( ~ df-pss analog.) $)
    pltval $p |- ( ( K e. A /\ X e. B /\ Y e. C )
        -> ( X S Y <-> ( X L Y /\ X =/= Y ) ) ) $=
      ( wcel wbr wne wa wb cid cdif pltfval breqd wn brdif adantl anbi2d syl5bb
      ideqg necon3bbid sylan9bb 3impb ) EAKZGBKZHCKZGHDLZGHFLZGHMZNZOUIULGHFPQZ
      LZUJUKNZUOUIDUPGHADEFIJRSUQUMGHPLZTZNURUOGHFPUAURUTUNUMUKUTUNOUJUKUSGHGHC
      UEUFUBUCUDUGUH $.
      $( [12-Oct-2011] $)

    $( Less-than implies less-than-or-equal.  ( ~ pssss analog.) $)
    pltle $p |- ( ( K e. A /\ X e. B /\ Y e. C ) -> ( X S Y -> X L Y ) ) $=
      ( wcel w3a wbr wne pltval simprbda ex ) EAKGBKHCKLZGHDMZGHFMZRSTGHNABCDEF
      GHIJOPQ $.
      $( [4-Dec-2011] $)
  $}

  ${
    pltne.s $e |- S = ( lt ` K ) $.
    $( Less-than relation.  ( ~ df-pss analog.) $)
    pltne $p |- ( ( K e. A /\ X e. B /\ Y e. C )
        -> ( X S Y -> X =/= Y ) ) $=
      ( wcel w3a wbr wne cple cfv eqid pltval simplbda ex ) EAIFBIGCIJZFGDKZFGL
      ZSTFGEMNZKUAABCDEUBFGUBOHPQR $.
      $( [2-Dec-2011] $)

    $( The less-than relation is not reflexive.  ( ~ pssirr analog.) $)
    pltirr $p |- ( ( K e. A /\ X e. B ) -> -. X S X ) $=
      ( wcel wa wceq wbr wn eqid wne wi pltne 3anidm23 necon2bd mpi ) DAGZEBGZH
      ZEEIEECJZKELUAUBEESTUBEEMNABBCDEEFOPQR $.
      $( [7-Feb-2012] $)
  $}

  ${
