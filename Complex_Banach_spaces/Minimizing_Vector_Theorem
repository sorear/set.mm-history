      $( [8-Feb-2008] $)

    minveccl.q $e |- Q = U. { b e. Y | ( N ` ( A M b ) ) = P } $.
    $( The minimizing vector of ~ minveceu belongs to the subspace ` Y ` . $)
    minveccl $p |- Q e. Y $=
      ( cv co cfv wceq crab cuni wreu wcel minveceu reucl ax-mp eqeltri ) ECMUD
      HUEIUFDUGZMLUHUIZLUCUPMLUJUQLUKABCDFGHIJKLMNOPQRSTUAUBULUPMLUMUNUO $.
      $( [24-Mar-2008] $)

    $( Distance of the minimizing vector of ~ minveceu . $)
    minvecdist $p |- ( N ` ( A M Q ) ) = P $=
      ( vf co cfv wceq cv crab cuni eqcomi wcel wreu wb minveccl minveceu
      hbrab1 hbuni hbxfreq wal wi ax-17 hbopr hbfv hbeq a1i opreq2 fveq2d
