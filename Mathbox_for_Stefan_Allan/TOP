  ${
    foo3.1 $e |- ph $.
    $( A theorem about the universal class. $)
    foo3 $p |- _V = { x | ph } $=
      ( cvv cv wceq cab df-v equid 2th abbii eqtri ) DBEZMFZBGABGBHNABNABICJKL
      $.
      $( [9-Dec-2008] $)
  $}

  $( A partial converse to ~ 19.9t . $)
  xfree $p |- ( A. x ( ph -> A. x ph ) <-> A. x ( E. x ph -> ph ) ) $=
    ( wal wi wex 19.9t a5i hbe1 stdpc5 19.8a syl5 impbii ) AABCZDZBCABEZADZBCZN
    PBABFGPNBQOMAOABABHIABJKGL $.
    $( [21-Dec-2008] $)

  $( A partial converse to ~ 19.9t . $)
  xfree2 $p |- ( A. x ( ph -> A. x ph ) <-> A. x ( -. ph -> A. x -. ph ) ) $=
    ( wal wi wex wn xfree df-ex imbi1i con1b bitri albii ) AABCDBCABEZADZBCAFZO
    BCZDZBCABGNQBNPFZADQMRAABHIPAJKLK $.
    $( [21-Dec-2008] $)

  $( A proof readability experiment for ~ addltmul .  (Contributed by Stefan
     Allan, 30-Oct-2010.) $)
  addltmulALT $p |- ( ( ( A e. RR /\ B e. RR ) /\ ( 2 < A /\ 2 < B ) )
              -> ( A + B ) < ( A x. B ) ) $=
    ( cr wcel wa c2 clt wbr c1 cmin co caddc cmul pm3.27 wb 2re a1i pm3.26 1re
    ltsub1 syl111anc 2cn ax1cn df-2 eqcomi subaddrii breq1i bitrd mpbid
    anim12i an4s wi peano2rem anim1i mulgt1 syl ex adantr cc wceq recn jca
    mulsub breq2d biimpd mulid2i eqcom biimpi ax-mp opreq2d mulid1 adantl
    opreq12d axaddrcl readdcl axmulrcl sylanc ltaddsub2 ltadd1 bicomd sylbird
    3syld mpd ) ACDZBCDZEZFAGHZFBGHZEZEZIAIJKZGHZIBIJKZGHZEZABLKZABMKZGHZWDWGWE
    WHWOWDWGEZWLWEWHEZWNWSWGWLWDWGNWSWGFIJKZWKGHZWLWSFCDZWDICDZWGXBOXCWSPQWDWGR
