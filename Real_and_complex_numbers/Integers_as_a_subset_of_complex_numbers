  $( The natural number property expressed in terms of nonnegative integers. $)
  elnnnn0b $p |- ( N e. NN <-> ( N e. NN0 /\ 0 < N ) ) $=
    ( cn wcel cn0 cc0 clt wbr wa nnnn0 nngt0 cz nn0z elnnz biimpri sylan impbii
    jca ) ABCZADCZEAFGZHRSTAIAJQSAKCZTRALRUATHAMNOP $.
    $( [1-Sep-2005] $)

  $( A nonnegative integer is either 0 or positive.  (Contributed by Paul
     Chapman, 22-Jun-2011.) $)
  elnn00nn $p |- ( N e. NN0 <-> ( N = 0 \/ N e. NN ) ) $=
    ( cn0 wcel cc0 wceq cn wo clt wbr cle nn0ge0 cr wb nn0re 0re leloe mpan syl
    mpbid elnnnn0b baib eqcom a1i orbi12d mpbird orcomd 0nn0 eleq1 mpbiri nnnn0
    jaoi impbii ) ABCZADEZAFCZGUMUOUNUMUOUNGDAHIZDAEZGZUMDAJIZURAKUMALCZUSURMZA
    NDLCUTVAODAPQRSUMUOUPUNUQUOUMUPATUAUNUQMUMADUBUCUDUEUFUNUMUOUNUMDBCUGADBUHU
    IAUJUKUL $.
    $( [22-Jun-2011] $)

  $( The natural number property expressed in terms of nonnegative integers. $)
  elnnnn0c $p |- ( N e. NN <-> ( N e. NN0 /\ 1 <_ N ) ) $=
    ( cn wcel cn0 c1 cle wbr wa nnnn0 nnge1 cz nn0z elnnz1 biimpri sylan impbii
