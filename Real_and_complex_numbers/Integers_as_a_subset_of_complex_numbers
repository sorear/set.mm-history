    UFUCUEEBUCUEEHEAELMNOPAQUERSTUA $.
    $( [9-May-2004] $)

  $( The negative of a nonnegative integer is an integer. $)
  nn0negz $p |- ( N e. NN0 -> -u N e. ZZ ) $=
    ( cn0 wcel cz cneg nn0z znegcl syl ) ABCADCAEDCAFAGH $.
    $( [9-May-2004] $)

