$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Integers (as a subset of complex numbers)
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Define the set of integers.  Definition of integers in [Apostol] p. 22. $)
  df-z $a |- ZZ = { n e. RR | ( n = 0 \/ n e. NN \/ -u n e. NN ) } $.

  ${
    $d x N $.
    $( Membership in the set of integers. $)
    elz $p |- ( N e. ZZ <->
              ( N e. RR /\ ( N = 0 \/ N e. NN \/ -u N e. NN ) ) ) $=
      ( vx cv cc0 wceq cn wcel cneg w3o cr cz eqeq1 eleq1 negeq eleq1d
      3orbi123d df-z elrab2 ) BCZDEZSFGZSHZFGZIADEZAFGZAHZFGZIBAJKSAEZTUDUAUEUC
      UGSADLSAFMUHUBUFFSANOPBQR $.
      $( [8-Jan-2002] $)
  $}

  $( The negative of a natural number is an integer. $)
  nnnegz $p |- ( N e. NN -> -u N e. ZZ ) $=
    ( cneg cz wcel cr cc0 wceq cn w3o elz nnre renegcl syl wo cc nncn negneg
    eleq1d biimprd mpcom olcd df-3or sylibr sylanbrc ) ABZCDUEEDZUEFGZUEHDZUEBZ
    HDZIZAHDZUEJULAEDUFAKALMULUGUHNZUJNUKULUJUMAODZULUJAPUNUJULUNUIAHAQRSTUAUGU
    HUJUBUCUD $.
    $( [12-Jan-2002] $)

  $( An integer is a real. $)
  zre $p |- ( N e. ZZ -> N e. RR ) $=
    ( cz wcel cr cc0 wceq cn cneg w3o elz simplbi ) ABCADCAEFAGCAHGCIAJK $.
    $( [8-Jan-2002] $)

  $( An integer is a complex number. $)
  zcn $p |- ( N e. ZZ -> N e. CC ) $=
    ( cz wcel zre recnd ) ABCAADE $.
    $( [9-May-2004] $)

  ${
    zre.1 $e |- A e. ZZ $.
    $( An integer is a real number. $)
    zrei $p |- A e. RR $=
      ( cz wcel cr zre ax-mp ) ACDAEDBAFG $.
      $( [14-Jul-2005] $)
  $}

  $( The integers are a subset of the reals. $)
  zssre $p |- ZZ C_ RR $=
    ( vx cz cr cv zre ssriv ) ABCADEF $.
    $( [2-Aug-2004] $)

  $( The integers are a subset of the complex numbers. $)
  zsscn $p |- ZZ C_ CC $=
    ( vx cz cc cv zcn ssriv ) ABCADEF $.
    $( [2-Aug-2004] $)

  $( The set of integers exists. $)
  zex $p |- ZZ e. _V $=
    ( cz cc axcnex zsscn ssexi ) ABCDE $.
    $( [30-Jul-2004] $)

  $( Natural number property expressed in terms of integers. $)
  elnnz $p |- ( N e. NN <-> ( N e. ZZ /\ 0 < N ) ) $=
    ( cn wcel cr cneg cc0 wceq wo wa clt wbr cz nnre orc nngt0 jca31 wi idd wn
    ioran lt0neg2 ltnsym renegcl 0re sylancl sylbid imp nsyl wne gt0ne0 df-ne
    sylib sylanbrc pm2.21d jaod ex com23 imp31 impbii w3o elz 3orrot 3orass
    bitri anbi2i anbi1i bitr4i ) ABCZADCZVHAEZBCZAFGZHZHZIZFAJKZIZALCZVPIVHVQVH
    VIVNVPAMVHVMNAOPVIVNVPVHVIVPVNVHVIVPVNVHQVIVPIZVHVHVMVSVHRVSVMVHVMSVKSVLSZV
    SVKVLTVSFVJJKZVKVIVPWASZVIVPVJFJKZWBAUAVJDCFDCWCWBQVIVJFUBAUCUDUEUFUGVJOUHV
    SAFUIVTAUJAFUKULUMUNUOUPUQURUSVRVOVPVRVIVLVHVKUTZIVOAVAWDVNVIWDVHVKVLUTVNVL
    VHVKVBVHVKVLVCVDVEVDVFVG $.
    $( [8-Jan-2002] $)

  $( Zero is an integer. $)
  0z $p |- 0 e. ZZ $=
    ( cc0 cz wcel cr wceq cn cneg w3o elz 0re wo eqid orci 3orass mpbir
    mpbir2an ) ABCADCAAEZAFCZAGFCZHZAIJTQRSKZKQUAALMQRSNOP $.
    $( [12-Jan-2002] $)

  $( Nonnegative integer property expressed in terms of integers. $)
  elnn0z $p |- ( N e. NN0 <-> ( N e. ZZ /\ 0 <_ N ) ) $=
    ( cn0 wcel cn cc0 wceq wo cz clt wbr wa cle elnn0 elnnz eqcom orbi12i id
    0z eleq1 mpbii jaoi orc impbii anbi1i ordir cr wb leloe 0re zre sylancr
    pm5.32i 3bitr4i 3bitri ) ABCADCZAEFZGAHCZEAIJZKZEAFZGZUQEALJZKZAMUOUSUPUTAN
    AEOPUQUTGZURUTGZKUQVEKVAVCVDUQVEVDUQUQUQUTUQQUTEHCUQREAHSTUAUQUTUBUCUDUQURU
    TUEUQVBVEEUFCAUFCVBVEUGUQEAUHUIAUJUKULUMUN $.
    $( [9-May-2004] $)

  $( Integer property expressed in terms nonnegative integers and natural
     numbers. $)
  elznn0nn $p |- ( N e. ZZ <-> ( N e. NN0 \/ ( N e. RR /\ -u N e. NN ) ) ) $=
    ( cz wcel cr cc0 wceq cn cneg w3o wa wo cn0 elz df-3or anbi2i andi nn0re
    pm4.71ri elnn0 orcom bitri orbi1i bitr4i 3bitri ) ABCADCZAEFZAGCZAHGCZIZJUE
    UFUGKZUHKZJZALCZUEUHJZKZAMUIUKUEUFUGUHNOULUEUJJZUNKUOUEUJUHPUMUPUNUMUEUMJUP
    UMUEAQRUMUJUEUMUGUFKUJASUGUFTUAOUAUBUCUD $.
    $( [10-May-2004] $)

  $( Integer property expressed in terms of nonnegative integers. $)
  elznn0 $p |- ( N e. ZZ <-> ( N e. RR /\ ( N e. NN0 \/ -u N e. NN0 ) ) ) $=
    ( cz wcel cr cc0 wceq cn cneg w3o wa cn0 wo elz wb elnn0 a1i cc negcon1
    recn 0cn sylancl neg0 eqeq1i eqcom bitri syl6bb orbi2d syl5bb orbi12d
    3orass orcom orordir 3bitrri syl6rbb pm5.32i ) ABCADCZAEFZAGCZAHZGCZIZJUPAK
    CZUSKCZLZJAMUPVAVDUPVDURUQLZUTUQLZLZVAUPVBVEVCVFVBVENUPAOPUPUTUSEFZLVFVCUPV
    HUQUTUPVHEHZAFZUQAQCEQCVHVJNUPAERASTUAVJEAFUQVIEAUBUCEAUDUEUFUGUSOUHUIVAUQU
    RUTLZLVKUQLVGUQURUTUJUQVKUKURUTUQULUMUNUOUE $.
    $( [9-May-2004] $)

  $( Integer property expressed in terms natural numbers and nonnegative
     integers. $)
  elznn $p |- ( N e. ZZ <-> ( N e. RR /\ ( N e. NN \/ -u N e. NN0 ) ) ) $=
    ( cz wcel cr cc0 wceq cn cneg w3o wa cn0 wo elz cc wb recn negeq0 syl
    orbi2d elnn0 syl6rbbr 3orrot 3orass bitri pm5.32i ) ABCADCZAEFZAGCZAHZGCZIZ
    JUFUHUIKCZLZJAMUFUKUMUFUMUHUJUGLZLZUKUFULUNUHUFUNUJUIEFZLULUFUGUPUJUFANCUGU
    POAPAQRSUITUASUKUHUJUGIUOUGUHUJUBUHUJUGUCUDUAUEUD $.
    $( [12-Jul-2005] $)

  $( Natural numbers are a subset of integers. $)
  nnssz $p |- NN C_ ZZ $=
    ( vx cn cz cv wcel cc0 clt wbr elnnz simplbi ssriv ) ABCADZBELCEFLGHLIJK $.
    $( [9-Jan-2002] $)

  $( Nonnegative integers are a subset of the integers. $)
  nn0ssz $p |- NN0 C_ ZZ $=
    ( cn0 cn cc0 csn cun cz df-n0 nnssz wcel wss 0z elisseti snss mpbi unssi
    eqsstri ) ABCDZEFGBQFHCFIQFJKCFCFKLMNOP $.
    $( [9-May-2004] $)

  $( A natural number is an integer. $)
  nnz $p |- ( N e. NN -> N e. ZZ ) $=
    ( cn cz nnssz sseli ) BCADE $.
    $( [9-May-2004] $)

  $( A nonnegative integer is an integer. $)
  nn0z $p |- ( N e. NN0 -> N e. ZZ ) $=
    ( cn0 cz nn0ssz sseli ) BCADE $.
    $( [9-May-2004] $)

  $( Natural number property expressed in terms of integers. $)
  elnnz1 $p |- ( N e. NN <-> ( N e. ZZ /\ 1 <_ N ) ) $=
    ( cn wcel cz c1 cle wbr wa nnz nnge1 jca cneg cc0 wceq wo wn ioran clt cr
    lt01 wb 1re lt0neg2 ax-mp mpbi wi leneg mpan renegcl renegcli 0re lelttr
    mp3an23 ltle mpan2 syld syl exp3a sylbid imp mpi lenlt sylancl adantr
    mpbid zre sylan nngt0 nsyl breq2 lenlti syl6bb con2bid mpbii con2i adantl
    sylanbrc w3o elz simpr 3orrot df-3or bitr3i sylib sylbi ord mpd impbii ) AB
    CZADCZEAFGZHZWIWJWKAIAJKWLALZBCZAMNZOZPZWIWQWNPWOPZWLWNWOQWLMWMRGZWNASCZWKW
