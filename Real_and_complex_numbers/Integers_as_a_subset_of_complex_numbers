  $( Second Peano postulate generalized to integers. $)
  peano2z $p |- ( N e. ZZ -> ( N + 1 ) e. ZZ ) $=
    ( cz wcel c1 caddc co 1z zaddcl mpan2 ) ABCDBCADEFBCGADHI $.
    $( [13-Feb-2005] $)

  ${
    peano2zd.1 $e |- ( ph -> N e. ZZ ) $.
    $( Deduction from second Peano postulate generalized to integers. $)
    peano2zdi $p |- ( ph -> ( N + 1 ) e. ZZ ) $=
      ( cz wcel c1 caddc co peano2z syl ) ABDEBFGHDECBIJ $.
      $( [26-Nov-2005] $)
  $}

  $( Closure of subtraction of integers. $)
  zsubcl $p |- ( ( M e. ZZ /\ N e. ZZ ) -> ( M - N ) e. ZZ ) $=
    ( cz wcel wa cneg caddc co cmin cc wceq negsub zcn syl2an zaddcl znegcl
    sylan2 eqeltrrd ) ACDZBCDZEABFZGHZABIHZCAJDBJDUBUCKSTABLAMBMNSUACDUBCDTAUAO
    BPQR $.
    $( [2-Aug-2006] $) $( [11-May-2004] $)

  $( "Reverse" second Peano postulate for integers. $)
  peano2zm $p |- ( N e. ZZ -> ( N - 1 ) e. ZZ ) $=
    ( cz wcel c1 cmin co 1z zsubcl mpan2 ) ABCDBCADEFBCGADHI $.
    $( [12-Sep-2005] $)

