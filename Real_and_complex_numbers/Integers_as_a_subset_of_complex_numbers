$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Integers (as a subset of complex numbers)
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Define the set of integers, which are the positive and negative natural
     numbers together with zero.  Definition of integers in [Apostol] p. 22.
     The letter Z abbreviates the German word Zahlen meaning "numbers." $)
  df-z $a |- ZZ = { n e. RR | ( n = 0 \/ n e. NN \/ -u n e. NN ) } $.

  ${
    $d x N $.
    $( Membership in the set of integers. $)
    elz $p |- ( N e. ZZ <->
              ( N e. RR /\ ( N = 0 \/ N e. NN \/ -u N e. NN ) ) ) $=
      ( vx cv cc0 wceq cn wcel cneg w3o cr cz eqeq1 eleq1 eleq1d 3orbi123d df-z
      negeq elrab2 ) BCZDEZSFGZSHZFGZIADEZAFGZAHZFGZIBAJKSAEZTUDUAUEUCUGSADLSAF
      MUHUBUFFSAQNOBPR $.
      $( [8-Jan-2002] $)
  $}

  $( The negative of a natural number is an integer. $)
  nnnegz $p |- ( N e. NN -> -u N e. ZZ ) $=
    ( cn wcel cneg cr cc0 wceq w3o cz nnre renegcl syl wo cc nncn negneg eleq1d
    biimprd mpcom olcd df-3or sylibr elz sylanbrc ) ABCZADZECZUFFGZUFBCZUFDZBCZ
    HZUFICUEAECUGAJAKLUEUHUIMZUKMULUEUKUMANCZUEUKAOUNUKUEUNUJABAPQRSTUHUIUKUAUB
    UFUCUD $.
    $( [12-Jan-2002] $)

  $( An integer is a real. $)
  zre $p |- ( N e. ZZ -> N e. RR ) $=
    ( cz wcel cr cc0 wceq cn cneg w3o elz simplbi ) ABCADCAEFAGCAHGCIAJK $.
    $( [8-Jan-2002] $)

  $( An integer is a complex number. $)
  zcn $p |- ( N e. ZZ -> N e. CC ) $=
    ( cz wcel zre recnd ) ABCAADE $.
    $( [9-May-2004] $)

  ${
    zre.1 $e |- A e. ZZ $.
    $( An integer is a real number. $)
    zrei $p |- A e. RR $=
      ( cz wcel cr zre ax-mp ) ACDAEDBAFG $.
      $( [14-Jul-2005] $)
  $}

  $( The integers are a subset of the reals. $)
  zssre $p |- ZZ C_ RR $=
    ( vx cz cr cv zre ssriv ) ABCADEF $.
    $( [2-Aug-2004] $)

  $( The integers are a subset of the complex numbers. $)
  zsscn $p |- ZZ C_ CC $=
    ( vx cz cc cv zcn ssriv ) ABCADEF $.
    $( [2-Aug-2004] $)

  ${
    $d x y $.
    $( The set of integers exists.  (Revised by Mario Carneiro,
       16-Jun-2013.) $)
    zex $p |- ZZ e. _V $=
      ( vy vx cz cn0 cn cv cneg cmpt crn cun nn0ex wfn cvv wcel negex a1i wa wo
      wceq syl eqid fnmpt mprg nnex fnex mp2an rnex cr cc0 w3o elz df-3or elnn0
      unex wi elun1 sylbir orcoms cfv negeq fvmpt recn negneg sylan9eq fnfvelrn
      cc mpan adantr eqeltrrd elun2 expcom jaod syl5bi imp sylbi ssriv ssexi )
      CDAEAFZGZHZIZJZDWAKVTVTELZEMNVTMNVSMNZWCAEAEVSVTMVTUAZUBWDVRENVROPUCZUDEM
      VTUEUFUGUNBCWBBFZCNWGUHNZWGUISZWGENZWGGZENZUJZQWGWBNZWGUKWHWMWNWMWIWJRZWL
      RWHWNWIWJWLULWHWOWNWLWOWNUOWHWJWIWNWJWIRWGDNWNWGUMWGDWAUPUQURPWLWHWNWLWHQ
      ZWGWANWNWPWKVTUSZWGWAWLWHWQWKGZWGAWKVSWREVTVRWKUTWEWKOVAWHWGVFNWRWGSWGVBW
      GVCTVDWLWQWANZWHWCWLWSWFEWKVTVEVGVHVIWGWADVJTVKVLVMVNVOVPVQ $.
      $( [16-Jun-2013] $) $( [30-Jul-2004] $)
  $}

  $( Natural number property expressed in terms of integers. $)
  elnnz $p |- ( N e. NN <-> ( N e. ZZ /\ 0 < N ) ) $=
    ( cn wcel cr cneg cc0 wceq wo wa clt wbr cz orc nngt0 jca31 wi wn w3o bitri
    nnre idd lt0neg2 renegcl 0re ltnsym sylancl sylbid imp nsyl wne df-ne sylib
    gt0ne0 ioran sylanbrc pm2.21d jaod ex com23 impbii elz 3orrot 3orass anbi2i
    imp31 anbi1i bitr4i ) ABCZADCZVHAEZBCZAFGZHZHZIZFAJKZIZALCZVPIVHVQVHVIVNVPA
    TVHVMMANOVIVNVPVHVIVPVNVHVIVPVNVHPVIVPIZVHVHVMVSVHUAVSVMVHVSVKQVLQZVMQVSFVJ
    JKZVKVIVPWAQZVIVPVJFJKZWBAUBVIVJDCFDCWCWBPAUCUDVJFUEUFUGUHVJNUIVSAFUJVTAUMA
    FUKULVKVLUNUOUPUQURUSVEUTVRVOVPVRVIVLVHVKRZIVOAVAWDVNVIWDVHVKVLRVNVLVHVKVBV
    HVKVLVCSVDSVFVG $.
    $( [8-Jan-2002] $)

  $( Zero is an integer. $)
  0z $p |- 0 e. ZZ $=
    ( cc0 cz wcel cr wceq cn cneg w3o elz 0re eqid orci 3orass mpbir mpbir2an
    wo ) ABCADCAAEZAFCZAGFCZHZAIJTQRSPZPQUAAKLQRSMNO $.
    $( [12-Jan-2002] $)

  $( Nonnegative integer property expressed in terms of integers. $)
  elnn0z $p |- ( N e. NN0 <-> ( N e. ZZ /\ 0 <_ N ) ) $=
    ( cn0 wcel cn cc0 wceq wo cz clt wbr wa cle elnn0 elnnz eqcom orbi12i id 0z
    eleq1 cr mpbii jaoi orc impbii anbi1i ordir 0re zre sylancr pm5.32i 3bitr4i
    wb leloe 3bitri ) ABCADCZAEFZGAHCZEAIJZKZEAFZGZUQEALJZKZAMUOUSUPUTANAEOPUQU
    TGZURUTGZKUQVEKVAVCVDUQVEVDUQUQUQUTUQQUTEHCUQREAHSUAUBUQUTUCUDUEUQURUTUFUQV
    BVEUQETCATCVBVEULUGAUHEAUMUIUJUKUN $.
    $( [9-May-2004] $)

  $( Integer property expressed in terms nonnegative integers and natural
     numbers. $)
  elznn0nn $p |- ( N e. ZZ <-> ( N e. NN0 \/ ( N e. RR /\ -u N e. NN ) ) ) $=
    ( cz wcel cr cc0 wceq cn cneg w3o wa wo cn0 elz df-3or anbi2i andi pm4.71ri
    nn0re elnn0 bitri orcom orbi1i bitr4i 3bitri ) ABCADCZAEFZAGCZAHGCZIZJUEUFU
    GKZUHKZJZALCZUEUHJZKZAMUIUKUEUFUGUHNOULUEUJJZUNKUOUEUJUHPUMUPUNUMUEUMJUPUMU
    EARQUMUJUEUMUGUFKUJASUGUFUATOTUBUCUD $.
    $( [10-May-2004] $)

  $( Integer property expressed in terms of nonnegative integers. $)
  elznn0 $p |- ( N e. ZZ <-> ( N e. RR /\ ( N e. NN0 \/ -u N e. NN0 ) ) ) $=
    ( cz wcel cr cc0 wceq cn cneg w3o wa cn0 wo elz wb elnn0 a1i recn 0cn bitri
    cc negcon1 sylancl eqeq1i eqcom syl6bb orbi2d syl5bb orbi12d 3orass orordir
    neg0 orcom 3bitrri syl6rbb pm5.32i ) ABCADCZAEFZAGCZAHZGCZIZJUPAKCZUSKCZLZJ
    AMUPVAVDUPVDURUQLZUTUQLZLZVAUPVBVEVCVFVBVENUPAOPVCUTUSEFZLUPVFUSOUPVHUQUTUP
    VHEHZAFZUQUPATCETCVHVJNAQRAEUAUBVJEAFUQVIEAUKUCEAUDSUEUFUGUHVAUQURUTLZLVKUQ
    LVGUQURUTUIUQVKULURUTUQUJUMUNUOS $.
    $( [9-May-2004] $)

  $( Integer property expressed in terms natural numbers and nonnegative
     integers. $)
  elznn $p |- ( N e. ZZ <-> ( N e. RR /\ ( N e. NN \/ -u N e. NN0 ) ) ) $=
    ( cz wcel cr cc0 wceq cn cneg w3o wa cn0 wo elz recn negeq0 orbi2d syl6rbbr
    cc wb bitri syl elnn0 3orrot 3orass pm5.32i ) ABCADCZAEFZAGCZAHZGCZIZJUFUHU
    IKCZLZJAMUFUKUMUFUMUHUJUGLZLZUKUFULUNUHUFUNUJUIEFZLULUFUGUPUJUFARCUGUPSANAO
    UAPUIUBQPUKUHUJUGIUOUGUHUJUCUHUJUGUDTQUET $.
    $( [12-Jul-2005] $)

  $( Natural numbers are a subset of integers. $)
  nnssz $p |- NN C_ ZZ $=
    ( vx cn cz cv wcel cc0 clt wbr elnnz simplbi ssriv ) ABCADZBELCEFLGHLIJK $.
    $( [9-Jan-2002] $)

  $( Nonnegative integers are a subset of the integers. $)
  nn0ssz $p |- NN0 C_ ZZ $=
    ( cn0 cn cc0 csn cun cz df-n0 nnssz wcel wss 0z elisseti snss unssi eqsstri
    mpbi ) ABCDZEFGBQFHCFIQFJKCFCFKLMPNO $.
    $( [9-May-2004] $)

  $( A natural number is an integer. $)
  nnz $p |- ( N e. NN -> N e. ZZ ) $=
    ( cn cz nnssz sseli ) BCADE $.
    $( [9-May-2004] $)

  $( A nonnegative integer is an integer. $)
  nn0z $p |- ( N e. NN0 -> N e. ZZ ) $=
    ( cn0 cz nn0ssz sseli ) BCADE $.
    $( [9-May-2004] $)

  $( Natural number property expressed in terms of integers. $)
  elnnz1 $p |- ( N e. NN <-> ( N e. ZZ /\ 1 <_ N ) ) $=
    ( cn wcel c1 cle wbr wa cneg cc0 wo wn clt cr lt01 wb 1re wi 0re adantr w3o
    cz nnz nnge1 jca wceq lt0neg2 ax-mp mpbi leneg mpan renegcl renegcli lelttr
    zre mp3an23 ltle mpan2 syl exp3a sylbid imp lenlt sylancl mpbid sylan nngt0
    syld mpi nsyl breq2 lenlti syl6bb con2bid mpbii con2i adantl ioran sylanbrc
    elz simpr 3orrot df-3or bitr3i sylib sylbi ord mpd impbii ) ABCZAUACZDAEFZG
    ZWIWJWKAUBAUCUDWLAHZBCZAIUEZJZKZWIWLWNKWOKZWQWLIWMLFZWNWJAMCZWKWSKZAUNWTWKG
    ZWMIEFZXAXBDHZILFZXCIDLFZXENDMCZXFXEOPDUFUGUHWTWKXEXCQZWTWKWMXDEFZXHXGWTWKX
    IOPDAUIUJWTXIXEXCWTWMMCZXIXEGZXCQAUKZXJXKWMILFZXCXJXDMCIMCZXKXMQDPULRWMXDIU
    MUOXJXNXMXCQRWMIUPUQVGURUSUTVAVHWTXCXAOZWKWTXJXNXOXLRWMIVBVCSVDVEWMVFVIWKWR
    WJWOWKWOXFWKKNWOWKXFWOWKDIEFXFKAIDEVJDIPRVKVLVMVNVOVPWNWOVQVRWJWQWIQWKWJWPW
    IWJWTWOWIWNTZGZWPWIJZAVSXQXPXRWTXPVTXPWNWOWITXRWNWOWIWAWNWOWIWBWCWDWEWFSWGW
    H $.
    $( [10-May-2004] $)

  $( An integer is not a natural number iff it is less than one. $)
  znnnlt1 $p |- ( N e. ZZ -> ( -. N e. NN <-> N < 1 ) ) $=
    ( cz wcel cn wn c1 cle wbr clt elnnz1 baib notbid cr zre 1re sylancl bitr4d
    wb ltnle ) ABCZADCZEFAGHZEZAFIHZTUAUBUATUBAJKLTAMCFMCUDUCRANOAFSPQ $.
    $( [13-Jul-2005] $)

  $( Natural numbers expressed as a subset of integers. $)
  nnzrab $p |- NN = { x e. ZZ | 1 <_ x } $=
    ( cn cv cz wcel c1 cle wbr wa cab crab elnnz1 abbi2i df-rab eqtr4i ) BACZDE
    FPGHZIZAJQADKRABPLMQADNO $.
    $( [3-Oct-2004] $)

  $( Nonnegative integers expressed as a subset of integers. $)
  nn0zrab $p |- NN0 = { x e. ZZ | 0 <_ x } $=
    ( cn0 cv cz wcel cc0 cle wbr wa cab crab elnn0z abbi2i df-rab eqtr4i ) BACZ
    DEFPGHZIZAJQADKRABPLMQADNO $.
    $( [3-Oct-2004] $)

  $( One is an integer. $)
  1z $p |- 1 e. ZZ $=
    ( c1 cn wcel cz 1nn nnz ax-mp ) ABCADCEAFG $.
    $( [10-May-2004] $)

  $( Two is an integer. $)
  2z $p |- 2 e. ZZ $=
    ( c2 cn wcel cz 2nn nnz ax-mp ) ABCADCEAFG $.
    $( [10-May-2004] $)

  $( Subtraction of nonnegative integers. $)
  nn0sub $p |- ( ( M e. NN0 /\ N e. NN0 ) ->
                ( M <_ N <-> ( N - M ) e. NN0 ) ) $=
    ( cn0 wcel cn cc0 wceq wo wbr cmin co wb clt cr eleq1d cc mpbiri jaoi eleq1
    syl cle elnn0 wa wi nnsub ex cneg nnre lt0neg1 df-neg eleq1i cz nnnegz baib
    elnnz syl5rbbr bitrd breq2 opreq1 bibi12d syl5ibrcom jaod nnz subid1 bitr2d
    zcn breq1 opreq2 syl5ibr 0re ltnri 0nnn 0cn subidi mtbir 2false nncn subeq0
    imp eqcom syl6rbbr ancoms syl2an orbi12d leloe a1i 3bitr4d syl2anb ) ACDAED
    ZAFGZHZBEDZBFGZHZABUAIZBAJKZCDZLBCDAUBBUBWKWNUCZABMIZABGZHZWPEDZWPFGZHZWOWQ
