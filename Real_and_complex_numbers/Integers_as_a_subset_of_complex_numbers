$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Integers (as a subset of complex numbers)
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Define the set of integers.  Definition of integers in [Apostol] p. 22. $)
  df-z $a |- ZZ = { n e. RR | ( n = 0 \/ n e. NN \/ -u n e. NN ) } $.

  ${
    $d x N $.
    $( Membership in the set of integers. $)
    elz $p |- ( N e. ZZ <->
              ( N e. RR /\ ( N = 0 \/ N e. NN \/ -u N e. NN ) ) ) $=
      ( vx cv cc0 wceq cn wcel cneg w3o cr cz eqeq1 eleq1 negeq eleq1d
      3orbi123d df-z elrab2 ) BCZDEZSFGZSHZFGZIADEZAFGZAHZFGZIBAJKSAEZTUDUAUEUC
      UGSADLSAFMUHUBUFFSANOPBQR $.
      $( [8-Jan-2002] $)
  $}

  $( The negative of a natural number is an integer. $)
  nnnegz $p |- ( N e. NN -> -u N e. ZZ ) $=
    ( cn wcel cneg cr cc0 wceq w3o wa cz nnret renegclt syl wo cc nncnt
    negnegt eleq1d biimprd mpcom olc df-3or sylibr jca elz ) ABCZADZECZUGFGZUGB
    CZUGDZBCZHZIUGJCUFUHUMUFAECUHAKALMUFUIUJNZULNZUMUFULUOAOCZUFULAPUPULUFUPUKA
    BAQRSTULUNUAMUIUJULUBUCUDUGUEUC $.
    $( [12-Jan-2002] $)

  $( An integer is a real. $)
  zret $p |- ( N e. ZZ -> N e. RR ) $=
    ( cz wcel cr cc0 wceq cn cneg w3o elz pm3.26bi ) ABCADCAEFAGCAHGCIAJK $.
    $( [8-Jan-2002] $)

  $( An integer is a complex number. $)
  zcnt $p |- ( N e. ZZ -> N e. CC ) $=
    ( cz wcel zret recnd ) ABCAADE $.
    $( [6-Jun-2004] $) $( [9-May-2004] $)

  ${
    zre.1 $e |- A e. ZZ $.
    $( An integer is a real number. $)
    zre $p |- A e. RR $=
      ( cz wcel cr zret ax-mp ) ACDAEDBAFG $.
      $( [15-Jul-2005] $) $( [14-Jul-2005] $)
  $}

  $( The integers are a subset of the reals. $)
  zssre $p |- ZZ (_ RR $=
    ( vx cz cr cv zret ssriv ) ABCADEF $.
    $( [5-Sep-2004] $) $( [2-Aug-2004] $)

  $( The integers are a subset of the complex numbers. $)
  zsscn $p |- ZZ (_ CC $=
    ( vx cz cc cv zcnt ssriv ) ABCADEF $.
    $( [10-Sep-2004] $) $( [2-Aug-2004] $)

  $( The set of integers exists. $)
  zex $p |- ZZ e. V $=
    ( cz cc axcnex zsscn ssexi ) ABCDE $.
    $( [25-Sep-2004] $) $( [30-Jul-2004] $)

  $( Natural number property expressed in terms of integers. $)
  elnnz $p |- ( N e. NN <-> ( N e. ZZ /\ 0 < N ) ) $=
