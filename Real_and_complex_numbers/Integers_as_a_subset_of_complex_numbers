  $( The natural number property expressed in terms of nonnegative integers. $)
  elnnnn0c $p |- ( N e. NN <-> ( N e. NN0 /\ 1 <_ N ) ) $=
    ( cn wcel cn0 c1 cle wbr wa nnnn0 nnge1 jca cz elnnz1 biimpri nn0z sylan
    impbii ) ABCZADCZEAFGZHRSTAIAJKALCZTRSRUATHAMNAOPQ $.
    $( [10-Jan-2006] $)

  $( A nonnegative integer plus 1 is a natural number.  (Contributed by Raph
     Levien, 30-Jun-2006.) $)
  nn0p1nn $p |- ( N e. NN0 -> ( N + 1 ) e. NN ) $=
    ( cn0 wcel cc c1 caddc co cn elnn0nn simprbi ) ABCADCAEFGHCAIJ $.
    $( [30-Jun-2006] $)

  $( A natural number minus 1 is a nonnegative integer.  (Contributed by Jason
     Orendorff, 24-Jan-2007.) $)
  nnm1nn0 $p |- ( N e. NN -> ( N - 1 ) e. NN0 ) $=
    ( cn wcel cc c1 cmin co cn0 elnnnn0 simprbi ) ABCADCAEFGHCAIJ $.
    $( [24-Jan-2007] $)

  $( The positive difference of unequal integers is a natural number.
     (Generalization of ~ nnsub .) $)
  znnsub $p |- ( ( M e. ZZ /\ N e. ZZ ) -> ( M < N <-> ( N - M ) e. NN ) ) $=
    ( cz wcel wa clt wbr cmin co cc0 cn cr wb posdif zre syl2an zsubcl ancoms
    biantrurd bitrd elnnz syl6bbr ) ACDZBCDZEZABFGZBAHIZCDZJUGFGZEZUGKDUEUFUIUJ
    ALDBLDUFUIMUCUDABNAOBOPUEUHUIUDUCUHBAQRSTUGUAUB $.
    $( [11-May-2004] $)

  $( The nonnegative difference of integers is a nonnegative integer.
     (Generalization of ~ nn0sub .) $)
  znn0sub $p |- ( ( M e. ZZ /\ N e. ZZ ) ->
                ( M <_ N <-> ( N - M ) e. NN0 ) ) $=
