    $( [18-Jul-2005] $) $( [12-Jul-2005] $)

  $( Natural numbers are a subset of integers. $)
  nnssz $p |- NN (_ ZZ $=
    ( vx cn cz cv wcel cc0 clt wbr elnnz pm3.26bi ssriv ) ABCADZBELCEFLGHLIJK
    $.
    $( [9-Jan-2002] $)

  $( Nonnegative integers are a subset of the integers. $)
  nn0ssz $p |- NN0 (_ ZZ $=
    ( cn0 cn cc0 csn cun cz df-n0 nnssz wcel wss 0z elisseti snss mpbi unssi
    eqsstr ) ABCDZEFGBQFHCFIQFJKCFCFKLMNOP $.
    $( [5-Jul-2004] $) $( [9-May-2004] $)

  $( A natural number is an integer. $)
  nnzt $p |- ( N e. NN -> N e. ZZ ) $=
    ( cn cz nnssz sseli ) BCADE $.
    $( [4-Jun-2004] $) $( [9-May-2004] $)

  $( A nonnegative integer is an integer. $)
  nn0zt $p |- ( N e. NN0 -> N e. ZZ ) $=
    ( cn0 cz nn0ssz sseli ) BCADE $.
    $( [5-Jul-2004] $) $( [9-May-2004] $)

