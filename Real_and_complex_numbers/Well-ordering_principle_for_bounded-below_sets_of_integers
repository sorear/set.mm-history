$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
          Well-ordering principle for bounded-below sets of integers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d z v B $.  $d x y v R $.  $d y z $.
    uzwo3lem.1 $e |- R = { z e. ZZ | B <_ z } $.
    $( Lemma for ~ uzwo3 and ~ zmin . $)
    uzwo3lem1 $p |- ( B e. RR -> E! x e. R A. y e. R x <_ y ) $=
      ( vv cr wcel cv clt wbr cz wrex cle wral wi wa crab c0 wreu btwnz wss wne
