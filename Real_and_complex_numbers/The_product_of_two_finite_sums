      DCEUHUIUJVNVOWAHUKIUKULUMUNVSTZWBTWFWESWIVRTZCVFUOWJCVFWJVMVQVIUPUQURCVFV
      RUSUTWAVAHIVSWBVBVCVDGVE $.
      $( [28-Apr-2014] $) $( [31-Dec-2005] $)
  $}

  ${
    $d j m n s t x y z B $.  $d i j k l m n s u y z G $.  $d j k m x y z ph $.
    $d k m n s t y A $.  $d j k m n s t y z E $.  $d i j k m n s t u y z K $.
    $d j m n u x y F $.  $d j k m n t y z ps $.  $d j k m n t w y z T $.
    $d k m x y H $.
    mertens.1 $e |- ( ( ph /\ j e. NN0 ) -> ( F ` j ) = A ) $.
    mertens.2 $e |- ( ( ph /\ j e. NN0 ) -> ( K ` j ) = ( abs ` A ) ) $.
    mertens.3 $e |- ( ( ph /\ j e. NN0 ) -> A e. CC ) $.
    mertens.4 $e |- ( ( ph /\ k e. NN0 ) -> ( G ` k ) = B ) $.
    mertens.5 $e |- ( ( ph /\ k e. NN0 ) -> B e. CC ) $.
    mertens.6 $e |- ( ( ph /\ k e. NN0 ) -> ( H ` k ) =
                      sum_ j e. ( 0 ... k ) ( A x. ( G ` ( k - j ) ) ) ) $.
    mertens.7 $e |- ( ph -> seq 0 ( + , K ) e. dom ~~> ) $.
    mertens.8 $e |- ( ph -> seq 0 ( + , G ) e. dom ~~> ) $.
    ${
      mertens.9 $e |- ( ph -> E e. RR+ ) $.
      mertens.10 $e |- T = { z | E. n e. ( 0 ... ( s - 1 ) ) z =
                        ( abs ` sum_ k e. ( ZZ>= ` ( n + 1 ) ) ( G ` k ) ) } $.
      mertens.11 $e |- ( ps <-> ( s e. NN /\ A. n e. ( ZZ>= ` s )
        ( abs ` sum_ k e. ( ZZ>= ` ( n + 1 ) ) ( G ` k ) ) <
          ( ( E / 2 ) / ( sum_ j e. NN0 ( K ` j ) + 1 ) ) ) ) $.
      ${
        mertens.12 $e |- ( ph -> ( ps /\ ( t e. NN0 /\ A. m e. ( ZZ>= ` t )
       ( K ` m ) < ( ( ( E / 2 ) / s ) / ( sup ( T , RR , < ) + 1 ) ) ) ) ) $.
        mertens.13 $e |- ( ph -> ( 0 <_ sup ( T , RR , < ) /\
          ( T C_ RR /\ T =/= (/) /\ E. z e. RR A. w e. T w <_ z ) ) ) $.
        $( Lemma for ~ mertens . $)
        mertenslem1 $p |- ( ph ->
        E. y e. NN0 A. m e. ( ZZ>= ` y ) ( abs ` sum_ j e. ( 0 ... m ) ( A x.
            sum_ k e. ( ZZ>= ` ( ( m - j ) + 1 ) ) B ) ) < E ) $=
          ( cv caddc co cn0 wcel cc0 cfz cmin c1 cuz cfv csu cmul cabs clt wral
          wbr wrex cdiv wa cr simpld sylib syl simprd syl2anc cc elfznn0 syl2an
          fzfid eqid peano2nn0 nn0z wceq simplll sylan cseq nn0uz eqeltrd mpbid
          cz simpll isumcl abscl fsumrecl crp rpre adantr wss cle wb adantl zre
          nn0re syl6eleq cvv sylanbrc elfzelz eluz mpbird imp remulcl syldan 0z
          sseld a1i peano2re absge0 breqtrrd ltp1 lelttrd elrp rpdivcl ad2antrr
          rerpdivcl jca sumeq2dv fveq2d syl3anc wi wne recnd rpcn fveq2 adantlr
          oveq2i cen oveq1d cn c2 nnnn0 nn0addcl simpl fznn0sub adantll eluznn0
          csup cli cdm iserex mulcl fsumcl fsumabs nn0ge0 eluzelz subge02 simpr
          nnz uzid uzaddcl uztrn2 vex elfzuzb ax-mp fznn0sub2 rphalfcl isumrecl
          fzss2 eqidd isumge0 elfzle2 ad2antlr lesub zsubcl oveq1 breq1d rcla4v
          0re sumeq1d sylc eqbrtrrd ltle mpd lemul2a syl31anc fsumcmp syl112anc
          rpne0 divass cbvsumv oveq1i fsummulc1 sumeq2i 3eqtr3g eqtrd isumcmpss
          syl5eqel ssriv ltmul1 ltdivmul fzss1 c0 w3a suprcl nnrp elfzuz eluzle
          leaddsub2 peano2uz uztrn ax-1cn subsub elfzle1 eqbrtrd peano2zm suble
          elfz5 eqcomd eqeq2d rcla4ev fvex eqeq1 rexbidv sylibr suprub lemul12a
          elab2 syl22anc mp2and cfn fsumconst 1z fzen zcn addcom sylancr pncan3
          oveq12d breqtrd hashen hashfz1 rpcnne0 divcan2 eqtr3d mulass 3eqtr2rd
          chash div23 nnne0 3eqtrd ltmul2 ltdivmul2 lt2add absmul cin fsumsplit
          fzdisj cun fzsplit eqtr2d 2halves 3brtr3d ralrimiva raleqdv ) ASUMZFU
