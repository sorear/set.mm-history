    $( Bounds on the cosine of a positive real number less than or equal to 1.
       (Contributed by Paul Chapman, 19-Jan-2008.) $)
    cos01bnd $p |- ( A e. ( 0 (,] 1 ) ->
                     ( ( 1 - ( 2 x. ( ( A ^ 2 ) / 3 ) ) ) < ( cos ` A ) /\
                       ( cos ` A ) < ( 1 - ( ( A ^ 2 ) / 3 ) ) ) ) $=
      ( vy vj cv cn0 wcel ci cmul co cexp cfa cfv cdiv wceq wa copab eqid
      cos01bndlem3 ) BACCDZEFBDGAHISJISKLMINOCBPZTQR $.
      $( [19-Jan-2008] $) $( [19-Jan-2008] $)
  $}

  $( Bounds on the cosine of 1.  (Contributed by Paul Chapman, 19-Jan-2008.) $)
  cos1bnd $p |- ( ( 1 / 3 ) < ( cos ` 1 ) /\ ( cos ` 1 ) < ( 2 / 3 ) ) $=
    ( c1 c3 cdiv co ccos cfv clt wbr c2 cexp cmul cmin sq1 opreq1i opreq2i 2cn
    3nn nncni nnne0i divreci eqtr4i ax1cn divcli reccli caddc df-3 dividi
    divdiri 3eqtr3ri subaddrii eqtri cr wcel cc0 cle wa 1re lt01 leidi w3a
