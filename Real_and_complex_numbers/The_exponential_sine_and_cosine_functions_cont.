  ${
    $d A j y $.
    $( Bounds on the sine of a positive real number less than or equal to 1.
       (Contributed by Paul Chapman, 19-Jan-2008.) $)
    sin01bnd $p |- ( A e. ( 0 (,] 1 ) ->
        ( ( A - ( ( A ^ 3 ) / 3 ) ) < ( sin ` A ) /\ ( sin ` A ) < A ) ) $=
      ( vy vj cv cn0 wcel ci cmul co cexp cfa cdiv wceq copab eqid sin01bndlem3
      cfv wa ) BACCDZEFBDGAHISJISKQLIMRCBNZTOP $.
      $( [19-Jan-2008] $)

    $( Bounds on the cosine of a positive real number less than or equal to 1.
       (Contributed by Paul Chapman, 19-Jan-2008.) $)
    cos01bnd $p |- ( A e. ( 0 (,] 1 ) ->
                     ( ( 1 - ( 2 x. ( ( A ^ 2 ) / 3 ) ) ) < ( cos ` A ) /\
                       ( cos ` A ) < ( 1 - ( ( A ^ 2 ) / 3 ) ) ) ) $=
      ( vy vj cv cn0 wcel ci cmul co cexp cfa cdiv wceq copab eqid cos01bndlem3
      cfv wa ) BACCDZEFBDGAHISJISKQLIMRCBNZTOP $.
      $( [19-Jan-2008] $)
  $}

  $( Bounds on the cosine of 1.  (Contributed by Paul Chapman, 19-Jan-2008.) $)
  cos1bnd $p |- ( ( 1 / 3 ) < ( cos ` 1 ) /\ ( cos ` 1 ) < ( 2 / 3 ) ) $=
    ( c1 c3 cdiv co clt wbr c2 cmul cmin opreq1i opreq2i 2cn ax-1cn caddc eqtri
    3nn cr wcel cc0 1re ccos cfv cexp nncni nnne0i divreci eqtr4i divcli reccli
    sq1 df-3 dividi divdiri 3eqtr3ri subaddrii cle wa lt01 leidi cioc wb elioc2
    w3a 0re mp2an cos01bnd sylbir mp3an simpli eqbrtrri simpri subadd2i breqtri
