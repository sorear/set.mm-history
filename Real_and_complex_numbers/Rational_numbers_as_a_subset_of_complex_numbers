$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Rational numbers (as a subset of complex numbers)
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d m n p q x y $.
    $( Soundness justification theorem for ~ df-q . $)
    qjust $p |- { x | E. m e. ZZ E. n e. NN x = ( m / n ) }
           = { y | E. p e. ZZ E. q e. NN y = ( p / q ) } $=
      ( cv cdiv co wceq cn wrex cab opreq2 eqeq2d cbvrexv rexbii opreq1 rexbidv
      cz bitri abbii eqeq1 2rexbidv cbvabv eqtri ) AGZCGZDGZHIZJZDKLZCTLZAMUGFG
      ZEGZHIZJZEKLZFTLZAMBGZUPJZEKLFTLZBMUMUSAUMUGUHUOHIZJZEKLZCTLUSULVECTUKVDD
      EKUIUOJUJVCUGUIUOUHHNOPQVEURCFTUHUNJZVDUQEKVFVCUPUGUHUNUOHROSPUAUBUSVBABU
      GUTJUQVAFETKUGUTUPUCUDUEUF $.
      $( [16-Feb-2013] $)
  $}

  ${
    $d m n x $.
    $( Define the set of rational numbers.  Definition of rationals in
       [Apostol] p. 22. $)
    df-q $a |- QQ = { x | E. m e. ZZ E. n e. NN x = ( m / n ) } $.
  $}

  ${
    $d x y z A $.
    $( Membership in the set of rationals. $)
    elq $p |- ( A e. QQ <-> E. x e. ZZ E. y e. NN A = ( x / y ) ) $=
      ( vz cq wcel cv cdiv co wceq cn wrex cz df-q eleq2i oprex eleq1 rexlimivw
      cab cvv mpbiri eqeq1 2rexbidv elab3 bitri ) CEFCDGZAGZBGZHIZJZBKLAMLZDSZF
      CUIJZBKLZAMLZEULCDABNOUKUODCUNCTFZAMUMUPBKUMUPUITFUGUHHPCUITQUARRUFCJUJUM
      ABMKUFCUIUBUCUDUE $.
      $( [8-Jan-2002] $)
  $}

  ${
    $d x y A $.  $d x y B $.
    $( The ratio of an integer and a natural number is a rational number. $)
    znq $p |- ( ( A e. ZZ /\ B e. NN ) -> ( A / B ) e. QQ ) $=
      ( vx vy cz wcel cn wa cdiv co cv wceq wrex cq rcla4eopr mp3an3 elq sylibr
      eqid ) AEFZBGFZHABIJZCKDKIJLDGMCEMZUBNFTUAUBUBLUCUBSCDEGABUBIOPCDUBQR $.
      $( [12-Jan-2002] $)
  $}

  ${
    $d x y A $.
    $( A rational number is a real number. $)
    qre $p |- ( A e. QQ -> A e. RR ) $=
      ( vx vy cq wcel cv cdiv co wceq cn wrex cz cr elq cc0 wne zre nnre nnne0
      wa jca redivcl 3expb syl2an eleq1 syl5ibrcom rexlimivv sylbi ) ADEABFZCFZ
      GHZIZCJKBLKAMEZBCANULUMBCLJUILEZUJJEZTUMULUKMEZUNUIMEZUJMEZUJOPZTUPUOUIQU
      OURUSUJRUJSUAUQURUSUPUIUJUBUCUDAUKMUEUFUGUH $.
      $( [14-Nov-2002] $)

    $( An integer is a rational number. $)
    zq $p |- ( A e. ZZ -> A e. QQ ) $=
      ( vx vy cv wceq cz wrex cdiv co cn wcel cq c1 cr cc recn div1 3syl eqeq2d
      zre eqcom syl6rbbr 1nn opreq2 rcla4ev mpan syl6bi reximia risset 3imtr4i
      elq ) BDZAEZBFGAULCDZHIZEZCJGZBFGAFKALKUMUQBFULFKZUMAULMHIZEZUQURUTAULEUM
      URUSULAURULNKULOKUSULEULTULPULQRSULAUAUBMJKUTUQUCUPUTCMJUNMEUOUSAUNMULHUD
      SUEUFUGUHBAFUIBCAUKUJ $.
      $( [9-Jan-2002] $)
  $}

  $( The integers are a subset of the rationals. $)
  zssq $p |- ZZ C_ QQ $=
    ( vx cz cq cv zq ssriv ) ABCADEF $.
    $( [9-Jan-2002] $)

  $( The nonnegative integers are a subset of the rationals. $)
  nn0ssq $p |- NN0 C_ QQ $=
    ( cn0 cz cq nn0ssz zssq sstri ) ABCDEF $.
    $( [31-Jul-2004] $)

  $( The natural numbers are a subset of the rationals. $)
  nnssq $p |- NN C_ QQ $=
    ( cn cz cq nnssz zssq sstri ) ABCDEF $.
    $( [31-Jul-2004] $)

  $( The rationals are a subset of the reals. $)
  qssre $p |- QQ C_ RR $=
    ( vx cq cr cv qre ssriv ) ABCADEF $.
    $( [9-Jan-2002] $)

  $( The rationals are a subset of the complex numbers. $)
  qsscn $p |- QQ C_ CC $=
    ( cq cr cc qssre ax-resscn sstri ) ABCDEF $.
    $( [2-Aug-2004] $)

  $( A natural number is rational. $)
  nnq $p |- ( A e. NN -> A e. QQ ) $=
    ( cn cq nnssq sseli ) BCADE $.
    $( [17-Nov-2004] $)

  $( A rational number is a complex number. $)
  qcn $p |- ( A e. QQ -> A e. CC ) $=
    ( cq cc qsscn sseli ) BCADE $.
    $( [2-Aug-2004] $)

  $( The set of rational numbers exists. $)
  qex $p |- QQ e. _V $=
    ( cq cr reex qssre ssexi ) ABCDE $.
    $( [30-Jul-2004] $)

