  $( 1 and 0 are distinct.  Axiom 14 of 25 for real and complex numbers,
     derived from ZF set theory. $)
  ax1ne0 $p |- 1 =/= 0 $=
    ( c1 cc0 wne wceq wn c1r c0r cop 1ne0sr cnr 1r elisseti eqresr mtbir df-1
    df-0 eqeq12i df-ne mpbir ) ABCABDZETFGHZGGHZDZUCFGDIFGFJKLMNAUABUBOPQNABRS
    $.
    $( [19-Mar-1996] $)

  ${
    $d x y A $.
