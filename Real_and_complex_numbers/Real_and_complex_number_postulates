  $( The class of complex numbers is a set, i.e. it is a member of the universe
     of sets ` _V ` (see ~ isset ).  Axiom 1 of 25 for real and complex
     numbers, derived from ZF set theory. $)
  axcnex $p |- CC e. _V $=
    ( cc cnr cxp cvv df-c srex xpex eqeltri ) ABBCDEBBFFGH $.
    $( [13-Aug-1995] $)

  $( The real numbers are a subset of the complex numbers.  Axiom 2 of 25 for
     real and complex numbers, derived from ZF set theory.  (The proof was
     shortened by Andrew Salmon, 12-Aug-2011.) $)
  axresscn $p |- RR C_ CC $=
    ( cnr c0r csn cxp cr cc wss wcel 0r snssi ax-mp xpss2 df-r df-c 3sstr4i )
    ABCZDZAADZEFPAGZQRGBAHSIBAJKPAALKMNO $.
    $( [12-Aug-2011] $) $( [1-Mar-1995] $)

  $( The real numbers are a subset of the complex numbers.  Axiom 2 of 25 for
     real and complex numbers, derived from ZF set theory. $)
  axresscnOLD $p |- RR C_ CC $=
    ( cnr c0r csn cxp cr cc wss ssid wcel 0r snssi ax-mp xpss12 mp2an df-r
    df-c 3sstr4i ) ABCZDZAADZEFAAGRAGZSTGAHBAIUAJBAKLAARAMNOPQ $.
    $( [1-Mar-1995] $)

  $( 1 is a complex number.  Axiom 3 of 25 for real and complex numbers,
     derived from ZF set theory. $)
  ax1cn $p |- 1 e. CC $=
    ( cr cc c1 axresscn c1r c0r cop df-1 wcel cnr 1r opelreal mpbir eqeltri
    sselii ) ABCDCEFGZAHPAIEJIKELMNO $.
    $( [12-Apr-2007] $)

  $( ` _i ` is a complex number.  Axiom 4 of 25 for real and complex numbers,
     derived from ZF set theory. $)
  axicn $p |- _i e. CC $=
    ( ci cc wcel c0r cnr c1r cop wa df-i eleq1i 1r elisseti opelcn bitri 0r
    mpbir2an ) ABCZDECZFECZQDFGZBCRSHATBIJDFFEKLMNOKP $.
    $( [23-Feb-1996] $)

  ${
    $d x y A $.  $d x y B $.
    $( Closure law for addition of complex numbers.  Axiom 5 of 25 for real and
       complex numbers, derived from ZF set theory. $)
    axaddcl $p |- ( ( A e. CC /\ B e. CC ) -> ( A + B ) e. CC ) $=
      ( cc caddc axaddopr foprcl ) ABCCCDEF $.
      $( [14-Jun-1995] $)

    $( Closure law for addition in the real subfield of complex numbers.  Axiom
       6 of 25 for real and complex numbers, derived from ZF set theory. $)
    axaddrcl $p |- ( ( A e. RR /\ B e. RR ) -> ( A + B ) e. RR ) $=
      ( vx vy cv c0r cop caddc co cr wcel cnr elreal wceq opreq1 eleq1d opreq2
      wa cplr addresr addclsr opelreal sylibr eqeltrd 2gencl ) CEZFGZDEZFGZHIZJ
      KAUIHIZJKABHIZJKCDUGUIABLJCAMDBMUGANUJUKJUGAUIHOPUIBNUKULJUIBAHQPUFLKUHLK
      RZUJUFUHSIZFGZJUFUHTUMUNLKUOJKUFUHUAUNUBUCUDUE $.
      $( [31-Mar-1996] $)

    $( Closure law for multiplication of complex numbers.  Axiom 7 of 25 for
       real and complex numbers, derived from ZF set theory. $)
    axmulcl $p |- ( ( A e. CC /\ B e. CC ) -> ( A x. B ) e. CC ) $=
      ( cc cmul axmulopr foprcl ) ABCCCDEF $.
      $( [10-Aug-1995] $)

    $( Closure law for multiplication in the real subfield of complex numbers.
       Axiom 8 of 25 for real and complex numbers, derived from ZF set
       theory. $)
    axmulrcl $p |- ( ( A e. RR /\ B e. RR ) -> ( A x. B ) e. RR ) $=
      ( vx vy cv c0r cop cmul co cr wcel cnr elreal wceq opreq1 eleq1d opreq2
      wa cmr visset mulresr mulclsr opelreal sylibr eqeltrd 2gencl ) CEZFGZDEZF
      GZHIZJKAUJHIZJKABHIZJKCDUHUJABLJCAMDBMUHANUKULJUHAUJHOPUJBNULUMJUJBAHQPUG
      LKUILKRZUKUGUISIZFGZJUGUIDTUAUNUOLKUPJKUGUIUBUOUCUDUEUF $.
      $( [31-Mar-1996] $)
