$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Infinite sequence builders "seq" and "seq0"
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c seq $.  $( Symbol for recursive sequence builder starting at an arbitrary
                index. $)
  $c seq0 $.  $( Symbol for recursive sequence builder starting at index 0. $)

  $( Extend class notation with arbitrarily-based recursive sequence
     builder. $)
  cseqz $a class seq $.
  $( Extend class notation with 0-based recursive sequence builder. $)
  cseq0 $a class seq0 $.

  ${
    $d g h k x $.
    $( Define a recursive sequence builder operation that starts at an
       arbitrary integer index.  See ~ seqz1 and ~ seqzp1 for its initial and
       successor values.  Theorems ~ seq0seqz and ~ seq1seqz derive the
       0-based ` seq0 ` and the 1-based ` seq1 ` as special cases. $)
    df-seqz $a |- seq = { <. <. x , g >. , h >. |
        h = ( ( ( ( 2nd ` x ) seq1 ( g shift ( 1 - ( 1st ` x ) ) ) )
           shift ( ( 1st ` x ) - 1 ) ) |` { k e. ZZ | ( 1st ` x ) <_ k } ) } $.
  $}

