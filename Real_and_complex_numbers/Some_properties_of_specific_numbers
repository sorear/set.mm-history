  $( 1 is not equal to 2. $)
  1ne2 $p |- 1 =/= 2 $=
    ( c2 c1 clt wbr wne 1lt2 1re 2re ltnei ax-mp necomi ) ABBACDABEFBAGHIJK $.
    $( [19-Oct-2012] $)

