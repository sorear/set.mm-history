  $( A sum is less than the whole if each term is less than half. $)
  lt2halves $p |- ( ( A e. RR /\ B e. RR /\ C e. RR ) ->
                ( ( A < ( C / 2 ) /\ B < ( C / 2 ) ) -> ( A + B ) < C ) ) $=
    ( cr wcel w3a c2 cdiv co clt wbr wa caddc wi 3simpa rehalfcl jca 3ad2ant3
    lt2add syl11anc wb cc wceq recn 2halves syl breq2d sylibd ) ADEZBDEZCDEZFZA
    CGHIZJKBUMJKLZABMIZUMUMMIZJKZUOCJKZULUIUJLUMDEZUSLZUNUQNUIUJUKOUKUIUTUJUKUS
    USCPZVAQRABUMUMSTUKUIUQURUAUJUKUPCUOJUKCUBEUPCUCCUDCUEUFUGRUH $.
    $( [13-Dec-2006] $)

  $( Sum is less than product for numbers greater than 2.  (Contributed by
     Stefan Allan, 24-Sep-2010.) $)
  addltmul $p |- ( ( ( A e. RR /\ B e. RR ) /\ ( 2 < A /\ 2 < B ) )
              -> ( A + B ) < ( A x. B ) ) $=
    ( cr wcel wa c2 clt wbr caddc co cmul c1 cmin wb 2re 1re ltsub1 mp3an13
    2cn ax1cn df-2 eqcomi subaddrii breq1i syl6bb bi2anan9 wi mulgt1 ex
    peano2rem syl2an sylbid cc wceq mulsub mpanl2 mpanr2 recn breq2d readdcl
    axmulrcl mpan2 remulcli sylancl ltaddsub2 mp3an2 syl11anc mulid1i opreq2i
    breq2i syl5rbbr ltadd1 mp3an3 mulid1 syl opreqan12d breq1d bitr3d 3bitrd
    sylibd imp ) ACDZBCDZEZFAGHZFBGHZEZABIJZABKJZGHZWDWGLALMJZBLMJZKJZGHZWJWDWG
    LWKGHZLWLGHZEZWNWBWEWOWCWFWPWBWEFLMJZWKGHZWOFCDZWBLCDZWEWSNOPFALQRWRLWKGFLL
