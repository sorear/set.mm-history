    GEAHGEADIJHGKLMADNO $.
    $( [9-Dec-2005] $)

  $( Two halves make a whole. $)
  2halves $p |- ( A e. CC -> ( ( A / 2 ) + ( A / 2 ) ) = A ) $=
    ( cc wcel c2 cmul co cdiv caddc 2times opreq1d cc0 wne wceq 2cn 2ne0
    divcan3 mp3an23 wa pm3.2i divdir mp3an3 anidms 3eqtr3rd ) ABCZDAEFZDGFZAAHF
    ZDGFZAADGFZUIHFZUDUEUGDGAIJUDDBCZDKLZUFAMNOADPQUDUHUJMZUDUDUKULRUMUKULNOSAA
    DTUAUBUC $.
    $( [11-Apr-2005] $)

  $( Closure of half-sum and half-difference.  (Contributed by Paul Chapman,
     12-Oct-2007.) $)
  halfaddsubcl $p |- ( ( A e. CC /\ B e. CC ) -> ( ( ( A + B ) / 2 ) e. CC /\
                         ( ( A - B ) / 2 ) e. CC ) ) $=
    ( cc wcel wa caddc co c2 cdiv cmin addcl halfcl syl subcl jca ) ACDBCDEZABF
    GZHIGCDZABJGZHIGCDZPQCDRABKQLMPSCDTABNSLMO $.
    $( [12-Oct-2007] $)

  $( Sum and difference of half-sum and half-difference.  (Contributed by Paul
     Chapman, 12-Oct-2007.) $)
  halfaddsub $p |- ( ( A e. CC /\ B e. CC ) ->
                      ( ( ( ( A + B ) / 2 ) + ( ( A - B ) / 2 ) ) = A /\
                        ( ( ( A + B ) / 2 ) - ( ( A - B ) / 2 ) ) = B ) ) $=
    ( cc wcel wa caddc co c2 cdiv cmin wceq cmul ppncan 3anidm13 2times adantr
    eqtr4d opreq1d addcl subcl cc0 wne 2cn 2ne0 pm3.2i divdir mp3an3 syl11anc
    divcan3 mp3an23 3eqtr3d pnncan 3anidm23 adantl divsubdir jca ) ACDZBCDZEZAB
    FGZHIGZABJGZHIGZFGZAKVAVCJGZBKUSUTVBFGZHIGZHALGZHIGZVDAUSVFVHHIUSVFAAFGZVHU
    QURVFVJKABAMNUQVHVJKURAOPQRUSUTCDZVBCDZVGVDKZABSZABTZVKVLHCDZHUAUBZEZVMVPVQ
    UCUDUEZUTVBHUFUGUHUQVIAKZURUQVPVQVTUCUDAHUIUJPUKUSUTVBJGZHIGZHBLGZHIGZVEBUS
