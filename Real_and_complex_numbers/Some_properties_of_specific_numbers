  $( Two plus two equals four.  For more information, see "2+2=4 Trivia" on the
     Metamath Proof Explorer Home Page:
     ~ http://us.metamath.org/mpegif/mmset.html#trivia . $)
  2p2e4 $p |- ( 2 + 2 ) = 4 $=
    ( c2 caddc co c1 c4 df-2 opreq2i c3 df-4 df-3 opreq1i 2cn ax1cn addassi
    3eqtri eqtr4i ) AABCADDBCZBCZEAQABFGEHDBCADBCZDBCRIHSDBJKADDLMMNOP $.
    $( [27-May-1999] $)

  $( 4 is a natural number. $)
  4nn $p |- 4 e. NN $=
    ( c2 caddc co c4 cn 2p2e4 wcel 2nn nnaddcl mp2an eqeltrri ) AABCZDEFAEGZMLE
    GHHAAIJK $.
    $( [8-Jan-2006] $)

  ${
    2times.1 $e |- A e. CC $.
    $( Two times a number. $)
    2timesi $p |- ( 2 x. A ) = ( A + A ) $=
      ( c2 cmul co c1 caddc df-2 opreq1i 1p1timesi eqtri ) CADEFFGEZADEAAGECLAD
      HIABJK $.
      $( [1-Aug-1999] $)
  $}

  $( Two times a number. $)
  2times $p |- ( A e. CC -> ( 2 x. A ) = ( A + A ) ) $=
    ( cc wcel c2 cmul co caddc wceq cc0 cif opreq2 id opreq12d eqeq12d 0cn
    elimel 2timesi dedth ) ABCZDAEFZAAGFZHDSAIJZEFZUBUBGFZHAIAUBHZTUCUAUDAUBDEK
    UEAUBAUBGUELZUFMNUBAIBOPQR $.
    $( [10-Oct-2004] $)

  $( A number times 2. $)
  times2 $p |- ( A e. CC -> ( A x. 2 ) = ( A + A ) ) $=
