  $( Two plus two equals four.  For more information, see "2+2=4 Trivia" on the
     Metamath Proof Explorer Home Page:
     ~ http://us.metamath.org/mpegif/mmset.html#trivia . $)
  2p2e4 $p |- ( 2 + 2 ) = 4 $=
    ( c2 caddc co c1 c4 df-2 oveq2i c3 df-4 oveq1i ax-1cn addassi 3eqtri eqtr4i
    df-3 2cn ) AABCADDBCZBCZEAQABFGEHDBCADBCZDBCRIHSDBOJADDPKKLMN $.
    $( [27-May-1999] $)

  ${
    2times.1 $e |- A e. CC $.
    $( Two times a number. $)
    2timesi $p |- ( 2 x. A ) = ( A + A ) $=
      ( c2 cmul co c1 caddc df-2 oveq1i 1p1timesi eqtri ) CADEFFGEZADEAAGECLADH
      IABJK $.
      $( [1-Aug-1999] $)
  $}

  $( Two times a number. $)
  2times $p |- ( A e. CC -> ( 2 x. A ) = ( A + A ) ) $=
    ( cc wcel c2 cmul co caddc wceq cc0 cif oveq2 id oveq12d eqeq12d 0cn elimel
    2timesi dedth ) ABCZDAEFZAAGFZHDSAIJZEFZUBUBGFZHAIAUBHZTUCUAUDAUBDEKUEAUBAU
    BGUELZUFMNUBAIBOPQR $.
    $( [10-Oct-2004] $)

  $( A number times 2. $)
  times2 $p |- ( A e. CC -> ( A x. 2 ) = ( A + A ) ) $=
    ( cc wcel c2 cmul co caddc wceq 2cn mulcom mpan2 2times eqtrd ) ABCZADEFZDA
    EFZAAGFNDBCOPHIADJKALM $.
    $( [16-Oct-2007] $)

  ${
    times2.1 $e |- A e. CC $.
    $( A number times 2. $)
    times2i $p |- ( A x. 2 ) = ( A + A ) $=
      ( cc wcel c2 cmul co caddc wceq times2 ax-mp ) ACDAEFGAAHGIBAJK $.
      $( [11-May-2004] $)
  $}

  $( 3 + 2 = 5. $)
  3p2e5 $p |- ( 3 + 2 ) = 5 $=
    ( c4 c1 caddc co c3 c5 c2 df-4 oveq1i df-5 df-2 oveq2i recni ax-1cn addassi
    3re eqtr4i 3eqtr4ri ) ABCDEBCDZBCDZFEGCDZASBCHIJUAEBBCDZCDTGUBECKLEBBEPMNNO
    QR $.
    $( [11-May-2004] $)

  $( 3 + 3 = 6. $)
  3p3e6 $p |- ( 3 + 3 ) = 6 $=
