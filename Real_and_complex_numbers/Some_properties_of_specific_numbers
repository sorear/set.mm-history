  $( Closure of half of a number (frequently used special case). $)
  halfcl $p |- ( A e. CC -> ( A / 2 ) e. CC ) $=
    ( cc wcel c2 cc0 wne cdiv co 2cn 2ne0 divcl mp3an23 ) ABCDBCDEFADGHBCIJADKL
    $.
    $( [1-Jan-2006] $)

  $( Real closure of half. $)
  rehalfcl $p |- ( A e. RR -> ( A / 2 ) e. RR ) $=
    ( cr wcel c2 cc0 wne cdiv co 2re 2ne0 redivcl mp3an23 ) ABCDBCDEFADGHBCIJAD
    KL $.
    $( [1-Jan-2006] $)

  $( Half of a number is zero iff the number is zero. $)
  half0 $p |- ( A e. CC -> ( ( A / 2 ) = 0 <-> A = 0 ) ) $=
    ( cc wcel c2 cc0 wne cdiv co wceq wb 2cn 2ne0 diveq0 mp3an23 ) ABCDBCDEFADG
    HEIAEIJKLADMN $.
    $( [20-Apr-2006] $)

  $( A positive number is greater than its half. $)
  halfpos $p |- ( A e. RR -> ( 0 < A <-> ( A / 2 ) < A ) ) $=
    ( cr wcel cc0 clt wbr c1 caddc co cdiv c2 wb cif wceq breq2 opreq1 id
    breq12d bibi12d 0re elimel halfposi dedth df-2 opreq2i breq1i syl6bbr ) ABC
    ZDAEFZAGGHIZJIZAEFZAKJIZAEFUHUIULLDUHADMZEFZUNUJJIZUNEFZLADAUNNZUIUOULUQAUN
    DEOURUKUPAUNEAUNUJJPURQRSUNADBTUAUBUCUMUKAEKUJAJUDUEUFUG $.
    $( [28-Oct-2004] $)

  $( A number is positive iff its half is positive. $)
  halfpos2 $p |- ( A e. RR -> ( 0 < A <-> 0 < ( A / 2 ) ) ) $=
    ( cr wcel c2 cc0 clt wbr cdiv co wb 2re 2pos gt0div mp3an23 ) ABCDBCEDFGEAF
    GEADHIFGJKLADMN $.
