  $( ` i ` squared. $)
  i2 $p |- ( i ^ 2 ) = -u 1 $=
    ( ci c2 cexp co cmul c1 cneg axicn sqval ixi eqtr ) ABCDAAEDFGAHIJK $.
    $( [29-May-2005] $) $( [6-May-1999] $)

