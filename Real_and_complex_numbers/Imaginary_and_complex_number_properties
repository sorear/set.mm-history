  $( The imaginary unit ` i ` is not zero. $)
  ine0 $p |- -. i = 0 $=
    ( ci cc0 wceq c1 wne wn ax1ne0 df-ne mpbi caddc co cmulc opreq2 axicn
    mulzer1 syl6req opreq1d 1cn addid2 axi2m1 3eqtr3g mto ) ABCZDBCZDBEUDFGDBHI
    UCBDJKAALKZDJKDBUCBUEDJUCUEABLKBABALMANOPQDRSTUAUB $.
    $( [6-May-99] $)

  $( ` i ` -squared is minus 1. $)
  isqm1 $p |- ( i x. i ) = -u 1 $=
    ( ci cmulc co cc0 caddc c1 cneg axicn mulcl 1cn negcl addass axi2m1
    opreq1i negid opreq2i 3eqtr3r addid1 addid2 3eqtr3 ) AABCZDECZDFGZECZUAUCUA
    FECZUCECUAFUCECZECUDUBUAFUCAAHHIZJFJKZLUEDUCEMNUFDUAEFJOPQUAUGRUCUHST $.
    $( [6-May-99] $)

  $( The reciprocal of ` i ` . $)
  irec $p |- ( 1 / i ) = -u i $=
    ( c1 ci cdiv co cneg wceq cmulc axicn mulneg2 isqm1 1cn mulcl negcon2
    mpbir eqtr4 negcl cc0 wne wn ine0 df-ne divmul ) ABCDBEZFBUCGDZAFUDBBGDZEZA
    BBHHIAUFFUEAEFJAUEKBBHHLMNOABUCKHBHPBQRBQFSTBQUANUBN $.
