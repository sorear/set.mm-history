      UOUPUQWJUJJAEURZUSUTCVAZJVAZWEWFWNWCVAZWBUJVBWBVAWPRWAVCUJWBVDTWBVEVFCWCD
      VGVHWJUJJVIWOWMWJUJJVJVKBCJVLVMVNVO $.
      $( [13-Sep-2013] $) $( [22-Jun-2011] $)

    $( ` ``' G ` maps the size function's value to ` card ` .  (Contributed by
       Paul Chapman, 22-Jun-2011.) $)
    hashginv $p |- ( A e. Fin -> ( `' G ` ( # ` A ) ) = ( card ` A ) ) $=
      ( cfn wcel ccrd cfv chash wceq ccnv hashgval com wi ficardom cn0 wf1o
      hashgf1o f1ocnvfv mpan syl mpd ) BEFZBGHZCHBIHZJZUECKHUDJZABCDLUCUDMFZUFU
      GNZBOMPCQUHUIACDRMPUDUECSTUAUB $.
      $( [15-Sep-2013] $) $( [22-Jun-2011] $)
  $}

  ${
