$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
               The ` # ` (finite set size) function
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c # $.

  $( Extend the definition of a class to include the size function. $)
  chash $a class # $.

  ${
    $d x y $.
    $( Define the ` # ` function, which gives the cardinality of a finite set
       as a member of ` NN0 ` . $)
    df-hash $a |- # = ( ( rec (
                   ( x e. _V |-> ( x + 1 ) ) , 0 ) |` om ) o. card ) $.
  $}

  ${
    $d A x y $.
    hashgval.1 $e |- G = ( rec ( ( x e. _V |-> ( x + 1 ) ) , 0 ) |` om ) $.
    $( The value of the ` # ` function in terms of the mapping ` G ` from
       ` om ` to ` NN0 ` .  The proof avoids the use of ~ ax-ac .  (Contributed
       by Paul Chapman, 22-Jun-2011.)  (Revised by Mario Carneiro,
       12-Feb-2013.) $)
    hashgval $p |- ( A e. Fin -> ( G ` ( card ` A ) ) = ( # ` A ) ) $=
      ( vy cfn wcel chash cfv ccrd ccom cv cc0 com wceq cen wbr con0 wrex wfun
      cvv c1 caddc co cmpt crdg cres df-hash coeq1i eqtr4i fveq1i cdm cab breq2
      finnum rexbidv elabg mpbird cardf2 fdmi syl6eleqr wfn rdgfnon funres mp2b
      fnfun funeqi mpbir wf ffun ax-mp fvco mp3an12 syl syl5req ) BFGZBHIBCJKZI
      ZBJICIZBHVQHAUAALZUBUCUDUEZMUFZNUGZJKVQAUHCWCJDUIUJUKVPBJULZGZVRVSOZVPBEL
      ZVTPQZERSZAUMZWDVPBWJGWGBPQZERSZEBUOWIWLABFVTBOWHWKERVTBWGPUNUPUQURWJRJAE
      USZUTVACTZJTZWEWFWNWCTZWBRVBWBTWPMWAVCRWBVFNWBVDVECWCDVGVHWJRJVIWOWMWJRJV
      JVKBCJVLVMVNVO $.
      $( [13-Sep-2013] $) $( [22-Jun-2011] $)

    $( ` ``' G ` maps the size function's value to ` card ` .  (Contributed by
       Paul Chapman, 22-Jun-2011.) $)
    hashginv $p |- ( A e. Fin -> ( `' G ` ( # ` A ) ) = ( card ` A ) ) $=
      ( cfn wcel ccrd cfv chash wceq ccnv hashgval com wi ficardom cn0 hashgf1o
      wf1o f1ocnvfv mpan syl mpd ) BEFZBGHZCHBIHZJZUECKHUDJZABCDLUCUDMFZUFUGNZB
      OMPCRUHUIACDQMPUDUECSTUAUB $.
      $( [15-Sep-2013] $) $( [22-Jun-2011] $)
  $}

  ${
    $d x y z $.
    $( The size function maps all finite sets to their cardinality, as members
       of ` NN0 ` . $)
