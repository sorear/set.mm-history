      EVHPEVJVTHUTATBTCVGVHVEVQUNUOUSUTVRVLVSVMNCAVEVQSCBVEVQSUPUQRUR $.
      $( [15-Sep-2013] $) $( [30-Nov-2012] $)

    $( The size of the union of finite sets is less than or equal to the sum of
       their sizes. $)
    hashun2 $p |- ( ( A e. Fin /\ B e. Fin ) ->
                   ( # ` ( A u. B ) ) <_ ( ( # ` A ) + ( # ` B ) ) ) $=
      ( vx cfn wcel chash cfv ccrd co caddc cle wbr wss com ficardom syl2an syl
      wceq wb hashgval cun coa cdom ficardun2 nnacl cardnn sseqtr4d unfi ssnnfi
      wa ssid sylancl ficarddom syl2anc mpbird hashdom cvv cv c1 cmpt crdg cres
      cc0 fveq2d eqid hashgadd 3eqtr3d opreqan12d eqtrd breqtrd ) ADEZBDEZUJZAB
      UAZFGZAHGZBHGZUBIZFGZAFGZBFGZJIZKVMVOVSKLZVNVRUCLZVMWDVNHGZVRHGZMZVMWEVRW
