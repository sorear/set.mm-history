$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
               The ` # ` (finite set size) function
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c # $.

  $( Extend the definition of a class to include the size function. $)
  chash $a class # $.

  ${
    $d x y $.
    $( Define the ` # ` function, which gives the cardinality of a finite set
       as a member of ` NN0 ` . $)
    df-hash $a |- # = ( ( rec (
                   ( x e. _V |-> ( x + 1 ) ) , 0 ) |` om ) o. card ) $.
  $}

  ${
    $d G n $.  $d n x y $.
    hashgf1o.1 $e |- G = ( rec ( ( x e. _V |-> ( x + 1 ) ) , 0 ) |` om ) $.
    $( ` G ` maps ` om ` one-to-one onto ` NN0 ` .  (Contributed by Paul
       Chapman, 22-Jun-2011.) $)
    hashgf1o $p |- G : om -1-1-onto-> NN0 $=
      ( com cn0 wf1o cc0 cuz cfv 0z om2uzf1oi wceq wb nn0uz f1oeq3 ax-mp mpbir
      ) DEBFZDGHIZBFZAGBJCKESLRTMNESDBOPQ $.
      $( [13-Sep-2013] $) $( [22-Jun-2011] $)
  $}

  ${
    $d x y $.
    $( A 1-based finite sequence of integers is finite.  (Contributed by Paul
       Chapman, 22-Jun-2011.)  (Proof shortened by Mario Carneiro,
       13-Feb-2013.) $)
    fz1fi $p |- ( N e. NN0 -> ( 1 ... N ) e. Fin ) $=
      ( vx cn0 wcel c1 cfz co cfn cvv cv caddc cmpt cc0 crdg com cres ccnv cfv
      wf1o eqid hashgf1o f1ocnvdm mpan con0 cin onfin2 inss2 eqsstri sseli syl
      cen wbr wb fvex fzennn enfi sylancr mpbird ) ACDZEAFGZHDZABIBJEKGLMNOPZQZ
      RZHDZUSVDODZVEOCVBSUSVFBVBVBTZUAOCAVBUBUCOHVDOUDHUEHUFUDHUGUHUIUJUSVDIDUT
      VDUKULVAVEUMAVCUNBVBAVGUOUTVDIUPUQUR $.
      $( [13-Sep-2013] $) $( [22-Jun-2011] $)
  $}

  ${
    $d A x y $.
    hashgval.1 $e |- G = ( rec ( ( x e. _V |-> ( x + 1 ) ) , 0 ) |` om ) $.
    $( The value of the ` # ` function in terms of the mapping ` G ` from
       ` om ` to ` NN0 ` .  The proof avoids the use of ~ ax-ac .  (Contributed
       by Paul Chapman, 22-Jun-2011.)  (Revised by Mario Carneiro,
       12-Feb-2013.) $)
    hashgval $p |- ( A e. Fin -> ( G ` ( card ` A ) ) = ( # ` A ) ) $=
      ( vy cfn wcel chash cfv ccrd ccom cvv cv c1 caddc co cmpt cc0 crdg com
      cres df-hash coeq1i eqtr4i fveq1i cdm wceq cen wbr con0 wrex cab finnum
      breq2 rexbidv elabg mpbird cardf2 fdmi syl6eleqr wfun wfn rdgfnon fnfun
      funres mp2b funeqi mpbir wf ffun ax-mp fvco mp3an12 syl syl5req ) BFGZBHI
