    $( [13-Feb-2013] $) $( [26-Oct-2012] $)

  $( The value of the size function on a singleton.  (Contributed by Paul
     Chapman, 26-Oct-2012.) $)
  hashsngOLD $p |- ( A e. V -> ( # ` { A } ) = 1 ) $=
