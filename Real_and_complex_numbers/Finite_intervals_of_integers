$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Finite intervals of integers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ... $. $( Dots. $)

  $( Extend class notation to include the notation for a contiguous finite set
     of integers.  Read " ` M ... N ` " as "the set of integers from ` M ` to
     ` N ` inclusive." $)
  cfz $a class ... $.

  ${
    $d m n k z $.
    $( Define an operation that produces a finite set of sequential integers.
       Read " ` M ... N ` " as "the set of integers from ` M ` to ` N `
       inclusive."  See ~ fzval for its value and additional comments. $)
    df-fz $a |- ... = { <. <. m , n >. , z >. | ( ( m e. ZZ /\ n e. ZZ ) /\
      z = { k e. ZZ | ( m <_ k /\ k <_ n ) } ) } $.
  $}

  ${
    $d k m n z M $.  $d k m n z N $.
    $( The value of a finite set of sequential integers.  E.g., ` 2 ... 5 `
       means the set ` { 2 , 3 , 4 , 5 } ` .  A special case of this definition
       (starting at 1) appears as Definition 11-2.1 of [Gleason] p. 141, where
       ` NN ` _k means our ` 1 ... k ` ; he calls these sets _segments_ of the
       integers. $)
    fzval $p |- ( ( M e. ZZ /\ N e. ZZ ) -> ( M ... N ) =
                 { k e. ZZ | ( M <_ k /\ k <_ N ) } ) $=
      ( vm vn vz cz cv cle wbr wa crab cfz zex rabex wceq breq1 anbi1d rabbidv
      breq2 anbi2d df-fz oprabval2 ) DEFBCGGDHZAHZIJZUEEHZIJZKZAGLBUEIJZUECIJZK
      ZAGLMUJUHKZAGLULAGNOUDBPZUIUMAGUNUFUJUHUDBUEIQRSUGCPZUMULAGUOUHUKUJUGCUEI
      TUASFADEUBUC $.
      $( [6-Sep-2005] $)
  $}

  ${
    $d j K $.  $d j M $.  $d j N $.
    $( Membership in a finite set of sequential integers. $)
    elfz1 $p |- ( ( M e. ZZ /\ N e. ZZ ) -> ( K e. ( M ... N ) <->
                 ( K e. ZZ /\ M <_ K /\ K <_ N ) ) ) $=
      ( vj cz wcel wa cfz co cv cle wbr crab w3a fzval eleq2d wceq breq2 breq1
      anbi12d elrab 3anass bitr4i syl6bb ) BEFCEFGZABCHIZFABDJZKLZUGCKLZGZDEMZF
      ZAEFZBAKLZACKLZNZUEUFUKADBCOPULUMUNUOGZGUPUJUQDAEUGAQUHUNUIUOUGABKRUGACKS
      TUAUMUNUOUBUCUD $.
      $( [21-Jul-2005] $)
  $}

  $( Membership in a finite set of sequential integers. $)
  elfz $p |- ( ( K e. ZZ /\ M e. ZZ /\ N e. ZZ ) -> ( K e. ( M ... N ) <->
                 ( M <_ K /\ K <_ N ) ) ) $=
    ( cz wcel cfz co cle wbr wa wb w3a elfz1 3anass baib sylan9bb 3impa 3comr
    ) BDEZCDEZADEZABCFGEZBAHIZACHIZJZKZSTUAUFSTJUBUAUCUDLZUAUEABCMUGUAUEUAUCUDN
    OPQR $.
    $( [29-Sep-2005] $)

  ${
    $d k n m z M $.  $d k n m z N $.
    $( Membership in a finite set of sequential integers.  We use the fact that
       an operation's value is empty outside of its domain to show ` M e. ZZ `
       and ` N e. ZZ ` . $)
    elfz2 $p |- ( N e. A -> ( K e. ( M ... N ) <->
           ( ( M e. ZZ /\ N e. ZZ /\ K e. ZZ ) /\ ( M <_ K /\ K <_ N ) ) ) ) $=
      ( vm vn vz vk wcel cfz co cz wa cle wbr w3a wb elfz1 ibar 3anass syl5bb
      bitrd adantl wn c0 cdm cxp wceq cv crab zex rabex df-fz dmoprab2 ndmoprg
      mp3an1 eleq2d noel pm2.21i simpl pm5.21ni pm2.61dan df-3an anbi1i anass
      bitr2i syl6bb ) DAIZBCDJKZIZCLIZDLIZMZBLIZCBNOZBDNOZMZMZMZVKVLVNPZVQMZVHV
      MVJVSQZVMWBVHVMVJVNVOVPPZVSBCDRVMVRVSWCVMVRSVNVOVPTUAUBUCVHVMUDZMZVJBUEIZ
      VSWEVIUEBJUFLLUGUHVHWDVIUEUHEFGLLEUIHUIZNOWGFUINOMZHLUJJWHHLUKULGHEFUMUNC
      DALLJUOUPUQWDWFVSQVHWFVMVSWFVMBURUSVMVRUTVAUCUBVBWAVMVNMZVQMVSVTWIVQVKVLV
      NVCVDVMVNVQVEVFVG $.
      $( [6-Sep-2005] $)

    $( Lemma for ~ elfzel1 and others. $)
    elfzlem $p |- ( K e. ( M ... N ) ->
                    ( ( M e. ZZ /\ K e. ZZ ) /\ ( M <_ K /\ K <_ N ) ) ) $=
      ( vm vn vz vk cvv wcel cfz co cz wa cle wbr wi w3a elfz2 3simpb anim1i
      syl6bi wn oprprc2 eleq2d brprc anbi2d simpl anim12i cr zre leid syl
      3ad2ant3 adantr jcad anass syl6ib c0 wceq cdm wrel cxp relxp cv crab zex
      rabex df-fz dmoprab2 releqi mpbir oprprc1 pm2.24d n0i syl5 pm2.61i
      syl5bir sylbid ) CHIZABCJKZIZBLIZALIZMZBANOZACNOZMZMZPVSWAWBCLIZWCQZWGMWH
      HABCRWJWDWGWBWIWCSTUAVSUBZWAABBJKZIZWHWKVTWLABCJUCUDWKWHWDWEAANOZMZMZWMWK
      WGWOWDWKWFWNWEACNUEUFUFBHIZWMWPPWQWMWDWEMZWNMWPWQWMWRWNWQWMWBWBWCQZWEABNO
      ZMZMZWRHABBRZWSWDXAWEWBWBWCSWEWTUGUHUAWQWMXBWNXCWSWNXAWCWBWNWBWCAUIIWNAUJ
      AUKULUMUNUAUOWDWEWNUPUQWQUBZWLURUSZUBWPWMXDXEWPBBJJUTZVALLVBZVALLVCXFXGDE
      FLLDVDGVDZNOXHEVDNOMZGLVEJXIGLVFVGFGDEVHVIVJVKVLVMWLAVNVOVPVQVRVP $.
      $( [6-Sep-2005] $)
  $}

  $( Membership in a finite set of sequential integers. $)
  elfz5 $p |- ( ( K e. ( ZZ>= ` M ) /\ N e. ZZ ) ->
                 ( K e. ( M ... N ) <-> K <_ N ) ) $=
    ( cuz cfv wcel cz wa cfz co cle wbr wb elfz 3expa eluzelz eluzel2 jca
    sylan eluzle biantrurd adantr bitr4d ) ABDEFZCGFZHABCIJFZBAKLZACKLZHZUHAGFZ
    BGFZHUEUFUIMZUDUJUKUEULABCNOUDUJUKBAPBAQRSUDUHUIMUEUDUGUHBATUAUBUC $.
    $( [26-Dec-2005] $)

  $( Membership in a finite set of sequential integers. $)
  elfz4 $p |- ( ( ( M e. ZZ /\ N e. ZZ /\ K e. ZZ ) /\ ( M <_ K /\ K <_ N ) )
               -> K e. ( M ... N ) ) $=
    ( cz wcel w3a cle wbr cfz co wi wa elfz1 biimprcd 3exp com4r 3impia imp32
    ) BDEZCDEZADEZFBAGHZACGHZABCIJEZSTUAUBUCUDKKUAUBUCSTLZUDUAUBUCUEUDKUEUDUAUB
    UCFABCMNOPQR $.
    $( [21-Jul-2005] $)

  $( Membership in a finite set of sequential integers in terms of sets of
     upper integers. $)
  elfzuzb $p |- ( N e. A -> ( K e. ( M ... N ) <->
         ( K e. ( ZZ>= ` M ) /\ N e. ( ZZ>= ` K ) ) ) ) $=
    ( wcel cfz co cz w3a cle wbr wa cuz cfv elfz2 an6 eluz2 anbi12i df-3an
    anandir ancom anbi2i 3bitri anbi1i bitr4i 3bitr4ri syl6bb ) DAEBCDFGECHEZDH
    EZBHEZIZCBJKZBDJKZLZLZBCMNEZDBMNEZLZABCDOUHUJULIZUJUIUMIZLUHUJLZUJUILZUNIZU
    RUOUHUJULUJUIUMPUPUSUQUTCBQBDQRUOVAVBLZUNLVCUKVDUNUKUHUILUJLVAUIUJLZLVDUHUI
    UJSUHUIUJTVEVBVAUIUJUAUBUCUDVAVBUNSUEUFUG $.
    $( [18-Sep-2005] $)

  $( Membership in a finite set of sequential integers. $)
  eluzfz $p |- ( ( K e. ( ZZ>= ` M ) /\ N e. ( ZZ>= ` K ) )
               -> K e. ( M ... N ) ) $=
    ( cz wcel cuz cfv wa cfz co eluzelz adantl elfzuzb biimpar mpancom ) CDEZAB
    FGEZCAFGEZHZABCIJEZRPQACKLPTSDABCMNO $.
    $( [4-Oct-2005] $)

  $( Membership in a finite set of sequential integers implies membership in a
     set of upper integers. $)
  elfzuz3 $p |- ( ( N e. A /\ K e. ( M ... N ) ) -> N e. ( ZZ>= ` K ) ) $=
    ( wcel cfz co cuz cfv elfzuzb simplbda ) DAEBCDFGEBCHIEDBHIEABCDJK $.
    $( [28-Sep-2005] $)

  ${
    elfzel2.1 $e |- N e. _V $.
    $( Membership in a finite set of sequential integer implies the upper bound
       is an integer. $)
    elfzel2i $p |- ( K e. ( M ... N ) -> N e. ZZ ) $=
      ( cfz co wcel cz w3a cle wbr wa cvv wb elfz2 ax-mp simpl2 sylbi ) ABCEFGZ
      BHGZCHGZAHGZIBAJKACJKLZLZUACMGSUDNDMABCOPTUAUBUCQR $.
      $( [6-Sep-2005] $)
  $}

  $( Membership in a finite set of sequential integers implies the upper bound
     is an integer. $)
  elfzel2g $p |- ( ( N e. A /\ K e. ( M ... N ) ) -> N e. ZZ ) $=
    ( wcel cfz co cz w3a cle wbr wa elfz2 simpl2 syl6bi imp ) DAEZBCDFGEZDHEZQR
    CHEZSBHEZICBJKBDJKLZLSABCDMTSUAUBNOP $.
    $( [6-Sep-2005] $)

  $( Membership in a finite set of sequential integer implies the lower bound
     is an integer. $)
  elfzel1 $p |- ( K e. ( M ... N ) -> M e. ZZ ) $=
    ( cfz co wcel cz wa cle wbr elfzlem simpll syl ) ABCDEFBGFZAGFZHBAIJACIJHZH
    NABCKNOPLM $.
    $( [6-Sep-2005] $)

  $( A member of a finite set of sequential integer is an integer. $)
  elfzelz $p |- ( K e. ( M ... N ) -> K e. ZZ ) $=
    ( cfz co wcel cz wa cle wbr elfzlem simplr syl ) ABCDEFBGFZAGFZHBAIJACIJHZH
    OABCKNOPLM $.
    $( [6-Sep-2005] $)

  $( A member of a finite set of sequential integer is greater than or equal to
     the lower bound. $)
  elfzle1 $p |- ( K e. ( M ... N ) -> M <_ K ) $=
    ( cfz co wcel cz wa cle wbr elfzlem simprl syl ) ABCDEFBGFAGFHZBAIJZACIJZHH
    OABCKNOPLM $.
    $( [6-Sep-2005] $)

  $( A member of a finite set of sequential integer is less than or equal to
     the upper bound. $)
  elfzle2 $p |- ( K e. ( M ... N ) -> K <_ N ) $=
    ( cfz co wcel cz wa cle wbr elfzlem simprr syl ) ABCDEFBGFAGFHZBAIJZACIJZHH
    PABCKNOPLM $.
    $( [6-Sep-2005] $)

  $( Membership in a finite set of sequential integer implies the bounds are
     comparable. $)
  elfzle3 $p |- ( ( N e. A /\ K e. ( M ... N ) ) -> M <_ N ) $=
    ( wcel cfz co wa cle wbr elfzle1 adantl elfzle2 cz wi elfzel1 elfzelz
    elfzel2g cr letr zre syl3an syl111anc mp2and ) DAEZBCDFGEZHZCBIJZBDIJZCDIJZ
    UFUHUEBCDKLUFUIUEBCDMLUGCNEZBNEZDNEZUHUIHUJOZUFUKUEBCDPLUFULUEBCDQLABCDRCSE
    BSEDSEUNUKULUMCBDTCUABUADUAUBUCUD $.
    $( [18-Sep-2005] $)

  $( Implication of membership in a finite set of sequential integers. $)
  elfzuz2 $p |- ( ( N e. A /\ K e. ( M ... N ) ) -> N e. ( ZZ>= ` M ) ) $=
    ( wcel cfz co wa cuz cfv elfzuzb ancom syl6bb biimpa uztrn syl ) DAEZBCDFGE
    ZHDBIJEZBCIJZEZHZDTEQRUBQRUASHUBABCDKUASLMNBDCOP $.
    $( [20-Sep-2005] $)

  $( Membership in a finite set of sequential integers - special case. $)
  eluzfz1 $p |- ( N e. ( ZZ>= ` M ) -> M e. ( M ... N ) ) $=
    ( cuz cfv wcel cz cle wbr w3a cfz co eluz2 simp1 simp2 cr zre leid syl
    3ad2ant1 simp3 elfz4 syl32anc sylbi ) BACDEAFEZBFEZABGHZIZAABJKEZABLUGUDUEU
    DAAGHZUFUHUDUEUFMZUDUEUFNUJUDUEUIUFUDAOEUIAPAQRSUDUEUFTAABUAUBUC $.
    $( [21-Jul-2005] $)

  $( A member of a finite set of sequential integers belongs to a set of upper
     integers. $)
  elfzuz $p |- ( K e. ( M ... N ) -> K e. ( ZZ>= ` M ) ) $=
    ( cfz co wcel cz cle wbr w3a cuz cfv elfzel1 elfzelz elfzle1 3jca eluz2
    sylibr ) ABCDEFZBGFZAGFZBAHIZJABKLFSTUAUBABCMABCNABCOPBAQR $.
    $( [17-Sep-2005] $)

  $( Membership in a finite set of sequential integers - special case. $)
  eluzfz2 $p |- ( N e. ( ZZ>= ` M ) -> N e. ( M ... N ) ) $=
    ( cuz cfv wcel cz cle wbr cfz co eluzel2 eluzelz eluzle cr zre leid 3syl
    elfz4 syl32anc ) BACDEZAFEBFEZUAABGHBBGHZBABIJEABKABLZUCABMTUABNEUBUCBOBPQB
    ABRS $.
    $( [13-Sep-2005] $)

  $( Membership in a finite set of sequential integers - special case. $)
  eluzfz2b $p |- ( N e. ( ZZ>= ` M ) <-> N e. ( M ... N ) ) $=
    ( cuz cfv wcel cfz co eluzfz2 elfzuz impbii ) BACDEBABFGEABHBABIJ $.
    $( [14-Sep-2005] $)

  $( Membership in a finite set of sequential integers containing one
     integer. $)
  elfz3 $p |- ( N e. ZZ -> N e. ( N ... N ) ) $=
    ( cz wcel cuz cfv cfz co uzid eluzfz1 syl ) ABCAADECAAAFGCAHAAIJ $.
    $( [21-Jul-2005] $)

  $( Membership in a finite set of sequential integers containing one
     integer. $)
  elfz1eq $p |- ( K e. ( N ... N ) -> K = N ) $=
    ( cfz co wcel cz wa cle wbr wceq elfzlem cr wb letri3 zre syl2an biimpar
    eqcomd syl ) ABBCDEBFEZAFEZGZBAHIABHIGZGZABJABBKUDBAUBBAJZUCBLEALEUEUCMTUAB
    ANBOAOPQRS $.
    $( [19-Sep-2005] $)

  ${
    $d K k m $.  $d M k m $.  $d N k m $.
    $( A finite set of sequential integers is empty if the bounds are
       reversed. $)
    fzn $p |- ( ( M e. ZZ /\ N e. ZZ ) -> ( N < M <-> ( M ... N ) = (/) ) ) $=
      ( vk cz wcel wa clt wbr cfz co c0 wceq cle wn cr wb lenlt zre syl2an w3a
      cuz cfv eluz2 eluzfz1 sylbir n0i syl 3expia wi cv wex elfzle3 ex
      19.23adv neq0 syl5ib adantl impbid bitr3d con4bid ) ADEZBDEZFZBAGHZABIJZK
      LZVCABMHZVDNZVFNZAOEBOEVGVHPVAVBABQARBRSVCVGVIVAVBVGVIVAVBVGTZAVEEZVIVJBA
      UAUBEVKABUCABUDUEVEAUFUGUHVBVIVGUIVAVBCUJZVEEZCUKVGVIVBVMVGCVBVMVGDVLABUL
      UMUNCVEUOUPUQURUSUT $.
      $( [18-Apr-2006] $) $( [22-Aug-2005] $)

    $( A shifted finite set of sequential integers is equinumerous to the
       original set.  (Contributed by Paul Chapman, 11-Apr-2009.) $)
    fzen $p |- ( ( M e. ZZ /\ N e. ZZ /\ K e. ZZ ) ->
                 ( M ... N ) ~~ ( ( M + K ) ... ( N + K ) ) ) $=
      ( vk vm cz wcel w3a cfz co caddc cv cmin cvv oprex a1i cle wbr wi wa
