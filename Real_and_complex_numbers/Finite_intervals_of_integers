    RUHUSVMVPVNVRVJVJBUTCUTVJVAVBVCVD $.
    $( [16-Sep-2005] $)

  $( A member of a finite set of sequential integers starting at 0 is a
     nonnegative integer. $)
  elfznn0 $p |- ( K e. ( 0 ... N ) -> K e. NN0 ) $=
    ( cn0 wcel cz cc0 cle wbr cfz co elnn0z elfzelz elfzle1 sylanbrc ) ACDAEDFA
    GHAFBIJDAKAFBLAFBMN $.
    $( [5-Aug-2005] $)

  $( The upper bound of a nonempty finite set of sequential integers starting
     at 0 is a nonnegative integer. $)
  elfz3nn0 $p |- ( ( N e. A /\ K e. ( 0 ... N ) ) -> N e. NN0 ) $=
    ( wcel cc0 cfz co cn0 cle wbr w3a elfz2nn0 simp2 syl6bi imp ) CADZBECFGDZCH
    DZPQBHDZRBCIJZKRABCLSRTMNO $.
    $( [16-Sep-2005] $)

  $( Subtraction closure for a member of a finite set of sequential
     integers. $)
  fznn0sub $p |- ( ( N e. A /\ K e. ( M ... N ) ) -> ( N - K ) e. NN0 ) $=
    ( wcel cfz co cmin cn0 cz w3a cle wbr wa elfz2 znn0sub2 3com12 3expa
    adantrl 3adantl1 syl6bi imp ) DAEZBCDFGEZDBHGIEZUCUDCJEZDJEZBJEZKCBLMZBDLMZ
    NZNUEABCDOUGUHUKUEUFUGUHNUJUEUIUGUHUJUEUHUGUJUEBDPQRSTUAUB $.
    $( [16-Sep-2005] $)

