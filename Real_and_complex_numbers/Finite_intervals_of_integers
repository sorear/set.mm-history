  $( A 1-based finite set of sequential integers is empty iff it ends at index
     ` 0 ` .  (Contributed by Paul Chapman, 22-Jun-2011.) $)
  fz1n $p |- ( N e. NN0 -> ( ( 1 ... N ) = (/) <-> N = 0 ) ) $=
    ( cn0 wcel c1 clt wbr cfz co c0 wceq cc0 cz wb nn0z 1z fzn mpan syl
    nn0lt10b bitr3d ) ABCZADEFZDAGHIJZAKJUAALCZUBUCMZANDLCUDUEODAPQRAST $.
    $( [22-Jun-2011] $)

  $( Two ways to say a finite 1-based sequence is empty.  (Contributed by Paul
     Chapman, 26-Oct-2012.) $)
  0fz1 $p |- ( ( N e. NN0 /\ F Fn ( 1 ... N ) ) -> ( F = (/) <-> N = 0 ) ) $=
    ( c1 cfz co wfn c0 wceq cn0 wcel cc0 fndmu fn0 sylan2br ex fneq2 syl6bb
    biimpcd impbid fz1n sylan9bbr ) ACBDEZFZAGHZUBGHZBIJBKHUCUDUEUCUDUEUCAGFZUE
    UDUBGALAMZNOUEUCUDUEUCUFUDUBGAPUGQRSBTUA $.
    $( [26-Oct-2012] $)

  $( 0-based and 1-based finite sets of sequential integers are equinumerous.
     (Contributed by Paul Chapman, 11-Apr-2009.) $)
  fz01en $p |- ( N e. ZZ -> ( 0 ... ( N - 1 ) ) ~~ ( 1 ... N ) ) $=
    ( cz wcel cc0 c1 cmin co cfz caddc cen wbr peano2zm 0z 1z fzen mp3an13 syl
    wceq ax1cn addid2i a1i cc npcan zcn sylancl opreq12d breq2d mpbid ) ABCZDAE
    FGZHGZDEIGZUJEIGZHGZJKZUKEAHGZJKUIUJBCZUOALDBCUQEBCUOMNEDUJOPQUIUNUPUKJUIUL
    EUMAHULERUIESTUAAUBCEUBCUMARUIAEUCAUDSUEUFUGUH $.
    $( [11-Apr-2009] $)

  $( A member of a finite set of sequential integers starting at 1 is a natural
     number. $)
  elfznn $p |- ( K e. ( 1 ... N ) -> K e. NN ) $=
    ( cn wcel cz c1 cle wbr cfz co elnnz1 elfzelz elfzle1 sylanbrc ) ACDAEDFAGH
    AFBIJDAKAFBLAFBMN $.
    $( [24-Aug-2005] $)

  $( Membership in a finite set of sequential integers starting at 0. $)
  elfz2nn0 $p |- ( N e. A ->
           ( K e. ( 0 ... N ) <-> ( K e. NN0 /\ N e. NN0 /\ K <_ N ) ) ) $=
    ( wcel cc0 cfz co cz w3a cle wbr wa cn0 elfz2 simpr simpl anim12i simpll
