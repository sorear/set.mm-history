  $( A 1-based finite set of sequential integers is empty iff it ends at index
     ` 0 ` .  (Contributed by Paul Chapman, 22-Jun-2011.) $)
  fz1n $p |- ( N e. NN0 -> ( ( 1 ... N ) = (/) <-> N = 0 ) ) $=
    ( cn0 wcel c1 clt wbr cfz co c0 wceq cc0 cz wb nn0z 1z fzn mpan syl
    nn0lt10b bitr3d ) ABCZADEFZDAGHIJZAKJUAALCZUBUCMZANDLCUDUEODAPQRAST $.
    $( [22-Jun-2011] $)

  $( Two ways to say a finite 1-based sequence is empty.  (Contributed by Paul
     Chapman, 26-Oct-2012.) $)
  0fz1 $p |- ( ( N e. NN0 /\ F Fn ( 1 ... N ) ) -> ( F = (/) <-> N = 0 ) ) $=
    ( c1 cfz co wfn c0 wceq cn0 wcel cc0 fn0 fndmu sylan2br ex fneq2 syl6bb
    biimpcd impbid fz1n sylan9bbr ) ACBDEZFZAGHZUBGHZBIJBKHUCUDUEUCUDUEUDUCAGFZ
    UEALZUBGAMNOUEUCUDUEUCUFUDUBGAPUGQRSBTUA $.
    $( [26-Oct-2012] $)

  $( There are no integers between 1 and 0.  (Contributed by Jeff Madsen,
     16-Jun-2010.) $)
  fz10 $p |- ( 1 ... 0 ) = (/) $=
    ( vx c1 cc0 cfz co c0 wceq cv wcel wn eq0 cle wbr clt lt01 0re 1re ltnlei
    mpbi cr elfzle3 mpan mto mpgbir ) BCDEZFGAHZUEIZJAAUEKUGBCLMZCBNMUHJOCBPQRS
    CTIUGUHPTUFBCUAUBUCUD $.
    $( [16-Jun-2010] $)

  ${
    $d M x $.  $d N x $.  $d A x $.
    $( Properties of a finite interval of integers which is non-empty.
       (Contributed by Jeff Madsen, 17-Jun-2010.) $)
    fzn0 $p |- ( ( N e. A /\ ( M ... N ) =/= (/) ) ->
                                      ( M e. ZZ /\ N e. ZZ /\ M <_ N ) ) $=
      ( vx cfz co c0 wne wcel cv wex cz cle wbr w3a n0 wa elfzel1 adantl
      elfzel2g elfzle3 3jca ex 19.23adv imp sylan2b ) BCEFZGHCAIZDJZUGIZDKZBLIZ
      CLIZBCMNZOZDUGPUHUKUOUHUJUODUHUJUOUHUJQULUMUNUJULUHUIBCRSAUIBCTAUIBCUAUBU
      CUDUEUF $.
      $( [17-Jun-2010] $)
  $}

  ${
    $d M x $.  $d N x $.  $d K x $.
    $( Split a finite interval of integers into two parts.  (Contributed by
       Jeff Madsen, 17-Jun-2010.) $)
