  $( A natural number is greater than one iff it is not equal to one. $)
  nngt1ne1t $p |- ( A e. NN -> ( 1 < A <-> -. A = 1 ) ) $=
    ( cn wcel c1 clt wbr wceq wn cr cle wb 1re leltnet mp3an1 nnret nnge1t
    sylanc eqcom negbii syl6bb ) ABCZDAEFZDAGZHZADGZHAICZDAJFZUBUDKZUADICUFUGUH
    LDAMNAOAPQUCUEDARST $.
    $( [17-Oct-2004] $) $( [7-Oct-2004] $)

  $( A natural number is less than or equal to one iff it is equal to one. $)
  nnle1eq1t $p |- ( A e. NN -> ( A <_ 1 <-> A = 1 ) ) $=
    ( cn wcel c1 cle wbr wa wceq nnge1t biantrud cr wb nnret 1re letri3t mpan2
    syl bitr4d ) ABCZADEFZTDAEFZGZADHZSUATAIJSAKCZUCUBLZAMUDDKCUENADOPQR $.
    $( [22-Apr-2005] $) $( [3-Apr-2005] $)

  $( A natural number is positive. $)
  nngt0t $p |- ( A e. NN -> 0 < A ) $=
    ( cr wcel c1 cle wbr cc0 clt cn lt01 wa wi 0re 1re ltletrt mp3an12 mpani
    nnret nnge1t sylc ) ABCZDAEFZGAHFZAICUAGDHFZUBUCJGBCDBCUAUDUBKUCLMNGDAOPQAR
    AST $.
    $( [26-Sep-1999] $)

  $( A number less than one is not a natural number. $)
  lt1nnn $p |- ( ( A e. RR /\ A < 1 ) -> -. A e. NN ) $=
    ( cr wcel c1 clt wbr cn wn cle wb 1re ltnlet mpan2 nnge1t con3i syl6bi imp
    ) ABCZADEFZAGCZHZRSDAIFZHZUARDBCSUCJKADLMTUBANOPQ $.
    $( [18-Jan-2004] $) $( [18-Jan-2004] $)

  $( Zero is not a natural number. $)
  0nnn $p |- -. 0 e. NN $=
    ( cc0 cr wcel c1 clt wbr cn wn 0re lt01 lt1nnn mp2an ) ABCADEFAGCHIJAKL $.
    $( [25-Aug-1999] $)

  $( A natural number is non-zero. $)
  nnne0t $p |- ( A e. NN -> A =/= 0 ) $=
    ( cn wcel cc0 wceq 0nnn eleq1 mtbiri necon2ai ) ABCZADADEJDBCFADBGHI $.
    $( [27-Sep-1999] $)

  ${
    nngt0.1 $e |- A e. NN $.
    $( A natural number is positive (inference version). $)
    nngt0 $p |- 0 < A $=
      ( cn wcel cc0 clt wbr nngt0t ax-mp ) ACDEAFGBAHI $.
      $( [17-Sep-1999] $)

    $( A natural number is non-zero (inference version). $)
    nnne0 $p |- A =/= 0 $=
      ( nnre nngt0 gt0ne0i ) AABCABDE $.
      $( [25-Aug-1999] $)
  $}

