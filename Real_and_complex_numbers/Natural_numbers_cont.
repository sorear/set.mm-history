  $( A natural number is positive. $)
  nngt0 $p |- ( A e. NN -> 0 < A ) $=
    ( cr wcel c1 cle wbr cc0 clt cn lt01 wa wi 0re 1re ltletr mp3an12 mpani
    nnre nnge1 sylc ) ABCZDAEFZGAHFZAICUAGDHFZUBUCJGBCDBCUAUDUBKUCLMNGDAOPQARAS
    T $.
