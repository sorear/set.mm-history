    GEUCZWGKVSWOUDEGUPTGEUETUAUFWJWKWBWGWNUBWBWFWNUAUFUFGBCZWPWAWMUGUQUQGGUIUJU
    KVBVCWAUGAGAGIULUMUNURUS $.
    $( [18-Oct-2005] $) $( [14-Oct-2005] $)

  $( Any (finite) real is less than plus infinity. $)
  ltpnft $p |- ( A e. RR -> A < +oo ) $=
    ( cr wcel cpnf clt wbr wa cltrr cmnf wceq wo eqid jctr orc olc 3syl cxr wb
    rexrt pnfxr ltxrt mpan2 syl mpbird ) ABCZADEFZUEDBCZGADHFGAIJZDDJZGKZUEUIGZ
    UHUGGZKZKZUEUKUMUNUEUIDLMUKULNUMUJOPUEAQCZUFUNRZASUODQCUPTADUAUBUCUD $.
    $( [19-Oct-2005] $) $( [14-Oct-2005] $)

  $( Minus infinity is less than any (finite) real. $)
  mnfltt $p |- ( A e. RR -> -oo < A ) $=
    ( cr wcel cmnf clt wbr wa cltrr wceq cpnf wo eqid jctl olc 3syl cxr wb
    rexrt mnfxr ltxrt mpan syl mpbird ) ABCZDAEFZDBCZUDGDAHFGDDIZAJIZGKZUFUHGZU
    GUDGZKZKZUDUKULUMUDUGDLMUKUJNULUINOUDAPCZUEUMQZARDPCUNUOSDATUAUBUC $.
    $( [17-Oct-2005] $) $( [14-Oct-2005] $)

  $( Minus infinity is less than plus infinity. $)
  mnfltpnf $p |- -oo < +oo $=
    ( cmnf cpnf clt wbr cr wcel wa cltrr wceq wo eqid olc mp2an orc ax-mp cxr
    wb mnfxr pnfxr ltxrt mpbir ) ABCDZAEFZBEFZGABHDGZAAIZBBIZGZJZUCUGGUFUDGJZJZ
    UIUKUFUGUIAKBKUHUELMUIUJNOAPFBPFUBUKQRSABTMUA $.
    $( [19-Oct-2005] $) $( [14-Oct-2005] $)

  $( Minus infinity is less than an extended real that is either real or plus
     infinity. $)
  mnfltxrt $p |- ( ( A e. RR \/ A = +oo ) -> -oo < A ) $=
    ( cr wcel cmnf clt wbr cpnf wceq mnfltt mnfltpnf breq2 mpbiri jaoi ) ABCDAE
    FZAGHZAIONDGEFJAGDEKLM $.
    $( [26-Feb-2006] $) $( [2-Feb-2006] $)

  $( No extended real is greater than plus infinity. $)
  pnfnltt $p |- ( A e. RR* -> -. +oo < A ) $=
    ( cxr wcel cpnf clt wbr cr wa cltrr cmnf wceq wo wnel wn pnfnre df-nel
    mpbi intnanr wne pnfnemnf df-ne pm3.2ni wb pnfxr ltxrt mpan mtbiri ) ABCZDA
    EFZDGCZAGCZHZDAIFZHZDJKZADKZHZLZUJUPHZUOUKHZLZLZURVAUNUQULUMUJUKDGMUJNODGPQ
    ZRRUOUPDJSUONTDJUAQZRUBUSUTUJUPVCRUOUKVDRUBUBDBCUHUIVBUCUDDAUEUFUG $.
    $( [19-Oct-2005] $) $( [15-Oct-2005] $)

  $( No extended real is less than minus infinity. $)
  nltmnft $p |- ( A e. RR* -> -. A < -oo ) $=
    ( cxr wcel cmnf clt wbr cr wa cltrr wceq cpnf wo wnel wn minfnre df-nel
    mpbi intnan intnanr wne pnfnemnf necom df-ne pm3.2ni wb mnfxr ltxrt mpan2
