    BCUKVDUEUFADUGUHUI $.
    $( [20-Oct-2005] $) $( [15-Oct-2005] $)

  $( Plus infinity is an upper bound for extended reals. $)
  pnfge $p |- ( A e. RR* -> A <_ +oo ) $=
    ( cxr wcel cpnf cle wbr clt wn pnfnlt wb pnfxr xrlenlt mpan2 mpbird ) ABCZA
    DEFZDAGFHZAIODBCPQJKADLMN $.
    $( [18-Feb-2006] $) $( [30-Jan-2006] $)

  $( Minus infinity is less than or equal to any extended real. $)
  mnfle $p |- ( A e. RR* -> -oo <_ A ) $=
    ( cxr wcel cmnf cle wbr clt wn nltmnf wb mnfxr xrlenlt mpan mpbird ) ABCZDA
    EFZADGFHZAIDBCOPQJKDALMN $.
    $( [25-Jan-2006] $) $( [19-Jan-2006] $)

  $( Ordering on the extended reals is not symmetric. $)
  xrltnsym $p |- ( ( A e. RR* /\ B e. RR* ) ->
                ( A < B -> -. B < A ) ) $=
    ( cr wcel cpnf wceq cmnf w3o clt wbr wn wi cxr ltnsym wa rexr pnfnlt syl
    adantr wb breq1 adantl mtbird a1d nltmnf breq2 pm2.21d 3jaodan elxr
    sylan2br mnfxr ax-mp breq12 mtbiri ancoms xrltnr 3jaoian syl2anb ) ACDZAEFZ
    AGFZHBCDZBEFZBGFZHZABIJZBAIJZKZLZAMDZBMDZUSVEVIUTVAUSVBVIVCVDABNUSVCOZVHVFV
    LVGEAIJZUSVMKZVCUSVJVNAPZAQRSVCVGVMTUSBEAIUAUBUCUDUSVDOZVFVHVPVFAGIJZUSVQKZ
    VDUSVJVRVOAUERSVDVFVQTUSBGAIUFUBUCUGUHUTVKVIVEUTVKOZVFVHVSVFEBIJZVKVTKUTBQU
    BUTVFVTTVKAEBIUASUCUGBUIZUJVAVBVIVCVDVAVBOZVHVFWBVGBGIJZVBWCKZVAVBVKWDBPBUE
    RUBVAVGWCTVBAGBIUFSUCUDVAVCOVHVFVCVAVHVCVAOVGEGIJZGMDZWEKUKGQULBEAGIUMUNUOU
    DVAVDOZVFVHWGVFGGIJZWFWHKUKGUPULAGBGIUMUNUGUHUQAUIWAUR $.
    $( [21-Oct-2005] $) $( [15-Oct-2005] $)

  $( 'Less than' is antisymmetric and irreflexive for extended reals. $)
  xrltnsym2 $p |- ( ( A e. RR* /\ B e. RR* ) -> -. ( A < B /\ B < A ) ) $=
    ( cxr wcel wa clt wbr wn wi xrltnsym imnan sylib ) ACDBCDEABFGZBAFGZHIMNEHA
    BJMNKL $.
    $( [8-Jun-2007] $) $( [6-Feb-2007] $)

  $( Ordering on the extended reals satisfies strict trichotomy. $)
  xrlttri $p |- ( ( A e. RR* /\ B e. RR* ) ->
                ( A < B <-> -. ( A = B \/ B < A ) ) ) $=
    ( cxr wcel wa wceq clt wbr wo wn wi xrltnr adantr wb breq2 adantl mtbid ex
    xrltnsym ancoms jaod cr cpnf cmnf w3o axlttri biimprd con1d ltpnf mpbird
    pm2.24d mnflt breq1 olcd a1d 3jaodan eqtr3 orcd mnfltpnf breq12 mpbiri
    3jaoian elxr syl2anb impbid con2bid ) ACDZBCDZEZABFZBAGHZIZABGHZVIVLVMJZVIV
    JVNVKVGVJVNKVHVGVJVNVGVJEAAGHZVMVGVOJVJALMVJVOVMNVGABAGOPQRMVHVGVKVNKBASTUA
    AUBDZAUCFZAUDFZUEBUBDZBUCFZBUDFZUEZVNVLKZVGVHVPWBWCVQVRVPVSWCVTWAVPVSEZVLVM
    WDVMVLJABUFUGUHVPVTEZVMVLWEVMAUCGHZVPWFVTAUIMVTVMWFNVPBUCAGOPUJUKVPWAEZVLVN
    WGVKVJWGVKUDAGHZVPWHWAAULMWAVKWHNVPBUDAGUMPUJUNUOUPVQVSWCVTWAVQVSEZVLVNWIVK
