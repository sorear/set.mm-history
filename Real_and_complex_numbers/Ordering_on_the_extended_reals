    $( [5-Mar-2006] $) $( [2-Feb-2006] $)

  $( A way of proving that an extended real is real. $)
  xrret $p |- ( ( ( A e. RR* /\ B e. RR ) /\ ( -oo < A /\ A <_ B ) ) ->
              A e. RR ) $=
    ( cxr wcel cr wa cmnf clt wbr cle cpnf simprl ltpnft adantl wi pnfxr
    xrlelttrt mp3an3 rexrt sylan2 mpan2d imp adantrl jca wb xrrebndt ad2antrr
    mpbird ) ACDZBEDZFZGAHIZABJIZFZFZAEDZULAKHIZFZUOULUQUKULUMLUKUMUQULUKUMUQUK
    UMBKHIZUQUJUSUIBMNUIBCDZUMUSFUQOZUJUIUTKCDVAPABKQRBSTUAUBUCUDUIUPURUEUJUNAU
    FUGUH $.
    $( [10-Mar-2005] $) $( [9-Mar-2006] $)

  $( An extended real between two others is real. $)
  xrre2t $p |- ( ( ( A e. RR* /\ B e. RR* /\ C e. RR* ) /\
              ( A < B /\ B < C ) ) -> B e. RR ) $=
    ( cxr wcel w3a clt wbr wa cr cmnf cpnf wi cle mnflet adantr mnfxr
    xrlelttrt mp3an1 mpand 3adant3 pnfget adantl pnfxr xrltletrt mp3an3 mpan2d
    3adant1 anim12d wb xrrebndt 3ad2ant2 sylibrd imp ) ADEZBDEZCDEZFZABGHZBCGHZ
    IZBJEZURVAKBGHZBLGHZIZVBURUSVCUTVDUOUPUSVCMUQUOUPIKANHZUSVCUOVFUPAOPKDEUOUP
