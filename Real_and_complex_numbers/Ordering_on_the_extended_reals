  $( 'Less than or equal' expressed in terms of 'less than' or 'equals', for
     extended reals. $)
  xrleloet $p |- ( ( A e. RR* /\ B e. RR* ) ->
               ( A <_ B <-> ( A < B \/ A = B ) ) ) $=
    ( cxr wcel wa cle wbr clt wn wceq wo xrlenltt wb xrlttrit ancoms con2bid
    eqcom orbi1i orcom bitr syl5rbbr bitrd ) ACDZBCDZEZABFGBAHGZIZABHGZABJZKZAB
    LUEBAJZUHKZUGUJUEUFULUDUCUFULIMBANOPULUIUHKUJUKUIUHBAQRUIUHSTUAUB $.
    $( [27-Jan-2006] $) $( [19-Jan-2006] $)

  $( 'Less than or equal to' implies 'less than' is not 'equals', for extended
     reals. $)
  xrleltnet $p |- ( ( A e. RR* /\ B e. RR* /\ A <_ B ) ->
                ( A < B <-> -. A = B ) ) $=
    ( cxr wcel cle wbr clt wceq wn wb wa wi xrlttri3t pm3.26 syl6bi adantr wo
    xrleloet biimpa ord impbid con2bid 3impa ) ACDZBCDZABEFZABGFZABHZIJUDUEKZUF
    KZUHUGUJUHUGIZUIUHUKLUFUIUHUKBAGFIZKUKABMUKULNOPUJUGUHUIUFUGUHQABRSTUAUBUC
