    BCUKVDUEUFADUGUHUI $.
    $( [20-Oct-2005] $) $( [15-Oct-2005] $)

  $( Plus infinity is an upper bound for extended reals. $)
  pnfget $p |- ( A e. RR* -> A <_ +oo ) $=
    ( cxr wcel cpnf cle wbr clt wn pnfnltt wb pnfxr xrlenltt mpan2 mpbird ) ABC
    ZADEFZDAGFHZAIODBCPQJKADLMN $.
    $( [18-Feb-2006] $) $( [30-Jan-2006] $)

  $( Minus infinity is less than or equal to any extended real. $)
  mnflet $p |- ( A e. RR* -> -oo <_ A ) $=
    ( cxr wcel cmnf cle wbr clt wn nltmnft wb mnfxr xrlenltt mpan mpbird ) ABCZ
    DAEFZADGFHZAIDBCOPQJKDALMN $.
    $( [25-Jan-2006] $) $( [19-Jan-2006] $)

  $( Ordering on the extended reals is not symmetric. $)
  xrltnsymt $p |- ( ( A e. RR* /\ B e. RR* ) ->
                ( A < B -> -. B < A ) ) $=
    ( cr wcel cpnf wceq cmnf w3o clt wbr wn wi cxr ltnsymt wa rexrt pnfnltt
    syl adantr wb breq1 adantl mtbird a1d nltmnft breq2 pm2.21d 3jaodan elxr
    sylan2br mnfxr ax-mp breq12 mtbiri ancoms xrltnrt 3jaoian syl2anb ) ACDZAEF
    ZAGFZHBCDZBEFZBGFZHZABIJZBAIJZKZLZAMDZBMDZUSVEVIUTVAUSVBVIVCVDABNUSVCOZVHVF
    VLVGEAIJZUSVMKZVCUSVJVNAPZAQRSVCVGVMTUSBEAIUAUBUCUDUSVDOZVFVHVPVFAGIJZUSVQK
    ZVDUSVJVRVOAUERSVDVFVQTUSBGAIUFUBUCUGUHUTVKVIVEUTVKOZVFVHVSVFEBIJZVKVTKUTBQ
    UBUTVFVTTVKAEBIUASUCUGBUIZUJVAVBVIVCVDVAVBOZVHVFWBVGBGIJZVBWCKZVAVBVKWDBPBU
