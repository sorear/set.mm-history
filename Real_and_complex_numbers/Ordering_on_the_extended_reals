$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
           Ordering on the extended reals
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Membership in the set of extended reals. $)
  elxr $p |- ( A e. RR* <-> ( A e. RR \/ A = +oo \/ A = -oo ) ) $=
    ( cxr wcel cr cpnf cmnf cpr cun wo wceq w3o df-xr eleq2i elun pnfxr
    elisseti mnfxr elpr2 orbi2i 3orass bitr4 3bitr ) ABCADEFGZHZCADCZAUCCZIZUEA
    EJZAFJZKZBUDALMADUCNUGUEUHUIIZIUJUFUKUEAEFEBOPFBQPRSUEUHUITUAUB $.
    $( [17-Oct-2005] $) $( [14-Oct-2005] $)

  $( Plus and minus infinity are distinguished elements of ` RR* ` . $)
  pnfnemnf $p |- +oo =/= -oo $=
    ( cpnf cmnf wne wceq wn cc csn wcel eirr axcnex snid eleq2 mpbiri mto
    df-pnf df-mnf eqeq12i mtbir df-ne mpbir ) ABCABDZEUAFFGZDZUCFFHZFIUCUDFUBHF
    JKFUBFLMNAFBUBOPQRABST $.
    $( [18-Oct-2005] $) $( [14-Oct-2005] $)

  $( No (finite) real equals plus infinity. $)
  renepnft $p |- ( A e. RR -> A =/= +oo ) $=
    ( cr wcel cpnf wceq wn wne wnel pnfnre df-nel mpbi nelneq mpan2 df-ne
    sylibr ) ABCZADEFZADGPDBCFZQDBHRIDBJKADBLMADNO $.
    $( [16-Oct-2005] $) $( [14-Oct-2005] $)

  $( No real equals minus infinity. $)
  renemnft $p |- ( A e. RR -> A =/= -oo ) $=
    ( cr wcel cmnf wceq wn wne wnel minfnre df-nel mpbi nelneq mpan2 df-ne
    sylibr ) ABCZADEFZADGPDBCFZQDBHRIDBJKADBLMADNO $.
    $( [26-Oct-2005] $) $( [14-Oct-2005] $)

  $( The reals and the infinities are disjoint. $)
  renfdisj $p |- ( RR i^i { +oo , -oo } ) = (/) $=
    ( cr cpnf cmnf cpr cin csn cun c0 df-pr ineq2i wceq wa wcel wn eqid wne
    renepnft df-ne sylib mt2 disjsn mpbir renemnft pm3.2i undisj2 mpbi eqtr )
    ABCDZEABFZCFZGZEZHUHUKABCIJAUIEHKZAUJEHKZLULHKUMUNUMBAMZNUOBBKZBOUOBBPUPNBQ
    BBRSTABUAUBUNCAMZNUQCCKZCOUQCCPURNCUCCCRSTACUAUBUDAUIUJUEUFUG $.
    $( [27-Oct-2005] $) $( [25-Oct-2005] $)

  ${
    $d x A $.
    $( The three (non-exclusive) possibilities implied by a subset of extended
       reals. $)
    ssxr $p |- ( A (_ RR* -> ( A (_ RR \/ +oo e. A \/ -oo e. A ) ) $=
      ( vx cxr wss cr cpnf wcel cmnf wo w3o wn cv cpr wral cin c0 wceq cun
