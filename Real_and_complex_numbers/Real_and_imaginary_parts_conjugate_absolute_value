$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Real and imaginary parts; conjugate; absolute value
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c Re $. $( Real part of a complex number. $)
  $c Im $. $( Imaginary part of a complex number. $)
  $c * $. $( Complex conjugate function. $)
  $c abs $. $( Absolute value function. $)

  $( Extend class notation to include real part of a complex number. $)
  cre $a class Re $.

  $( Extend class notation to include imaginary part of a complex number. $)
  cim $a class Im $.

  $( Extend class notation to include complex conjugate function. $)
  ccj $a class * $.

  $( Extend class notation to include a function for the absolute value
     (modulus) of a complex number. $)
  cabs $a class abs $.

  ${
    $d x y z w $.
    $( Define a function whose value is the real part of a complex number.
       See ~ revalt for its value, ~ recl for its closure, and ~ replimt
       for its use in decomposing a complex number. $)
    df-re $a |- Re = { <. x , y >. | ( x e. CC /\
                  y = U. { z e. RR | E. w e. RR x = ( z + ( i x. w ) ) } ) } $.

    $( Define a function whose value is the imaginary part of a complex
       number.  See ~ imvalt for its value, ~ imcl for its closure, and
       ~ replimt for its use in decomposing a complex number. $)
    df-im $a |- Im = { <. x , y >. | ( x e. CC /\
                  y = U. { w e. RR | E. z e. RR x = ( z + ( i x. w ) ) } ) } $.

    $( Define the complex conjugate function.  See ~ cjcl for its closure and
       ~ cjvalt for its value. $)
    df-cj $a |- * = { <. x , y >. | ( x e. CC /\
                        y = ( ( Re ` x ) - ( i x. ( Im ` x ) ) ) ) } $.

    $( Define the function for the absolute value (modulus) of a complex
       number.  See ~ abscl for its closure and ~ absvalt or ~ absval2
       for its value. $)
    df-abs $a |- abs = { <. x , y >. | ( x e. CC /\ y = ( sqr `
                 ( x x. ( * ` x ) ) ) ) } $.
  $}

  ${
    $d x y z w A $.
    $( The value of the real part of a complex number. $)
    revalt $p |- ( A e. CC ->
        ( Re ` A ) = U. { x e. RR | E. y e. RR A = ( x + ( i x. y ) ) } ) $=
      ( vz vw cv ci cmul co caddc wceq cr wrex crab cuni cc cre eqeq1 rexbidv
      rabbisdv unieqd df-re reex rabex uniex fvopab4 ) DECDFZAFGBFHIJIZKZBLMZAL
      NZOCUHKZBLMZALNZOPQUGCKZUKUNUOUJUMALUOUIULBLUGCUHRSTUADEABUBUNUMALUCUDUEU
      F $.
      $( [11-Mar-2008] $) $( [9-May-1999] $)

    $( The value of the imaginary part of a complex number. $)
    imvalt $p |- ( A e. CC ->
        ( Im ` A ) = U. { y e. RR | E. x e. RR A = ( x + ( i x. y ) ) } ) $=
      ( vz vw cv ci cmul co caddc wceq cr wrex crab cuni cc cim eqeq1 rexbidv
      rabbisdv unieqd df-im reex rabex uniex fvopab4 ) DECDFZAFGBFHIJIZKZALMZBL
      NZOCUHKZALMZBLNZOPQUGCKZUKUNUOUJUMBLUOUIULALUGCUHRSTUADEABUBUNUMBLUCUDUEU
      F $.
      $( [11-Mar-2008] $) $( [9-May-1999] $)

    $( The real part of a complex number is real. $)
    reclt $p |- ( A e. CC -> ( Re ` A ) e. RR ) $=
      ( vx vy cc wcel cre cfv cv ci cmul co caddc wceq cr wrex crab cuni
      revalt wreu creur reucl syl eqeltrd ) ADEZAFGABHICHJKLKMCNOZBNPQZNBCARUDU
      EBNSUFNEBCATUEBNUAUBUC $.
      $( [9-May-1999] $)

    $( The imaginary part of a complex number is real. $)
    imclt $p |- ( A e. CC -> ( Im ` A ) e. RR ) $=
      ( vy vx cc wcel cim cfv cv ci cmul co caddc wceq cr wrex crab cuni
      imvalt wreu creui reucl syl eqeltrd ) ADEZAFGABHICHJKLKMBNOZCNPQZNBCARUDU
      ECNSUFNEBCATUECNUAUBUC $.
      $( [9-May-1999] $)

    $( Domain and codomain of the real part function.  (Contributed by Paul
       Chapman, 22-Oct-2007.) $)
    ref $p |- Re : CC --> RR $=
      ( vx vy vz vw cc cr cre wf wfn cv cfv wcel wral ffnfv ci cmul co caddc
      wceq wrex crab cuni reex rabex uniex df-re fnopab2 reclt rgen mpbir2an )
      EFGHGEIAJZGKFLZAEMAEFGNABEUKCJODJPQRQSDFTZCFUAZUBGUNUMCFUCUDUEABCDUFUGULA
      EUKUHUIUJ $.
      $( [22-Oct-2007] $) $( [22-Oct-2007] $)

    $( Domain and codomain of the imaginary part function. (Contributed by Paul
       Chapman, 22-Oct-2007.) $)
    imf $p |- Im : CC --> RR $=
      ( vx vy vw vz cc cr cim wf wfn cv cfv wcel wral ffnfv ci cmul co caddc
      wceq wrex crab cuni reex rabex uniex df-im fnopab2 imclt rgen mpbir2an )
      EFGHGEIAJZGKFLZAEMAEFGNABEUKCJODJPQRQSCFTZDFUAZUBGUNUMDFUCUDUEABCDUFUGULA
      EUKUHUIUJ $.
      $( [22-Oct-2007] $) $( [22-Oct-2007] $)

    $( Reconstruct a complex number from its real and imaginary parts. $)
    replimt $p |- ( A e. CC -> A = ( ( Re ` A ) + ( i x. ( Im ` A ) ) ) ) $=
      ( vx vy cc wcel cv cr wa ci cim cfv cmul co caddc wceq cre wex wrex eqid
      wb imclt wi opreq2 opreq2d eqeq2d rexbidv eqeq2 bibi12d imbi2d crab cuni
      wreu reuuni1 creui sylan2 imvalt eqeq1d adantl bitr4d ex vtoclga mpcom
      mpbiri df-rex sylib reclt opreq1 creur revalt jca an4 2exbii eeanv bitr
      sylibr pm3.26 ad2antll a1d ancld ancrd anim12d imp crut syl eqtr2t
      syl5bi imp32 pm3.26d opreq1d eqtrd 19.23advv mpd ) ADEZBFZGEZCFZGEZHZAWNI
      AJKZLMZNMZOZAAPKZIWPLMZNMZOZHZHZCQBQZAXCWTNMZOZWMWOXBHZBQZWQXFHZCQZHZXIWM
      XMXOWMXBBGRZXMWMXQWSWSOZWSSWSGEZWMXQXRTZAUAZWMAWNXDNMZOZBGRZWSWPOZTZUBWMX
      TUBCWSGWPWSOZYFXTWMYGYDXQYEXRYGYCXBBGYGYBXAAYGXDWTWNNWPWSILUCUDUEUFWPWSWS
      UGUHUIWQWMYFWQWMHYDYDCGUJUKZWPOZYEWQYDCGULYDYITWMYDCGUMBCAUNUOWMYEYITWQWM
      WSYHWPBCAUPUQURUSUTVAVBVCXBBGVDVEWMXFCGRZXOWMYJXCXCOZXCSXCGEZWMYJYKTZAVFZ
      WMYCCGRZXCWNOZTZUBWMYMUBBXCGWNXCOZYQYMWMYRYOYJYPYKYRYCXFCGYRYBXEAWNXCXDNV
