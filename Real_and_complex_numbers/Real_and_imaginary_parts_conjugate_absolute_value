    $( Generalized triangle inequality:  absolute value of a partial sum
       is less than or equal to sum of absolute values. $)
    ser1absdif $p |- ( ( A e. NN /\ B e. NN /\ A < B ) ->
       ( abs ` ( ( ( + seq1 F ) ` B ) - ( ( + seq1 F ) ` A ) ) ) <_
       ( ( ( + seq1 G ) ` B ) - ( ( + seq1 G ) ` A ) ) ) $=
