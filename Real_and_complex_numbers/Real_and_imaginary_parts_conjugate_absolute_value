  $( The imaginary part of a real number is 0. $)
  reim0 $p |- ( A e. RR -> ( Im ` A ) = 0 ) $=
    ( cr wcel cim cfv ci cmul co cc0 wceq cc recn imcl syl cre cmin caddc
    replim eqcomd wb recl axicn recnd mulcl sylancr subadd syl111anc mpbird
    resubcl mpdan eqeltrrd rimul syl11anc ) ABCZADEZBCZFUOGHZBCUOIJUNAKCZUPALZA
