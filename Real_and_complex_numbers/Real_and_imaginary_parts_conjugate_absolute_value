    VAUIUJUKUOULUM $.
    $( [18-Mar-2005] $)

  $( A number is real iff its imaginary part is 0. $)
  reim0b $p |- ( A e. CC -> ( A e. RR <-> ( Im ` A ) = 0 ) ) $=
    ( cc wcel cr cim cfv cc0 wceq reim0 wa cre ci cmul co caddc replim adantr
    opreq2 axicn mul01i syl6eq opreq2d recl recnd addid1 syl sylan9eqr eqtrd
    eqeltrd ex impbid2 ) ABCZADCZAEFZGHZAIULUOUMULUOJZAAKFZDUPAUQLUNMNZONZUQULA
