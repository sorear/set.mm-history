$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Real and imaginary parts; conjugate; absolute value
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c Re $. $( Real part of a complex number. $)
  $c Im $. $( Imaginary part of a complex number. $)
  $c * $. $( Complex conjugate function. $)
  $c abs $. $( Absolute value function. $)

  $( Extend class notation to include real part of a complex number. $)
  cre $a class Re $.

  $( Extend class notation to include imaginary part of a complex number. $)
  cim $a class Im $.

  $( Extend class notation to include complex conjugate function. $)
  ccj $a class * $.

  $( Extend class notation to include a function for the absolute value
     (modulus) of a complex number. $)
  cabs $a class abs $.

  ${
    $d x y z w $.
    $( Define a function whose value is the real part of a complex number.  See
       ~ reval for its value, ~ recli for its closure, and ~ replim for its use
       in decomposing a complex number. $)
    df-re $a |- Re = { <. x , y >. | ( x e. CC /\
                 y = U. { z e. RR | E. w e. RR x = ( z + ( _i x. w ) ) } ) } $.

    $( Define a function whose value is the imaginary part of a complex
       number.  See ~ imval for its value, ~ imcli for its closure, and
       ~ replim for its use in decomposing a complex number. $)
    df-im $a |- Im = { <. x , y >. | ( x e. CC /\
                 y = U. { w e. RR | E. z e. RR x = ( z + ( _i x. w ) ) } ) } $.

    $( Define the complex conjugate function.  See ~ cjcli for its closure and
       ~ cjval for its value. $)
    df-cj $a |- * = { <. x , y >. | ( x e. CC /\
                        y = ( ( Re ` x ) - ( _i x. ( Im ` x ) ) ) ) } $.

    $( Define the function for the absolute value (modulus) of a complex
       number.  See ~ abscli for its closure and ~ absval or ~ absval2i for its
       value. $)
    df-abs $a |- abs = { <. x , y >. | ( x e. CC /\ y = ( sqr `
                 ( x x. ( * ` x ) ) ) ) } $.
  $}

  ${
    $d x y z w A $.
    $( The value of the real part of a complex number. $)
    reval $p |- ( A e. CC ->
        ( Re ` A ) = U. { x e. RR | E. y e. RR A = ( x + ( _i x. y ) ) } ) $=
      ( vz vw cv ci cmul co caddc wceq cr wrex crab cuni cc cre eqeq1 rexbidv
      rabbidv unieqd df-re reex rabex uniex fvopab4 ) DECDFZAFGBFHIJIZKZBLMZALN
      ZOCUHKZBLMZALNZOPQUGCKZUKUNUOUJUMALUOUIULBLUGCUHRSTUADEABUBUNUMALUCUDUEUF
      $.
      $( [11-Mar-2008] $) $( [9-May-1999] $)

    $( The value of the imaginary part of a complex number. $)
    imval $p |- ( A e. CC ->
        ( Im ` A ) = U. { y e. RR | E. x e. RR A = ( x + ( _i x. y ) ) } ) $=
      ( vz vw cv ci cmul co caddc wceq cr wrex crab cuni cc cim eqeq1 rexbidv
      rabbidv unieqd df-im reex rabex uniex fvopab4 ) DECDFZAFGBFHIJIZKZALMZBLN
      ZOCUHKZALMZBLNZOPQUGCKZUKUNUOUJUMBLUOUIULALUGCUHRSTUADEABUBUNUMBLUCUDUEUF
      $.
      $( [11-Mar-2008] $) $( [9-May-1999] $)

    $( The real part of a complex number is real. $)
    recl $p |- ( A e. CC -> ( Re ` A ) e. RR ) $=
      ( vx vy cc wcel cre cfv cv ci cmul co caddc wceq cr wrex crab cuni reval
      wreu creur reucl syl eqeltrd ) ADEZAFGABHICHJKLKMCNOZBNPQZNBCARUDUEBNSUFN
      EBCATUEBNUAUBUC $.
      $( [9-May-1999] $)

    $( The imaginary part of a complex number is real. $)
    imcl $p |- ( A e. CC -> ( Im ` A ) e. RR ) $=
      ( vy vx cc wcel cim cfv cv ci cmul co caddc wceq cr wrex crab cuni imval
      wreu creui reucl syl eqeltrd ) ADEZAFGABHICHJKLKMBNOZCNPQZNBCARUDUECNSUFN
      EBCATUECNUAUBUC $.
      $( [9-May-1999] $)

    $( Domain and codomain of the real part function.  (Contributed by Paul
       Chapman, 22-Oct-2007.) $)
    ref $p |- Re : CC --> RR $=
      ( vx vy vz vw cc cr cre wf wfn cv cfv wcel wral ffnfv ci cmul co caddc
      wceq wrex crab cuni reex rabex uniex df-re fnopab2 recl rgen mpbir2an )
      EFGHGEIAJZGKFLZAEMAEFGNABEUKCJODJPQRQSDFTZCFUAZUBGUNUMCFUCUDUEABCDUFUGULA
      EUKUHUIUJ $.
      $( [22-Oct-2007] $)

    $( Domain and codomain of the imaginary part function.  (Contributed by
       Paul Chapman, 22-Oct-2007.) $)
    imf $p |- Im : CC --> RR $=
      ( vx vy vw vz cc cr cim wf wfn cv cfv wcel wral ffnfv ci cmul co caddc
      wceq wrex crab cuni reex rabex uniex df-im fnopab2 imcl rgen mpbir2an )
      EFGHGEIAJZGKFLZAEMAEFGNABEUKCJODJPQRQSCFTZDFUAZUBGUNUMDFUCUDUEABCDUFUGULA
      EUKUHUIUJ $.
      $( [22-Oct-2007] $)

    $( Reconstruct a complex number from its real and imaginary parts. $)
    replim $p |- ( A e. CC -> A = ( ( Re ` A ) + ( _i x. ( Im ` A ) ) ) ) $=
      ( vx vy cc wcel cv cr wa ci cim cfv cmul co caddc wceq cre wex wrex eqid
      wb imcl wi opreq2 opreq2d eqeq2d rexbidv eqeq2 bibi12d imbi2d crab cuni
      wreu creui reuuni1 sylan2 imval eqeq1d adantl bitr4d ex vtoclga mpcom
      mpbiri df-rex sylib recl opreq1 creur reval an4 2exbii eeanv bitri
      sylanbrc simprrl a1d ancld ancrd anim12d imp cru syl eqtr2 syl5bi impr
      simpld opreq1d eqtrd 19.23advv mpd ) ADEZBFZGEZCFZGEZHZAWLIAJKZLMZNMZOZAA
      PKZIWNLMZNMZOZHZHZCQBQZAXAWRNMZOZWKWMWTHZBQZWOXDHZCQZXGWKWTBGRZXKWKXNWQWQ
      OZWQSWQGEZWKXNXOTZAUAZWKAWLXBNMZOZBGRZWQWNOZTZUBWKXQUBCWQGWNWQOZYCXQWKYDY
      AXNYBXOYDXTWTBGYDXSWSAYDXBWRWLNWNWQILUCUDUEUFWNWQWQUGUHUIWOWKYCWOWKHYAYAC
      GUJUKZWNOZYBWKWOYACGULYAYFTBCAUMYACGUNUOWKYBYFTWOWKWQYEWNBCAUPUQURUSUTVAV
      BVCWTBGVDVEWKXDCGRZXMWKYGXAXAOZXASXAGEZWKYGYHTZAVFZWKXTCGRZXAWLOZTZUBWKYJ
      UBBXAGWLXAOZYNYJWKYOYLYGYMYHYOXTXDCGYOXSXCAWLXAXBNVGUEUFWLXAXAUGUHUIWMWKY
      NWMWKHYLYLBGUJUKZWLOZYMWKWMYLBGULYLYQTBCAVHYLBGUNUOWKYMYQTWMWKXAYPWLBCAVI
      UQURUSUTVAVBVCXDCGVDVEXGXJXLHZCQBQXKXMHXFYRBCWMWOWTXDVJVKXJXLBCVLVMVNWKXF
      XIBCWKXFXIWKXFHZAWSXHWKWPWTXDVOYSWLXAWRNYSYOYBWKWPXEYOYBHZWKWPHZWSXCOZYTX
      EUUAWMXPHZYIWOHZHZUUBYTTWKWPUUEWKWMUUCWOUUDWKWMXPWKXPWMXRVPVQWKWOYIWKYIWO
      YKVPVRVSVTWLWQXAWNWAWBAWSXCWCWDWEWFWGWHUTWIWJ $.
      $( [11-Mar-2008] $) $( [10-May-1999] $)

    $( The absolute value (modulus) of a complex number.  Proposition 10-3.7(a)
       of [Gleason] p. 133. $)
    absval $p |- ( A e. CC -> ( abs ` A ) = ( sqr ` ( A x. ( * ` A ) ) ) ) $=
      ( vx vy cv ccj cfv cmul co csqr cc cabs wceq id fveq2 opreq12d fveq2d
      df-abs fvex fvopab4 ) BCABDZTEFZGHZIFAAEFZGHZIFJKTALZUBUDIUETAUAUCGUEMTAE
      NOPBCQUDIRS $.
      $( [27-Jul-1999] $)
  $}

  ${
    $d x y A $.
    $( Value of the conjugate of a complex number.  The value is the real part
       minus ` _i ` times the imaginary part.  Definition 10-3.2 of [Gleason]
       p. 132. $)
    cjval $p |- ( A e. CC ->
                      ( * ` A ) = ( ( Re ` A ) - ( _i x. ( Im ` A ) ) ) ) $=
      ( vx vy cv cre cfv ci cim cmul co cmin cc ccj wceq fveq2 opreq2d
      opreq12d df-cj oprex fvopab4 ) BCABDZEFZGUAHFZIJZKJAEFZGAHFZIJZKJLMUAANZU
      BUEUDUGKUAAEOUHUCUFGIUAAHOPQBCRUEUGKST $.
      $( [11-Mar-2008] $) $( [10-May-1999] $)
  $}

  $( The conjugate of a complex number is a complex number (closure law). $)
  cjcl $p |- ( A e. CC -> ( * ` A ) e. CC ) $=
    ( cc wcel ccj cfv cre ci cim cmul co cmin cjval recl recnd axicn imcl
    mulcl sylancr subcl syl11anc eqeltrd ) ABCZADEAFEZGAHEZIJZKJZBALUBUCBCUEBCZ
    UFBCUBUCAMNUBGBCUDBCUGOUBUDAPNGUDQRUCUESTUA $.
    $( [10-May-1999] $)

  ${
    recl.1 $e |- A e. CC $.
    $( The real part of a complex number is real (closure law). $)
    recli $p |- ( Re ` A ) e. RR $=
      ( cc wcel cre cfv cr recl ax-mp ) ACDAEFGDBAHI $.
      $( [11-May-1999] $)

    $( The imaginary part of a complex number is real (closure law). $)
    imcli $p |- ( Im ` A ) e. RR $=
      ( cc wcel cim cfv cr imcl ax-mp ) ACDAEFGDBAHI $.
      $( [11-May-1999] $)

    $( Closure law for complex conjugate. $)
    cjcli $p |- ( * ` A ) e. CC $=
      ( cc wcel ccj cfv cjcl ax-mp ) ACDAEFCDBAGH $.
      $( [11-May-1999] $)

    $( Construct a complex number from its real and imaginary parts. $)
    replimi $p |- A = ( ( Re ` A ) + ( _i x. ( Im ` A ) ) ) $=
      ( cc wcel cre cfv ci cim cmul co caddc wceq replim ax-mp ) ACDAAEFGAHFIJK
      JLBAMN $.
      $( [11-Mar-2008] $) $( [1-Oct-1999] $)
  $}

  $( The real part of a complex number representation.  Definition 10-3.1 of
     [Gleason] p. 132. $)
  crre $p |- ( ( A e. RR /\ B e. RR ) -> ( Re ` ( A + ( _i x. B ) ) ) = A ) $=
    ( cr wcel wa ci cmul co caddc cre cfv wceq cim cc recn axicn mulcl sylancr
    addcl syl2an replim syl eqcomd wb recl imcl jca cru mpancom mpbid simpld )
    ACDZBCDZEZAFBGHZIHZJKZALZUPMKZBLZUNUQFUSGHIHZUPLZURUTEZUNUPVAUNUPNDZUPVALUL
    ANDUONDZVDUMAOUMFNDBNDVEPBOFBQRAUOSTZUPUAUBUCUQCDZUSCDZEUNVBVCUDUNVGVHUNVDV
    GVFUPUEUBUNVDVHVFUPUFUBUGUQUSABUHUIUJUK $.
    $( [11-Mar-2005] $) $( [12-May-2005] $)

  $( The real part of a complex number representation.  Definition 10-3.1 of
     [Gleason] p. 132. $)
  crim $p |- ( ( A e. RR /\ B e. RR ) -> ( Im ` ( A + ( _i x. B ) ) ) = B ) $=
    ( cr wcel wa ci cmul co caddc cre cfv wceq cim cc recn axicn mulcl sylancr
    addcl syl2an replim syl eqcomd wb recl imcl jca cru mpancom mpbid simprd )
    ACDZBCDZEZAFBGHZIHZJKZALZUPMKZBLZUNUQFUSGHIHZUPLZURUTEZUNUPVAUNUPNDZUPVALUL
    ANDUONDZVDUMAOUMFNDBNDVEPBOFBQRAUOSTZUPUAUBUCUQCDZUSCDZEUNVBVCUDUNVGVHUNVDV
    GVFUPUEUBUNVDVHVFUPUFUBUGUQUSABUHUIUJUK $.
    $( [11-Mar-2005] $) $( [12-May-2005] $)

  ${
    crre.1 $e |- A e. RR $.
    crre.2 $e |- B e. RR $.
    $( The real part of a complex number representation.  Definition 10-3.1 of
       [Gleason] p. 132. $)
    crrei $p |- ( Re ` ( A + ( _i x. B ) ) ) = A $=
      ( cr wcel ci cmul co caddc cre cfv wceq crre mp2an ) AEFBEFAGBHIJIKLAMCDA
      BNO $.
      $( [11-Mar-2005] $) $( [10-May-1999] $)

    $( The imaginary part of a complex number representation.  Definition
       10-3.1 of [Gleason] p. 132. $)
    crimi $p |- ( Im ` ( A + ( _i x. B ) ) ) = B $=
      ( cr wcel ci cmul co caddc cim cfv wceq crim mp2an ) AEFBEFAGBHIJIKLBMCDA
      BNO $.
      $( [11-Mar-2005] $) $( [10-May-1999] $)
  $}

  $( The imaginary part of a complex number in terms of the real part
