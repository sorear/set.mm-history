    UEUFUGUHULUQDCUOVAQUIUJUK $.
    $( [24-Jul-2008] $) $( [26-Sep-2005] $)

  $( A number is real iff it equals its real part.  Proposition 10-3.4(f) of
     [Gleason] p. 133. $)
  rereb $p |- ( A e. CC -> ( A e. RR <-> ( Re ` A ) = A ) ) $=
    ( cc wcel cr cre cfv wceq wa ci cim cmul co caddc cc0 replim adantr reim0
    opreq2d axicn mul01i syl6eq adantl recl recnd addid1 syl 3eqtrrd simpr
