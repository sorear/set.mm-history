      MVAUHUIUJ $.
      $( [2-Oct-1999] $)

    ${
      readdi.2 $e |- B e. CC $.
      $( Real part distributes over addition. $)
      readdi $p |- ( Re ` ( A + B ) ) = ( ( Re ` A ) + ( Re ` B ) ) $=
        ( caddc co cre cfv ci cim cmul recli recni axicn imcli mulcli add4i
        replimi opreq12i adddii opreq2i 3eqtr4i fveq2i readdcli crrei eqtri )
        ABEFZGHAGHZBGHZEFZIAJHZBJHZEFZKFZEFZGHUJUGUOGUHIUKKFZEFZUIIULKFZEFZEFUJ
        UPUREFZEFUGUOUHUPUIURUHACLZMIUKNUKACOZMZPUIBDLZMIULNULBDOZMZPQAUQBUSEAC
        RBDRSUNUTUJEIUKULNVCVFTUAUBUCUJUMUHUIVAVDUDUKULVBVEUDUEUF $.
        $( [28-Jul-1999] $)

      $( Imaginary part distributes over addition. $)
      imaddi $p |- ( Im ` ( A + B ) ) = ( ( Im ` A ) + ( Im ` B ) ) $=
        ( caddc co cim cfv cre ci cmul recli recni axicn imcli mulcli add4i
        replimi opreq12i adddii opreq2i 3eqtr4i fveq2i readdcli crimi eqtri )
