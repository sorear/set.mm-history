$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Nonnegative integers (as a subset of complex numbers)
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Define the set of nonnegative integers. $)
  df-n0 $a |- NN0 = ( NN u. { 0 } ) $.

  $( Nonnegative integers expressed in terms of naturals and zero.
     (Contributed by Raph Levien, 10-Dec-2002.) $)
  elnn0 $p |- ( A e. NN0 <-> ( A e. NN \/ A = 0 ) ) $=
    ( cn0 wcel cn cc0 csn cun wo wceq df-n0 eleq2i elun cc 0cn elisseti elsnc2
    orbi2i 3bitri ) ABCADEFZGZCADCZASCZHUAAEIZHBTAJKADSLUBUCUAAEEMNOPQR $.
    $( [14-May-2003] $) $( [10-Dec-2002] $)

  $( Positive naturals are a subset of nonnegative integers.  (Contributed
     by Raph Levien, 10-Dec-2002.) $)
  nnssnn0 $p |- NN (_ NN0 $=
    ( cn cc0 csn cun cn0 ssun1 df-n0 sseqtr4i ) AABCZDEAIFGH $.
    $( [14-May-2003] $) $( [10-Dec-2002] $)

  $( Nonnegative integers are a subset of the reals.  (Contributed by Raph
     Levien, 10-Dec-2002.) $)
  nn0ssre $p |- NN0 (_ RR $=
    ( cn0 cn cc0 csn cun cr df-n0 nnssre wcel wss 0re snssi ax-mp unssi
    eqsstri ) ABCDZEFGBPFHCFIPFJKCFLMNO $.
    $( [14-May-2003] $) $( [10-Dec-2002] $)

  $( Nonnegative integers are a subset of the complex numbers.) $)
  nn0sscn $p |- NN0 (_ CC $=
    ( cn0 cr cc nn0ssre axresscn sstri ) ABCDEF $.
    $( [25-Jun-2004] $) $( [9-May-2004] $)

  $( The set of nonnegative integers exists. $)
  nn0ex $p |- NN0 e. V $=
    ( cn0 cc axcnex nn0sscn ssexi ) ABCDE $.
    $( [24-Jul-2004] $) $( [18-Jul-2004] $)

  $( A natural number is a nonnegative integer. $)
  nnnn0 $p |- ( A e. NN -> A e. NN0 ) $=
    ( cn cn0 nnssnn0 sseli ) BCADE $.
    $( [10-May-2004] $) $( [9-May-2004] $)

  ${
    nnnn0.1 $e |- N e. NN $.
    $( A natural number is a nonnegative integer. $)
    nnnn0i $p |- N e. NN0 $=
      ( cn wcel cn0 nnnn0 ax-mp ) ACDAEDBAFG $.
      $( [21-Jun-2005] $) $( [20-Jun-2005] $)
  $}

  $( A nonnegative integer is a real number. $)
  nn0re $p |- ( A e. NN0 -> A e. RR ) $=
    ( cn0 cr nn0ssre sseli ) BCADE $.
    $( [25-May-2004] $) $( [9-May-2004] $)

  $( A nonnegative integer is a complex number. $)
  nn0cn $p |- ( A e. NN0 -> A e. CC ) $=
    ( cn0 cc nn0sscn sseli ) BCADE $.
    $( [3-Jul-2004] $) $( [9-May-2004] $)

  ${
    nn0re.1 $e |- A e. NN0 $.
    $( A nonnegative integer is a real number. $)
    nn0rei $p |- A e. RR $=
      ( cn0 cr nn0ssre sselii ) CDAEBF $.
      $( [14-May-2003] $)

    $( A nonnegative integer is a complex number. $)
    nn0cni $p |- A e. CC $=
      ( nn0rei recni ) AABCD $.
      $( [14-May-2003] $)
  $}

  $( The set of natural numbers (positive integers) defined in terms of
     nonnegative integers. $)
  dfn2 $p |- NN = ( NN0 \ { 0 } ) $=
    ( vx cn cn0 cc0 csn cdif cv wcel wne wa nnnn0 nnne0 jca wceq wo elnn0
    biimpi ord necon1ad imp impbii eldifsn bitr4i eqriv ) ABCDEFZAGZBHZUFCHZUFD
    IZJZUFUEHUGUJUGUHUIUFKUFLMUHUIUGUHUGUFDUHUGUFDNZUHUGUKOUFPQRSTUAUFCDUBUCUD
    $.
    $( [23-Sep-2007] $) $( [23-Sep-2007] $)

  $( 0 is a nonnegative integer.  (Contributed by Raph Levien, 10-Dec-2002.) $)
  0nn0 $p |- 0 e. NN0 $=
    ( cc0 wceq cn0 wcel eqid cn wo elnn0 biimpri olcs ax-mp ) AABZACDZAEAFDZLMM
    NLGAHIJK $.
    $( [21-Mar-2007] $) $( [10-Dec-2002] $)

  $( 1 is a nonnegative integer.  (Contributed by Raph Levien, 10-Dec-2002.) $)
  1nn0 $p |- 1 e. NN0 $=
    ( c1 1nn nnnn0i ) ABC $.
    $( [10-Dec-2002] $)

  $( 2 is a nonnegative integer.  (Contributed by Raph Levien, 10-Dec-2002.) $)
  2nn0 $p |- 2 e. NN0 $=
    ( c2 2nn nnnn0i ) ABC $.
    $( [10-Dec-2002] $)

  $( No number less than zero is a nonnegative integer. $)
  lt0nnn0 $p |- ( ( A e. RR /\ A < 0 ) -> -. A e. NN0 ) $=
    ( cr wcel cc0 clt wbr cn0 wn cn wceq wo c1 lt01 wa wi 0re 1re axlttrn
    mp3an23 mpan2i lt1nnn ex syld con2d breq2 notbid ltnr syl5cbi jaod elnn0
    syl5ib imp ) ABCZADEFZAGCZHUMUOUNUMAICZADJZKUNHZUOUMUPURUQUMUNUPUMUNALEFZUP
    HZUMUNDLEFZUSMUMDBCLBCUNVANUSOPQADLRSTUMUSUTAUAUBUCUDUQAAEFZHURUMUQVBUNADAE
    UEUFAUGUHUIAUJUKUDUL $.
    $( [27-Jun-2004] $) $( [9-May-2004] $)

  $( A nonnegative integer is greater than or equal to zero. $)
  nn0ge0 $p |- ( N e. NN0 -> 0 <_ N ) $=
    ( cn0 wcel cc0 cle wbr clt wn cr nn0re lt0nnn0 ex con2d mpcom wb 0re lenlt
    sylan2 mpan mpbird ) ABCZDAEFZADGFZHZAICZUAUDAJZUEUCUAUEUCUAHAKLMNDICZUAUBU
    DOZPUGUEUHUADAQUFRST $.
    $( [3-Jul-2004] $) $( [9-May-2004] $)

  ${
    nn0ge0.1 $e |- N e. NN0 $.
    $( Nonnegative integers are nonnegative.  (Contributed by Raph Levien,
       10-Dec-2002.) $)
    nn0ge0i $p |- 0 <_ N $=
      ( cn0 wcel cc0 cle wbr nn0ge0 ax-mp ) ACDEAFGBAHI $.
      $( [14-May-2003] $) $( [10-Dec-2002] $)
  $}

  $( A nonnegative integer is less than or equal to zero iff it is equal to
     zero. $)
  nn0le0eq0 $p |- ( N e. NN0 -> ( N <_ 0 <-> N = 0 ) ) $=
    ( cn0 wcel cc0 cle wbr wa wceq nn0ge0 biantrud cr wb nn0re 0re letri3
    mpan2 syl bitr4d ) ABCZADEFZTDAEFZGZADHZSUATAIJSAKCZUCUBLZAMUDDKCUENADOPQR
    $.
    $( [30-Dec-2005] $) $( [9-Dec-2005] $)

  $( Closure of addition of nonnegative integers.  (Contributed by Raph Levien,
     10-Dec-2002.) $)
  nn0addcl $p |- ( ( M e. NN0 /\ N e. NN0 ) -> ( M + N ) e. NN0 ) $=
    ( cn wcel cc0 wceq wo caddc co cn0 wa nnaddcl nnnn0 syl cc opreq1 addid2
    sylan9eq nncn sylan2 adantl eqeltrd opreq2 addid1 sylan9eqr sylan adantr
    opreq12 0cn addid1i syl6eq 0nn0 syl6eqel ccase elnn0 syl2anb ) ACDZAEFZGBCD
    ZBEFZGABHIZJDZAJDZBJDZUQUSURUTVBUQUSKVACDVBABLVAMNURUSKVABJURBODZVABFUSURVE
    VAEBHIBAEBHPBQRBSTUSVDURBMUAUBUQUTKVAAJAODZUTVAAFUQUTVFVAAEHIABEAHUCAUDUEAS
    UFUQVCUTAMUGUBURUTKZVAEJVGVAEEHIEAEBEHUHEUIUJUKULUMUNAUOBUOUP $.
    $( [14-May-2003] $) $( [10-Dec-2002] $)

  ${
    nn0addcl.1 $e |- M e. NN0 $.
    nn0addcl.2 $e |- N e. NN0 $.
    $( Closure of addition of nonnegative integers, inference form.
      (Contributed  by Raph Levien, 10-Dec-2002.) $)
    nn0addcli $p |- ( M + N ) e. NN0 $=
      ( cn0 wcel caddc co nn0addcl mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [14-May-2003] $) $( [10-Dec-2002] $)

    $( Closure of multiplication of nonnegative integers, inference form.
       (Contributed by Raph Levien, 10-Dec-2002.) $)
    nn0mulcli $p |- ( M x. N ) e. NN0 $=
      ( cn wcel cc0 wceq wo cmul co cn0 elnn0 mpbi wa nnmulcl nnnn0 syl opreq1
      nn0cni mul02i syl6eq 0nn0 syl6eqel opreq2 mul01i ccase2 mp2an ) AEFZAGHZI
      ZBEFZBGHZIZABJKZLFZALFUKCAMNBLFUNDBMNUIULUJUMUPUIULOUOEFUPABPUOQRUJUOGLUJ
      UOGBJKGAGBJSBBDTUAUBUCUDUMUOGLUMUOAGJKGBGAJUEAACTUFUBUCUDUGUH $.
      $( [14-May-2003] $) $( [10-Dec-2002] $)
  $}

  $( Closure of multiplication of nonnegative integers. $)
  nn0mulcl $p |- ( ( M e. NN0 /\ N e. NN0 ) -> ( M x. N ) e. NN0 ) $=
    ( cn0 wcel cmul co cc0 cif wceq opreq1 eleq1d opreq2 0nn0 elimel nn0mulcli
    dedth2h ) ACDZBCDZABEFZCDQAGHZBEFZCDTRBGHZEFZCDABGGATISUACATBEJKBUBIUAUCCBU
    BTELKTUBAGCMNBGCMNOP $.
    $( [30-Aug-2004] $) $( [22-Jul-2004] $)

  $( Second Peano postulate for nonnegative integers. $)
  peano2nn0 $p |- ( N e. NN0 -> ( N + 1 ) e. NN0 ) $=
    ( cn0 wcel c1 caddc co 1nn0 nn0addcl mpan2 ) ABCDBCADEFBCGADHI $.
    $( [24-May-2004] $) $( [9-May-2004] $)

  $( A natural number plus a nonnegative integer is a natural number. $)
  nnnn0addcl $p |- ( ( M e. NN /\ N e. NN0 ) -> ( M + N ) e. NN ) $=
    ( cn wcel cn0 caddc co cc0 wceq wo nnaddcl ex opreq2 eleq1d cc nncn addid1
    syl ibir syl5cbir jaod elnn0 syl5ib imp ) ACDZBEDZABFGZCDZUEBCDZBHIZJUHUFUE
    UIUHUJUEUIUHABKLUJUHAHFGZCDZUEUJUGUKCBHAFMNUEULUEUKACUEAODUKAIAPAQRNSTUABUB
    UCUD $.
    $( [23-Apr-2005] $) $( [20-Apr-2005] $)

  $( A nonnegative integer plus a natural number is a natural number. $)
  nn0nnaddcl $p |- ( ( M e. NN0 /\ N e. NN ) -> ( M + N ) e. NN ) $=
    ( cn wcel cn0 caddc co wa cc wceq addcom nncn nn0cn syl2an nnnn0addcl
    eqeltrrd ancoms ) BCDZAEDZABFGZCDRSHBAFGZTCBIDAIDUATJRSBAKBLAMNBAOPQ $.
    $( [24-Jan-2006] $) $( [22-Dec-2005] $)

  $( Nonnegative integer ordering relation.  (Contributed by Raph Levien,
     10-Dec-2002.) $)
  nn0ltp1le $p |- ( ( M e. NN0 /\ N e. NN0 ) ->
