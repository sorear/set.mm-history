    $( Lemma for ~ nn0opth .  (Contributed by Raph Levien, 10-Dec-02.) $)
    nn0opthlem2 $p |- ( ( B <_ A /\ D <_ C ) ->
              ( A < C -> -. ( ( A x. A ) + B ) = ( ( C x. C ) + D ) ) ) $=
