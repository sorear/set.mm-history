  $( Second Peano postulate for a set of upper integers. $)
  peano2uz $p |- ( N e. ( ZZ>= ` M ) ->
                 ( N + 1 ) e. ( ZZ>= ` M ) ) $=
    ( cz wcel cle wbr w3a c1 caddc co cuz cfv 3simp1 peano2z 3ad2ant2 cr
    letrp1 zre syl3an2 syl3an1 3jca eluz2 3imtr4i ) ACDZBCDZABEFZGZUDBHIJZCDZAU
    HEFZGBAKLZDUHUKDUGUDUIUJUDUEUFMUEUDUIUFBNOAPDZUEUFUJUDULBPDUFUJUEABQBRSARTU
    AABUBAUHUBUC $.
    $( [12-Sep-2005] $) $( [7-Sep-2005] $)

  $( Reversed second Peano axiom for upper integers. $)
  peano2uzr $p |- ( ( M e. ZZ /\
             N e. ( ZZ>= ` ( M + 1 ) ) ) -> N e. ( ZZ>= ` M ) ) $=
    ( cz wcel c1 caddc co cuz cfv wa cmin wceq cc eluzelz zcn ax1cn npcan
    mpan2 3syl adantl eluzp1m1 peano2uz syl eqeltrrd ) ACDZBAEFGZHIDZJZBEKGZEFG
    ZBAHIZUGUJBLZUEUGBCDBMDZULUFBNBOUMEMDULPBEQRSTUHUIUKDUJUKDABUAAUIUBUCUD $.
