$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Upper partititions of integers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ZZ>= $. $( ` ZZ ` with greater-than-or-equal-to subscript. $)

  $( Extend class notation with the upper integer function.  Read
     " ` ZZ>= `` M ` " as "the set of integers greater than or equal to
     ` M ` ." $)
  cuz $a class ZZ>= $.

  ${
    $d j k y N $.  $d k M $.
    $( Define a function whose value at ` j ` is the semi-infinite set of
       contiguous integers starting at ` j ` , which we will also call the
       upper integers starting at ` j ` .  Read " ` ZZ>= `` M ` " as "the set
       of integers greater than or equal to ` M ` ." See ~ uzval for its
       value, ~ uzssz for its relationship to ` ZZ ` , ~ nnuz and ~ nn0uz for
       its relationships to ` NN ` and ` NN0 ` , and ~ eluz1 and ~ eluz2 for
       its membership relations. $)
    df-uz $a |- ZZ>= =
                  { <. j , y >. | ( j e. ZZ /\ y = { k e. ZZ | j <_ k } ) } $.

    $( The value of the upper integers function. $)
    uzval $p |- ( N e. ZZ -> ( ZZ>= ` N ) = { k e. ZZ | N <_ k } ) $=
      ( vj vy cv cle wbr cz crab cuz wceq breq1 rabbisdv df-uz zex rabex
      fvopab4 ) CDBCEZAEZFGZAHIBSFGZAHIHJRBKTUAAHRBSFLMDCANUAAHOPQ $.
      $( [11-Sep-2005] $) $( [5-Sep-2005] $)

    $( Membership in the set of upper integers starting at ` M ` . $)
    eluz1 $p |- ( M e. ZZ ->
           ( N e. ( ZZ>= ` M ) <-> ( N e. ZZ /\ M <_ N ) ) ) $=
      ( vk cz wcel cuz cfv cv cle wbr crab wa uzval eleq2d breq2 elrab syl6bb
      ) ADEZBAFGZEBACHZIJZCDKZEBDEABIJZLRSUBBCAMNUAUCCBDTBAIOPQ $.
      $( [11-Sep-2005] $) $( [5-Sep-2005] $)

    $( Membership in a set of upper integers.  We use the fact that a
       function's value (under our function value definition) is empty outside
       of its domain to show ` M e. ZZ ` . $)
    eluz2 $p |- ( N e. ( ZZ>= ` M ) <-> ( M e. ZZ /\ N e. ZZ /\ M <_ N ) ) $=
      ( vj vy vk cuz cfv wcel cz cle wbr w3a cdm elfvdm cv crab zex rabex
      df-uz dmopab2 syl6eleq 3simp1 wa eluz1 ibar bitrd 3anass syl6bbr
      pm5.21nii ) BAFGHZAIHZUKBIHZABJKZLZUJAFMIBAFNCDICOEOJKZEIPFUOEIQRDCESTUAU
      KULUMUBUKUJUKULUMUCZUCZUNUKUJUPUQABUDUKUPUEUFUKULUMUGUHUI $.
      $( [12-Sep-2005] $) $( [5-Sep-2005] $)
  $}

  ${
    eluz.1 $e |- M e. ZZ $.
    $( Membership in a set of upper integers. $)
    eluz1i $p |- ( N e. ( ZZ>= ` M ) <-> ( N e. ZZ /\ M <_ N ) ) $=
      ( cz wcel cuz cfv cle wbr wa wb eluz1 ax-mp ) ADEBAFGEBDEABHIJKCABLM $.
      $( [12-Sep-2005] $) $( [5-Sep-2005] $)
  $}

  $( Implication of membership in a set of upper integers. $)
  eluzelz $p |- ( N e. ( ZZ>= ` M ) -> N e. ZZ ) $=
    ( cuz cfv wcel cz cle wbr w3a eluz2 3simp2 sylbi ) BACDEAFEZBFEZABGHZINABJM
    NOKL $.
    $( [14-Sep-2005] $) $( [6-Sep-2005] $)

  $( Implication of membership in a set of upper integers. $)
  eluzel2 $p |- ( N e. ( ZZ>= ` M ) -> M e. ZZ ) $=
    ( cuz cfv wcel cz cle wbr w3a eluz2 3simp1 sylbi ) BACDEAFEZBFEZABGHZIMABJM
    NOKL $.
    $( [14-Sep-2005] $) $( [6-Sep-2005] $)

  $( Implication of membership in a set of upper integers. $)
  eluzle $p |- ( N e. ( ZZ>= ` M ) -> M <_ N ) $=
    ( cuz cfv wcel cz cle wbr w3a eluz2 3simp3 sylbi ) BACDEAFEZBFEZABGHZIOABJM
    NOKL $.
    $( [14-Sep-2005] $) $( [6-Sep-2005] $)

  $( Membership in a set of upper integers. $)
  eluz $p |- ( ( M e. ZZ /\ N e. ZZ ) ->
         ( N e. ( ZZ>= ` M ) <-> M <_ N ) ) $=
    ( cz wcel cuz cfv cle wbr wb wa wi eluz1 eluzelz pm4.71ri syl5bbr pm5.32
    sylibr imp ) ACDZBCDZBAEFDZABGHZIZSTUAJZTUBJZITUCKSUAUEUDABLUATABMNOTUAUBPQ
    R $.
    $( [4-Oct-2005] $) $( [2-Oct-2005] $)

  $( Membership of the least member in a set of upper integers. $)
  uzid $p |- ( M e. ZZ -> M e. ( ZZ>= ` M ) ) $=
    ( cz wcel cuz cfv cle wbr wa cr zre leid syl ancli eluz1 mpbird ) ABCZAADEC
    PAAFGZHPQPAICQAJAKLMAANO $.
    $( [12-Sep-2005] $) $( [2-Sep-2005] $)

  $( Transitive law for sets of upper integers. $)
  uztrn $p |- ( ( M e. ( ZZ>= ` K ) /\ K e. ( ZZ>= ` N ) ) ->
              M e. ( ZZ>= ` N ) ) $=
    ( cuz cfv wcel wa cz cle wbr w3a eluzel2 adantl eluzelz adantr eluzle wi
    cr letr zre syl3an syl3anc mp2and 3jca eluz2 sylibr ) BADEFZACDEZFZGZCHFZBH
    FZCBIJZKBUHFUJUKULUMUIUKUGCALMZUGULUIABNOZUJCAIJZABIJZUMUIUPUGCAPMUGUQUIABP
    OUKAHFZULUPUQGUMQZUJCRFARFBRFUSUKURULCABSCTATBTUAUNUIURUGCANMUOUBUCUDCBUEUF
    $.
    $( [23-Sep-2005] $) $( [20-Sep-2005] $)

  $( Contraposition law for upper integers. $)
  uzneg $p |- ( N e. ( ZZ>= ` M ) -> -u M e. ( ZZ>= ` -u N ) ) $=
    ( cuz cfv wcel cneg cle wbr eluzle cz wb cr leneg zre syl2an eluzel2
    eluzelz sylanc mpbid eluz znegcl mpbird ) BACDEZAFZBFZCDEZUEUDGHZUCABGHZUGA
    BIAJEZBJEZUHUGKZUCALEBLEUKUIUJABMANBNOABPZABQZRSUJUIUFUGKZUCUEJEUDJEUNUJUIU
    EUDTBUAAUAOUMULRUB $.
    $( [6-Jan-2006] $) $( [28-Nov-2005] $)

  ${
    $d j k y M $.  $d k N $.
    $( A set of upper integers is a subset of all integers. $)
    uzssz $p |- ( ZZ>= ` M ) (_ ZZ $=
      ( vk vj vy cz wcel cuz cfv wss cv cle wbr crab uzval ssrab2 a1i eqsstrd
      wn c0 cdm wceq wa copab df-uz dmeqi dmopabss eqsstri sseli con3i ndmfv
      syl 0ss pm2.61i ) AEFZAGHZEIUNUOABJZKLZBEMZEBANUREIUNUQBEOPQUNRZUOSEUSAGT
      ZFZRUOSUAVAUNUTEAUTCJZEFDJVBUPKLBEMUAZUBCDUCZTEGVDDCBUDUEVCCDEUFUGUHUIAGU
      JUKSEIUSEULPQUM $.
      $( [13-Sep-2005] $) $( [2-Sep-2005] $)

    $( Subset relationship for two sets of upper integers. $)
    uzss $p |- ( N e. ( ZZ>= ` M ) ->
               ( ZZ>= ` N ) (_ ( ZZ>= ` M ) ) $=
      ( vk cuz cfv wcel cv cz cle wbr wa wi eluzle adantr cr letr zre syl3an
      3expa eluzel2 eluzelz jca sylan mpand ex imdistand wb eluz1 syl 3imtr4d
      ssrdv ) BADEZFZCBDEZULUMCGZHFZBUOIJZKZUPAUOIJZKZUOUNFZUOULFZUMUPUQUSUMUPU
      QUSLUMUPKABIJZUQUSUMVCUPABMNAHFZBHFZKUPVCUQKUSLZUMVDVEUPVFAOFBOFUOOFVFVDV
      EUPABUOPAQBQUOQRSUMVDVEABTZABUAZUBUCUDUEUFUMVEVAURUGVHBUOUHUIUMVDVBUTUGVG
      AUOUHUIUJUK $.
      $( [11-Sep-2005] $) $( [5-Sep-2005] $)
  $}

  $( The upper integers function is one-to-one. $)
  uz11 $p |- ( M e. ZZ ->
                ( ( ZZ>= ` M ) = ( ZZ>= ` N ) <-> M = N ) ) $=
    ( cz wcel cuz cfv wceq wa eleq2 eluzel2 syl6bi uzid syl5 impcom cle wbr
    syl5bir eluzle syl6 syl5bi anim12d imp anassrs ancoms wb cr letri3 zre
    syl2an adantlr mpbird mpdan ex fveq2 impbid1 ) ACDZAEFZBEFZGZABGZUPUSUTUPUS
    HZBCDZUTUSUPVBUSAUQDZVBUPUSVCAURDZVBUQURAIZBAJKALZMNVAVBHUTABOPZBAOPZHZUPUS
    VBVIUSVBHUPVIUSVBUPVIUSVBUPHVIUSVBVGUPVHUSVBBUQDZVGUSVJBURDVBUQURBIBLQABRSU
    SUPVDVHUSVCVDUPVEVFTBARSUAUBUCUDUCUPVBUTVIUEZUSAUFDBUFDVKUPVBABUGAUHBUHUIUJ
    UKULUMABEUNUO $.
    $( [25-Dec-2005] $) $( [12-Dec-2005] $)

  $( Membership in the next set of upper integers. $)
  eluzp1m1 $p |- ( ( M e. ZZ /\
             N e. ( ZZ>= ` ( M + 1 ) ) ) -> ( N - 1 ) e. ( ZZ>= ` M ) ) $=
    ( cz wcel c1 caddc co cuz cfv cmin cle wbr wa peano2zm ad2antrl cr wb 1re
    leaddsub mp3an2 zre syl2an biimpa anasss jca ex peano2z eluz1 syl 3imtr4d
    imp ) ACDZBAEFGZHIDZBEJGZAHIDZULBCDZUMBKLZMZUOCDZAUOKLZMZUNUPULUSVBULUSMUTV
    AUQUTULURBNOULUQURVAULUQMURVAAPDZBPDZURVAQZULUQVCEPDVDVERAEBSTAUABUAUBUCUDU
    EUFULUMCDUNUSQAUGUMBUHUIAUOUHUJUK $.
    $( [28-Sep-2005] $) $( [12-Sep-2005] $)

  $( Strict ordering implied by membership in the next set of upper
     integers. $)
  eluzp1l $p |- ( ( M e. ZZ /\
             N e. ( ZZ>= ` ( M + 1 ) ) ) -> M < N ) $=
    ( cz wcel c1 caddc co cuz cfv wa clt wbr cle eluzle adantl wb zltp1le
    eluzelz sylan2 mpbird ) ACDZBAEFGZHIDZJABKLZUBBMLZUCUEUAUBBNOUABCDUDUEPUCAB
    QUBBRST $.
    $( [1-Oct-2005] $) $( [12-Sep-2005] $)

  $( Membership in the next set of upper integers. $)
  eluzp1p1 $p |- ( N e. ( ZZ>= ` M ) ->
                       ( N + 1 ) e. ( ZZ>= ` ( M + 1 ) ) ) $=
    ( cz wcel cle wbr w3a c1 caddc co cuz cfv peano2z 3ad2ant1 3ad2ant2 cr wb
    1re leadd1 mp3an3 zre syl2an biimp3a 3jca eluz2 3imtr4i ) ACDZBCDZABEFZGZAH
    IJZCDZBHIJZCDZUKUMEFZGBAKLDUMUKKLDUJULUNUOUGUHULUIAMNUHUGUNUIBMOUGUHUIUOAPD
    ZBPDZUIUOQZUGUHUPUQHPDURRABHSTAUABUAUBUCUDABUEUKUMUEUF $.
    $( [5-Oct-2005] $) $( [5-Oct-2005] $)

 ${
    eluzaddi.1 $e |- M e. ZZ $.
    eluzaddi.2 $e |- K e. ZZ $.
    $( Membership in a later set of upper integers.  (Contributed by Paul
