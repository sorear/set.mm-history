$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Upper partititions of integers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ZZ>= $. $( ` ZZ ` with greater-than-or-equal-to subscript. $)

  $( Extend class notation with the upper integer function.
     Read " ` ZZ>= `` M ` " as "the set of integers greater than or equal to
     ` M ` ." $)
  cuz $a class ZZ>= $.

  ${
    $d j k y N $.  $d k M $.
    $( Define a function whose value at ` j ` is the semi-infinite set of
       contiguous integers starting at ` j ` , which we will also call the
       upper integers starting at ` j ` .  Read " ` ZZ>= `` M ` " as "the set
       of integers greater than or equal to ` M ` ."  See ~ uzval for its
       value, ~ uzssz for its relationship to ` ZZ ` , ~ nnuz and ~ nn0uz for
       its relationships to ` NN ` and ` NN0 ` , and ~ eluz1 and ~ eluz2 for
       its membership relations. $)
    df-uz $a |- ZZ>= = ( j e. ZZ |-> { k e. ZZ | j <_ k } ) $.

    $( The value of the upper integers function.  (Revised by Mario Carneiro,
       3-Nov-2013.) $)
    uzval $p |- ( N e. ZZ -> ( ZZ>= ` N ) = { k e. ZZ | N <_ k } ) $=
      ( vj cv cle wbr cz crab cuz wceq breq1 rabbidv df-uz zex rabex fvmpt ) CB
      CDZADZEFZAGHBREFZAGHGIQBJSTAGQBREKLCAMTAGNOP $.
      $( [3-Nov-2013] $) $( [5-Sep-2005] $)

    $( The domain and range of the upper integers function.  (Contributed by
       Scott Fenton, 8-Aug-2013.)  (Revised by Mario Carneiro, 3-Nov-2013.) $)
    uzf $p |- ZZ>= : ZZ --> ~P ZZ $=
      ( vj vk cv cle wbr cz crab cpw wcel wral cuz wf wss ssrab2 zex rabex elpw
      mpbir a1i rgen df-uz fmpt mpbi ) ACZBCDEZBFGZFHZIZAFJFUGKLUHAFUHUDFIUHUFF
      MUEBFNUFFUEBFOPQRSTAFUGUFKABUAUBUC $.
      $( [3-Nov-2013] $)

    $( Membership in the set of upper integers starting at ` M ` . $)
    eluz1 $p |- ( M e. ZZ ->
           ( N e. ( ZZ>= ` M ) <-> ( N e. ZZ /\ M <_ N ) ) ) $=
      ( vk cz wcel cuz cfv cv cle wbr crab wa uzval eleq2d breq2 elrab syl6bb )
      ADEZBAFGZEBACHZIJZCDKZEBDEABIJZLRSUBBCAMNUAUCCBDTBAIOPQ $.
      $( [5-Sep-2005] $)

    $( Implication of membership in a set of upper integers.  (Revised by Mario
       Carneiro, 3-Nov-2013.) $)
    eluzel2 $p |- ( N e. ( ZZ>= ` M ) -> M e. ZZ ) $=
      ( cuz cfv wcel cdm cz elfvdm cpw uzf fdmi syl6eleq ) BACDEACFGBACHGGICJKL
      $.
      $( [3-Nov-2013] $) $( [6-Sep-2005] $)

    $( Membership in a set of upper integers.  We use the fact that a
       function's value (under our function value definition) is empty outside
       of its domain to show ` M e. ZZ ` .  (Revised by Mario Carneiro,
       3-Nov-2013.) $)
    eluz2 $p |- ( N e. ( ZZ>= ` M ) <-> ( M e. ZZ /\ N e. ZZ /\ M <_ N ) ) $=
      ( cuz cfv wcel cz cle wbr w3a eluzel2 simp1 wa eluz1 bitrd 3anass syl6bbr
      ibar pm5.21nii ) BACDEZAFEZTBFEZABGHZIZABJTUAUBKTSTUAUBLZLZUCTSUDUEABMTUD
      QNTUAUBOPR $.
      $( [3-Nov-2013] $) $( [5-Sep-2005] $)
  $}

  ${
    eluz.1 $e |- M e. ZZ $.
    $( Membership in a set of upper integers. $)
    eluz1i $p |- ( N e. ( ZZ>= ` M ) <-> ( N e. ZZ /\ M <_ N ) ) $=
      ( cz wcel cuz cfv cle wbr wa wb eluz1 ax-mp ) ADEBAFGEBDEABHIJKCABLM $.
      $( [5-Sep-2005] $)
  $}

  $( A member of a set of upper integers is an integer. $)
  eluzelz $p |- ( N e. ( ZZ>= ` M ) -> N e. ZZ ) $=
    ( cuz cfv wcel cz cle wbr eluz2 simp2bi ) BACDEAFEBFEABGHABIJ $.
    $( [6-Sep-2005] $)

  $( A member of a set of upper integers is a real. $)
  eluzelre $p |- ( N e. ( ZZ>= ` M ) -> N e. RR ) $=
    ( cuz cfv wcel cz cr eluzelz zre syl ) BACDEBFEBGEABHBIJ $.
    $( [31-Aug-2013] $)

  $( Implication of membership in a set of upper integers. $)
  eluzle $p |- ( N e. ( ZZ>= ` M ) -> M <_ N ) $=
    ( cuz cfv wcel cz cle wbr eluz2 simp3bi ) BACDEAFEBFEABGHABIJ $.
    $( [6-Sep-2005] $)

  $( Membership in a set of upper integers. $)
  eluz $p |- ( ( M e. ZZ /\ N e. ZZ ) ->
         ( N e. ( ZZ>= ` M ) <-> M <_ N ) ) $=
    ( cz wcel cuz cfv cle wb wa wi eluzelz pm4.71ri eluz1 syl5bbr pm5.32 sylibr
    wbr imp ) ACDZBCDZBAEFDZABGQZHZSTUAIZTUBIZHTUCJUDUASUEUATABKLABMNTUAUBOPR
    $.
    $( [2-Oct-2005] $)

  $( Membership of the least member in a set of upper integers. $)
  uzid $p |- ( M e. ZZ -> M e. ( ZZ>= ` M ) ) $=
    ( cz wcel cuz cfv cle wbr wa cr zre leid syl ancli eluz1 mpbird ) ABCZAADEC
    PAAFGZHPQPAICQAJAKLMAANO $.
    $( [2-Sep-2005] $)

  $( Transitive law for sets of upper integers. $)
  uztrn $p |- ( ( M e. ( ZZ>= ` K ) /\ K e. ( ZZ>= ` N ) ) ->
              M e. ( ZZ>= ` N ) ) $=
    ( cuz cfv wcel wa cz cle wbr w3a eluzel2 adantl eluzelz adantr eluzle wi cr
    zre letr syl3an syl111anc mp2and 3jca eluz2 sylibr ) BADEFZACDEZFZGZCHFZBHF
    ZCBIJZKBUHFUJUKULUMUIUKUGCALMZUGULUIABNOZUJCAIJZABIJZUMUIUPUGCAPMUGUQUIABPO
    UJUKAHFZULUPUQGUMQZUNUIURUGCANMUOUKCRFURARFULBRFUSCSASBSCABTUAUBUCUDCBUEUF
    $.
    $( [20-Sep-2005] $)

  $( Contraposition law for upper integers. $)
  uzneg $p |- ( N e. ( ZZ>= ` M ) -> -u M e. ( ZZ>= ` -u N ) ) $=
    ( cuz cfv wcel cneg cle wbr eluzle cz wb eluzel2 eluzelz zre leneg syl11anc
    cr syl2an mpbid znegcl eluz mpbird ) BACDEZAFZBFZCDEZUEUDGHZUCABGHZUGABIUCA
    JEZBJEZUHUGKZABLZABMZUIAQEBQEUKUJANBNABORPSUCUJUIUFUGKZUMULUJUEJEUDJEUNUIBT
    ATUEUDUARPUB $.
    $( [28-Nov-2005] $)

  ${
    $d j k y M $.  $d k N $.
    $( A set of upper integers is a subset of all integers.  (Revised by Mario
       Carneiro, 3-Nov-2013.) $)
    uzssz $p |- ( ZZ>= ` M ) C_ ZZ $=
      ( cuz cdm wcel cfv cz wss cpw uzf ffvelrni elpwi syl fdmi eleq2s wn ndmfv
      c0 0ss a1i eqsstrd pm2.61i ) ABCZDZABEZFGZUEAFUBAFDUDFHZDUEFUFABIJUDFKLFU
      FBIMNUCOZUDQFABPQFGUGFRSTUA $.
      $( [3-Nov-2013] $) $( [2-Sep-2005] $)

    $( Subset relationship for two sets of upper integers. $)
    uzss $p |- ( N e. ( ZZ>= ` M ) ->
               ( ZZ>= ` N ) C_ ( ZZ>= ` M ) ) $=
      ( vk cuz cfv wcel cv cz cle wbr wa wi eluzle adantr eluzel2 cr zre wb syl
      eluz1 eluzelz jca letr syl3an 3expa sylan mpand imdistand 3imtr4d ssrdv
      ex ) BADEZFZCBDEZULUMCGZHFZBUOIJZKZUPAUOIJZKZUOUNFZUOULFZUMUPUQUSUMUPUQUS
      LUMUPKABIJZUQUSUMVCUPABMNUMAHFZBHFZKUPVCUQKUSLZUMVDVEABOZABUAZUBVDVEUPVFV
      DAPFVEBPFUPUOPFVFAQBQUOQABUOUCUDUEUFUGUKUHUMVEVAURRVHBUOTSUMVDVBUTRVGAUOT
      SUIUJ $.
      $( [5-Sep-2005] $)
  $}

  $( Totality of the ordering relation on integers, stated in terms of upper
     integers.  (Contributed by Mario Carneiro, 25-Jun-2013.) $)
  uztric $p |- ( ( M e. ZZ /\ N e. ZZ ) ->
      ( N e. ( ZZ>= ` M ) \/ M e. ( ZZ>= ` N ) ) ) $=
    ( cz wcel wa cuz cfv wo cle wbr cr zre letric syl2an eluz wb ancoms orbi12d
    mpbird ) ACDZBCDZEZBAFGDZABFGDZHABIJZBAIJZHZTAKDBKDUGUAALBLABMNUBUCUEUDUFAB
    OUATUDUFPBAOQRS $.
    $( [25-Jun-2013] $) $( [6-Jul-2005] $)

  $( The upper integers function is one-to-one. $)
  uz11 $p |- ( M e. ZZ ->
                ( ( ZZ>= ` M ) = ( ZZ>= ` N ) <-> M = N ) ) $=
    ( cz wcel cuz cfv wceq wa uzid eleq2 eluzel2 syl6bi syl5 cle eluzle anassrs
    wbr syl6 cr zre impcom syl5ibr syl5ib anim12d ancoms wb letri3 syl2an mpdan
    imp adantlr mpbird ex fveq2 impbid1 ) ACDZAEFZBEFZGZABGZUPUSUTUPUSHZBCDZUTU
    SUPVBUPAUQDZUSVBAIZUSVCAURDZVBUQURAJZBAKLMUAVAVBHUTABNQZBANQZHZUPUSVBVIUSVB
    HUPVIUSVBUPVIUSVBUPHVIUSVBVGUPVHUSVBBUQDZVGVBVJUSBURDBIUQURBJUBABORUSUPVEVH
    UPVCUSVEVDVFUCBAORUDUJPUEPUPVBUTVIUFZUSUPASDBSDVKVBATBTABUGUHUKULUIUMABEUNU
    O $.
    $( [12-Dec-2005] $)

  $( Membership in the next set of upper integers. $)
  eluzp1m1 $p |- ( ( M e. ZZ /\
             N e. ( ZZ>= ` ( M + 1 ) ) ) -> ( N - 1 ) e. ( ZZ>= ` M ) ) $=
    ( cz wcel c1 caddc co cuz cfv cmin cle wbr wa peano2zm ad2antrl cr wb eluz1
    zre 1re leaddsub mp3an2 syl2an biimpa anasss jca ex peano2z syl 3imtr4d imp
    ) ACDZBAEFGZHIDZBEJGZAHIDZULBCDZUMBKLZMZUOCDZAUOKLZMZUNUPULUSVBULUSMUTVAUQU
    TULURBNOULUQURVAULUQMURVAULAPDZBPDZURVAQZUQASBSVCEPDVDVETAEBUAUBUCUDUEUFUGU
    LUMCDUNUSQAUHUMBRUIAUORUJUK $.
    $( [12-Sep-2005] $)

  $( Strict ordering implied by membership in the next set of upper
     integers. $)
  eluzp1l $p |- ( ( M e. ZZ /\
             N e. ( ZZ>= ` ( M + 1 ) ) ) -> M < N ) $=
    ( cz wcel c1 caddc cuz cfv clt wbr cle eluzle adantl eluzelz zltp1le sylan2
    co wa wb mpbird ) ACDZBAEFQZGHDZRABIJZUBBKJZUCUEUAUBBLMUCUABCDUDUESUBBNABOP
    T $.
    $( [12-Sep-2005] $)

  $( Membership in the next set of upper integers. $)
  eluzp1p1 $p |- ( N e. ( ZZ>= ` M ) ->
                       ( N + 1 ) e. ( ZZ>= ` ( M + 1 ) ) ) $=
    ( cz wcel cle wbr w3a c1 caddc co cuz cfv peano2z 3ad2ant1 3ad2ant2 zre 1re
    cr wb eluz2 leadd1 mp3an3 syl2an biimp3a 3jca 3imtr4i ) ACDZBCDZABEFZGZAHIJ
    ZCDZBHIJZCDZUKUMEFZGBAKLDUMUKKLDUJULUNUOUGUHULUIAMNUHUGUNUIBMOUGUHUIUOUGARD
    ZBRDZUIUOSZUHAPBPUPUQHRDURQABHUAUBUCUDUEABTUKUMTUF $.
    $( [5-Oct-2005] $)

  ${
    eluzaddi.1 $e |- M e. ZZ $.
    eluzaddi.2 $e |- K e. ZZ $.
    $( Membership in a later set of upper integers.  (Contributed by Paul
       Chapman, 22-Nov-2007.) $)
    eluzaddi $p |- ( N e. ( ZZ>= ` M )
          -> ( N + K ) e. ( ZZ>= ` ( M + K ) ) ) $=
      ( cuz cfv wcel caddc co cz cle wbr eluzelz zaddcl sylancl wa eluz1i zrei
      cr wb zre leadd1 mp3an13 syl biimpa sylbi mp2an sylanbrc ) CBFGHZCAIJZKHZ
      BAIJZUKLMZUKUMFGHUJCKHZAKHZULBCNECAOPUJUOBCLMZQUNBCDRUOUQUNUOCTHZUQUNUAZC
      UBBTHURATHUSBDSAESBCAUCUDUEUFUGUMUKBKHUPUMKHDEBAOUHRUI $.
      $( [22-Nov-2007] $)

    $( Membership in an earlier set of upper integers.  (Contributed by Paul
       Chapman, 22-Nov-2007.) $)
    eluzsubi $p |- ( N e. ( ZZ>= ` ( M + K ) )
         -> ( N - K ) e. ( ZZ>= ` M ) ) $=
      ( caddc co cuz cfv wcel cmin cz cle eluzelz zsubcl sylancl eluz1i cr zrei
      wbr wa zaddcl mp2an wb zre leaddsub mp3an12 syl biimpa sylbi sylanbrc )
      CBAFGZHIJZCAKGZLJZBUNMTZUNBHIJUMCLJZALJZUOULCNECAOPUMUQULCMTZUAUPULCBLJUR
      ULLJDEBAUBUCQUQUSUPUQCRJZUSUPUDZCUEBRJARJUTVABDSAESBACUFUGUHUIUJBUNDQUK
      $.
      $( [22-Nov-2007] $)
  $}

  $( Membership in a later set of upper integers.  (Contributed by Jeff Madsen,
     2-Sep-2009.) $)
  eluzadd $p |- ( ( N e. ( ZZ>= ` M ) /\ K e. ZZ ) ->
                                      ( N + K ) e. ( ZZ>= ` ( M + K ) ) ) $=
    ( cuz cfv wcel cz caddc co eluzel2 cc0 cif wceq eleq2d fveq2d opreq2 elimel
    wa wi 0z fveq2 opreq1 imbi12d eleq12d imbi2d eluzaddi dedth2h com12 mpand
    imp ) CBDEZFZAGFZCAHIZBAHIZDEZFZULBGFZUMUQBCJURUMRULUQURUMULUQSCURBKLZDEZFZ
    UNUSAHIZDEZFZSVACUMAKLZHIZUSVEHIZDEZFZSBAKKBUSMZULVAUQVDVJUKUTCBUSDUANVJUPV
    CUNVJUOVBDBUSAHUBONUCAVEMZVDVIVAVKUNVFVCVHAVECHPVKVBVGDAVEUSHPOUDUEVEUSCBKG
    TQAKGTQUFUGUHUIUJ $.
    $( [2-Sep-2009] $)

  $( Membership in an earlier set of upper integers.  (Contributed by Jeff
     Madsen, 2-Sep-2009.) $)
  eluzsub $p |- ( ( M e. ZZ /\ K e. ZZ /\ N e. ( ZZ>= ` ( M + K ) ) )
                                          -> ( N - K ) e. ( ZZ>= ` M ) ) $=
    ( cz wcel caddc co cuz cfv cmin wi cc0 wceq fveq2d eleq2d imbi12d opreq2 0z
    cif elimel opreq1 fveq2 eleq1d eluzsubi dedth2h 3impia ) BDEZADEZCBAFGZHIZE
    ZCAJGZBHIZEZUGUHUKUNKCUGBLSZAFGZHIZEZULUOHIZEZKCUOUHALSZFGZHIZEZCVAJGZUSEZK
    BALLBUOMZUKURUNUTVGUJUQCVGUIUPHBUOAFUANOVGUMUSULBUOHUBOPAVAMZURVDUTVFVHUQVC
    CVHUPVBHAVAUOFQNOVHULVEUSAVACJQUCPVAUOCBLDRTALDRTUDUEUF $.
    $( [2-Sep-2009] $)

  $( Choices for an element of an upper interval of integers.  (Contributed by
     Jeff Madsen, 2-Sep-2009.) $)
  uzm1 $p |- ( N e. ( ZZ>= ` M ) -> ( N = M \/ ( N - 1 ) e. ( ZZ>= ` M ) ) ) $=
    ( cuz cfv wcel wceq c1 cmin co wn cz cle wbr w3a eluzel2 a1d cr wb syl11anc
    syl eluzelz peano2zm clt wne df-ne eluzle wa eluzelre ltlen biimprd syl5bir
    zre mpand zltlem1 sylibd 3jcad eluz2 syl6ibr orrd ) BACDZEZBAFZBGHIZUTEZVAV
    BJZAKEZVCKEZAVCLMZNVDVAVEVFVGVHVAVFVEABOZPVAVGVEVABKEZVGABUAZBUBTPVAVEABUCM
    ZVHVEBAUDZVAVLBAUEVAABLMZVMVLABUFVAVLVNVMUGZVAAQEZBQEVLVORVAVFVPVIAULTABUHA
    BUISUJUMUKVAVFVJVLVHRVIVKABUNSUOUPAVCUQURUS $.
    $( [2-Sep-2009] $)

  $( Choices for an element of an upper interval of integers.  (Contributed by
     Jeff Madsen, 2-Sep-2009.) $)
  uzp1 $p |- ( N e. ( ZZ>= ` M ) -> ( N = M \/ N e. ( ZZ>= ` ( M + 1 ) ) ) ) $=
    ( cuz cfv wcel wceq c1 cmin co wo caddc uzm1 eluzp1p1 cz eluzelz zcn ax-1cn
    cc npcan mpan2 3syl eleq1d syl5ib orim2d mpd ) BACDZEZBAFZBGHIZUFEZJUHBAGKI
    CDZEZJABLUGUJULUHUJUIGKIZUKEUGULAUIMUGUMBUKUGBNEBREZUMBFZABOBPUNGREUOQBGSTU
    AUBUCUDUE $.
    $( [2-Sep-2009] $)

  $( Nonnegative integers expressed as a set of upper integers. $)
  nn0uz $p |- NN0 = ( ZZ>= ` 0 ) $=
    ( vk cn0 cc0 cv cle wbr cz crab cuz nn0zrab wcel wceq 0z uzval ax-mp eqtr4i
    cfv ) BCADEFAGHZCIQZAJCGKSRLMACNOP $.
    $( [2-Sep-2005] $)

  $( Natural numbers expressed as a set of upper integers. $)
  nnuz $p |- NN = ( ZZ>= ` 1 ) $=
    ( vk cn c1 cv cle wbr cz crab cuz cfv nnzrab wcel wceq uzval ax-mp eqtr4i
    1z ) BCADEFAGHZCIJZAKCGLSRMQACNOP $.
    $( [2-Sep-2005] $)

  $( A natural number expressed as a member of a set of upper integers. $)
  elnnuz $p |- ( N e. NN <-> N e. ( ZZ>= ` 1 ) ) $=
    ( cn c1 cuz cfv nnuz eleq2i ) BCDEAFG $.
    $( [6-Jun-2006] $)

  $( A nonnegative integer expressed as a member a set of upper integers. $)
  elnn0uz $p |- ( N e. NN0 <-> N e. ( ZZ>= ` 0 ) ) $=
    ( cn0 cc0 cuz cfv nn0uz eleq2i ) BCDEAFG $.
    $( [6-Jun-2006] $)

  ${
    $d m n M $.
    $( Restricted universal quantification in a set of upper integers. $)
    raluz $p |- ( M e. ZZ -> ( A. n e. ( ZZ>= ` M ) ph <->
               A. n e. ZZ ( M <_ n -> ph ) ) ) $=
      ( cz wcel cv cle wbr wi cuz cfv wa eluz1 imbi1d impexp syl6bb ralbidv2 )
      CDEZACBFZGHZAIZBCJKZDRSUBEZAISDEZTLZAIUDUAIRUCUEACSMNUDTAOPQ $.
      $( [9-Sep-2005] $)

    $( Restricted universal quantification in a set of upper integers. $)
    raluz2 $p |- ( A. n e. ( ZZ>= ` M ) ph <->
               ( M e. ZZ -> A. n e. ZZ ( M <_ n -> ph ) ) ) $=
      ( cuz cfv wral cz wcel cv cle wbr wi w3a eluz2 3anass bitri imbi1i impexp
      wa imbi2i bi2.04 3bitri ralbii2 r19.21v ) ABCDEZFCGHZCBIZJKZALZLZBGFUFUIB
      GFLAUJBUEGUGUEHZALUFUGGHZUHSZSZALZUFULUILZLZULUJLUKUNAUKUFULUHMUNCUGNUFUL
      UHOPQUOUFUMALZLUQUFUMARURUPUFULUHARTPUFULUIUAUBUCUFUIBGUDP $.
      $( [9-Sep-2005] $)

    $( Restricted existential quantification in a set of upper integers. $)
    rexuz $p |- ( M e. ZZ -> ( E. n e. ( ZZ>= ` M ) ph <->
               E. n e. ZZ ( M <_ n /\ ph ) ) ) $=
      ( cz wcel cv cle wbr wa cuz cfv eluz1 anbi1d anass syl6bb rexbidv2 ) CDEZ
      ACBFZGHZAIZBCJKZDQRUAEZAIRDEZSIZAIUCTIQUBUDACRLMUCSANOP $.
      $( [9-Sep-2005] $)

    $( Restricted existential quantification in a set of upper integers. $)
    rexuz2 $p |- ( E. n e. ( ZZ>= ` M ) ph <->
               ( M e. ZZ /\ E. n e. ZZ ( M <_ n /\ ph ) ) ) $=
      ( cuz cfv wrex cz wcel cv cle wbr wa eluz2 df-3an bitri anbi1i anass an12
      w3a 3bitri rexbii2 r19.42v ) ABCDEZFCGHZCBIZJKZALZLZBGFUDUGBGFLAUHBUCGUEU
      CHZALUDUEGHZLZUFLZALUKUGLZUJUHLZUIULAUIUDUJUFSULCUEMUDUJUFNOPUKUFAQUMUDUJ
      UGLLUNUDUJUGQUDUJUGROTUAUDUGBGUBO $.
      $( [9-Sep-2005] $)

    $( Double existential quantification in a set of upper integers. $)
    2rexuz $p |- ( E. m E. n e. ( ZZ>= ` m ) ph <->
                  E. m e. ZZ E. n e. ZZ ( m <_ n /\ ph ) ) $=
      ( cv cuz cfv wrex wex cz wcel cle wbr wa rexuz2 exbii df-rex bitr4i ) ACB
      DZEFGZBHRIJRCDKLAMCIGZMZBHTBIGSUABACRNOTBIPQ $.
      $( [3-Nov-2005] $)
  $}

  $( Second Peano postulate for a set of upper integers. $)
  peano2uz $p |- ( N e. ( ZZ>= ` M ) ->
                 ( N + 1 ) e. ( ZZ>= ` M ) ) $=
    ( cz wcel cle wbr w3a c1 caddc co cuz cfv simp1 peano2z 3ad2ant2 zre letrp1
    cr syl3an2 eluz2 syl3an1 3jca 3imtr4i ) ACDZBCDZABEFZGZUDBHIJZCDZAUHEFZGBAK
    LZDUHUKDUGUDUIUJUDUEUFMUEUDUIUFBNOUDARDZUEUFUJAPUEULBRDUFUJBPABQSUAUBABTAUH
    TUC $.
    $( [7-Sep-2005] $)

  $( Reversed second Peano axiom for upper integers. $)
  peano2uzr $p |- ( ( M e. ZZ /\
             N e. ( ZZ>= ` ( M + 1 ) ) ) -> N e. ( ZZ>= ` M ) ) $=
    ( cz wcel c1 caddc co cuz cfv wa cmin wceq cc eluzelz zcn ax-1cn npcan 3syl
    mpan2 adantl eluzp1m1 peano2uz syl eqeltrrd ) ACDZBAEFGZHIDZJZBEKGZEFGZBAHI
    ZUGUJBLZUEUGBCDBMDZULUFBNBOUMEMDULPBEQSRTUHUIUKDUJUKDABUAAUIUBUCUD $.
    $( [2-Jan-2006] $)

  ${
    $d j K $.  $d j k M $.  $d j k N $.
    $( Addition closure law for a set of upper integers. $)
    uzaddcl $p |- ( ( N e. ( ZZ>= ` M ) /\ K e. NN0 ) ->
                   ( N + K ) e. ( ZZ>= ` M ) ) $=
      ( vj vk cn0 wcel caddc co cv wi cc0 c1 wa wceq syl eleq1d opreq2 imbi2d
      cc cuz cfv cz eluzelz zcn nn0cn ax-1cn addass mp3an3 syl2an ancoms adantr
      peano2uz adantl eqeltrrd exp31 a2d addid1 ibir nn0indALT impcom ) AFGCBUA
      UBZGZCAHIZVBGZVCCDJZHIZVBGZKVCCLHIZVBGZKVCCEJZHIZVBGZKVCCVKMHIZHIZVBGZKVC
