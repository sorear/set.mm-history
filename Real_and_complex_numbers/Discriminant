        cmin ledivmul syl112anc ltletrd mulneg1 df-neg syl6eq breqtrd ltaddsub2
        eqtrd ltp1 mpbird lt01 ltmul1 mul02 lelttrd ltnle pm2.65d lelttric mt3d
        wo ord ) ALCMNZCLUANZAYGLCFUBUCOZPOZDFPOZQOZEQOZMNZAYGYMAYGUDZFRSZLCBUE
