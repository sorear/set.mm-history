       number. $)
    hbneg $p |- ( y e. -u A -> A. x y e. -u A ) $=
      ( cneg cc0 cmin co df-neg cv wcel ax-17 hbopr hbxfr ) ABCEFCGHCIABFCGBJZF
      KALOGKALDMN $.
      $( [12-Jun-2005] $)
  $}

