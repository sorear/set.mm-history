      MABUHUNUJUOVFVGUPUQURUSUTVAABVBVC $.
      $( [7-Nov-2013] $) $( [14-Jul-2005] $)
  $}

  $( "Complementing" its second argument doesn't change a binary
     coefficient. $)
  bccmpl $p |- ( ( N e. V /\ K e. ( 0 ... N ) ) -> ( N _C K ) =
                    ( N _C ( N - K ) ) ) $=
    ( wcel cc0 cfz co wa cfa cfv cmin cmul cdiv cbc cc wceq fznn0sub2 cn0 cn
    elfznn0 faccl nncn 3syl syl adantl mulcom syl11anc elfz3nn0 nn0cn nncan
    syl2an fveq2d opreq1d eqtr4d opreq2d bcval2 syldan 3eqtr4d ) BCDZAEBFGZDZHZ
