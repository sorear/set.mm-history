  $( The ceiling function returns an integer (closure law).  (Contributed by
     Jeffrey Hankins, 10-Jun-2007.) $)
  ceicl $p |- ( A e. RR -> -u ( |_ ` -u A ) e. ZZ ) $=
    ( cr wcel cneg cfl cfv cz renegcl flcl znegcl 3syl ) ABCADZBCLEFZGCMDGCAHLI
    MJK $.
    $( [8-Jun-2007] $)

  $( The ceiling of a real number is greater than or equal to that number.
     (Contributed by Jeffrey Hankins, 10-Jun-2007.) $)
  ceige $p |- ( A e. RR -> A <_ -u ( |_ ` -u A ) ) $=
    ( cr wcel cneg cfl cfv cle wbr renegcl reflcl syl wa flle adantr lenegcon2
    mpbird mpdan ) ABCZADZEFZBCZATDGHZRSBCZUAAIZSJKRUALUBTSGHZRUEUARUCUEUDSMKNA
    TOPQ $.
    $( [9-Jun-2007] $)

  $( One less than the ceiling of a real number is strictly less than that
     number.  (Contributed by Jeffrey Hankins, 10-Jun-2007.) $)
  ceim1l $p |- ( A e. RR -> ( -u ( |_ ` -u A ) - 1 ) < A ) $=
    ( cr wcel cneg cfl cfv c1 cmin co caddc clt cc wceq renegcl reflcl syl
    recnd ax1cn negdi sylancl negcl negsub mpan2 3syl eqtr2d wbr peano2re wa
    flltp1 adantr ltnegcon1 mpbid mpdan eqbrtrd ) ABCZADZEFZDZGHIZUQGJIZDZAKUOV
    AURGDJIZUSUOUQLCZGLCZVAVBMUOUQUOUPBCZUQBCZANZUPOPZQZRUQGSTUOVCURLCZVBUSMZVI
    UQUAVJVDVKRURGUBUCUDUEUOUTBCZVAAKUFZUOVFVLVHUQUGPUOVLUHUPUTKUFZVMUOVNVLUOVE
    VNVGUPUIPUJAUTUKULUMUN $.
    $( [9-Jun-2007] $)

  $( The ceiling of a real number is the smallest integer greater than or equal
     to it.  (Contributed by Jeffrey Hankins, 10-Jun-2007.) $)
  ceile $p |- ( ( A e. RR /\ B e. ZZ /\ A <_ B ) -> -u ( |_ ` -u A ) <_ B ) $=
    ( cr wcel cz cle wbr cneg cfl cfv wa c1 cmin co clt ceim1l adantr wi ceicl
    zre peano2rem 3syl simpl adantl ltletr syl111anc mpand wb zlem1lt sylan
    sylibrd 3impia ) ACDZBEDZABFGZAHIJHZBFGZUMUNKZUOUPLMNZBOGZUQURUSAOGZUOUTUMV
