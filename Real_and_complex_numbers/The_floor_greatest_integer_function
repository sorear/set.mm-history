      IFGWIXOVTXIVPWKXIWAVLVMVQVNUSUTVFVAEWDWAVOVRVS $.
      $( [15-Nov-2004] $)

    $( A condition equivalent to floor. $)
    flbi $p |- ( ( A e. RR /\ B e. ZZ ) ->
                ( ( |_ ` A ) = B <-> ( B <_ A /\ A < ( B + 1 ) ) ) ) $=
      ( vx vy cr wcel cz wa cfl cfv wceq cv cle wbr c1 caddc co clt crio wb
      flval eqeq1d adantr wreu rebtwnz ax-17 a17d opreq1 breq2d anbi12d riota2f
      breq1 sylan2 ancoms bitr4d ) AEFZBGFZHAIJZBKZCLZAMNZAUTOPQZRNZHZCGSZBKZBA
      MNZABOPQZRNZHZUPUSVFTUQUPURVEBCAUAUBUCUQUPVJVFTZUPUQVDCGUDVKCAUEVDVJCDGBD
      LBFCUFUQVJCUGUTBKZVAVGVCVIUTBAMULVLVBVHARUTBOPUHUIUJUKUMUNUO $.
      $( [2-Nov-2013] $) $( [11-Mar-2005] $)
  $}

  $( A condition equivalent to floor. $)
  flbi2 $p |- ( ( N e. ZZ /\ F e. RR ) ->
              ( ( |_ ` ( N + F ) ) = N <-> ( 0 <_ F /\ F < 1 ) ) ) $=
    ( cz wcel cr wa caddc co cfl cfv wceq cle wbr c1 clt cc0 zre readdcl sylan
    wb simpl flbi syl11anc addge01 1re ltadd2 mp3an2 ancoms anbi12d bitr4d ) BC
    DZAEDZFZBAGHZIJBKZBUNLMZUNBNGHOMZFZPALMZANOMZFZUMUNEDZUKUOURTUKBEDZULVBBQZB
