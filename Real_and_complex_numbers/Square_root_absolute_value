    $( Generalized triangle inequality: absolute value of a partial sum is less
       than or equal to sum of absolute values. $)
    ser1absdifi $p |- ( ( A e. NN /\ B e. NN /\ A < B ) ->
       ( abs ` ( ( ( + seq1 F ) ` B ) - ( ( + seq1 F ) ` A ) ) ) <_
       ( ( ( + seq1 G ) ` B ) - ( ( + seq1 G ) ` A ) ) ) $=
      ( cn wcel clt wbr w3a cmin co caddc cseq1 cfv cabs cle simp1 nnsub
      biimp3a ser1absdiflem syl11anc wceq wa cc nncn pncan3 syl2an fveq2d
      opreq1d 3adant3 3brtr3d ) BIJZCIJZBCKLZMZBCBNOZPOZPDQOZRZBVBRZNOZSRZVAPEQ
      OZRZBVGRZNOZCVBRZVDNOZSRZCVGRZVINOZTUSUPUTIJZVFVJTLUPUQURUAUPUQURVPBCUBUC
