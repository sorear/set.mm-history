  ${
    reccl.1 $e |- A e. CC $.
    reccl.2 $e |- A =/= 0 $.
    $( Closure law for reciprocal. $)
    reccli $p |- ( 1 / A ) e. CC $=
      ( c1 ax1cn divcli ) DAEBCF $.
      $( [30-Apr-2005] $)
  $}

  ${
    recclz.1 $e |- A e. CC $.
    $( Closure law for reciprocal. $)
    recclzi $p |- ( A =/= 0 -> ( 1 / A ) e. CC ) $=
      ( c1 ax1cn divclzi ) CADBE $.
      $( [30-Apr-2005] $)
  $}

  $( Closure law for reciprocal. $)
  reccl $p |- ( ( A e. CC /\ A =/= 0 ) -> ( 1 / A ) e. CC ) $=
    ( c1 cc wcel cc0 wne cdiv co ax1cn divcl mp3an1 ) BCDACDAEFBAGHCDIBAJK $.
    $( [30-Apr-2005] $)

  ${
    divcan.1 $e |- A e. CC $.
    divcan.2 $e |- B e. CC $.
    ${
      divcan.3 $e |- B =/= 0 $.
      $( A cancellation law for division. $)
      divcan2i $p |- ( B x. ( A / B ) ) = A $=
        ( cdiv co wceq cmul eqid divcli divmuli mpbi ) ABFGZNHBNIGAHNJABNCDABCD
        EKELM $.
        $( [18-Aug-2008] $) $( [9-Feb-1995] $)

      $( A cancellation law for division. $)
      divcan1i $p |- ( ( A / B ) x. B ) = A $=
        ( cdiv co cmul divcli mulcomi divcan2i eqtri ) ABFGZBHGBMHGAMBABCDEIDJA
        BCDEKL $.
        $( [18-Aug-2008] $) $( [18-May-1999] $)
    $}

    $( A cancellation law for division. $)
    divcan1zi $p |- ( B =/= 0 -> ( ( A / B ) x. B ) = A ) $=
      ( cc0 wne cdiv co cmul wceq c1 cif opreq2 id opreq12d eqeq1d cc ax1cn
      keepel elimne0 divcan1i dedth ) BEFZABGHZBIHZAJAUCBKLZGHZUFIHZAJBKBUFJZUE
      UHAUIUDUGBUFIBUFAGMUINOPAUFCUCBKQDRSBTUAUB $.
      $( [18-Aug-2008] $) $( [2-Oct-1999] $)

    $( A cancellation law for division.  We eliminate the third hypothesis of
       ~ divcan2i using the weak deduction theorem ~ dedth and keep the other
       two.  Because the first hypothesis shares the class variable ` B ` with
       the hypothesis we're eliminating, we need to use ~ keepel in order to
       keep the first hypothesis. $)
    divcan2zi $p |- ( B =/= 0 -> ( B x. ( A / B ) ) = A ) $=
      ( cc0 wne cdiv co cmul wceq c1 cif id opreq2 opreq12d eqeq1d cc ax1cn
      keepel elimne0 divcan2i dedth ) BEFZBABGHZIHZAJUCBKLZAUFGHZIHZAJBKBUFJZUE
      UHAUIBUFUDUGIUIMBUFAGNOPAUFCUCBKQDRSBTUAUB $.
      $( [21-Aug-2008] $) $( [10-Aug-1999] $)
  $}

  $( A cancellation law for division. $)
  divcan1 $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 )
        -> ( ( A / B ) x. B ) = A ) $=
    ( cc wcel cc0 wne cdiv co cmul wceq wi cif opreq1 opreq1d id eqeq12d
    imbi2d neeq1 opreq2 opreq12d eqeq1d imbi12d 0cn elimel divcan1zi dedth2h
    3impia ) ACDZBCDZBEFZABGHZBIHZAJZUHUIUJUMKUJUHAELZBGHZBIHZUNJZKUIBELZEFZUNU
    RGHZURIHZUNJZKABEEAUNJZUMUQUJVCULUPAUNVCUKUOBIAUNBGMNVCOPQBURJZUJUSUQVBBURE
    RVDUPVAUNVDUOUTBURIBURUNGSVDOTUAUBUNURAECUCUDBECUCUDUEUFUG $.
    $( [18-Aug-2008] $) $( [5-Jun-2004] $)

  $( A cancellation law for division. $)
  divcan2 $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                 ( B x. ( A / B ) ) = A ) $=
    ( cc wcel cc0 wne w3a cdiv co cmul wceq mulcom 3simp2 divcl sylanc divcan1
    eqtrd ) ACDZBCDZBEFZGZBABHIZJIZUBBJIZASUBCDUCUDKUABUBLRSTMABNOABPQ $.
    $( [21-Aug-2008] $) $( [3-Feb-2004] $)

  $( The ratio of nonzero numbers is nonzero. $)
  divne0b $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                  ( A =/= 0 <-> ( A / B ) =/= 0 ) ) $=
    ( cc wcel cc0 wne w3a cdiv co wceq cmul wb mul01 eqeq1d eqcom syl6rbbr
    3ad2ant2 wa 0cn divmul mp3an2 3impb bitr4d necon3bid ) ACDZBCDZBEFZGZAEABHI
    ZEUHAEJZBEKIZAJZUIEJZUFUEUJULLUGUFULEAJUJUFUKEABMNAEOPQUEUFUGUMULLZUEECDUFU
    GRUNSAEBTUAUBUCUD $.
    $( [1-Jun-2005] $) $( [2-Aug-2004] $)

  $( The ratio of nonzero numbers is nonzero. $)
  divne0 $p |- ( ( ( A e. CC /\ A =/= 0 ) /\ ( B e. CC /\ B =/= 0 ) ) ->
                  ( A / B ) =/= 0 ) $=
    ( cc wcel cc0 wne cdiv co wi w3a divne0b biimpd 3exp com23 com24 imp43 ) AC
    DZAEFZBCDZBEFZABGHEFZQTSRUAQSTRUAIZQSTUBQSTJRUAABKLMNOP $.
    $( [28-Dec-2007] $)

  ${
    divneq0.1 $e |- A e. CC $.
    divneq0.2 $e |- B e. CC $.
    divneq0.3 $e |- A =/= 0 $.
    divneq0.4 $e |- B =/= 0 $.
    $( The ratio of nonzero numbers is nonzero. $)
    divne0i $p |- ( A / B ) =/= 0 $=
      ( cc wcel cc0 wne cdiv co w3a divne0b mpbii mp3an ) AGHZBGHZBIJZABKLIJZCD
      FQRSMAIJTEABNOP $.
