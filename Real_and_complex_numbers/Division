$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Division
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y z w $.
    $( Define division.  Theorem ~ divmul relates it to multiplication, and
       ~ divcl and ~ redivcl prove its closure laws. $)
    df-div $a |- / = { <. <. x , y >. , z >. |
                   ( ( x e. CC /\ y e. ( CC \ { 0 } ) ) /\
                   z = U. { w e. CC | ( y x. w ) = x } ) } $.
    $(
    Future simpler alternative?
    df-div $a |- / = { <. <. x , y >. , z >. |
                ( ( y e. CC /\ z e. CC ) /\  x = ( y x. z ) ) } $.
    $)
  $}


  ${
    $d x y z w A $.  $d x y z w B $.
    divval.1 $e |- A e. CC $.
    divval.2 $e |- B e. CC $.
    divval.3 $e |- B =/= 0 $.
    $( Value of division: the (unique) element ` x ` such that
        ` ( B x. x ) = A ` .  This is meaningful only when ` B ` is nonzero. $)
    divval $p |- ( A / B ) = U. { x e. CC | ( B x. x ) = A } $=
      ( vy vz vw cc wcel cc0 csn cdif cdiv co cv cmul wceq crab cuni wn eldif
      elisseti elsnc nemtbir mpbir2an axcnex rabex uniex eqeq2 rabbisdv unieqd
      opreq1 eqeq1d df-div oprabval2 mp2an ) BJKCJLMZNZKZBCOPCAQZRPZBSZAJTZUAZS
      DVACJKCUSKZUBCJUSUCEVGCLFCLCJEUDUEUFUGGHIBCJUTHQZVBRPZGQZSZAJTZUAVFOVIBSZ
      AJTZUAVEVDAJUHUIUJVJBSZVLVNVOVKVMAJVJBVIUKULUMVHCSZVNVEVPVMVDAJVPVIVCBVHC
      VBRUNUOULUMGHIAUPUQUR $.
      $( [8-May-1999] $)
  $}

  ${
    $d x A $.  $d x B $.  $d x C $.
    divmul.1 $e |- A e. CC $.
    divmul.2 $e |- B e. CC $.
    divmul.3 $e |- C e. CC $.
    divmul.4 $e |- B =/= 0 $.
    $( Relationship between division and multiplication. $)
    divmul $p |- ( ( A / B ) = C <-> ( B x. C ) = A ) $=
      ( vx cc wcel cdiv co wceq cmul wb cv eqeq2 opreq2 eqeq1d bibi12d crab
      cuni wreu receu reuuni1 mpan2 divval eqeq1i syl6rbbr vtoclga ax-mp ) CIJA
      BKLZCMZBCNLZAMZOZFULHPZMZBUQNLZAMZOUPHCIUQCMZURUMUTUOUQCULQVAUSUNAUQCBNRS
      TUQIJZUTUTHIUAUBZUQMZURVBUTHIUCUTVDOHBAEDGUDUTHIUEUFULVCUQHABDEGUGUHUIUJU
      K $.
      $( [2-Feb-1995] $)
  $}

  ${
    divmulz.1 $e |- A e. CC $.
    divmulz.2 $e |- B e. CC $.
    divmulz.3 $e |- C e. CC $.
    $( Relationship between division and multiplication. $)
    divmulz $p |- ( B =/= 0 -> ( ( A / B ) = C <-> ( B x. C ) = A ) ) $=
      ( cc0 wne cdiv co wceq cmul wb c1 cif opreq2 eqeq1d opreq1 bibi12d cc
      ax1cn keepel elimne0 divmul dedth ) BGHZABIJZCKZBCLJZAKZMAUFBNOZIJZCKZUKC
      LJZAKZMBNBUKKZUHUMUJUOUPUGULCBUKAIPQUPUIUNABUKCLRQSAUKCDUFBNTEUAUBFBUCUDU
      E $.
      $( [8-May-1999] $)
  $}

  $( Relationship between division and multiplication. $)
  divmult $p |- ( ( ( A e. CC /\ B e. CC /\ C e. CC ) /\ B =/= 0 ) ->
               ( ( A / B ) = C <-> ( B x. C ) = A ) ) $=
    ( cc wcel w3a cc0 wne cdiv co wceq cmul wb wi cif opreq1 eqeq1d eqeq2
    bibi12d imbi2d neeq1 opreq2 imbi12d 0cn elimel divmulz dedth3h imp ) ADEZBD
    EZCDEZFBGHZABIJZCKZBCLJZAKZMZUIUJUKULUQNULUIAGOZBIJZCKZUOURKZMZNABCGGGUJBGO
    ZGHZURVCIJZCKZVCCLJZURKZMZNVDVEUKCGOZKZVCVJLJZURKZMZNAURKZUQVBULVOUNUTUPVAV
    OUMUSCAURBIPQAURUORSTBVCKZULVDVBVIBVCGUAVPUTVFVAVHVPUSVECBVCURIUBQVPUOVGURB
    VCCLPQSUCCVJKZVIVNVDVQVFVKVHVMCVJVERVQVGVLURCVJVCLUBQSTURVCVJAGDUDUEBGDUDUE
    CGDUDUEUFUGUH $.
    $( [4-Aug-2004] $) $( [2-Aug-2004] $)

  $( Relationship between division and multiplication. $)
  divmul2t $p |- ( ( ( A e. CC /\ B e. CC /\ C e. CC ) /\ B =/= 0 ) ->
               ( ( A / B ) = C <-> A = ( B x. C ) ) ) $=
    ( cc wcel w3a cc0 wne wa cdiv co wceq cmul divmult eqcom syl6bb ) ADEBDECDE
    FBGHIABJKCLBCMKZALAQLABCNQAOP $.
    $( [24-Feb-2006] $) $( [7-Feb-2006] $)

  $( Relationship between division and multiplication. $)
  divmul3t $p |- ( ( ( A e. CC /\ B e. CC /\ C e. CC ) /\ B =/= 0 ) ->
               ( ( A / B ) = C <-> A = ( C x. B ) ) ) $=
    ( cc wcel w3a cc0 wne wa cdiv co wceq cmul divmul2t wb axmulcom eqeq2d
    3adant1 adantr bitrd ) ADEZBDEZCDEZFZBGHZIABJKCLABCMKZLZACBMKZLZABCNUDUGUIO
    ZUEUBUCUJUAUBUCIUFUHABCPQRST $.
    $( [27-Feb-2006] $) $( [13-Feb-2006] $)

  ${
    $d x A $.  $d x B $.
    divcl.1 $e |- A e. CC $.
    divcl.2 $e |- B e. CC $.
    divcl.3 $e |- B =/= 0 $.
    $( Closure law for division. $)
    divcl $p |- ( A / B ) e. CC $=
      ( vx cdiv co cv cmul wceq cc crab cuni divval wreu wcel receu reucl
      ax-mp eqeltr ) ABGHBFIJHAKZFLMNZLFABCDEOUBFLPUCLQFBADCERUBFLSTUA $.
      $( [2-Feb-1995] $)
  $}

  ${
    divclz.1 $e |- A e. CC $.
    divclz.2 $e |- B e. CC $.
    $( Closure law for division. $)
    divclz $p |- ( B =/= 0 -> ( A / B ) e. CC ) $=
      ( cc0 wne cdiv co cc wcel c1 cif wceq opreq2 eleq1d ax1cn keepel elimne0
      divcl dedth ) BEFZABGHZIJAUABKLZGHZIJBKBUCMUBUDIBUCAGNOAUCCUABKIDPQBRST
      $.
      $( [7-May-1999] $)
  $}

  $( Closure law for division. $)
  divclt $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) -> ( A / B ) e. CC ) $=
    ( cc wcel cc0 wne cdiv co wi cif wceq opreq1 eleq1d imbi2d neeq1 opreq2
    imbi12d 0cn elimel divclz dedth2h 3impia ) ACDZBCDZBEFZABGHZCDZUCUDUEUGIUEU
    CAEJZBGHZCDZIUDBEJZEFZUHUKGHZCDZIABEEAUHKZUGUJUEUOUFUICAUHBGLMNBUKKZUEULUJU
    NBUKEOUPUIUMCBUKUHGPMQUHUKAECRSBECRSTUAUB $.
    $( [4-May-2005] $) $( [21-Jul-2001] $)

  ${
    reccl.1 $e |- A e. CC $.
    reccl.2 $e |- A =/= 0 $.
    $( Closure law for reciprocal. $)
    reccl $p |- ( 1 / A ) e. CC $=
      ( c1 ax1cn divcl ) DAEBCF $.
      $( [2-May-2005] $) $( [30-Apr-2005] $)
  $}

  ${
    recclz.1 $e |- A e. CC $.
    $( Closure law for reciprocal. $)
    recclz $p |- ( A =/= 0 -> ( 1 / A ) e. CC ) $=
      ( c1 ax1cn divclz ) CADBE $.
      $( [2-May-2005] $) $( [30-Apr-2005] $)
  $}

  $( Closure law for reciprocal. $)
  recclt $p |- ( ( A e. CC /\ A =/= 0 ) -> ( 1 / A ) e. CC ) $=
    ( c1 cc wcel cc0 wne cdiv co ax1cn divclt mp3an1 ) BCDACDAEFBAGHCDIBAJK $.
    $( [2-May-2005] $) $( [30-Apr-2005] $)

  ${
    divcan.1 $e |- A e. CC $.
    divcan.2 $e |- B e. CC $.
    ${
      divcan.3 $e |- A =/= 0 $.
      $( A cancellation law for division. $)
      divcan2 $p |- ( A x. ( B / A ) ) = B $=
        ( cdiv co wceq cmul eqid divcl divmul mpbi ) BAFGZNHANIGBHNJBANDCBADCEK
        ELM $.
        $( [9-Feb-1995] $)

      $( A cancellation law for division. $)
      divcan1 $p |- ( ( B / A ) x. A ) = B $=
        ( cdiv co cmul divcl mulcom divcan2 eqtr ) BAFGZAHGAMHGBMABADCEICJABCDE
        KL $.
        $( [18-May-1999] $)
    $}

    $( A cancellation law for division. $)
    divcan1z $p |- ( A =/= 0 -> ( ( B / A ) x. A ) = B ) $=
      ( cc0 wne cdiv co cmul wceq c1 cif opreq2 id opreq12d eqeq1d cc ax1cn
      keepel elimne0 divcan1 dedth ) AEFZBAGHZAIHZBJBUCAKLZGHZUFIHZBJAKAUFJZUEU
      HBUIUDUGAUFIAUFBGMUINOPUFBUCAKQCRSDATUAUB $.
      $( [2-Oct-1999] $)

    $( A cancellation law for division.  We eliminate the third hypothesis of
       ~ divcan2 using the weak deduction theorem ~ dedth and keep the other
       two.  Because the first hypothesis shares the class variable ` A `
       with the hypothesis we're eliminating, we need to use ~ keepel in order
       to keep the first hypothesis. $)
    divcan2z $p |- ( A =/= 0 -> ( A x. ( B / A ) ) = B ) $=
      ( cc0 wne cdiv co cmul wceq c1 cif id opreq2 opreq12d eqeq1d cc ax1cn
      keepel elimne0 divcan2 dedth ) AEFZABAGHZIHZBJUCAKLZBUFGHZIHZBJAKAUFJZUEU
      HBUIAUFUDUGIUIMAUFBGNOPUFBUCAKQCRSDATUAUB $.
      $( [10-Aug-1999] $)
  $}

  $( A cancellation law for division. $)
  divcan1t $p |- ( ( A e. CC /\ B e. CC /\ A =/= 0 ) ->
                 ( ( B / A ) x. A ) = B ) $=
    ( cc wcel cc0 wne cdiv co cmul wceq wi cif neeq1 opreq2 id opreq12d eqeq1d
    imbi12d opreq1 opreq1d eqeq12d imbi2d 0cn elimel divcan1z dedth2h 3impia )
    ACDZBCDZAEFZBAGHZAIHZBJZUHUIUJUMKUHAELZEFZBUNGHZUNIHZBJZKUOUIBELZUNGHZUNIHZ
    USJZKABEEAUNJZUJUOUMURAUNEMVCULUQBVCUKUPAUNIAUNBGNVCOPQRBUSJZURVBUOVDUQVABU
    SVDUPUTUNIBUSUNGSTVDOUAUBUNUSAECUCUDBECUCUDUEUFUG $.
    $( [15-May-2005] $) $( [5-Jun-2004] $)

  $( A cancellation law for division. $)
  divcan2t $p |- ( ( A e. CC /\ B e. CC /\ A =/= 0 ) ->
                 ( A x. ( B / A ) ) = B ) $=
    ( cc wcel cc0 wne cdiv co cmul wceq wi cif neeq1 id opreq2 opreq12d eqeq1d
    imbi12d opreq1 opreq2d eqeq12d imbi2d 0cn elimel divcan2z dedth2h 3impia )
    ACDZBCDZAEFZABAGHZIHZBJZUHUIUJUMKUHAELZEFZUNBUNGHZIHZBJZKUOUNUIBELZUNGHZIHZ
    USJZKABEEAUNJZUJUOUMURAUNEMVCULUQBVCAUNUKUPIVCNAUNBGOPQRBUSJZURVBUOVDUQVABU
    SVDUPUTUNIBUSUNGSTVDNUAUBUNUSAECUCUDBECUCUDUEUFUG $.
    $( [29-May-2005] $) $( [3-Feb-2004] $)

  ${
    muldiv.1  $e |- ( C x. A ) = B $.
    muldiv.2  $e |- C e. CC $.
    muldiv.3  $e |- B e. CC $.
    muldiv.4  $e |- C =/= 0 $.
    muldiv.5  $e |- A e. CC $.
    $( MAY BE REVISED - replace with divmul ? $)
    $( Convert a multiplication into a division.  (Contributed by FL,
       10-Dec-2006.) $)
    muldiv  $p |- A = ( B / C ) $=
      ( cdiv co wceq cmul divmul mpbir eqcomi ) BCIJZAPAKCALJBKDBCAFEHGMNO $.
      $( [10-Dec-2006] $) $( [4-Dec-2006] $)
  $}

  $( The ratio of non-zero numbers is non-zero. $)
  divne0bt $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                  ( A =/= 0 <-> ( A / B ) =/= 0 ) ) $=
    ( cc wcel cc0 wne w3a cdiv co wceq cmul wb mul01t eqeq1d eqcom syl6rbbr
    3ad2ant2 0cn divmult mp3anl3 3impa bitr4d eqneqd ) ACDZBCDZBEFZGZAEABHIZEUG
    AEJZBEKIZAJZUHEJZUEUDUIUKLUFUEUKEAJUIUEUJEABMNAEOPQUDUEUFULUKLZUDUEECDUFUMR
    ABESTUAUBUC $.
    $( [1-Jun-2005] $) $( [2-Aug-2004] $)

  ${
    divneq0.1 $e |- A e. CC $.
    divneq0.2 $e |- B e. CC $.
    divneq0.3 $e |- A =/= 0 $.
    divneq0.4 $e |- B =/= 0 $.
    $( The ratio of non-zero numbers is non-zero. $)
    divne0 $p |- ( A / B ) =/= 0 $=
      ( cc wcel cc0 wne cdiv co w3a divne0bt mpbii mp3an ) AGHZBGHZBIJZABKLIJZC
      DFQRSMAIJTEABNOP $.
