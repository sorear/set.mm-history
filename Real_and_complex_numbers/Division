$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Division
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y z $.
    $( Define division.  Theorem ~ divmuli relates it to multiplication, and
       ~ divcli and ~ redivcli prove its closure laws. $)
    df-div $a |- / = ( x e. CC , y e. ( CC \ { 0 } ) |->
                   ( iota_ z e. CC ( y x. z ) = x ) ) $.
    $(
    Future simpler alternative?
    df-div $a |- / = { <. <. x , y >. , z >. |
                ( ( y e. ( CC \ { 0 } ) /\ z e. CC ) /\  x = ( y x. z ) ) } $.
    $)
  $}

  ${
    $d x y z A $.  $d x z B $.
    divval.1 $e |- A e. CC $.
    divval.2 $e |- B e. CC $.
    divval.3 $e |- B =/= 0 $.
    $( Value of division: the (unique) element ` x ` such that
       ` ( B x. x ) = A ` .  This is meaningful only when ` B ` is nonzero. $)
    divvali $p |- ( A / B ) = ( iota_ x e. CC ( B x. x ) = A ) $=
      ( vy vz cc wcel cc0 csn cdif cdiv co cv cmul wceq crio riotabidv mpbir2an
      wne eldifsn eqeq2 opreq1 eqeq1d df-div riotaex ovmpt2 mp2an ) BIJCIKLMZJZ
      BCNOCAPZQOZBRZAISZRDULCIJCKUBCIKUCEFUAGHBCIUKHPZUMQOZGPZRZAISUPNURBRZAISU
      SBRUTVAAIUSBURUDTUQCRZVAUOAIVBURUNBUQCUMQUEUFTGHAUGUOAIUHUIUJ $.
      $( [2-Nov-2013] $) $( [8-May-1999] $)
  $}

  ${
    $d x y A $.  $d x y B $.  $d x y C $.
    divmul.1 $e |- A e. CC $.
    divmul.2 $e |- B e. CC $.
    divmul.3 $e |- C e. CC $.
    divmul.4 $e |- B =/= 0 $.
    $( Relationship between division and multiplication. $)
    divmuli $p |- ( ( A / B ) = C <-> ( B x. C ) = A ) $=
      ( vx vy cdiv co wceq cv cmul cc crio divvali eqeq1i wcel ax-17 receui wal
      wreu wb wi a1i opreq2 eqeq1d riota2f mp2an bitr4i ) ABJKZCLBHMZNKZALZHOPZ
      CLZBCNKZALZULUPCHABDEGQRCOSZUOHOUCUSUQUDFHBAEDGUAUOUSHIOCIMCSHTUSUSHUBUEU
      TUSHTUFUMCLUNURAUMCBNUGUHUIUJUK $.
      $( [24-Nov-2011] $) $( [2-Feb-1995] $)
  $}

  ${
    divmulz.1 $e |- A e. CC $.
    divmulz.2 $e |- B e. CC $.
    divmulz.3 $e |- C e. CC $.
    $( Relationship between division and multiplication. $)
    divmulzi $p |- ( B =/= 0 -> ( ( A / B ) = C <-> ( B x. C ) = A ) ) $=
      ( cc0 wne cdiv co wceq cmul wb c1 cif opreq2 eqeq1d opreq1 bibi12d cc
      ax-1cn keepel elimne0 divmuli dedth ) BGHZABIJZCKZBCLJZAKZMAUFBNOZIJZCKZU
      KCLJZAKZMBNBUKKZUHUMUJUOUPUGULCBUKAIPQUPUIUNABUKCLRQSAUKCDUFBNTEUAUBFBUCU
      DUE $.
      $( [8-May-1999] $)
  $}

  $( Relationship between division and multiplication. $)
  divmul $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
               -> ( ( A / C ) = B <-> ( C x. B ) = A ) ) $=
    ( cc wcel cc0 wne cdiv co wceq wb wi cif opreq1 eqeq1d eqeq2 bibi12d elimel
    cmul 0cn imbi2d neeq1 opreq2 imbi12d divmulzi dedth3h 3exp com23 imp4a 3imp
    wa ) ADEZBDEZCDEZCFGZUKACHIZBJZCBSIZAJZKZULUMUNUOUTULUNUMUOUTLZULUNUMVAULUN
    UMVAUOULAFMZCHIZBJZURVBJZKZLUNCFMZFGZVBVGHIZBJZVGBSIZVBJZKZLVHVIUMBFMZJZVGV
    NSIZVBJZKZLACBFFFAVBJZUTVFUOVSUQVDUSVEVSUPVCBAVBCHNOAVBURPQUACVGJZUOVHVFVMC
    VGFUBVTVDVJVEVLVTVCVIBCVGVBHUCOVTURVKVBCVGBSNOQUDBVNJZVMVRVHWAVJVOVLVQBVNVI
    PWAVKVPVBBVNVGSUCOQUAVBVGVNAFDTRCFDTRBFDTRUEUFUGUHUIUJ $.
    $( [15-Nov-2008] $) $( [2-Aug-2004] $)

  $( Relationship between division and multiplication. $)
  divmul2 $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
              -> ( ( A / C ) = B <-> A = ( C x. B ) ) ) $=
    ( cc wcel cc0 wne wa w3a cdiv co wceq cmul divmul eqcom syl6bb ) ADEBDECDEC
    FGHIACJKBLCBMKZALAQLABCNQAOP $.
    $( [16-Nov-2008] $) $( [7-Feb-2006] $)

  $( Relationship between division and multiplication. $)
  divmul3 $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
              -> ( ( A / C ) = B <-> A = ( B x. C ) ) ) $=
    ( cc wcel cc0 wne wa w3a cdiv co wceq divmul2 mulcom adantrr 3adant1 eqeq2d
    cmul bitr4d ) ADEZBDEZCDEZCFGZHZIZACJKBLACBRKZLABCRKZLABCMUEUGUFAUAUDUGUFLZ
    TUAUBUHUCBCNOPQS $.
    $( [27-Nov-2008] $) $( [13-Feb-2006] $)

  ${
    $d x A $.  $d x B $.
    divcl.1 $e |- A e. CC $.
    divcl.2 $e |- B e. CC $.
    divcl.3 $e |- B =/= 0 $.
    $( Closure law for division. $)
    divcli $p |- ( A / B ) e. CC $=
      ( vx cdiv co cv cmul wceq cc crio divvali wreu wcel ax-cnex riotaclb mpbi
      receui eqeltri ) ABGHBFIJHAKZFLMZLFABCDENUBFLOUCLPFBADCETUBFLQRSUA $.
      $( [24-Nov-2011] $) $( [2-Feb-1995] $)
  $}

  ${
    divclz.1 $e |- A e. CC $.
    divclz.2 $e |- B e. CC $.
    $( Closure law for division. $)
    divclzi $p |- ( B =/= 0 -> ( A / B ) e. CC ) $=
      ( cc0 wne cdiv co cc wcel c1 cif wceq opreq2 eleq1d ax-1cn keepel elimne0
      divcli dedth ) BEFZABGHZIJAUABKLZGHZIJBKBUCMUBUDIBUCAGNOAUCCUABKIDPQBRST
      $.
      $( [7-May-1999] $)
  $}

  $( Closure law for division. $)
  divcl $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) -> ( A / B ) e. CC ) $=
    ( cc wcel cc0 wne cdiv co wi wceq opreq1 eleq1d imbi2d neeq1 opreq2 imbi12d
    cif 0cn elimel divclzi dedth2h 3impia ) ACDZBCDZBEFZABGHZCDZUCUDUEUGIUEUCAE
    QZBGHZCDZIUDBEQZEFZUHUKGHZCDZIABEEAUHJZUGUJUEUOUFUICAUHBGKLMBUKJZUEULUJUNBU
    KENUPUIUMCBUKUHGOLPUHUKAECRSBECRSTUAUB $.
    $( [4-May-2005] $) $( [21-Jul-2001] $)

  ${
    reccl.1 $e |- A e. CC $.
    reccl.2 $e |- A =/= 0 $.
    $( Closure law for reciprocal. $)
    reccli $p |- ( 1 / A ) e. CC $=
      ( c1 ax-1cn divcli ) DAEBCF $.
      $( [30-Apr-2005] $)
  $}

  ${
    recclz.1 $e |- A e. CC $.
    $( Closure law for reciprocal. $)
    recclzi $p |- ( A =/= 0 -> ( 1 / A ) e. CC ) $=
      ( c1 ax-1cn divclzi ) CADBE $.
      $( [30-Apr-2005] $)
  $}

  $( Closure law for reciprocal. $)
  reccl $p |- ( ( A e. CC /\ A =/= 0 ) -> ( 1 / A ) e. CC ) $=
    ( c1 cc wcel cc0 wne cdiv co ax-1cn divcl mp3an1 ) BCDACDAEFBAGHCDIBAJK $.
    $( [30-Apr-2005] $)

  ${
    divcan.1 $e |- A e. CC $.
    divcan.2 $e |- B e. CC $.
    ${
      divcan.3 $e |- B =/= 0 $.
      $( A cancellation law for division. $)
      divcan2i $p |- ( B x. ( A / B ) ) = A $=
        ( cdiv co wceq cmul eqid divcli divmuli mpbi ) ABFGZNHBNIGAHNJABNCDABCD
        EKELM $.
        $( [18-Aug-2008] $) $( [9-Feb-1995] $)

      $( A cancellation law for division. $)
      divcan1i $p |- ( ( A / B ) x. B ) = A $=
        ( cdiv co cmul divcli mulcomi divcan2i eqtri ) ABFGZBHGBMHGAMBABCDEIDJA
        BCDEKL $.
        $( [18-Aug-2008] $) $( [18-May-1999] $)
    $}

    $( A cancellation law for division. $)
    divcan1zi $p |- ( B =/= 0 -> ( ( A / B ) x. B ) = A ) $=
      ( cc0 wne cdiv co cmul wceq c1 opreq2 id opreq12d eqeq1d cc ax-1cn keepel
      cif elimne0 divcan1i dedth ) BEFZABGHZBIHZAJAUCBKSZGHZUFIHZAJBKBUFJZUEUHA
      UIUDUGBUFIBUFAGLUIMNOAUFCUCBKPDQRBTUAUB $.
      $( [18-Aug-2008] $) $( [2-Oct-1999] $)

    $( A cancellation law for division.  We eliminate the third hypothesis of
       ~ divcan2i using the weak deduction theorem ~ dedth and keep the other
       two.  Because the first hypothesis shares the class variable ` B ` with
       the hypothesis we're eliminating, we need to use ~ keepel in order to
       keep the first hypothesis. $)
    divcan2zi $p |- ( B =/= 0 -> ( B x. ( A / B ) ) = A ) $=
      ( cc0 wne cdiv co cmul wceq c1 id opreq2 opreq12d eqeq1d cc ax-1cn keepel
      cif elimne0 divcan2i dedth ) BEFZBABGHZIHZAJUCBKSZAUFGHZIHZAJBKBUFJZUEUHA
      UIBUFUDUGIUILBUFAGMNOAUFCUCBKPDQRBTUAUB $.
      $( [21-Aug-2008] $) $( [10-Aug-1999] $)
  $}

  $( A cancellation law for division. $)
  divcan1 $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 )
        -> ( ( A / B ) x. B ) = A ) $=
    ( cc wcel cc0 wne cdiv co cmul wceq wi cif opreq1 opreq1d id eqeq12d imbi2d
    neeq1 0cn elimel opreq2 opreq12d eqeq1d imbi12d divcan1zi dedth2h 3impia )
    ACDZBCDZBEFZABGHZBIHZAJZUHUIUJUMKUJUHAELZBGHZBIHZUNJZKUIBELZEFZUNURGHZURIHZ
    UNJZKABEEAUNJZUMUQUJVCULUPAUNVCUKUOBIAUNBGMNVCOPQBURJZUJUSUQVBBURERVDUPVAUN
    VDUOUTBURIBURUNGUAVDOUBUCUDUNURAECSTBECSTUEUFUG $.
    $( [18-Aug-2008] $) $( [5-Jun-2004] $)

  $( A cancellation law for division. $)
  divcan2 $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                 ( B x. ( A / B ) ) = A ) $=
    ( cc wcel cc0 wne w3a cdiv co cmul wceq simp2 divcl mulcom syl11anc divcan1
    eqtrd ) ACDZBCDZBEFZGZBABHIZJIZUBBJIZAUASUBCDUCUDKRSTLABMBUBNOABPQ $.
    $( [21-Aug-2008] $) $( [3-Feb-2004] $)

  $( The ratio of nonzero numbers is nonzero. $)
  divne0b $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                  ( A =/= 0 <-> ( A / B ) =/= 0 ) ) $=
    ( cc wcel cc0 wne cdiv co wceq cmul wb mul01 eqeq1d eqcom syl6rbbr 3ad2ant2
    w3a wa 0cn divmul mp3an2 3impb bitr4d necon3bid ) ACDZBCDZBEFZQZAEABGHZEUHA
    EIZBEJHZAIZUIEIZUFUEUJULKUGUFULEAIUJUFUKEABLMAENOPUEUFUGUMULKZUEECDUFUGRUNS
    AEBTUAUBUCUD $.
    $( [1-Jun-2005] $) $( [2-Aug-2004] $)

  $( The ratio of nonzero numbers is nonzero. $)
  divne0 $p |- ( ( ( A e. CC /\ A =/= 0 ) /\ ( B e. CC /\ B =/= 0 ) ) ->
                  ( A / B ) =/= 0 ) $=
    ( cc wcel cc0 wne cdiv co wi w3a divne0b biimpd 3exp com23 com24 imp43 ) AC
    DZAEFZBCDZBEFZABGHEFZQTSRUAQSTRUAIZQSTUBQSTJRUAABKLMNOP $.
    $( [28-Dec-2007] $)

  ${
    divneq0.1 $e |- A e. CC $.
    divneq0.2 $e |- B e. CC $.
    divneq0.3 $e |- A =/= 0 $.
    divneq0.4 $e |- B =/= 0 $.
    $( The ratio of nonzero numbers is nonzero. $)
    divne0i $p |- ( A / B ) =/= 0 $=
      ( cc wcel cc0 wne cdiv co w3a divne0b mpbii mp3an ) AGHZBGHZBIJZABKLIJZCD
      FQRSMAIJTEABNOP $.
      $( [9-Feb-1995] $)
  $}

  ${
    recneq0z.1 $e |- A e. CC $.
    $( The reciprocal of a nonzero number is nonzero. $)
    recne0zi $p |- ( A =/= 0 -> ( 1 / A ) =/= 0 ) $=
      ( cc0 wne c1 cdiv co cif wceq opreq2 neeq1d ax-1cn keepel ax-1ne0 elimne0
      cc divne0i dedth ) ACDZEAFGZCDESAEHZFGZCDAEAUAITUBCAUAEFJKEUALSAEPBLMNAOQ
      R $.
