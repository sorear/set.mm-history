  ${
    recid.1 $e |- A e. CC $.
    recid.2 $e |- A =/= 0 $.
    $( Multiplication of a number and its reciprocal. $)
    recidi $p |- ( A x. ( 1 / A ) ) = 1 $=
      ( c1 ax-1cn divcan2i ) DAEBCF $.
      $( [9-Feb-1995] $)
  $}

  ${
