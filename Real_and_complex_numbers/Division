$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Division
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y z w $.
    $( Define division.  Theorem ~ divmuli relates it to multiplication, and
       ~ divcli and ~ redivcli prove its closure laws. $)
    df-div $a |- / = { <. <. x , y >. , z >. |
                   ( ( x e. CC /\ y e. ( CC \ { 0 } ) ) /\
                   z = ( iota_ w e. CC ( y x. w ) = x ) ) } $.
    $(
    Future simpler alternative?
    df-div $a |- / = { <. <. x , y >. , z >. |
                ( ( y e. CC /\ z e. CC ) /\  x = ( y x. z ) ) } $.
    $)
  $}


  ${
    $d x y z w A $.  $d x y z w B $.
    divval.1 $e |- A e. CC $.
    divval.2 $e |- B e. CC $.
    divval.3 $e |- B =/= 0 $.
    $( Value of division: the (unique) element ` x ` such that
       ` ( B x. x ) = A ` .  This is meaningful only when ` B ` is nonzero. $)
    divvali $p |- ( A / B ) = ( iota_ x e. CC ( B x. x ) = A ) $=
      ( vz vy vw cc wcel cc0 csn cdif cdiv co cv cmul wceq crio wne eldifsn
      mpbir2an riotaex eqeq2 riotabidv opreq1 eqeq1d df-div oprabval2 mp2an )
      BJKCJLMNZKZBCOPCAQZRPZBSZAJTZSDUMCJKCLUACJLUBEFUCGHIBCJULHQZUNRPZGQZSZAJT
      UQOUSBSZAJTUPAJUDUTBSVAVBAJUTBUSUEUFURCSZVBUPAJVCUSUOBURCUNRUGUHUFGHIAUIU
      JUK $.
      $( [24-Nov-2011] $) $( [8-May-1999] $)
  $}

  ${
    $d x y A $.  $d x y B $.  $d x y C $.
    divmul.1 $e |- A e. CC $.
    divmul.2 $e |- B e. CC $.
    divmul.3 $e |- C e. CC $.
    divmul.4 $e |- B =/= 0 $.
    $( Relationship between division and multiplication. $)
    divmuli $p |- ( ( A / B ) = C <-> ( B x. C ) = A ) $=
      ( vx vy cdiv co wceq cv cmul cc crio divvali eqeq1i wcel wreu wb receui
      ax-17 wal wi a1i opreq2 eqeq1d riota2f mp2an bitr4i ) ABJKZCLBHMZNKZALZHO
      PZCLZBCNKZALZULUPCHABDEGQRCOSZUOHOTUSUQUAFHBAEDGUBUOUSHIOCIMCSHUCUSUSHUDU
      EUTUSHUCUFUMCLUNURAUMCBNUGUHUIUJUK $.
      $( [24-Nov-2011] $) $( [2-Feb-1995] $)
  $}

  ${
    divmulz.1 $e |- A e. CC $.
    divmulz.2 $e |- B e. CC $.
    divmulz.3 $e |- C e. CC $.
    $( Relationship between division and multiplication. $)
    divmulzi $p |- ( B =/= 0 -> ( ( A / B ) = C <-> ( B x. C ) = A ) ) $=
      ( cc0 wne cdiv co wceq cmul wb c1 cif opreq2 eqeq1d opreq1 bibi12d cc
      ax1cn keepel elimne0 divmuli dedth ) BGHZABIJZCKZBCLJZAKZMAUFBNOZIJZCKZUK
      CLJZAKZMBNBUKKZUHUMUJUOUPUGULCBUKAIPQUPUIUNABUKCLRQSAUKCDUFBNTEUAUBFBUCUD
      UE $.
      $( [8-May-1999] $)
  $}

  $( Relationship between division and multiplication. $)
  divmul $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
               -> ( ( A / C ) = B <-> ( C x. B ) = A ) ) $=
    ( cc wcel cc0 wne wa cdiv co wceq cmul wb wi cif opreq1 eqeq1d eqeq2
    bibi12d imbi2d neeq1 opreq2 imbi12d 0cn elimel divmulzi dedth3h 3exp com23
    imp4a 3imp ) ADEZBDEZCDEZCFGZHACIJZBKZCBLJZAKZMZULUMUNUOUTULUNUMUOUTNZULUNU
    MVAULUNUMVAUOULAFOZCIJZBKZURVBKZMZNUNCFOZFGZVBVGIJZBKZVGBLJZVBKZMZNVHVIUMBF
    OZKZVGVNLJZVBKZMZNACBFFFAVBKZUTVFUOVSUQVDUSVEVSUPVCBAVBCIPQAVBURRSTCVGKZUOV
    HVFVMCVGFUAVTVDVJVEVLVTVCVIBCVGVBIUBQVTURVKVBCVGBLPQSUCBVNKZVMVRVHWAVJVOVLV
    QBVNVIRWAVKVPVBBVNVGLUBQSTVBVGVNAFDUDUECFDUDUEBFDUDUEUFUGUHUIUJUK $.
    $( [15-Nov-2008] $) $( [2-Aug-2004] $)

  $( Relationship between division and multiplication. $)
  divmul2 $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
              -> ( ( A / C ) = B <-> A = ( C x. B ) ) ) $=
    ( cc wcel cc0 wne wa w3a cdiv co wceq cmul divmul eqcom syl6bb ) ADEBDECDEC
    FGHIACJKBLCBMKZALAQLABCNQAOP $.
    $( [16-Nov-2008] $) $( [7-Feb-2006] $)

  $( Relationship between division and multiplication. $)
  divmul3 $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
              -> ( ( A / C ) = B <-> A = ( B x. C ) ) ) $=
    ( cc wcel cc0 wne wa w3a cdiv co wceq cmul divmul2 mulcom adantrr 3adant1
    eqeq2d bitr4d ) ADEZBDEZCDEZCFGZHZIZACJKBLACBMKZLABCMKZLABCNUEUGUFAUAUDUGUF
    LZTUAUBUHUCBCOPQRS $.
    $( [27-Nov-2008] $) $( [13-Feb-2006] $)

  ${
    $d x A $.  $d x B $.
    divcl.1 $e |- A e. CC $.
    divcl.2 $e |- B e. CC $.
    divcl.3 $e |- B =/= 0 $.
    $( Closure law for division. $)
    divcli $p |- ( A / B ) e. CC $=
      ( vx cdiv co cv cmul wceq cc crio divvali wreu wcel receui axcnex
      riotaclb mpbi eqeltri ) ABGHBFIJHAKZFLMZLFABCDENUBFLOUCLPFBADCEQUBFLRSTUA
      $.
      $( [24-Nov-2011] $) $( [2-Feb-1995] $)
  $}

  ${
    divclz.1 $e |- A e. CC $.
    divclz.2 $e |- B e. CC $.
    $( Closure law for division. $)
    divclzi $p |- ( B =/= 0 -> ( A / B ) e. CC ) $=
      ( cc0 wne cdiv co cc wcel c1 cif wceq opreq2 eleq1d ax1cn keepel elimne0
      divcli dedth ) BEFZABGHZIJAUABKLZGHZIJBKBUCMUBUDIBUCAGNOAUCCUABKIDPQBRST
      $.
      $( [7-May-1999] $)
  $}

  $( Closure law for division. $)
  divcl $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) -> ( A / B ) e. CC ) $=
    ( cc wcel cc0 wne cdiv co wi cif wceq opreq1 eleq1d imbi2d neeq1 opreq2
    imbi12d 0cn elimel divclzi dedth2h 3impia ) ACDZBCDZBEFZABGHZCDZUCUDUEUGIUE
    UCAEJZBGHZCDZIUDBEJZEFZUHUKGHZCDZIABEEAUHKZUGUJUEUOUFUICAUHBGLMNBUKKZUEULUJ
    UNBUKEOUPUIUMCBUKUHGPMQUHUKAECRSBECRSTUAUB $.
