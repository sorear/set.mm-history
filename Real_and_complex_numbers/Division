  $( The ratio of non-zero numbers is non-zero. $)
  divneq0bt $p |- ( ( ( A e. CC /\ B e. CC ) /\ B =/= 0 ) ->
                  ( A =/= 0 <-> ( A / B ) =/= 0 ) ) $=
    ( cc wcel wa cc0 wne wceq wn cdiv co cmulc wb mulzer1t cleq1d cleqcom
