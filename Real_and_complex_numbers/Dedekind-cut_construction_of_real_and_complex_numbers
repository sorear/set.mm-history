  ${
    eqresr.1 $e |- A e. _V $.
    $( Equality of real numbers in terms of intermediate signed reals. $)
    eqresr $p |- ( <. A , 0R >. = <. B , 0R >. <-> A = B ) $=
      ( c0r cop wceq cnr 0r elexi opth eqid mpbiran2 ) ADEBDEFABFDDFADBDCDGHIZM
      JDKL $.
      $( [10-May-1996] $)
  $}

  $( Addition of real numbers in terms of intermediate signed reals. $)
  addresr $p |- ( ( A e. R. /\ B e. R. ) ->
           ( <. A , 0R >. + <. B , 0R >. ) = <. ( A +R B ) , 0R >. ) $=
    ( cnr wcel wa c0r cop caddc co cplr wceq 0r addcnsr an4s 0idsr ax-mp opeq2i
    mpanr12 syl6eq ) ACDZBCDZEZAFGBFGHIZABJIZFFJIZGZUDFGUBFCDZUGUCUFKZLLTUGUAUG
    UHAFBFMNRUEFUDUGUEFKLFOPQS $.
    $( [10-May-1996] $)

  ${
    $( (We don't bother to eliminate the redundant hypothesis.) $)
    mulresr.1 $e |- B e. _V $.
    $( Multiplication of real numbers in terms of intermediate signed reals. $)
    mulresr $p |- ( ( A e. R. /\ B e. R. ) ->
           ( <. A , 0R >. x. <. B , 0R >. ) = <. ( A .R B ) , 0R >. ) $=
      ( cnr wcel wa c0r cop cmul co cmr cm1r cplr wceq 00sr ax-mp opreq2i 0idsr
      0r syl5eq mulcnsr an4s mpanr12 m1r eqtri mulclsr syl mulcomsr opreqan12rd
      elexi syl6eq opeq12d eqtrd ) ADEZBDEZFZAGHBGHIJZABKJZLGGKJZKJZMJZGBKJZAGK
      JZMJZHZURGHUPGDEZVFUQVENZSSUNVFUOVFVGAGBGUAUBUCUPVAURVDGUPVAURGMJZURUTGUR
      MUTLGKJZGUSGLKVFUSGNSGOPQLDEVIGNUDLOPUEQUPURDEVHURNABUFURRUGTUPVDGGMJZGUO
      UNVBGVCGMUOVBBGKJGGBGDSUJCUHBOTAOUIVFVJGNSGRPUKULUM $.
      $( [10-May-1996] $)
  $}

  ${
    $d x y z w A $.  $d x y z w B $.
    ltresr.1 $e |- A e. _V $.
    ltresr.2 $e |- B e. _V $.
    $( Ordering of real subset of complex numbers in terms of signed reals. $)
    ltresr $p |- ( <. A , 0R >. <RR <. B , 0R >. <-> A <R B ) $=
      ( vz vw vx vy c0r cop cltrr cnr wcel wa cltr cr cv wceq wex anbi1d wbr wb
