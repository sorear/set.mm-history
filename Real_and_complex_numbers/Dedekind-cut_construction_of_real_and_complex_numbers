  $( No positive integer is less than one. $)
  nlt1pi $p |- -. A <N 1o $=
    ( cnpi wcel c1o clti wbr wn c0 wne com elni simprbi wceq wa noel wo wb 1pi
    ltpiord mpan2 csuc df-1o eleq2i elsucg syl5bb bitrd biimpa ord mpi ex
    necon3ad mpd elisseti ltrelpi brel simpld con3i pm2.61i ) ABCZADEFZGZUSAHIZ
