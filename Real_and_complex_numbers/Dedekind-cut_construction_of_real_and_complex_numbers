      VAVBVC $.
      $( [3-Sep-1995] $)

    $( Lemma used in lemma showing compatibility of multiplication. $)
    mulcmpblnrlem $p |- ( ( ( A +P. D ) = ( B +P. C ) /\
                            ( F +P. S ) = ( G +P. R ) ) ->
                    ( ( D .P. F ) +P. ( ( ( A .P. F ) +P. ( B .P. G ) ) +P.
                            ( ( C .P. S ) +P. ( D .P. R ) ) ) ) =
                    ( ( D .P. F ) +P. ( ( ( A .P. G ) +P. ( B .P. F ) ) +P.
                            ( ( C .P. R ) +P. ( D .P. S ) ) ) ) ) $=
      ( vx vy vz cpp co wceq wa cmp opreq1 cv visset mulcompr distrpr
      caoprdistrr 3eqtr3g opreq1d oprex addasspr opreq2 opreq2d syl5eq
