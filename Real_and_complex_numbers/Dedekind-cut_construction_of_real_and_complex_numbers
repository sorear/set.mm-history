    $( A non-empty, bounded set of positive signed reals has a supremum. $)
    suppsr $p |- ( ( ( A. x ( x e. A -> 0R <R x ) /\ -. A = (/) ) /\
         E. x ( 0R <R x /\ A. y ( 0R <R y -> ( y e. A -> y <R x ) ) ) ) ->
