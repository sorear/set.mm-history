$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Real and imaginary parts; conjugate
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c Re $. $( Real part of a complex number. $)
  $c Im $. $( Imaginary part of a complex number. $)
  $c * $. $( Complex conjugate function. $)

  $( Extend class notation to include real part of a complex number. $)
  cre $a class Re $.

  $( Extend class notation to include imaginary part of a complex number. $)
  cim $a class Im $.

  $( Extend class notation to include complex conjugate function. $)
  ccj $a class * $.

  ${
    $d x y $.
    $( Define the complex conjugate function.  See ~ cjcli for its closure and
       ~ cjval for its value. $)
    df-cj $a |- * = ( x e. CC |-> ( iota_ y e. CC
                 ( ( x + y ) e. RR /\ ( _i x. ( x - y ) ) e. RR ) ) ) $.

    $( Define a function whose value is the real part of a complex number.  See
       ~ reval for its value, ~ recli for its closure, and ~ replim for its use
       in decomposing a complex number. $)
    df-re $a |- Re = ( x e. CC |-> ( ( x + ( * ` x ) ) / 2 ) ) $.

    $( Define a function whose value is the imaginary part of a complex
       number.  See ~ imval for its value, ~ imcli for its closure, and
       ~ replim for its use in decomposing a complex number. $)
    df-im $a |- Im = ( x e. CC |-> ( Re ` ( x / _i ) ) ) $.
  $}

  ${
    $d x y A $.
    $( The value of the conjugate of a complex number. $)
    cjval $p |- ( A e. CC -> ( * ` A ) = ( iota_ x e. CC
          ( ( A + x ) e. RR /\ ( _i x. ( A - x ) ) e. RR ) ) ) $=
      ( vy cv caddc co cr wcel ci cmin cmul crio ccj wceq opreq1 eleq1d opreq2d
      wa cc anbi12d riotabidv df-cj riotaex fvmpt ) CBCDZADZEFZGHZIUEUFJFZKFZGH
      ZRZASLBUFEFZGHZIBUFJFZKFZGHZRZASLSMUEBNZULURASUSUHUNUKUQUSUGUMGUEBUFEOPUS
      UJUPGUSUIUOIKUEBUFJOQPTUACAUBURASUCUD $.
      $( [6-Nov-2013] $)

    $( The defining property of the complex conjugate. $)
    cjth $p |- ( A e. CC ->
          ( ( A + ( * ` A ) ) e. RR /\ ( _i x. ( A - ( * ` A ) ) ) e. RR ) ) $=
      ( vx cc wcel cv caddc co cr ci cmin cmul wa ccj cfv wsbc crio wceq opreq2
      syl eleq1d wreu cju riota4 cjval dfsbcq mpbird fvex opreq2d anbi12d sbcie
      wb sylib ) ACDZABEZFGZHDZIAUNJGZKGZHDZLZBAMNZOZAVAFGZHDZIAVAJGZKGZHDZLZUM
      VBUTBUTBCPZOZUMUTBCUAVJBAUBUTBCUCSUMVAVIQVBVJUKBAUDUTBVAVIUESUFUTVHBVAAMU
      GUNVAQZUPVDUSVGVKUOVCHUNVAAFRTVKURVFHVKUQVEIKUNVAAJRUHTUIUJUL $.
      $( [6-Nov-2013] $)

    $( Domain and codomain of the conjugate function. $)
    cjf $p |- * : CC --> CC $=
      ( vx vy cv caddc co cr wcel ci cmin cmul wa cc crio wral ccj wreu riotacl
      wf cju syl rgen df-cj fmpt mpbi ) ACZBCZDEFGHUEUFIEJEFGKZBLMZLGZALNLLORUI
      ALUELGUGBLPUIBUESUGBLQTUAALLUHOABUBUCUD $.
      $( [6-Nov-2013] $)

    $( The conjugate of a complex number is a complex number (closure law). $)
    cjcl $p |- ( A e. CC -> ( * ` A ) e. CC ) $=
      ( cc ccj cjf ffvelrni ) BBACDE $.
      $( [6-Nov-2013] $) $( [10-May-1999] $)

    $( The value of the real part of a complex number. $)
    reval $p |- ( A e. CC -> ( Re ` A ) = ( ( A + ( * ` A ) ) / 2 ) ) $=
      ( vx cv ccj cfv caddc co c2 cdiv cc cre fveq2 opreq12 mpdan opreq1d df-re
      wceq oprex fvmpt ) BABCZTDEZFGZHIGAADEZFGZHIGJKTAQZUBUDHIUEUAUCQUBUDQTADL
      TAUAUCFMNOBPUDHIRS $.
      $( [6-Nov-2013] $) $( [9-May-1999] $)

    $( The value of the imaginary part of a complex number. $)
    imval $p |- ( A e. CC -> ( Im ` A ) = ( Re ` ( A / _i ) ) ) $=
      ( vx cv ci cdiv co cre cfv cc cim wceq opreq1 fveq2d df-im fvex fvmpt ) B
      ABCZDEFZGHADEFZGHIJQAKRSGQADELMBNSGOP $.
      $( [6-Nov-2013] $) $( [9-May-1999] $)

    $( The imaginary part of a complex number in terms of the real part
       function. $)
    imre $p |- ( A e. CC -> ( Im ` A ) = ( Re ` ( -u _i x. A ) ) ) $=
      ( cc wcel cim cfv ci cdiv co cre cneg cmul imval cc0 wne wceq ax-icn ine0
      c1 divrec2 mp3an23 irec opreq1i syl6eq fveq2d eqtrd ) ABCZADEAFGHZIEFJZAK
      HZIEALUFUGUIIUFUGRFGHZAKHZUIUFFBCFMNUGUKOPQAFSTUJUHAKUAUBUCUDUE $.
      $( [6-Nov-2013] $) $( [12-May-2005] $)

    $( The real part of a complex number is real. $)
    recl $p |- ( A e. CC -> ( Re ` A ) e. RR ) $=
      ( cc wcel cre cfv ccj caddc co c2 cdiv cr reval cmin cmul simpld rehalfcl
      ci cjth syl eqeltrd ) ABCZADEAAFEZGHZIJHZKALUAUCKCZUDKCUAUEQAUBMHNHKCAROU
      CPST $.
      $( [6-Nov-2013] $) $( [9-May-1999] $)

    $( The imaginary part of a complex number is real. $)
    imcl $p |- ( A e. CC -> ( Im ` A ) e. RR ) $=
      ( cc wcel cim cfv ci cneg cmul co cre cr imre ax-icn negcli ax-mulcl mpan
      recl syl eqeltrd ) ABCZADEFGZAHIZJEZKALTUBBCZUCKCUABCTUDFMNUAAOPUBQRS $.
      $( [6-Nov-2013] $) $( [9-May-1999] $)

    $( Domain and codomain of the real part function.  (Contributed by Paul
       Chapman, 22-Oct-2007.) $)
    ref $p |- Re : CC --> RR $=
      ( vx cv ccj cfv caddc co c2 cdiv cr wcel cc wral reval recl eqeltrrd rgen
      cre wf df-re fmpt mpbi ) ABZUBCDEFGHFZIJZAKLKIQRUDAKUBKJUBQDUCIUBMUBNOPAK
      IUCQASTUA $.
      $( [6-Nov-2013] $) $( [22-Oct-2007] $)

    $( Domain and codomain of the imaginary part function.  (Contributed by
       Paul Chapman, 22-Oct-2007.) $)
    imf $p |- Im : CC --> RR $=
      ( vx cv ci cdiv co cre cfv cr wcel cc wral imval imcl eqeltrrd rgen df-im
      cim wf fmpt mpbi ) ABZCDEFGZHIZAJKJHQRUCAJUAJIUAQGUBHUALUAMNOAJHUBQAPST
      $.
      $( [6-Nov-2013] $) $( [22-Oct-2007] $)

    $( The real part of a complex number representation.  Definition 10-3.1 of
       [Gleason] p. 132. $)
