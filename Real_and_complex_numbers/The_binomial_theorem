  ${
    $d k A $.  $d k N $.
    binom1p.1 $e |- A e. CC $.
    $( Special case of the binomial theorem for ` ( 1 + A ) ^ N ` .
       (Contributed by Paul Chapman, 10-May-2007.) $)
    binom1pi $p |- ( N e. NN0 -> ( ( 1 + A ) ^ N ) =
                  sum_ k e. ( 0 ... N ) ( ( N _C k ) x. ( A ^ k ) ) ) $=
      ( cn0 wcel c1 caddc co cexp cc0 cfz cv cbc cmin cmul csu ax1cn binomi wa
      wceq fznn0sub 1exp syl opreq1d elfznn0 cc expcl mpan mulid2 adantl eqtrd
      opreq2d sumeq2dv ) CEFZGAHICJIKCLIZCBMZNIZGCUQOIZJIZAUQJIZPIZPIZBQUPURVAP
      IZBQGABCRDSUOUPVCVDBUOUQUPFZTZVBVAURPVFVBGVAPIZVAVFUTGVAPVFUSEFUTGUAEUQKC
      UBUSUCUDUEVEVGVAUAZUOVEUQEFZVHUQCUFVIVAUGFZVHAUGFVIVJDAUQUHUIVAUJUDUDUKUL
      UMUNUL $.
      $( [10-May-2007] $)
  $}

  ${
    $d M m $.  $d N j k m $.
    $( Lemma for ~ bcxmas . $)
    bcxmaslem1 $p |- ( A = B -> ( ( N + A ) _C A ) = ( ( N + B ) _C B ) ) $=
      ( caddc co wceq cbc opreq2 opreq12 mpancom ) CADEZCBDEZFABFKAGELBGEFABCDH
      KLABGIJ $.
      $( [18-May-2007] $)

    $( Lemma for ~ bcxmas . $)
    bcxmaslem2 $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
                          ( A + ( B + C ) ) = ( ( A + C ) + B ) ) $=
      ( cc wcel w3a caddc co addass add32 eqtr3d ) ADEBDECDEFABGHCGHABCGHGHACGH
      BGHABCIABCJK $.
      $( [18-May-2007] $)

    $( Parallel summation (Christmas Stocking) theorem for Pascal's Triangle.
       (Contributed by Paul Chapman, 18-May-2007.) $)
    bcxmas $p |- ( ( N e. NN0 /\ M e. NN0 ) ->
                     ( ( ( N + 1 ) + M ) _C M ) =
                     sum_ j e. ( 0 ... M ) ( ( N + j ) _C j ) ) $=
      ( vm vk cn0 wcel c1 caddc co cbc cc0 cfz cv csu wceq wi bcxmaslem1
      opreq2 sumeq1d eqeq12d imbi2d 0nn0 wa nn0addcl bcn0 syl mpan2 cz 1nn0
      syl6eqel 0z fsum1i sylancl peano2nn0 jctir 3syl 3eqtr4rd cuz cfv simpr
      elnn0uz sylib oprex fsump1i cc nn0cn adantr adantl ax1cn a1i bcxmaslem2
      syl111anc opreq1d opreq2d eqtrd opreq1 cmin pncan cn cle wbr nn0p1nn
      nnnn0addcl sylan cr nnre simpl nn0addge2 syl11anc breqtrd bcpasc2 eqtr3d
      nnnn0 nnz bccl nn0z addcom peano2cn addass 3eqtr3d 3eqtr2rd ex expcom
      a2d nn0ind impcom ) BFGCFGZCHIJZBIJBKJZLBMJZCANZIJZXLKJZAOZPZXHXIDNZIJXQK
