  ${
    $d j m M $.  $d j k m N $.
    $( Lemma for ~ bcxmas . $)
    bcxmaslem1 $p |- ( A = B -> ( ( N + A ) _C A ) = ( ( N + B ) _C B ) ) $=
      ( wceq caddc co cbc opreq2 id opreq12d ) ABDZCAEFCBEFABGABCEHKIJ $.
      $( [18-May-2007] $)

    $( Lemma for ~ bcxmas . $)
    bcxmaslem2 $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
                          ( A + ( B + C ) ) = ( ( A + C ) + B ) ) $=
      ( cc wcel w3a caddc co addass add32 eqtr3d ) ADEBDECDEFABGHCGHABCGHGHACGH
      BGHABCIABCJK $.
      $( [18-May-2007] $)

    $( Parallel summation (Christmas Stocking) theorem for Pascal's Triangle.
       (Contributed by Paul Chapman, 18-May-2007.) $)
    bcxmas $p |- ( ( N e. NN0 /\ M e. NN0 ) ->
                     ( ( ( N + 1 ) + M ) _C M ) =
                     sum_ j e. ( 0 ... M ) ( ( N + j ) _C j ) ) $=
      ( cn0 wcel c1 caddc co cbc cc0 cfz csu wceq wi bcxmaslem1 wa syl cc nn0cn
      adantl vm vk cv opreq2 sumeq1d eqeq12d imbi2d 0nn0 nn0addcl bcn0 mpan2 cz
      1nn0 syl6eqel fsum1 sylancr peano2nn0 jctir 3syl 3eqtr4rd cuz cfv elnn0uz
      0z simpr sylib simpl elfznn0 syl2an elfzelz bccl fsump1 adantr ax-1cn a1i
      syl2anc bcxmaslem2 syl3anc opreq1d opreq2d eqtrd cmin pncan sylancl sylan
      opreq1 cn nn0p1nn nnz bcpasc eqtr3d nnnn0addcl nnnn0 nn0z addcom peano2cn
      addass 3eqtr3d 3eqtr2rd ex expcom a2d nn0ind impcom ) BDECDEZCFGHZBGHBIHZ
