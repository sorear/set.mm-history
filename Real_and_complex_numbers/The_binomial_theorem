    $( Lemma for ~ bcxmas . $)
    bcxmaslem2 $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
                          ( A + ( B + C ) ) = ( ( A + C ) + B ) ) $=
      ( cc wcel w3a caddc co addass add23 eqtr3d ) ADEBDECDEFABGHCGHABCGHGHACGH
      BGHABCIABCJK $.
      $( [18-May-2007] $) $( [18-May-2007] $)

