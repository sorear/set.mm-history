      QKCUEUSUFRUGVEVGVAQZUOVEUQEFZVHUQCUHVIVASFZVHASFVIVJDAUQUIUJVAUKRRULTUMUN
      T $.
      $( [10-May-2007] $)
  $}

  ${
    $d M m $.  $d N j k m $.
    $( Lemma for ~ bcxmas . $)
    bcxmaslem1 $p |- ( A = B -> ( ( N + A ) _C A ) = ( ( N + B ) _C B ) ) $=
      ( caddc co wceq cbc opreq2 opreq12 mpancom ) CADEZCBDEZFABFKAGELBGEFABCDH
      KLABGIJ $.
      $( [18-May-2007] $)

    $( Lemma for ~ bcxmas . $)
    bcxmaslem2 $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
                          ( A + ( B + C ) ) = ( ( A + C ) + B ) ) $=
      ( cc wcel w3a caddc co addass add32 eqtr3d ) ADEBDECDEFABGHCGHABCGHGHACGH
      BGHABCIABCJK $.
      $( [18-May-2007] $)

    $( Parallel summation (Christmas Stocking) theorem for Pascal's Triangle.
       (Contributed by Paul Chapman, 18-May-2007.) $)
    bcxmas $p |- ( ( N e. NN0 /\ M e. NN0 ) ->
                     ( ( ( N + 1 ) + M ) _C M ) =
                     sum_ j e. ( 0 ... M ) ( ( N + j ) _C j ) ) $=
      ( cn0 wcel c1 caddc co cbc cc0 cfz csu wceq bcxmaslem1 opreq2 sumeq1d syl
      wi wa cc vm vk cv eqeq12d imbi2d 0nn0 nn0addcl bcn0 mpan2 syl6eqel fsum1i
      cz 1nn0 sylancl peano2nn0 jctir 3syl 3eqtr4rd cuz cfv simpr elnn0uz sylib
      oprex fsump1i nn0cn adantr adantl ax-1cn a1i bcxmaslem2 syl111anc opreq1d
      0z opreq2d eqtrd opreq1 pncan sylan cn nn0p1nn nnz bcpasc syl11anc eqtr3d
      cmin nnnn0addcl nnnn0 bccl addcom peano2cn addass 3eqtr3d 3eqtr2rd expcom
      nn0z ex a2d nn0ind impcom ) BDECDEZCFGHZBGHBIHZJBKHZCAUCZGHZXEIHZALZMZXAX
