$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        The exponential, sine, and cosine functions
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c exp $. $( Exponential function. $)
  $c _e $. $( Letter e (Euler's constant = 2.71828...) $)
  $c sin $. $( Sine function. $)
  $c cos $. $( Cosine function. $)
  $c pi $. $( Pi = 3.14159... $)

  $( Extend class notation to include the exponential function. $)
  ce $a class exp $.

  $( Extend class notation to include Euler's constant = 2.7182818.... $)
  ceu $a class _e $.

  $( Extend class notation to include the sine function. $)
  csin $a class sin $.

  $( Extend class notation to include the cosine function. $)
  ccos $a class cos $.

  $( Extend class notation to include pi = 3.14159.... $)
  cpi $a class pi $.

  ${
    $d j k x y w $.
    $( Define the exponential function. $)
    df-ef $a |- exp = { <. x , y >. | ( x e. CC /\ y =
                  sum_ k e. NN0 ( ( x ^ k ) / ( ! ` k ) ) ) } $.

    $( Define Euler's constant 2.71828.... $)
    df-e $a |- _e = ( exp ` 1 ) $.

    $( Define the sine function. $)
    df-sin $a |- sin = { <. x , y >. | ( x e. CC /\ y =
 ( ( ( exp ` ( _i x. x ) ) - ( exp ` ( -u _i x. x ) ) ) / ( 2 x. _i ) ) ) } $.

$(
    @( Define the cosine function. @)
    df-cos @a |- cos =
        { <. x , y >. | ( x e. CC /\ y = ( sin ` ( x + ( pi / 2 ) ) ) ) } @.
$)

    $( Define the cosine function. $)
    df-cos $a |- cos = { <. x , y >. | ( x e. CC /\ y =
        ( ( ( exp ` ( _i x. x ) ) + ( exp ` ( -u _i x. x ) ) ) / 2 ) ) } $.

    $( Define pi = 3.14159..., which is the smallest positive number whose sine
       is zero.  Definition of pi in [Gleason] p. 311.  (We use the inverse of
       of less-than, " ` ``' < ` ", to turn supremum into infimum; currently we
       don't have infimum defined separately.) $)
    df-pi $a |- pi =
          sup ( { x e. RR | ( 0 < x /\ ( sin ` x ) = 0 ) } , RR , `' < ) $.
  $}

  $( Closure of a term in the series expansion of the exponential function.
     (Contributed by Paul Chapman, 11-Sep-2007.) $)
  eftcl $p |- ( ( A e. CC /\ K e. NN0 ) ->
                 ( ( A ^ K ) / ( ! ` K ) ) e. CC ) $=
    ( cexp co cc wcel cfa cfv cc0 wne cdiv cn0 wa divcl expcl cn faccl nncn
    syl adantl facne0 syl3anc ) ABCDZEFBGHZEFZUDIJZUCUDKDEFAEFZBLFZMUCUDNABOUHU
    EUGUHUDPFUEBQUDRSTUHUFUGBUATUB $.
    $( [13-Sep-2007] $) $( [11-Sep-2007] $)

  ${
