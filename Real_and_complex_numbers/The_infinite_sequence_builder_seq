      serf1o.9 $e |- ( ph -> A. g A. f
                       ( ( f : ( M ... N ) -1-1-onto-> ( M ... N ) /\
                           g : ( M ... N ) --> S ) ->
              ( seq M ( P , ( g o. f ) ) ` N ) = ( seq M ( P , g ) ` N ) ) ) $.
