      UHUI $.
      $( [4-Sep-2013] $)

    $( Equality theorem for the sequence builder operation. $)
    seqeq1 $p |- ( M = N -> seq M ( P , F ) = seq N ( P , F ) ) $=
      ( vx vy wceq cvv cv c1 caddc co cfv cop cmpt2 crdg com cima cseq df-seq
      fveq2 opeq12 mpdan rdgeq2 syl imaeq1d 3eqtr4g ) CDGZEFHHEIJKLZFIUIBMALNOZ
