$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Factorial function
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ! $. $( Factorial. $)

  $( Extend class notation to include the factorial of nonnegative integers. $)
  cfa $a class ! $.

  $( Define the factorial function on nonnegative integers.  In the literature,
     the factorial function is written as a postscript exclamation point. $)
  df-fac $a |- ! = ( { <. 0 , 1 >. } u. seq 1 ( x. , _I ) ) $.

  $( Value of the factorial function for positive integers.  (Revised by Mario
     Carneiro, 13-Jul-2013.) $)
  facnn $p |- ( N e. NN ->
      ( ! ` N ) = ( seq 1 ( x. , _I ) ` N ) ) $=
    ( cfa cfv cmul cid c1 cseq wceq cn0 cc0 csn cdif cn 0nn0 elexi 1nn cun cres
    dfn2 eqtr3i cop df-fac cuz nnuz reseq2i cz wcel wfn 1z seqfn fnresdm uneq2i
    mp2b eqtr4i fvsnun2 eleq2s ) ABCADEFGZCHAIJKLZMJFIAUQBJINOFMPOBJFUAKZUQQUSU
    QURRZQUBUTUQUSUQFUCCZRZUTUQVAURUQMVAURUDSTUEFUFUGUQVAUHVBUQHUIDEFUJVAUQUKUM
    TULUNUOSUP $.
    $( [13-Jul-2013] $) $( [2-Dec-2004] $)

  $( The factorial of 0.  (Revised by Mario Carneiro, 13-Jul-2013.) $)
  fac0 $p |- ( ! ` 0 ) = 1 $=
    ( cc0 cn0 cmul cid cseq cfa 0nn0 elexi 1nn cop csn cun cdif cres df-fac cuz
    c1 cn cfv eqtr3i nnuz dfn2 reseq2i cz wcel wfn wceq 1z seqfn fnresdm uneq2i
    mp2b eqtr4i fvsnun1 ) AQBCDQEZFABGHQRIHFAQJKZUOLUPUOBAKMZNZLOURUOUPUOQPSZNZ
    URUOUSUQUORUSUQUAUBTUCQUDUEUOUSUFUTUOUGUHCDQUIUSUOUJULTUKUMUN $.
    $( [13-Jul-2013] $) $( [2-Dec-2004] $)

  $( The factorial of 1.  (Revised by Mario Carneiro, 13-Jul-2013.) $)
  fac1 $p |- ( ! ` 1 ) = 1 $=
    ( c1 cfa cfv cmul cid cseq cn wcel wceq 1nn facnn ax-mp cz seq1 fvi 3eqtri
    1z ) ABCZADEAFCZAECZAAGHZRSIJAKLAMHSTIQDEANLUATAIJAGOLP $.
    $( [13-Jul-2013] $) $( [2-Dec-2004] $)

  $( The factorial of a successor.  (Revised by Mario Carneiro,
     13-Jul-2013.) $)
  facp1 $p |- ( N e. NN0 ->
               ( ! ` ( N + 1 ) ) = ( ( ! ` N ) x. ( N + 1 ) ) ) $=
    ( cn0 wcel cn cc0 wceq wo c1 caddc co cfa cfv cmul elnn0 cid cseq facnn cvv
    ax-1cn eqtri peano2nn syl cuz seqp1 nnuz eleq2s opreq1d oprex ax-mp opreq2i
    fvi syl6reqr 3eqtrd addid2i fveq2i fac1 opreq1 fveq2 opreq12d fac0 opreq12i
    fveq2d mulid1i syl6eq 3eqtr4a jaoi sylbi ) ABCADCZAEFZGAHIJZKLZAKLZVJMJZFZA
    NVHVNVIVHVKVJMOHPZLZAVOLZVJOLZMJZVMVHVJDCVKVPFAUAVJQUBVPVSFAHUCLDMOHAUDUEUF
    VHVMVQVJMJVSVHVLVQVJMAQUGVRVJVQMVJRCVRVJFAHIUHVJRUKUIUJULUMVIEHIJZKLZHVKVMW
    AHKLHVTHKHSUNZUOUPTVIVJVTKAEHIUQZVBVIVMEKLZVTMJZHVIVLWDVJVTMAEKURWCUSWEHHMJ
    HWDHVTHMUTWBVAHSVCTVDVEVFVG $.
    $( [13-Jul-2013] $) $( [2-Dec-2004] $)

  $( The factorial of 2. $)
  fac2 $p |- ( ! ` 2 ) = 2 $=
    ( c2 cfa cfv c1 caddc co cmul df-2 fveq2i wcel wceq 1nn0 facp1 ax-mp eqcomi
    cn0 fac1 opreq12i 2cn mulid2i eqtri 3eqtri ) ABCDDEFZBCZDBCZUCGFZAAUCBHIDPJ
    UDUFKLDMNUFDAGFAUEDUCAGQAUCHORASTUAUB $.
    $( [17-Mar-2005] $)

  $( The factorial of 3. $)
  fac3 $p |- ( ! ` 3 ) = 6 $=
    ( c3 cfa cfv c2 c1 caddc co cmul df-3 fveq2i cn0 wcel wceq 2nn0 facp1 ax-mp
    c6 fac2 eqcomi 3eqtri opreq12i 2cn 3re recni mulcomi 3t2e6 ) ABCDEFGZBCZDBC
    ZUGHGZQAUGBIJDKLUHUJMNDOPUJDAHGADHGQUIDUGAHRAUGISUADAUBAUCUDUEUFTT $.
    $( [17-Mar-2005] $)

  $( Value of the factorial function expressed recursively. $)
  facnn2 $p |- ( N e. NN ->
               ( ! ` N ) = ( ( ! ` ( N - 1 ) ) x. N ) ) $=
    ( cn wcel cc c1 cmin co cn0 wa cfa cfv cmul wceq elnnnn0 caddc facp1 adantl
    ax-1cn npcan adantr mpan2 fveq2d opreq2d 3eqtr3d sylbi ) ABCADCZAEFGZHCZIZA
    JKZUGJKZALGZMANUIUGEOGZJKZUKUMLGZUJULUHUNUOMUFUGPQUFUNUJMUHUFUMAJUFEDCUMAMR
    AESUAZUBTUFUOULMUHUFUMAUKLUPUCTUDUE $.
    $( [2-Dec-2004] $)

  ${
    $d j k N $.
    $( Closure of the factorial function. $)
    faccl $p |- ( N e. NN0 -> ( ! ` N ) e. NN ) $=
      ( vj vk cv cfa cfv cn wcel cc0 c1 caddc co wceq fveq2 eleq1d fac0 eqeltri
      1nn cn0 wa cmul facp1 adantl nn0p1nn nnmulcl sylan2 eqeltrd expcom nn0ind
      ) BDZEFZGHIEFZGHCDZEFZGHZUMJKLZEFZGHZAEFZGHBCAUJIMUKULGUJIENOUJUMMUKUNGUJ
      UMENOUJUPMUKUQGUJUPENOUJAMUKUSGUJAENOULJGPRQUOUMSHZURUOUTTUQUNUPUALZGUTUQ
      VAMUOUMUBUCUTUOUPGHVAGHUMUDUNUPUEUFUGUHUI $.
      $( [2-Dec-2004] $)
  $}

  $( The factorial function is nonzero. $)
  facne0 $p |- ( N e. NN0 -> ( ! ` N ) =/= 0 ) $=
    ( cn0 wcel cfa cfv cn cc0 wne faccl nnne0 syl ) ABCADEZFCLGHAILJK $.
    $( [26-Apr-2005] $)

  ${
    $d j M $.  $d j k N $.
    $( A natural number divides the factorial of an equal or larger number. $)
    facdiv $p |- ( ( M e. NN0 /\ N e. NN /\ N <_ M ) ->
                  ( ( ! ` M ) / N ) e. NN ) $=
      ( cn0 wcel cn cle wbr cfa cfv cdiv co cc0 wceq breq2 fveq2 opreq1d eleq1d
      wi imbi12d imbi2d vj vk cv c1 caddc clt wn nngt0 cr wb nnre ltnle sylancr
      0re mpbid pm2.21d wa cmul wo peano2nn0 nn0re leloe syl2an nnnn0 nn0leltp1
      syl sylan nn0p1nn nnmulcl sylan2 expcom adantl wne faccl nncn nn0cn nnne0
      cc jca adantr div23 syl3anc sylibrd imim2d sylbird divcan4 eqeltrd opreq2
      com23 syl5ibcom a1dd jaod sylbid com34 com12 imp4d facp1 exp4d a2d nn0ind
      ex 3imp ) ACDBEDZBAFGZAHIZBJKZEDZXCBUAUCZFGZXHHIZBJKZEDZRZRXCBLFGZLHIZBJK
      ZEDZRZRXCBUBUCZFGZXSHIZBJKZEDZRZRXCBXSUDUEKZFGZYEHIZBJKZEDZRZRXCXDXGRZRUA
