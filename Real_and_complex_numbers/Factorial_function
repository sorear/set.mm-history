$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Factorial function
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ! $. $( Factorial. $)

  $( Extend class notation to include the factorial of nonnegative integers. $)
  cfa $a class ! $.

  ${
    $d x y $.
    $( Define the factorial function on nonnegative integers.  In the
       literature, the factorial function is written as a postscript
       exclamation point. $)
    df-fac $a |- ! = ( { <. 0 , 1 >. } u. ( x. seq1 ( I |` NN ) ) ) $.
  $}

  $( Value of the factorial function for positive integers. $)
  facnnt $p |- ( N e. NN -> ( ! ` N ) = ( ( x. seq1 ( I |` NN ) ) ` N ) ) $=
    ( cn wcel cn0 cc0 csn cdif cfa cfv cmul cid cres cseq1 co wceq dfn2 eleq2i
    c1 0nn0 elisseti 1nn cop cun df-fac reseq2 ax-mp wfn mulex cvv nnex
    resiexg seq1fn fnresdm eqtr3 uneq2i eqtr4 fvsnun2 sylbi ) ABCADEFGZCAHIAJKB
    LZMNZIOBUSAPQERDAVAHEDSTRBUATHERUBFZVAUCVBVAUSLZUCUDVCVAVBVABLZVCVABUSOVDVC
    OPBUSVAUEUFVABUGVDVAOJUTUHBUICUTUICUJBUIUKUFULBVAUMUFUNUOUPUQUR $.
    $( [3-Dec-2004] $) $( [2-Dec-2004] $)

  $( The factorial of 0. $)
  fac0 $p |- ( ! ` 0 ) = 1 $=
    ( cc0 c1 cn0 cmul cid cn cres cseq1 co cfa 0nn0 elisseti 1nn cop csn cun
    cdif df-fac wfn wceq mulex cvv wcel nnex resiexg ax-mp seq1fn fnresdm dfn2
    reseq2 eqtr3 uneq2i eqtr fvsnun1 ) ABCDEFGZHIZJACKLBFMLJABNOZUPPUQUPCAOQZGZ
    PRUPUSUQUPFGZUPUSUPFSUTUPTDUOUAFUBUCUOUBUCUDFUBUEUFUGFUPUHUFFURTUTUSTUIFURU
    PUJUFUKULUMUN $.
    $( [3-Dec-2004] $) $( [2-Dec-2004] $)

  $( The factorial of 1. $)
  fac1 $p |- ( ! ` 1 ) = 1 $=
    ( c1 cfa cfv cmul cid cn cres cseq1 co wcel wceq 1nn facnnt ax-mp mulex
    wfun cvv funi nnex resfunexg mp2an seq11 fvres fvi eqtr 3eqtr ) ABCZADEFGZH
    ICZAUHCZAAFJZUGUIKLAMNDUHOEPFQJUHQJRSEFQTUAUBUJAECZAUKUJULKLAFEUCNUKULAKLAF
    UDNUEUF $.
    $( [3-Dec-2004] $) $( [2-Dec-2004] $)

  $( The factorial of a successor. $)
  facp1t $p |- ( N e. NN0 ->
               ( ! ` ( N + 1 ) ) = ( ( ! ` N ) x. ( N + 1 ) ) ) $=
    ( cn0 wcel cn cc0 wceq wo c1 caddc co cfa cfv cmul elnn0 cid cres cseq1
    peano2nn facnnt syl mulex wfun cvv funi nnex resfunexg mp2an seq1p1 fvres
    oprex fvi ax-mp syl6eq opreq2d opreq1d eqtr4d 3eqtrd ax1cn addid2 fveq2i
    fac1 eqtr opreq1 fveq2d fveq2 opreq12d fac0 opreq12i mulid1 3eqtr4a jaoi
    sylbi ) ABCADCZAEFZGAHIJZKLZAKLZVOMJZFZANVMVSVNVMVPVOMODPZQJZLZAWALZVOVTLZM
    JZVRVMVODCZVPWBFARZVOSTAMVTUAOUBDUCCVTUCCUDUEODUCUFUGUHVMWEWCVOMJVRVMWDVOWC
    MVMWDVOOLZVOVMWFWDWHFWGVODOUITVOUCCWHVOFAHIUJVOUCUKULUMUNVMVQWCVOMASUOUPUQV
