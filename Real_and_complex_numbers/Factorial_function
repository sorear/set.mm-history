$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Factorial function
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ! $. $( Factorial. $)

  $( Extend class notation to include the factorial of nonnegative integers. $)
  cfa $a class ! $.

  $( Define the factorial function on nonnegative integers.  In the literature,
     the factorial function is written as a postscript exclamation point. $)
  df-fac $a |- ! = ( { <. 0 , 1 >. } u. seq 1 ( x. , _I ) ) $.

  $( Value of the factorial function for positive integers.  (Revised by Mario
     Carneiro, 13-Jul-2013.) $)
  facnn $p |- ( N e. NN ->
      ( ! ` N ) = ( seq 1 ( x. , _I ) ` N ) ) $=
    ( cfa cfv cmul cid c1 cseq wceq cn0 cc0 csn cdif cn 0nn0 elisseti 1nn cop
    cun cres df-fac cuz nnuz dfn2 eqtr3i reseq2i cz wcel wfn 1z seqfn fnresdm
    mp2b uneq2i eqtr4i fvsnun2 eleq2s ) ABCADEFGZCHAIJKLZMJFIAUQBJINOFMPOBJFQKZ
    UQRUSUQURSZRTUTUQUSUQFUACZSZUTUQVAURUQMVAURUBUCUDUEFUFUGUQVAUHVBUQHUIDEFUJV
    AUQUKULUDUMUNUOUCUP $.
    $( [13-Jul-2013] $) $( [2-Dec-2004] $)

  $( The factorial of 0.  (Revised by Mario Carneiro, 13-Jul-2013.) $)
  fac0 $p |- ( ! ` 0 ) = 1 $=
    ( cc0 c1 cn0 cmul cid cseq cfa 0nn0 elisseti cn 1nn cop csn cun cdif cres
    df-fac cuz cfv nnuz dfn2 eqtr3i reseq2i cz wcel wfn wceq 1z seqfn fnresdm
    mp2b uneq2i eqtr4i fvsnun1 ) ABCDEBFZGACHIBJKIGABLMZUONUPUOCAMOZPZNQURUOUPU
    OBRSZPZURUOUSUQUOJUSUQTUAUBUCBUDUEUOUSUFUTUOUGUHDEBUIUSUOUJUKUBULUMUN $.
    $( [13-Jul-2013] $) $( [2-Dec-2004] $)

  $( The factorial of 1.  (Revised by Mario Carneiro, 13-Jul-2013.) $)
  fac1 $p |- ( ! ` 1 ) = 1 $=
    ( c1 cfa cfv cmul cid cseq cn wcel wceq 1nn facnn ax-mp cz 1z seq1NEW fvi
    3eqtri ) ABCZADEAFCZAECZAAGHZRSIJAKLAMHSTINDEAOLUATAIJAGPLQ $.
    $( [13-Jul-2013] $) $( [2-Dec-2004] $)

  $( The factorial of a successor.  (Revised by Mario Carneiro,
     13-Jul-2013.) $)
  facp1 $p |- ( N e. NN0 ->
               ( ! ` ( N + 1 ) ) = ( ( ! ` N ) x. ( N + 1 ) ) ) $=
    ( cn0 wcel cn cc0 wceq wo c1 caddc co cfa cfv cmul elnn0 cid cseq peano2nn
    facnn syl cuz seqp1 nnuz eleq2s opreq1d cvv oprex fvi ax-mp opreq2i
    syl6reqr 3eqtrd ax-1cn addid2i fveq2i fac1 eqtri opreq1 fveq2d fveq2
    opreq12d fac0 opreq12i mulid1i syl6eq 3eqtr4a jaoi sylbi ) ABCADCZAEFZGAHIJ
    ZKLZAKLZVJMJZFZANVHVNVIVHVKVJMOHPZLZAVOLZVJOLZMJZVMVHVJDCVKVPFAQVJRSVPVSFAH
    TLDMOHAUAUBUCVHVMVQVJMJVSVHVLVQVJMARUDVRVJVQMVJUECVRVJFAHIUFVJUEUGUHUIUJUKV
    IEHIJZKLZHVKVMWAHKLHVTHKHULUMZUNUOUPVIVJVTKAEHIUQZURVIVMEKLZVTMJZHVIVLWDVJV
    TMAEKUSWCUTWEHHMJHWDHVTHMVAWBVBHULVCUPVDVEVFVG $.
    $( [13-Jul-2013] $) $( [2-Dec-2004] $)

  $( The factorial of 2. $)
  fac2 $p |- ( ! ` 2 ) = 2 $=
    ( c2 cfa cfv c1 caddc co cmul df-2 fveq2i cn0 wcel wceq 1nn0 facp1 ax-mp
    fac1 eqcomi opreq12i 2cn mulid2i eqtri 3eqtri ) ABCDDEFZBCZDBCZUCGFZAAUCBHI
    DJKUDUFLMDNOUFDAGFAUEDUCAGPAUCHQRASTUAUB $.
    $( [17-Mar-2005] $)

  $( The factorial of 3. $)
  fac3 $p |- ( ! ` 3 ) = 6 $=
    ( c3 cfa cfv c2 c1 caddc co cmul c6 df-3 fveq2i cn0 wcel wceq 2nn0 facp1
    ax-mp fac2 eqcomi opreq12i 2cn 3re recni mulcomi 3t2e6 3eqtri ) ABCDEFGZBCZ
    DBCZUGHGZIAUGBJKDLMUHUJNODPQUJDAHGADHGIUIDUGAHRAUGJSTDAUAAUBUCUDUEUFUF $.
    $( [17-Mar-2005] $)

  $( Value of the factorial function expressed recursively. $)
  facnn2 $p |- ( N e. NN ->
               ( ! ` N ) = ( ( ! ` ( N - 1 ) ) x. N ) ) $=
    ( cn wcel cc c1 cmin co cn0 wa cfa cfv cmul wceq elnnnn0 caddc facp1
    adantl ax-1cn npcan mpan2 fveq2d adantr opreq2d 3eqtr3d sylbi ) ABCADCZAEFG
    ZHCZIZAJKZUGJKZALGZMANUIUGEOGZJKZUKUMLGZUJULUHUNUOMUFUGPQUFUNUJMUHUFUMAJUFE
    DCUMAMRAESTZUAUBUFUOULMUHUFUMAUKLUPUCUBUDUE $.
    $( [2-Dec-2004] $)

  ${
    $d j k N $.
    $( Closure of the factorial function. $)
    faccl $p |- ( N e. NN0 -> ( ! ` N ) e. NN ) $=
      ( vj vk cv cfa cfv cn wcel cc0 c1 caddc co wceq fveq2 eleq1d fac0 1nn
      eqeltri cn0 wa cmul facp1 adantl nn0p1nn nnmulcl sylan2 eqeltrd expcom
      nn0ind ) BDZEFZGHIEFZGHCDZEFZGHZUMJKLZEFZGHZAEFZGHBCAUJIMUKULGUJIENOUJUMM
      UKUNGUJUMENOUJUPMUKUQGUJUPENOUJAMUKUSGUJAENOULJGPQRUOUMSHZURUOUTTUQUNUPUA
      LZGUTUQVAMUOUMUBUCUTUOUPGHVAGHUMUDUNUPUEUFUGUHUI $.
      $( [2-Dec-2004] $)
  $}

  $( The factorial function is nonzero. $)
  facne0 $p |- ( N e. NN0 -> ( ! ` N ) =/= 0 ) $=
    ( cn0 wcel cfa cfv cn cc0 wne faccl nnne0 syl ) ABCADEZFCLGHAILJK $.
    $( [26-Apr-2005] $)

  ${
    $d j M $.  $d j k N $.
    $( A natural number divides the factorial of an equal or larger number. $)
    facdiv $p |- ( ( M e. NN0 /\ N e. NN /\ N <_ M ) ->
                  ( ( ! ` M ) / N ) e. NN ) $=
      ( vj vk cn0 wcel cn cle wbr cfa cfv cdiv co cv wi cc0 c1 caddc wceq
