  $( No natural number (greater than one) divides the factorial plus one of an
     equal or larger number. $)
  facndiv $p |- ( ( ( M e. NN0 /\ N e. NN ) /\ ( 1 < N /\ N <_ M ) ) ->
                -. ( ( ( ! ` M ) + 1 ) / N ) e. ZZ ) $=
    ( cn0 wcel cn wa c1 clt wbr cle cfa cfv caddc co cdiv cz wn cr nnre recnz
    sylan ad2ant2lr cmin zsubcl ex facdiv 3expa nnz syl adantrl syl5com cc cc0
    wne wceq faccl nncn peano2cn ad2antrr nnne0 jca ad2antlr divsubdir
    syl111anc ax1cn pncan2 sylancl opreq1d eqtr3d eleq1d sylibd mtod ) ACDZBEDZ
    FZGBHIZBAJIZFZFZAKLZGMNZBONZPDZGBONZPDZVNVPWEQZVMVQVNBRDVPWFBSBTUAUBVSWCWBV
