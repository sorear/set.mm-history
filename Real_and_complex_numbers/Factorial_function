$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Factorial function
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ! $. $( Factorial. $)

  $( Extend class notation to include the factorial of nonnegative integers. $)
  cfa $a class ! $.

  ${
    $d x y $.
    $( Define the factorial function on nonnegative integers.  In the
       literature, the factorial function is written as a postscript
       exclamation point. $)
    df-fac $a |- ! = ( ( x. seq1 ( I |` NN ) ) u. { <. 0 , 1 >. } ) $.
  $}

  $( Value of the factorial function for positive integers. $)
  facnnt $p |- ( N e. NN -> ( ! ` N ) = ( ( x. seq1 ( I |` NN ) ) ` N ) ) $=
    ( cn wcel cfa cres cfv cmul cid cseq1 co wceq cc0 c1 cop csn cun c0 df-fac
    reseq1 ax-mp resundir cin incom wn 0nnn disjsn mpbir eqtr wfn wb wf1o cn0
    0nn0 elisseti 1nn f1osn f1ofn fnresdisj mpbi uneq2i un0 3eqtr fveq1i a1i
    fvres 3eqtr3d ) ABCZADBEZFZAGHBEIJZBEZFZADFAVJFVIVLKVGAVHVKVHVJLMNOZPZBEZVK
    QPZVKDVNKVHVOKRDVNBSTVOVKVMBEZPVPVJVMBUAVQQVKLOZBUBZQKZVQQKZVSBVRUBZQVRBUCW
    BQKLBCUDUEBLUFUGUHVMVRUIZVTWAUJVRMOZVMUKWCLMLULUMUNMBUOUNUPVRWDVMUQTVRBVMUR
    TUSUTUHVKVAVBVCVDABDVEABVJVEVF $.
    $( [3-Dec-2004] $) $( [2-Dec-2004] $)

  $( The factorial of 0. $)
  fac0 $p |- ( ! ` 0 ) = 1 $=
    ( cc0 cfa csn cres cfv c1 cop cmul cid cn cseq1 co cun wceq df-fac reseq1
    ax-mp resundir c0 cin wcel wn 0nnn disjsn mpbir wfn wb mulex wfun cvv funi
    nnex resfunexg mp2an seq1fn fnresdisj mpbi wrel cdm wss cn0 0nn0 elisseti
    relsn dmsnop eqimssi relssres uneq12i uncom un0 3eqtr fveq1i snid fvres
    1nn0 fvsn 3eqtr3 ) ABACZDZEZAAFGCZEABEZFAVSWAVSHIJDZKLZWAMZVRDZWDVRDZWAVRDZ
    MZWABWENVSWFNOBWEVRPQWDWAVRRWISWAMWASMWAWGSWHWAJVRTSNZWGSNZWJAJUAUBUCJAUDUE
    WDJUFWJWKUGHWCUHIUIJUJUAWCUJUAUKULIJUJUMUNUOJVRWDUPQUQWAURWAUSZVRUTWHWANAFA
    VAVBVCZVDWLVRAFVEVFWAVRVGUNVHSWAVIWAVJVKVKVLAVRUAVTWBNAWMVMAVRBVNQAFWMFVAVO
    VCVPVQ $.
    $( [3-Dec-2004] $) $( [2-Dec-2004] $)

  $( The factorial of 1. $)
  fac1 $p |- ( ! ` 1 ) = 1 $=
    ( c1 cfa cfv cmul cid cn cres cseq1 co wcel wceq 1nn facnnt ax-mp mulex
    wfun cvv funi nnex resfunexg mp2an seq11 fvres fvi eqtr 3eqtr ) ABCZADEFGZH
    ICZAUHCZAAFJZUGUIKLAMNDUHOEPFQJUHQJRSEFQTUAUBUJAECZAUKUJULKLAFEUCNUKULAKLAF
    UDNUEUF $.
    $( [3-Dec-2004] $) $( [2-Dec-2004] $)

  $( The factorial of a successor. $)
  facp1t $p |- ( N e. NN0 ->
               ( ! ` ( N + 1 ) ) = ( ( ! ` N ) x. ( N + 1 ) ) ) $=
    ( cn0 wcel cn cc0 wceq wo c1 caddc co cfa cfv cmul elnn0 cid cres cseq1
    peano2nn facnnt syl mulex wfun cvv funi nnex resfunexg mp2an seq1p1 fvres
    oprex fvi ax-mp syl6eq opreq2d opreq1d eqtr4d 3eqtrd ax1cn addid2 fveq2i
    fac1 eqtr opreq1 fveq2d fveq2 opreq12d fac0 opreq12i mulid1 3eqtr4a jaoi
    sylbi ) ABCADCZAEFZGAHIJZKLZAKLZVOMJZFZANVMVSVNVMVPVOMODPZQJZLZAWALZVOVTLZM
    JZVRVMVODCZVPWBFARZVOSTAMVTUAOUBDUCCVTUCCUDUEODUCUFUGUHVMWEWCVOMJVRVMWDVOWC
    MVMWDVOOLZVOVMWFWDWHFWGVODOUITVOUCCWHVOFAHIUJVOUCUKULUMUNVMVQWCVOMASUOUPUQV
    NEHIJZKLZHVPVRWJHKLHWIHKHURUSZUTVAVBVNVOWIKAEHIVCZVDVNVREKLZWIMJZHVNVQWMVOW
    IMAEKVEWLVFWNHHMJHWMHWIHMVGWKVHHURVIVBUMVJVKVL $.
    $( [3-Dec-2004] $) $( [2-Dec-2004] $)

  $( The factorial of 2. $)
  fac2 $p |- ( ! ` 2 ) = 2 $=
    ( c2 cfa cfv c1 caddc co cmul df-2 fveq2i cn0 wcel wceq 1nn0 facp1t ax-mp
    fac1 eqcomi opreq12i 2cn mulid2 eqtr 3eqtr ) ABCDDEFZBCZDBCZUCGFZAAUCBHIDJK
    UDUFLMDNOUFDAGFAUEDUCAGPAUCHQRASTUAUB $.
    $( [7-Jul-2005] $) $( [17-Mar-2005] $)

  $( The factorial of 3. $)
  fac3 $p |- ( ! ` 3 ) = 6 $=
    ( c3 cfa cfv c2 c1 caddc co cmul c6 df-3 fveq2i cn0 wcel wceq 2nn0 facp1t
    ax-mp fac2 eqcomi opreq12i 2cn 3re recn mulcom 3t2e6 3eqtr ) ABCDEFGZBCZDBC
    ZUGHGZIAUGBJKDLMUHUJNODPQUJDAHGADHGIUIDUGAHRAUGJSTDAUAAUBUCUDUEUFUF $.
    $( [19-Jul-2005] $) $( [17-Mar-2005] $)

  $( Value of the factorial function expressed recursively. $)
  facnn2t $p |- ( N e. NN ->
               ( ! ` N ) = ( ( ! ` ( N - 1 ) ) x. N ) ) $=
    ( cn wcel cc c1 cmin co cn0 wa cfa cfv cmul wceq elnnnn0 caddc facp1t
    adantl ax1cn npcant mpan2 fveq2d adantr opreq2d 3eqtr3d sylbi ) ABCADCZAEFG
    ZHCZIZAJKZUGJKZALGZMANUIUGEOGZJKZUKUMLGZUJULUHUNUOMUFUGPQUFUNUJMUHUFUMAJUFE
    DCUMAMRAESTZUAUBUFUOULMUHUFUMAUKLUPUCUBUDUE $.
    $( [5-Feb-2005] $) $( [2-Dec-2004] $)

  ${
    $d j k N $.
    $( Closure of the factorial function. $)
    facclt $p |- ( N e. NN0 -> ( ! ` N ) e. NN ) $=
      ( vj vk cv cfa cfv cn wcel cc0 c1 caddc co wceq fveq2 eleq1d fac0 1nn
      eqeltr cn0 wa cmul facp1t adantl nnmulclt nn0p1nnt sylan2 eqeltrd expcom
      nn0ind ) BDZEFZGHIEFZGHCDZEFZGHZUMJKLZEFZGHZAEFZGHBCAUJIMUKULGUJIENOUJUMM
      UKUNGUJUMENOUJUPMUKUQGUJUPENOUJAMUKUSGUJAENOULJGPQRUOUMSHZURUOUTTUQUNUPUA
      LZGUTUQVAMUOUMUBUCUOUPGHVAGHUTUNUPUDUMUEUFUGUHUI $.
      $( [29-Dec-2004] $) $( [2-Dec-2004] $)
  $}

  $( The factorial function is nonzero. $)
  facne0t $p |- ( N e. NN0 -> ( ! ` N ) =/= 0 ) $=
    ( cn0 wcel cfa cfv cn cc0 wne facclt nnne0t syl ) ABCADEZFCLGHAILJK $.
    $( [3-May-2005] $) $( [26-Apr-2005] $)

  ${
