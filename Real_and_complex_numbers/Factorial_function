  $( No natural number (greater than one) divides the factorial plus one of an
     equal or larger number. $)
  facndiv $p |- ( ( ( M e. NN0 /\ N e. NN ) /\ ( 1 < N /\ N <_ M ) ) ->
                -. ( ( ( ! ` M ) + 1 ) / N ) e. ZZ ) $=
    ( cn0 wcel cn wa c1 clt wbr cle cfa co cdiv cz cmin syl wceq nncn ad2antrr
    cc cfv caddc wn cr recnz sylan ad2ant2lr facdiv 3expa nnz adantrl zsubcl ex
    nnre syl5com cc0 wne faccl peano2cn nnne0 ad2antlr divsubdir syl3anc ax-1cn
    jca pncan2 sylancl opreq1d eqtr3d eleq1d sylibd mtod ) ACDZBEDZFZGBHIZBAJIZ
    FZFZAKUAZGUBLZBMLZNDZGBMLZNDZVNVPWEUCZVMVQVNBUDDVPWFBUNBUEUFUGVSWCWBVTBMLZO
    LZNDZWEVSWGNDZWCWIVOVQWJVPVOVQFWGEDZWJVMVNVQWKABUHUIWGUJPUKWCWJWIWBWGULUMUO
