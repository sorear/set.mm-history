    ( cxr wcel cle wbr cif wn iffalse breq1d xrleidt syl5bir com12 iftrue id
    eqbrtrd pm2.61d2 adantl ) BCDZABEFZABGZBEFZACDSTUBTHZSUBUCUBBBEFSUCUABBETAB
    IJBKLMTUAABETABNTOPQR $.
    $( [12-Feb-2007] $) $( [7-Feb-2007] $)

  $( Two ways of saying the maximum of two extended reals is less than a
     third. $)
  xrmaxltt $p |- ( ( A e. RR* /\ B e. RR* /\ C e. RR* ) ->
               ( if ( A <_ B , B , A ) < C <-> ( A < C /\ B < C ) ) ) $=
    ( cxr wcel w3a cle wbr cif clt wa xrmax1 3adant3 wi xrlelttrt 3simp1 ifcl
    ancoms 3simp3 syl3anc mpand xrmax2 3simp2 jcad breq1 ifboth impbid1 ) ADEZB
    DEZCDEZFZABGHZBAIZCJHZACJHZBCJHZKUKUNUOUPUKAUMGHZUNUOUHUIUQUJABLMUHUMDEZUJU
    QUNKUONUKAUMCOUHUIUJPUHUIURUJUIUHURULBADQRMZUHUIUJSZTUAUKBUMGHZUNUPUHUIVAUJ
    ABUBMUIURUJVAUNKUPNUKBUMCOUHUIUJUCUSUTTUAUDUPUOUNULUPUOUNBABUMCJUEAUMCJUEUF
    RUG $.
    $( [12-Feb-2007] $) $( [7-Feb-2007] $)

  $( Two ways of saying an extended real is less than the minimum of two
     others. $)
  xrltmint $p |- ( ( A e. RR* /\ B e. RR* /\ C e. RR* ) ->
               ( A < if ( B <_ C , B , C ) <-> ( A < B /\ A < C ) ) ) $=
    ( cxr wcel w3a cle wbr cif clt wa xrmin1 3adant1 wi xrltletrt 3simp1 ifcl
    3simp2 syl3anc mpan2d xrmin2 3simp3 jcad breq2 ifboth impbid1 ) ADEZBDEZCDE
    ZFZABCGHZBCIZJHZABJHZACJHZKUJUMUNUOUJUMULBGHZUNUHUIUPUGBCLMUGULDEZUHUMUPKUN
    NUJAULBOUGUHUIPZUHUIUQUGUKBCDQMZUGUHUIRSTUJUMULCGHZUOUHUIUTUGBCUAMUGUQUIUMU
    TKUONUJAULCOURUSUGUHUIUBSTUCUKUNUOUMBCBULAJUDCULAJUDUEUF $.
    $( [12-Feb-2007] $) $( [7-Feb-2007] $)

  $( A number is less than or equal to the maximum of it and another. $)
  max1 $p |- ( ( A e. RR /\ B e. RR ) -> A <_ if ( A <_ B , B , A ) ) $=
    ( cxr wcel cle wbr cif cr xrmax1 rexrt syl2an ) ACDBCDAABEFBAGEFAHDBHDABIAJ
    BJK $.
    $( [8-Jun-2005] $) $( [3-Apr-2005] $)

  $( A number is less than or equal to the maximum of it and another. $)
  max1ALT $p |- ( A e. RR -> A <_ if ( A <_ B , B , A ) ) $=
