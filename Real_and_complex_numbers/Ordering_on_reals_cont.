  $( Equality implies 'less than or equal to'. $)
  eqlet $p |- ( ( A e. RR /\ A = B ) -> A <_ B ) $=
    ( cle wbr wceq cr wcel breq2 biimpac leidt sylan ) AACDZABEZABCDZAFGMLNABAC
    HIAJK $.
    $( [6-Apr-2005] $) $( [4-Apr-2005] $)

  ${
    lt.1 $e |- A e. RR $.

    ${
      lt.2 $e |- B e. RR $.

      $( Consequence of trichotomy. $)
      lttri2 $p |- ( -. A = B <-> ( A < B \/ B < A ) ) $=
        ( cr wcel wceq wn clt wbr wo wb lttri2t mp2an ) AEFBEFABGHABIJBAIJKLCDA
        BMN $.
        $( [19-Jan-1997] $)

      $( Consequence of trichotomy. $)
      lttri3 $p |- ( A = B <-> ( -. A < B /\ -. B < A ) ) $=
        ( cr wcel wceq clt wbr wn wa wb lttri3t mp2an ) AEFBEFABGABHIJBAHIJKLCD
        ABMN $.
        $( [14-May-1999] $)

      $( Consequence of trichotomy. $)
      letri3 $p |- ( A = B <-> ( A <_ B /\ B <_ A ) ) $=
        ( cr wcel wceq cle wbr wa wb letri3t mp2an ) AEFBEFABGABHIBAHIJKCDABLM
        $.
        $( [14-May-1999] $)

      $( 'Less than or equal to' in terms of 'less than'. $)
      leloe $p |- ( A <_ B <-> ( A < B \/ A = B ) ) $=
        ( cr wcel cle wbr clt wceq wo wb leloet mp2an ) AEFBEFABGHABIHABJKLCDAB
        MN $.
        $( [14-May-1999] $)

      $( 'Less than' expressed in terms of 'less than or equal to'. $)
      ltlen $p |- ( A < B <-> ( A <_ B /\ -. A = B ) ) $=
        ( cr wcel clt wbr cle wceq wn wa wb ltlent mp2an ) AEFBEFABGHABIHABJKLM
        CDABNO $.
        $( [27-Oct-1999] $)

      $( 'Less than' is not symmetric. $)
      ltnsym $p |- ( A < B -> -. B < A ) $=
        ( cr wcel clt wbr wn wi ltnsymt mp2an ) AEFBEFABGHBAGHIJCDABKL $.
        $( [6-May-1999] $)

      $( 'Less than or equal to'  in terms of 'less than'. $)
      lenlt $p |- ( A <_ B <-> -. B < A ) $=
        ( cr wcel cle wbr clt wn wb lenltt mp2an ) AEFBEFABGHBAIHJKCDABLM $.
        $( [24-May-1999] $)

      $( 'Less than'  in terms of 'less than or equal to'. $)
      ltnle $p |- ( A < B <-> -. B <_ A ) $=
        ( cle wbr clt lenlt con2bii ) BAEFABGFBADCHI $.
        $( [12-Jul-2005] $) $( [11-Jul-2005] $)

      $( 'Less than' implies 'less than or equal to'. $)
      ltle $p |- ( A < B -> A <_ B ) $=
        ( cr wcel clt wbr cle wi ltlet mp2an ) AEFBEFABGHABIHJCDABKL $.
        $( [14-May-1999] $)

      ${
        ltlei.1 $e |- A < B $.
        $( 'Less than' implies 'less than or equal to' (inference). $)
        ltlei $p |- A <_ B $=
          ( clt wbr cle ltle ax-mp ) ABFGABHGEABCDIJ $.
          $( [22-Aug-1999] $)
      $}

      $( Equality implies 'less than or equal to'. $)
      eqle $p |- ( A = B -> A <_ B ) $=
        ( wceq cle wbr letri3 pm3.26bd ) ABEABFGBAFGABCDHI $.
        $( [23-May-1999] $)

      $( 'Less than' implies not equal. $)
      ltne $p |- ( A < B -> -. A = B ) $=
        ( cr wcel clt wbr wceq wn wi ltnet mp2an ) AEFBEFABGHABIJKCDABLM $.
        $( [28-Jul-1999] $)

      $( Trichotomy law for 'less than or equal to'. $)
      letri $p |- ( A <_ B \/ B <_ A ) $=
        ( cle wbr wn clt ltnle ltle sylbir orri ) ABEFZBAEFZMGBAHFNBADCIBADCJKL
        $.
        $( [2-Aug-1999] $)

      ${
        lt.3 $e |- C e. RR $.
        $( 'Less than' is transitive.  Theorem I.17 of [Apostol] p. 20. $)
        lttr $p |- ( ( A < B /\ B < C ) -> A < C ) $=
          ( cr wcel clt wbr wa wi axlttrn mp3an ) AGHBGHCGHABIJBCIJKACIJLDEFABC
          MN $.
          $( [14-May-1999] $)

        $( 'Less than or equal to', 'less than' transitive law. $)
        lelttr $p |- ( ( A <_ B /\ B < C ) -> A < C ) $=
          ( cr wcel cle wbr clt wa wi lelttrt mp3an ) AGHBGHCGHABIJBCKJLACKJMDE
          FABCNO $.
          $( [14-May-1999] $)

        $( 'Less than', 'less than or equal to' transitive law. $)
        ltletr $p |- ( ( A < B /\ B <_ C ) -> A < C ) $=
          ( cr wcel clt wbr cle wa wi ltletrt mp3an ) AGHBGHCGHABIJBCKJLACIJMDE
          FABCNO $.
          $( [14-May-1999] $)

        $( 'Less than or equal to' is transitive. $)
        letr $p |- ( ( A <_ B /\ B <_ C ) -> A <_ C ) $=
          ( cr wcel cle wbr wa wi letrt mp3an ) AGHBGHCGHABIJBCIJKACIJLDEFABCMN
          $.
          $( [14-May-1999] $)

        $( Extended trichotomy law for 'less than or equal to'. $)
        le2tri3 $p |- ( ( A <_ B /\ B <_ C /\ C <_ A ) <->
                        ( A = B /\ B = C /\ C = A ) ) $=
          ( cle wbr w3a wceq wa letri3 biimpr letr sylan2 3impb 3comr eqcomd
          sylan 3impa 3jca eqle 3anim123i impbi ) ABGHZBCGHZCAGHZIZABJZBCJZCAJZ
          IUHUIUJUKUEUFUGUIUEBAGHZUIUFUGKUIUEULKABDELMBCAEFDNOPUFUGUEUJUFUGUEUJ
          UFCBGHZUJUGUEKUJUFUMKBCEFLMCABFDENOPQUEUFUGUKACGHZUGUKUEUFKUNUGKZACAC
          JUOACDFLMRABCDEFNSTUAUIUEUJUFUKUGABDEUBBCEFUBCAFDUBUCUD $.
          $( [14-Aug-2000] $)

        $( Addition to both sides of 'less than'. $)
        ltadd2 $p |- ( A < B <-> ( C + A ) < ( C + B ) ) $=
          ( clt wbr caddc co cr wcel wi axltadd mp3an cneg readdcl renegcl cc0
          recn addcom negid eqtr opreq1i addass addid2 3eqtr3 3brtr3g impbi )
          ABGHZCAIJZCBIJZGHZAKLBKLCKLUJUMMDEFABCNOUMCPZUKIJZUNULIJZABGUKKLULKLU
          NKLUMUOUPGHMCAFDQCBFEQCFRZUKULUNNOUNCIJZAIJSAIJUOAURSAIURCUNIJSUNCUNU
          QTZCFTZUACUTUBUCZUDUNCAUSUTADTZUEAVBUFUGURBIJSBIJUPBURSBIVAUDUNCBUSUT
          BETZUEBVCUFUGUHUI $.
          $( [21-Jan-1997] $)

        $( Addition to both sides of 'less than'.  Theorem I.18 of [Apostol]
           p. 20. $)
        ltadd1 $p |- ( A < B <-> ( A + C ) < ( B + C ) ) $=
          ( clt wbr caddc co ltadd2 recn addcom breq12i bitr ) ABGHCAIJZCBIJZGH
          ACIJZBCIJZGHABCDEFKPRQSGCACFLZADLMCBTBELMNO $.
          $( [21-Jan-1997] $)

        $( Addition to both sides of 'less than or equal to'. $)
        leadd1 $p |- ( A <_ B <-> ( A + C ) <_ ( B + C ) ) $=
          ( clt wbr wn caddc co cle ltadd1 negbii lenlt readdcl 3bitr4 ) BAGHZI
          BCJKZACJKZGHZIABLHTSLHRUABACEDFMNABDEOTSACDFPBCEFPOQ $.
          $( [11-Aug-1999] $)

        $( Addition to both sides of 'less than or equal to'. $)
        leadd2 $p |- ( A <_ B <-> ( C + A ) <_ ( C + B ) ) $=
          ( cle wbr caddc co leadd1 recn addcom breq12i bitr ) ABGHACIJZBCIJZGH
          CAIJZCBIJZGHABCDEFKPRQSGACADLCFLZMBCBELTMNO $.
          $( [11-Aug-1999] $)

        $( 'Less than' relationship between subtraction and addition. $)
        ltsubadd $p |- ( ( A - B ) < C <-> A < ( C + B ) ) $=
          ( cneg caddc co clt wbr cmin renegcl readdcl ltadd1 recn negsub
          breq1i cc0 addass addcom negid eqtr opreq2i addid1 3eqtr 3bitr3 ) ABG
          ZHIZCJKUIBHIZCBHIZJKABLIZCJKAUKJKUICBAUHDBEMZNFEOUIULCJABADPZBEPZQRUJ
          AUKJUJAUHBHIZHIASHIAAUHBUNUHUMPZUOTUPSAHUPBUHHISUHBUQUOUABUOUBUCUDAUN
          UEUFRUG $.
          $( [21-Jan-1997] $)

        $( 'Less than or equal to' relationship between subtraction and
           addition. $)
        lesubadd $p |- ( ( A - B ) <_ C <-> A <_ ( C + B ) ) $=
          ( cmin co clt wbr wceq wo caddc cle ltsubadd recn subadd addcom
          eqeq1i eqcom 3bitr orbi12i resubcl leloe readdcl 3bitr4 ) ABGHZCIJZUG
          CKZLACBMHZIJZAUJKZLUGCNJAUJNJUHUKUIULABCDEFOUIBCMHZAKUJAKULABCADPBEPZ
          CFPZQUMUJABCUNUORSUJATUAUBUGCABDEUCFUDAUJDCBFEUEUDUF $.
          $( [30-Sep-1999] $)

        ${
          lt.4 $e |- D e. RR $.
          $( Adding both side of two inequalities.  Theorem I.25 of [Apostol]
