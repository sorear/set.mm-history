  $( A number is less than or equal to the maximum of it and another. $)
  max2 $p |- ( ( A e. RR /\ B e. RR ) -> B <_ if ( A <_ B , B , A ) ) $=
    ( cxr wcel cle wbr cif cr xrmax2 rexrt syl2an ) ACDBCDBABEFBAGEFAHDBHDABIAJ
    BJK $.
    $( [22-Apr-2005] $) $( [3-Apr-2005] $)

  $( Two ways of saying the maximum of two numbers is less than or equal to a
     third. $)
  maxlet $p |- ( ( A e. RR /\ B e. RR /\ C e. RR ) ->
               ( if ( A <_ B , B , A ) <_ C <-> ( A <_ C /\ B <_ C ) ) ) $=
    ( cr wcel w3a cle wbr cif wa max1 3adant3 wi letrt 3simp1 ifcl ancoms
    3simp3 syl3anc mpand max2 3simp2 jcad breq1 ifboth impbid1 ) ADEZBDEZCDEZFZ
    ABGHZBAIZCGHZACGHZBCGHZJUJUMUNUOUJAULGHZUMUNUGUHUPUIABKLUGULDEZUIUPUMJUNMUJ
