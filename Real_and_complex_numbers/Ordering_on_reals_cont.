    $( 'Less than' is irreflexive. $)
    ltnr $p |- -. A < A $=
      ( cr wcel clt wbr wn ltnrt ax-mp ) ACDAAEFGBAHI $.
      $( [18-Aug-1999] $)

    $( 'Less than or equal to' is reflexive. $)
    leid $p |- A <_ A $=
      ( cr wcel cle wbr leidt ax-mp ) ACDAAEFBAGH $.
      $( [18-Aug-1999] $)

    $( Positive means non-zero (useful for ordering theorems involving
       division). $)
    gt0ne0 $p |- ( 0 < A -> A =/= 0 ) $=
      ( cc0 clt wbr wne wceq wn 0re ltne df-ne sylibr necom ) CADEZCAFZACFNCAGH
      OCAIBJCAKLACML $.
