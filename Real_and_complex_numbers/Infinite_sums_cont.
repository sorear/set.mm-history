  ${
    $d k F $.  $d k G $.  $d k M $.  $d k ph $.  $d k Z $.
    isumcmp.1 $e |- Z = ( ZZ>= ` M ) $.
    isumcmp.2 $e |- ( ph -> M e. ZZ ) $.
    isumcmp.3 $e |- ( ( ph /\ k e. Z ) -> ( F ` k ) = A ) $.
    isumcmp.4 $e |- ( ( ph /\ k e. Z ) -> A e. RR ) $.
    isumcmp.5 $e |- ( ( ph /\ k e. Z ) -> ( G ` k ) = B ) $.
    isumcmp.6 $e |- ( ( ph /\ k e. Z ) -> B e. RR ) $.
    isumcmp.7 $e |- ( ( ph /\ k e. Z ) -> A <_ B ) $.
    isumcmp.8 $e |- ( ph -> seq M ( + , F ) e. dom ~~> ) $.
    isumcmp.9 $e |- ( ph -> seq M ( + , G ) e. dom ~~> ) $.
    $( Comparison of two infinite sums.  (Contributed by Paul Chapman,
       13-Nov-2007.)  (Revised by Mario Carneiro, 24-Apr-2014.) $)
    isumcmp $p |- ( ph -> sum_ k e. Z A <_ sum_ k e. Z B ) $=
      ( cli cvv wcel caddc cseq cfv csu cle fvex a1i cdm wbr climdm sylib cv wa
      cr eqeltrd 3brtr4d isercmp recnd isumclim ) AUAEGUBZRUCZUAFGUBZRUCZHBDUDH
      CDUDUEAVAVCDEFGSHIJVASTAUTRUFUGZAUTRUHZTUTVARUIPUTUJUKZAVBVETVBVCRUIQVBUJ
      UKZADULZHTUMZVHEUCZBUNKLUOVIVHFUCZCUNMNUOVIBCVJVKUEOKMUPUQABVADEGSHIJKVIB
      LURVDVFUSACVCDFGSHIJMVICNURVCSTAVBRUFUGVGUSUP $.
      $( [24-Apr-2014] $) $( [13-Nov-2007] $)
  $}

  ${
    $d j k A $.  $d j k F $.  $d k M $.  $d k ph $.  $d j k Z $.
    isumcmpss.1 $e |- Z = ( ZZ>= ` M ) $.
    isumcmpss.2 $e |- ( ph -> M e. ZZ ) $.
    isumcmpss.3 $e |- ( ph -> A e. Fin ) $.
    isumcmpss.4 $e |- ( ph -> A C_ Z ) $.
    isumcmpss.5 $e |- ( ( ph /\ k e. Z ) -> ( F ` k ) = B ) $.
    isumcmpss.6 $e |- ( ( ph /\ k e. Z ) -> B e. RR ) $.
    isumcmpss.7 $e |- ( ( ph /\ k e. Z ) -> 0 <_ B ) $.
    isumcmpss.8 $e |- ( ph -> seq M ( + , F ) e. dom ~~> ) $.
    $( A finite sum of nonnegative numbers is less or equal to its limit.
       (Contributed by Mario Carneiro, 24-Apr-2014.) $)
    isumcmpss $p |- ( ph -> sum_ k e. A B <_ sum_ k e. Z B ) $=
      ( wcel cc0 cle cfv cr vj csu cv cif wss cc wral cuz cfn wo wceq sseld imp
      recnd syldan ralrimiva eqimssi orci a1i sumss2 syl21anc eleq1 fveq2 eqidd
      wa cmpt ifbieq12d eqid fvex 0re elexi ifex fvmpt adantl ifeq1d eqtrd ifcl
      sylancl wbr leid breq1 ifboth sylan syl2anc fsumcvg3 isumcmp eqbrtrd ) AB
