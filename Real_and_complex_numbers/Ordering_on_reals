$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Ordering on reals
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Define 'less than or equal to' on real subset of compex numbers.  Theorem
     ~ leloet relates it to 'less than'. $)
  df-le $a |- <_ = ( ( RR X. RR ) \ `' < ) $.

  $( 'Less than or equal to' expressed in terms of 'less than'. $)
  leltt $p |- ( ( A e. RR /\ B e. RR ) -> ( A <_ B <-> -. B < A ) ) $=
    ( cr wcel wa cle wbr cop clt ccnv wn cxp wb opelxpi cdif df-le eleq2i
    eldif bitr baib syl df-br syl5bb opelcnvg syl6rbbr binegd bitr4d ) ACDBCDEZ
    ABFGZABHZIJZDZKZBAIGZKUHUJFDZUMUIUHUJCCLZDZUOUMMABCCNUOUQUMUOUJUPUKOZDUQUME
    FURUJPQUJUPUKRSTUAABFUBUCUHUNULUHULBAHIDUNABCCIUDBAIUBUEUFUG $.
    $( [13-May-99] $)

  ${
    $d x y z $. $( ` x y z ` are dummy. $)
    $( 'Less than' is a strict ordering.  Note: do not shorten this with
       ~ ltsor , and do not use ~ ltsor in complex number proofs, in order
       to maintain a "clean" derivation of all complex number proofs directly
       from postulates. $)
    ltso $p |- < Or RR $=
      ( vx vy vz cr clt cv wcel w3a wbr weq wo wn wb wa wi axlttri 3adant3
      axlttrn jca so ) ABCDEAFZDGZBFZDGZCFZDGZHUAUCEIZABJUCUAEIKLMZUGUCUEEINUAU
      EEIOUBUDUHUFUAUCPQUAUCUERST $.
      $( [19-Jan-97] $)
  $}

  $( Consequence of trichotomy. $)
  lttri2t $p |- ( ( A e. RR /\ B e. RR ) ->
                ( -. A = B <-> ( A < B \/ B < A ) ) ) $=
    ( cr wcel wa clt wbr wo wceq wn wor wb ltso sotrieq mpan bicomd bicon1d )
    ACDBCDEZABFGBAFGHZABIZRTSJZCFKRTUALMCABFNOPQ $.
    $( [9-Oct-99] $)

  $( Trichotomy law for 'less than'. $)
  lttri3t $p |- ( ( A e. RR /\ B e. RR ) ->
                ( A = B <-> ( -. A < B /\ -. B < A ) ) ) $=
    ( cr clt wor wcel wa wceq wbr wn wb ltso sotrieq2 mpan ) CDEACFBCFGABHABDIJ
    BADIJGKLCABDMN $.
    $( [5-May-99] $)

  $( 'Less than' implies not equal. $)
  ltnet $p |- ( ( A e. RR /\ B e. RR ) -> ( A < B -> -. A = B ) ) $=
    ( cr wcel wa wceq wn clt wbr wo lttri2t orc syl5bir ) ACDBCDEABFGABHIZBAHIZ
    JNABKNOLM $.
    $( [9-Oct-99] $)

  $( Trichotomy law. $)
  letri3t $p |- ( ( A e. RR /\ B e. RR ) ->
                ( A = B <-> ( A <_ B /\ B <_ A ) ) ) $=
    ( cr wcel wa wceq clt wbr wn cle lttri3t ancom syl6bbr leltt wb ancoms
    bi2and bitr4d ) ACDZBCDZEZABFZBAGHIZABGHIZEZABJHZBAJHZEUAUBUDUCEUEABKUCUDLM
    UAUFUCUGUDABNTSUGUDOBANPQR $.
    $( [14-May-99] $)

  $( 'Less than or equal to' expressed in terms of 'less than' or 'equals'. $)
  leloet $p |- ( ( A e. RR /\ B e. RR ) ->
               ( A <_ B <-> ( A < B \/ A = B ) ) ) $=
    ( cr wcel wa cle wbr clt wn wceq wo leltt wb axlttri ancoms bicon2d
    cleqcom biror orcom bitr syl5rbbr bitrd ) ACDZBCDZEZABFGBAHGZIZABHGZABJZKZA
    BLUEBAJZUHKZUGUJUEUFULUDUCUFULIMBANOPULUIUHKUJUKUIUHBAQRUIUHSTUAUB $.
    $( [13-May-99] $)
  $( Equality in terms of 'less than or equal to', 'less than'. $)
  lenltt $p |- ( ( A e. RR /\ B e. RR ) ->
               ( A = B <-> ( A <_ B /\ -. A < B ) ) ) $=
    ( cr wcel wa wceq cle wbr clt wn letri3t wb leltt ancoms biland bitrd ) ACD
    ZBCDZEZABFABGHZBAGHZETABIHJZEABKSUAUBTRQUAUBLBAMNOP $.
    $( [7-Apr-01] $)

  $( 'Less than' implies 'less than or equal to'. $)
  ltlet $p |- ( ( A e. RR /\ B e. RR ) ->
               ( A < B -> A <_ B ) ) $=
    ( cr wcel wa cle wbr clt wceq wo leloet orc syl5bir ) ACDBCDEABFGABHGZABIZJ
    NABKNOLM $.
    $( [25-Aug-99] $)

  $( 'Less than or equal to' implies 'less than' is not 'equals'. $)
  leltnet $p |- ( ( A e. RR /\ B e. RR ) ->
               ( A <_ B -> ( A < B <-> -. A = B ) ) ) $=
    ( cr wcel wa cle wbr clt wceq wn wb wi lttri3t pm3.26 syl6bi adantr wo
    leloet biimpa ord impbid bicon2d exp ) ACDBCDEZABFGZABHGZABIZJKUDUEEZUGUFUH
    UGUFJZUDUGUILUEUDUGUIBAHGJZEUIABMUIUJNOPUHUFUGUDUEUFUGQABRSTUAUBUC $.
    $( [27-Jul-99] $)

  $( 'Less than' expressed in terms of 'less than or equal to'. $)
  ltlent $p |- ( ( A e. RR /\ B e. RR ) ->
               ( A < B <-> ( A <_ B /\ -. A = B ) ) ) $=
    ( cr wcel wa clt wbr cle wceq wn ltlet ltnet jcad wo wi leloet ax-1 pm2.21
    com12 jaoi syl6bi imp3a impbid ) ACDBCDEZABFGZABHGZABIZJZEUDUEUFUHABKABLMUD
    UFUHUEUDUFUEUGNUHUEOZABPUEUIUGUEUHQUHUGUEUGUERSTUAUBUC $.
    $( [27-Oct-99] $)

  $( Transitive law. $)
  lelttrt $p |- ( ( A e. RR /\ B e. RR /\ C e. RR ) ->
             ( ( A <_ B /\ B < C ) -> A < C ) ) $=
    ( cr wcel w3a cle wbr clt wceq wo wi wb leloet 3adant3 axlttrn exp3a breq1
    biimprd a1i jaod sylbid imp3a ) ADEZBDEZCDEZFZABGHZBCIHZACIHZUGUHABIHZABJZK
    ZUIUJLZUDUEUHUMMUFABNOUGUKUNULUGUKUIUJABCPQULUNLUGULUJUIABCIRSTUAUBUC $.
    $( [23-May-99] $)

  $( Transitive law. $)
  ltletrt $p |- ( ( A e. RR /\ B e. RR /\ C e. RR ) ->
             ( ( A < B /\ B <_ C ) -> A < C ) ) $=
    ( cr wcel w3a clt wbr cle wceq wo wi wb leloet 3adant1 axlttrn exp3a com23
    breq2 biimpd a1i jaod sylbid imp3a ) ADEZBDEZCDEZFZABGHZBCIHZACGHZUHUJUIUKU
    HUJBCGHZBCJZKZUIUKLZUFUGUJUNMUEBCNOUHULUOUMUHUIULUKUHUIULUKABCPQRUMUOLUHUMU
    IUKBCAGSTUAUBUCRUD $.
    $( [25-Aug-99] $)

  $( Transitive law. $)
  letrt $p |- ( ( A e. RR /\ B e. RR /\ C e. RR ) ->
             ( ( A <_ B /\ B <_ C ) -> A <_ C ) ) $=
    ( cr wcel w3a cle wbr wi wa clt wceq wo wb leloet 3adant1 adantr lelttrt
    ltlet 3adant2 syld exp3a imp breq2 biimpcd adantl jaod sylbid exp imp3a )
    ADEZBDEZCDEZFZABGHZBCGHZACGHZUNUOUPUQIUNUOJZUPBCKHZBCLZMZUQUNUPVANZUOULUMVB
    UKBCOPQURUSUQUTUNUOUSUQIUNUOUSUQUNUOUSJACKHZUQABCRUKUMVCUQIULACSTUAUBUCUOUT
    UQIUNUTUOUQBCAGUDUEUFUGUHUIUJ $.
    $( [12-Nov-99] $)

  $( 'Less than' is irreflexive. $)
  ltnrt $p |- ( A e. RR -> -. A < A ) $=
    ( cr clt wor wcel wbr wn ltso sonr mpan ) BCDABEAACFGHBACIJ $.
    $( [18-Aug-99] $)

  $( 'Less than or equal to' is reflexive. $)
  leidt $p |- ( A e. RR -> A <_ A ) $=
    ( cr wcel cle wbr wa clt wceq wo wn cleqid a1i orri leloet mpbiri anidms )
    ABCZAADEZQQFRAAGEZAAHZISTTSJAKLMAANOP $.
    $( [18-Aug-99] $)

  $( 'Less than' is not symmetric. $)
  ltnsymt $p |- ( ( A e. RR /\ B e. RR ) -> ( A < B -> -. B < A ) ) $=
    ( cr wcel wa clt wbr wceq wo wn axlttri olc con3i syl6bi ) ACDBCDEABFGABHZB
    AFGZIZJPJABKPQPOLMN $.
    $( [8-Jan-02] $)

  ${
    lt.1 $e |- A e. RR $.

    ${
      lt.2 $e |- B e. RR $.

      $( Consequence of trichotomy. $)
      lttri2 $p |- ( -. A = B <-> ( A < B \/ B < A ) ) $=
        ( cr wcel wceq wn clt wbr wo wb lttri2t mp2an ) AEFBEFABGHABIJBAIJKLCDA
        BMN $.
        $( [19-Jan-97] $)

      $( Consequence of trichotomy. $)
      lttri3 $p |- ( A = B <-> ( -. A < B /\ -. B < A ) ) $=
        ( cr wcel wceq clt wbr wn wa wb lttri3t mp2an ) AEFBEFABGABHIJBAHIJKLCD
        ABMN $.
        $( [14-May-99] $)

      $( Consequence of trichotomy. $)
      letri3 $p |- ( A = B <-> ( A <_ B /\ B <_ A ) ) $=
        ( cr wcel wceq cle wbr wa wb letri3t mp2an ) AEFBEFABGABHIBAHIJKCDABLM
        $.
        $( [14-May-99] $)

      $( 'Less than or equal to' in terms of 'less than'. $)
      leloe $p |- ( A <_ B <-> ( A < B \/ A = B ) ) $=
        ( cr wcel cle wbr clt wceq wo wb leloet mp2an ) AEFBEFABGHABIHABJKLCDAB
        MN $.
        $( [14-May-99] $)

      $( 'Less than' expressed in terms of 'less than or equal to'. $)
      ltlen $p |- ( A < B <-> ( A <_ B /\ -. A = B ) ) $=
        ( cr wcel clt wbr cle wceq wn wa wb ltlent mp2an ) AEFBEFABGHABIHABJKLM
        CDABNO $.
        $( [27-Oct-99] $)

      $( 'Less than' is not symmetric. $)
      ltnsym $p |- ( A < B -> -. B < A ) $=
        ( cr wcel clt wbr wn wi ltnsymt mp2an ) AEFBEFABGHBAGHIJCDABKL $.
        $( [6-May-99] $)

      $( 'Less than or equal to'  in terms of 'less than'. $)
      lelt $p |- ( A <_ B <-> -. B < A ) $=
        ( cr wcel cle wbr clt wn wb leltt mp2an ) AEFBEFABGHBAIHJKCDABLM $.
        $( [24-May-99] $)

      $( 'Less than' implies 'less than or equal to'. $)
      ltle $p |- ( A < B -> A <_ B ) $=
        ( cr wcel clt wbr cle wi ltlet mp2an ) AEFBEFABGHABIHJCDABKL $.
        $( [14-May-99] $)

      ${
        ltlei.1 $e |- A < B $.
        $( 'Less than' implies 'less than or equal to' (inference). $)
        ltlei $p |- A <_ B $=
          ( clt wbr cle ltle ax-mp ) ABFGABHGEABCDIJ $.
          $( [22-Aug-99] $)
      $}

      $( Equality implies 'less than or equal to'. $)
      eqle $p |- ( A = B -> A <_ B ) $=
        ( wceq cle wbr letri3 pm3.26bd ) ABEABFGBAFGABCDHI $.
        $( [23-May-99] $)

      $( 'Less than' implies not equal. $)
      ltne $p |- ( A < B -> -. A = B ) $=
        ( cr wcel clt wbr wceq wn wi ltnet mp2an ) AEFBEFABGHABIJKCDABLM $.
        $( [28-Jul-99] $)

      $( Trichotomy law for 'less than or equal to'. $)
      letri $p |- ( A <_ B \/ B <_ A ) $=
        ( cle wbr wn clt lelt bicon2 ltle sylbir orri ) ABEFZBAEFZNGBAHFZONPABC
        DIJBADCKLM $.
        $( [2-Aug-99] $)

      ${
        lt.3 $e |- C e. RR $.
        $( 'Less than' is transitive. $)
        lttr $p |- ( ( A < B /\ B < C ) -> A < C ) $=
          ( cr wcel clt wbr wa wi axlttrn mp3an ) AGHBGHCGHABIJBCIJKACIJLDEFABC
          MN $.
          $( [14-May-99] $)

        $( 'Less than or equal to', 'less than' transitive law. $)
        lelttr $p |- ( ( A <_ B /\ B < C ) -> A < C ) $=
          ( cr wcel cle wbr clt wa wi lelttrt mp3an ) AGHBGHCGHABIJBCKJLACKJMDE
          FABCNO $.
          $( [14-May-99] $)

        $( 'Less than', 'less than or equal to' transitive law. $)
        ltletr $p |- ( ( A < B /\ B <_ C ) -> A < C ) $=
          ( cr wcel clt wbr cle wa wi ltletrt mp3an ) AGHBGHCGHABIJBCKJLACIJMDE
          FABCNO $.
          $( [14-May-99] $)

        $( 'Less than or equal to' is transitive. $)
        letr $p |- ( ( A <_ B /\ B <_ C ) -> A <_ C ) $=
          ( cr wcel cle wbr wa wi letrt mp3an ) AGHBGHCGHABIJBCIJKACIJLDEFABCMN
          $.
          $( [14-May-99] $)

        $( Exended trichotomy law for 'less than or equal to'. $)
        le2tri3 $p |- ( ( A <_ B /\ B <_ C /\ C <_ A ) <->
                        ( A = B /\ B = C /\ C = A ) ) $=
          ( cle wbr w3a wceq wa letri3 biimpr letr sylan5 3impb 3comr cleqcomd
          sylan 3impa 3jca eqle im3an impbi ) ABGHZBCGHZCAGHZIZABJZBCJZCAJZIUHU
          IUJUKUEUFUGUIUEBAGHZUIUFUGKUIUEULKABDELMBCAEFDNOPUFUGUEUJUFUGUEUJUFCB
          GHZUJUGUEKUJUFUMKBCEFLMCABFDENOPQUEUFUGUKACGHZUGUKUEUFKUNUGKZACACJUOA
          CDFLMRABCDEFNSTUAUIUEUJUFUKUGABDEUBBCEFUBCAFDUBUCUD $.
          $( [14-Aug-00] $)

        $( Addition to both sides of an inequality. $)
        ltadd2 $p |- ( A < B <-> ( C + A ) < ( C + B ) ) $=
          ( clt wbr caddc co cr wcel wi axltadd mp3an cneg addrcl negrcl cc0
