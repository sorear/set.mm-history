    VKVDIBVGJKTUEUBUCUTVGAECUFUGBECUFUGUHUI $.
    $( [3-Oct-99] $)

  $( Swap denominator with other side of inequality. $)
  ltdiv23t $p |- ( ( A e. RR /\ B e. RR /\ C e. RR ) ->
                  ( ( 0 < B /\ 0 < C ) ->
                  ( ( A / B ) < C <-> ( A / C ) < B ) ) ) $=
    ( cr wcel cc0 clt wbr wa cdiv co wb wi cded wceq opreq1 breq1d bi2bid
    bilimd breq2 birand opreq2 bi2imd biland ax0re elimel ltdiv23 dedth3h ) ADE
