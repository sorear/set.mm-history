      $( Addition of 2 positive numbers is positive. $)
      addgt0 $p |- ( ( 0 < A /\ 0 < B ) -> 0 < ( A + B ) ) $=
        ( cc0 clt wbr caddc co ax0re addrcl lttr ltadd2 recn addid1 breq1i
