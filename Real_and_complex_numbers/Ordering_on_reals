        $( [19-Aug-01] $)

      $( Two nonnegative numbers are zero iff their sum is zero. $)
      add20 $p |- ( ( 0 <_ A /\ 0 <_ B ) ->
        ( ( A + B ) = 0 <-> ( A = 0 /\ B = 0 ) ) ) $=
        ( cc0 cle wbr wa caddc co wceq clt wo wi wn addrcl ax0re lttri3
