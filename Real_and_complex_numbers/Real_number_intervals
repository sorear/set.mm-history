$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Real number intervals
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c (,) $.
  $c (,] $.
  $c [,) $.
  $c [,] $.

  $( Extend class notation with the set of open intervals of extended reals. $)
  cioo $a class (,) $.

  $( Extend class notation with the set of open-below, closed-above intervals
     of extended reals. $)
  cioc $a class (,] $.

  $( Extend class notation with the set of closed-below, open-above intervals
     of extended reals. $)
  cico $a class [,) $.

  $( Extend class notation with the set of closed intervals of extended
     reals. $)
  cicc $a class [,] $.

  ${
    $d x y z w $.
    $( Define the set of open intervals of extended reals. $)
    df-ioo $a |- (,) = { <. <. x , y >. , z >. | ( ( x e. RR* /\ y e. RR* )
                   /\ z = { w e. RR* | ( x < w /\ w < y ) } ) } $.

    $( Define the set of open-below, closed-above intervals of extended
       reals. $)
    df-ioc $a |- (,] = { <. <. x , y >. , z >. | ( ( x e. RR* /\ y e. RR* )
                   /\ z = { w e. RR* | ( x < w /\ w <_ y ) } ) } $.

    $( Define the set of closed-below, open-above intervals of extended
       reals. $)
    df-ico $a |- [,) = { <. <. x , y >. , z >. | ( ( x e. RR* /\ y e. RR* )
                   /\ z = { w e. RR* | ( x <_ w /\ w < y ) } ) } $.

    $( Define the set of closed intervals of extended reals. $)
    df-icc $a |- [,] = { <. <. x , y >. , z >. | ( ( x e. RR* /\ y e. RR* )
                   /\ z = { w e. RR* | ( x <_ w /\ w <_ y ) } ) } $.
  $}

  ${
    $d w x y z A $.  $d w x y z B $.  $d x C $.  $d x D $.
    $( The set of open intervals of extended reals exists. $)
    iooex $p |- (,) e. _V $=
      ( vx vy vz vw cxr cv clt wbr wa crab cioo xrex df-ioo oprabex2 ) ABCEEAFD
      FZGHOBFGHIDEJKLLABCDMN $.
      $( [6-Feb-2007] $)

    $( Value of the open interval function. $)
    iooval $p |- ( ( A e. RR* /\ B e. RR* ) -> ( A (,) B ) =
                 { x e. RR* | ( A < x /\ x < B ) } ) $=
      ( vy vz vw cxr cv clt wbr wa crab cioo xrex rabex wceq breq1 anbi1d
      rabbidv breq2 anbi2d df-ioo oprabval2 ) DEFBCGGDHZAHZIJZUEEHZIJZKZAGLBUEI
      JZUECIJZKZAGLMUJUHKZAGLULAGNOUDBPZUIUMAGUNUFUJUHUDBUEIQRSUGCPZUMULAGUOUHU
      KUJUGCUEITUASDEFAUBUC $.
      $( [24-Dec-2006] $)

    $( Value of the open interval function. $)
    iooval2 $p |- ( ( A e. RR* /\ B e. RR* ) -> ( A (,) B ) =
                 { x e. RR | ( A < x /\ x < B ) } ) $=
      ( cxr wcel wa cioo co cv clt wbr crab cr iooval cab wi w3a xrre2 ex
      3com23 3exp imp4b simpr a1i jcad rexr anim1i impbid1 abbidv df-rab
      3eqtr4g eqtrd ) BDEZCDEZFZBCGHBAIZJKUPCJKFZADLZUQAMLZABCNUOUPDEZUQFZAOUPM
      EZUQFZAOURUSUOVAVCAUOVAVCUOVAVBUQUMUNUTUQVBUMUNUTUQVBPZUMUTUNVDUMUTUNQUQV
      BBUPCRSTUAUBVAUQPUOUTUQUCUDUEVBUTUQUPUFUGUHUIUQADUJUQAMUJUKUL $.
      $( [6-Feb-2007] $)

    $( An empty open interval of extended reals. $)
    ioo0 $p |- ( ( A e. RR* /\ B e. RR* ) -> ( ( A (,) B ) = (/) <->
                 B <_ A ) ) $=
      ( vx cxr wcel wa cioo co c0 wceq cv clt wbr crab cle iooval eqeq1d wn
      wrex wne df-ne rabn0 bitr3i wi xrlttr 3com23 3expa r19.23adva w3a cq
      qbtwnxr cr qre rexr syl anim1i reximi2 3expia impbid syl5bb xrltnle
      bitrd con4bid ) ADEZBDEZFZABGHZIJACKZLMVHBLMFZCDNZIJZBAOMZVFVGVJICABPQVFV
      KVLVFVKRZABLMZVLRVMVICDSZVFVNVMVJITVOVJIUAVICDUBUCVFVOVNVFVIVNCDVDVEVHDEZ
      VIVNUDZVDVPVEVQAVHBUEUFUGUHVDVEVNVOVDVEVNUIVICUJSVOCABUKVIVICUJDVHUJEZVPV
      IVRVHULEVPVHUMVHUNUOUPUQUOURUSUTABVAVBVCVB $.
      $( [6-Feb-2007] $)

    $( An open interval of extended reals is nonempty iff the lower argument is
       less than the upper argument. $)
    ioon0 $p |- ( ( A e. RR* /\ B e. RR* ) -> ( ( A (,) B ) =/= (/) <->
                 A < B ) ) $=
      ( cxr wcel wa clt wbr cioo co c0 wceq cle wn ioo0 wb xrlenlt ancoms
      bitr2d necon1abid ) ACDZBCDZEZABFGZABHIZJUBUDJKBALGZUCMZABNUATUEUFOBAPQRS
      $.
      $( [2-Mar-2007] $)

    $( The open interval function's value is empty outside of its domain. $)
    ndmioo $p |- ( -. ( A e. RR* /\ B e. RR* ) -> ( A (,) B ) = (/) ) $=
      ( vx vy vz vw cxr wcel wa wn cvv cioo co c0 wceq cdm cxp cv clt wbr crab
      xrex rabex df-ioo dmoprab2 ndmoprg mp3an1 expcom oprprc2 cle xrleid
      adantr ioo0 mpbird wrel relxp releqi mpbir oprprc1 pm2.61d1 pm2.61i
      syl6eq ) AGHZBGHIJZBKHZABLMZNOZVEVDVGLPZGGQZOZVEVDVGCDEGGCRFRZSTVKDRSTIZF
      GUALVLFGUBUCCDEFUDUEZABKGGLUFUGUHVEJVFAALMZNABLUIVCVCIZVNNOZVOVPAAUJTZVCV
