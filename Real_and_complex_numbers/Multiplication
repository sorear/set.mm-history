  $( A theorem for reals analogous the second Peano postulate ~ peano2nn . $)
  peano2re $p |- ( A e. RR -> ( A + 1 ) e. RR ) $=
    ( cr wcel c1 caddc co 1re axaddrcl mpan2 ) ABCDBCADEFBCGADHI $.
    $( [6-Jul-2005] $) $( [5-Jul-2005] $)

  $( Closure law for negative of reals.  The weak deduction theorem ~ dedth is
     used to convert hypothesis of the inference (deduction) form of this
     theorem, ~ renegcl , to an antecedent. $)
  renegclt $p |- ( A e. RR -> -u A e. RR ) $=
    ( cr wcel cneg c1 cif wceq negeq eleq1d 1re elimel renegcl dedth ) ABCZADZB
    CNAEFZDZBCAEAPGOQBAPHIPAEBJKLM $.
    $( [20-Jan-1997] $)

  $( Closure law for subtraction of reals. $)
  resubclt $p |- ( ( A e. RR /\ B e. RR ) -> ( A - B ) e. RR ) $=
    ( cr wcel wa cneg caddc co cmin cc wceq negsubt recnt syl2an axaddrcl
    renegclt sylan2 eqeltrrd ) ACDZBCDZEABFZGHZABIHZCAJDBJDUBUCKSTABLAMBMNSUACD
    UBCDTAUAOBPQR $.
    $( [2-Aug-2006] $) $( [20-Jan-1997] $)

  ${
    resubcl.1 $e |- A e. RR $.
    resubcl.2 $e |- B e. RR $.
    $( Closure law for subtraction of reals. $)
    resubcl $p |- ( A - B ) e. RR $=
      ( cr wcel cmin co resubclt mp2an ) AEFBEFABGHEFCDABIJ $.
      $( [17-Jan-1997] $)
  $}

  ${
    $d x y $.
    $( 0 is a real number.  Proved without referencing ~ 1re .  (Contributed
       by Eric Schmidt, 21-May-2007.) $)
    0re $p |- 0 e. RR $=
      ( vx vy cv cr wcel wex cc0 ci cmul co caddc wceq wrex cc 0cn axcnre
      ax-mp wa df-rex pm3.26 19.22i sylbi axrnegex pm3.27 axaddrcl adantr
      eqeltrrd ex r19.23adva mpd 19.23aiv ) ACZDEZAFZGDEZGULHBCZIJKJLBDMZADMZUN
      GNEUROABGPQURUMUQRZAFUNUQADSUSUMAUMUQTUAUBQUMUOAUMULUPKJZGLZBDMUOBULUCUMV
      AUOBDUMUPDERZVAUOVBVARUTGDVBVAUDVBUTDEVAULUPUEUFUGUHUIUJUKQ $.
      $( [22-May-2007] $) $( [22-May-2007] $)
  $}

  $( 0 is a real number. $)
  0reALT $p |- 0 e. RR $=
    ( c1 cmin co cc0 cr ax1cn subid 1re resubcl eqeltrr ) AABCDEAFGAAHHIJ $.
    $( [20-Feb-2005] $) $( [19-Feb-2005] $)

  $( "Reverse" second Peano postulate analog for reals. $)
  peano2rem $p |- ( N e. RR -> ( N - 1 ) e. RR ) $=
    ( cr wcel c1 cmin co 1re resubclt mpan2 ) ABCDBCADEFBCGADHI $.
    $( [7-Feb-2007] $) $( [6-Feb-2007] $)

  $( Multiplication by 0.  Theorem I.6 of [Apostol] p. 18. $)
  mul01t $p |- ( A e. CC -> ( A x. 0 ) = 0 ) $=
    ( cc wcel cc0 cmul co wceq cif opreq1 eqeq1d 0cn elimel mul01 dedth ) ABCZA
    DEFZDGOADHZDEFZDGADAQGPRDAQDEIJQADBKLMN $.
    $( [15-May-1999] $)

  $( Multiplication by 0.  Theorem I.6 of [Apostol] p. 18. $)
  mul02t $p |- ( A e. CC -> ( 0 x. A ) = 0 ) $=
    ( cc wcel cc0 cmul co wceq 0cn axmulcom mpan mul01t eqtrd ) ABCZDAEFZADEFZD
    DBCMNOGHDAIJAKL $.
    $( [10-Aug-1999] $)

  ${
    mulneg.1 $e |- A e. CC $.
    mulneg.2 $e |- B e. CC $.

    $( Product with negative is negative of product.  Theorem I.12 of [Apostol]
       p. 18. $)
    mulneg1 $p |- ( -u A x. B ) = -u ( A x. B ) $=
      ( cc0 cmul co cmin cneg mul01 mulcom opreq12i df-neg opreq1i 0cn subcl
      subdi 3eqtr 3eqtr4 ) BEFGZBAFGZHGZEABFGZHGAIZBFGZUCITEUAUCHBDJBADCKLUEEAH
      GZBFGBUFFGUBUDUFBFAMNUFBEAOCPDKBEADOCQRUCMS $.
      $( [10-Feb-1995] $)

    $( Product with negative is negative of product. $)
    mulneg2 $p |- ( A x. -u B ) = -u ( A x. B ) $=
      ( cneg cmul co negcl mulcom mulneg1 negeqi 3eqtr ) ABEZFGMAFGBAFGZEABFGZE
      AMCBDHIBADCJNOBADCIKL $.
      $( [31-Jul-1999] $)

    $( Product of two negatives.  Theorem I.12 of [Apostol] p. 18. $)
    mul2neg $p |- ( -u A x. -u B ) = ( A x. B ) $=
      ( cneg cmul co negcl mulneg1 mulcom eqtr negeqi mulcl negneg 3eqtr ) AEBE
      ZFGZBAFGZABFGQAPFGZEREZERAPCBDHZISTSPAFGTAPCUAJBADCIKLRBADCMNOBADCJK $.
      $( [14-Feb-1995] $)

    $( Distribution of negative over addition. $)
    negdi $p |- -u ( A + B ) = ( -u A + -u B ) $=
      ( c1 caddc co cmul cneg addcl mulid2 negeqi ax1cn negcl adddi mulneg1
      eqtr opreq12i 3eqtr3 eqtr3 ) EABFGZHGZIZUAIAIZBIZFGZUBUAUAABCDJZKLEIZUAHG
      UHAHGZUHBHGZFGUCUFUHABEMNCDOEUAMUGPUIUDUJUEFUIEAHGZIUDEAMCPUKAACKLQUJEBHG
      ZIUEEBMDPULBBDKLQRST $.
      $( [28-Jul-1999] $)

    $( Distribution of negative over subtraction. $)
    negsubdi $p |- -u ( A - B ) = ( -u A + B ) $=
      ( cneg caddc co cmin negcl negdi negsub negeqi negneg opreq2i 3eqtr3 ) AB
      EZFGZEAEZPEZFGABHGZERBFGAPCBDIJQTABCDKLSBRFBDMNO $.
      $( [6-Aug-1999] $)

    $( Distribution of negative over subtraction. $)
    negsubdi2 $p |- -u ( A - B ) = ( B - A ) $=
      ( cmin co cneg caddc negsubdi negcl addcom negsub 3eqtr ) ABEFGAGZBHFBNHF
      BAEFABCDINBACJDKBADCLM $.
      $( [1-Oct-1999] $)
  $}

  $( Product with negative is negative of product.  Theorem I.12 of [Apostol]
     p. 18. $)
  mulneg1t $p |- ( ( A e. CC /\ B e. CC ) ->
                 ( -u A x. B ) = -u ( A x. B ) ) $=
    ( cc wcel cneg cmul co wceq cc0 cif negeq opreq1d opreq1 negeqd eqeq12d
    opreq2 0cn elimel mulneg1 dedth2h ) ACDZBCDZAEZBFGZABFGZEZHUAAIJZEZBFGZUGBF
    GZEZHUHUBBIJZFGZUGULFGZEZHABIIAUGHZUDUIUFUKUPUCUHBFAUGKLUPUEUJAUGBFMNOBULHZ
    UIUMUKUOBULUHFPUQUJUNBULUGFPNOUGULAICQRBICQRST $.
    $( [14-May-1999] $)

  $( The product with a negative is the negative of the product. $)
  mulneg2t $p |- ( ( A e. CC /\ B e. CC ) ->
                 ( A x. -u B ) = -u ( A x. B ) ) $=
    ( cc wcel wa cneg cmul co wceq mulneg1t ancoms axmulcom negclt sylan2
    negeqd 3eqtr4d ) ACDZBCDZEZBFZAGHZBAGHZFZATGHZABGHZFRQUAUCIBAJKQTCDUDUAIRAT
    LBMNSUEUBABLOP $.
    $( [25-Aug-2004] $) $( [30-Jul-2004] $)

  $( Swap the negative sign in a product. $)
  mulneg12t $p |- ( ( A e. CC /\ B e. CC ) ->
                  ( -u A x. B ) = ( A x. -u B ) ) $=
    ( cc wcel wa cneg cmul co mulneg1t mulneg2t eqtr4d ) ACDBCDEAFBGHABGHFABFGH
    ABIABJK $.
    $( [2-Sep-2004] $) $( [30-Jul-2004] $)

  $( Product of two negatives.  Theorem I.12 of [Apostol] p. 18. $)
  mul2negt $p |- ( ( A e. CC /\ B e. CC ) -> ( -u A x. -u B ) = ( A x. B ) ) $=
    ( cc wcel cneg cmul co wceq cc0 cif negeq opreq1d opreq1 eqeq12d opreq2d
    opreq2 0cn elimel mul2neg dedth2h ) ACDZBCDZAEZBEZFGZABFGZHUAAIJZEZUDFGZUGB
    FGZHUHUBBIJZEZFGZUGUKFGZHABIIAUGHZUEUIUFUJUOUCUHUDFAUGKLAUGBFMNBUKHZUIUMUJU
    NUPUDULUHFBUKKOBUKUGFPNUGUKAICQRBICQRST $.
    $( [30-Aug-2004] $) $( [30-Jul-2004] $)

  $( Distribution of negative over addition. $)
  negdit $p |- ( ( A e. CC /\ B e. CC ) ->
                 -u ( A + B ) = ( -u A + -u B ) ) $=
    ( cc wcel caddc co cneg wceq cc0 cif opreq1 negeqd negeq opreq1d eqeq12d
    opreq2 opreq2d 0cn elimel negdi dedth2h ) ACDZBCDZABEFZGZAGZBGZEFZHUBAIJZBE
    FZGZUIGZUGEFZHUIUCBIJZEFZGZULUNGZEFZHABIIAUIHZUEUKUHUMUSUDUJAUIBEKLUSUFULUG
