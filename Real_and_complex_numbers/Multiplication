    $( [1-Aug-2006] $) $( [4-Oct-1999] $)

  $( Convert a subtraction to addition using multiplication by a negative. $)
  submul2t $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
                 ( A - ( B x. C ) ) = ( A + ( B x. -u C ) ) ) $=
    ( cc wcel cmul co cmin cneg caddc wceq wa mulneg2t adantl opreq2d negsubt
    axmulcl sylan2 eqtr2d 3impb ) ADEZBDEZCDEZABCFGZHGZABCIFGZJGZKUAUBUCLZLZUGA
    UDIZJGZUEUIUFUJAJUHUFUJKUABCMNOUAUDDEUKUEKUHAUDPBCQRST $.
