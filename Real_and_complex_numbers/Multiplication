  $( Identity law for multiplication.  Note:  see ~ ax1id for commuted
     version. $)
  mulid2t $p |- ( A e. CC -> ( 1 x. A ) = A ) $=
    ( cc wcel c1 cmul co wceq ax1cn axmulcom mpan ax1id eqtrd ) ABCZDAEFZADEFZA
    DBCMNOGHDAIJAKL $.
    $( [8-Oct-1999] $)

  $( Commutative/associative law for multiplication. $)
  mul12t $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
               ( A x. ( B x. C ) ) = ( B x. ( A x. C ) ) ) $=
    ( cc wcel w3a cmul co wceq wa axmulcom opreq1d 3adant3 axmulass 3com12
    3eqtr3d ) ADEZBDEZCDEZFABGHZCGHZBAGHZCGHZABCGHGHBACGHGHZQRUAUCISQRJTUBCGABK
    LMABCNRQSUCUDIBACNOP $.
    $( [29-May-2005] $) $( [30-Apr-2005] $)

  $( Commutative/associative law. $)
  mul23t $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
               ( ( A x. B ) x. C ) = ( ( A x. C ) x. B ) ) $=
    ( cc wcel w3a cmul co wceq wa axmulcom opreq2d 3adant1 axmulass 3com23
    3eqtr4d ) ADEZBDEZCDEZFABCGHZGHZACBGHZGHZABGHCGHACGHBGHZRSUAUCIQRSJTUBAGBCK
    LMABCNQSRUDUCIACBNOP $.
    $( [8-Oct-1999] $)

  $( Rearrangement of 4 factors. $)
  mul4t $p |- ( ( ( A e. CC /\ B e. CC ) /\ ( C e. CC /\ D e. CC ) ) ->
              ( ( A  x. B )  x. ( C  x. D ) ) =
                 ( ( A  x. C )  x. ( B  x. D ) ) ) $=
    ( cc wcel wa cmul co wceq w3a mul23t opreq1d 3expa adantrr axmulass 3expb
    axmulcl sylan an4s 3eqtr3d ) AEFZBEFZGZCEFZDEFZGZGABHIZCHIZDHIZACHIZBHIZDHI
    ZUHCDHIHIZUKBDHIHIZUDUEUJUMJZUFUBUCUEUPUBUCUEKUIULDHABCLMNOUHEFZUGUJUNJZUDU
    QUEUFURUHCDPQABRSUBUEUCUFUMUOJZUKEFZUCUFGUSUBUEGUTUCUFUSUKBDPQACRSTUA $.
    $( [8-Oct-1999] $)

  $( Product of two sums. $)
  muladdt $p |- ( ( ( A e. CC /\ B e. CC ) /\ ( C e. CC /\ D e. CC ) ) ->
               ( ( A + B ) x. ( C + D ) ) =
       ( ( ( A x. C ) + ( D x. B ) ) + ( ( A x. D ) + ( C x. B ) ) ) ) $=
    ( cc wcel wa caddc co cmul wceq axdistr axaddcl adantr simprl simprr
    syl3anc adddirt 3expa adantrr adantrl opreq12d add23t axmulcl ad2ant2r
    adantll syl2an anandirs axmulcom ad2ant2l opreq2d axaddass adantlr ancoms
    3eqtr3d adantl an42s an4s 3eqtrd ) AEFZBEFZGZCEFZDEFZGZGZABHIZCDHIJIZVGCJIZ
    VGDJIZHIZACJIZBCJIZHIZADJIZBDJIZHIZHIZVLDBJIZHIZVOCBJIZHIHIZVGEFZVCVDVHVKKV
    FVGCDLVBWCVEABMNVBVCVDOVBVCVDPQVFVIVNVJVQHVBVCVIVNKZVDUTVAVCWDABCRSTVBVDVJV
    QKZVCUTVAVDWEABDRSUAUBVFVRVLVQHIZVMHIZVTVOHIZWAHIZWBVLEFZVMEFZVQEFZVRWGKVFV
    LVMVQUCUTVCWJVAVDACUDZUEZVAVEWKUTVAVCWKVDBCUDTUFVBVDWLVCUTVAVDWLVOEFZVPEFZW
    LUTVDGZVAVDGZVOVPMADUDZBDUDZUGUHUAQVFWFWHVMWAHVFVLVOHIZVPHIZXAVSHIZWFWHVFVP
    VSXAHVAVDVPVSKUTVCBDUIUJUKWJWOWPXBWFKVFVLVOVPULWNUTVEWOVAUTVDWOVCWSUAUMZVAV
    DWPUTVCWTUJQWJWOVSEFZXCWHKVFVLVOVSUCWNXDVAVDXEUTVCVDVAXEDBUDUNZUJQUOUTVDVAV
    CVMWAKZVAVCGZXGWQBCUIUPUQUBVTEFZWOWAEFZWIWBKVFVTVOWAULUTVCVAVDXIWJXEXIUTVCG
    WRVLVSMWMXFUGURXDUTVDVAVCXJXHXJWQVCVAXJCBUDUNUPUQQUSUS $.
    $( [15-Jan-2006] $) $( [14-Jan-2006] $)

  $( A simple product of sums expansion. $)
  muladd11t $p |- ( ( A e. CC /\ B e. CC ) -> ( ( 1 + A ) x. ( 1 + B ) ) =
                 ( ( 1 + A ) + ( B + ( A x. B ) ) ) ) $=
    ( cc wcel wa c1 caddc co cmul wceq ax1cn axdistr mp3an2 axaddcl mpan sylan
    ax1id syl adantr adddirt mp3an1 mulid2t adantl opreq1d eqtrd opreq12d ) ACD
    ZBCDZEZFAGHZFBGHIHZUJFIHZUJBIHZGHZUJBABIHZGHZGHUJCDZUHUKUNJZUGUQFCDZUHURKUJ
    FBLMUSUGUQKFANOZPUIULUJUMUPGUGULUJJZUHUGUQVAUTUJQRSUIUMFBIHZUOGHZUPUSUGUHUM
    VCJKFABTUAUIVBBUOGUHVBBJUGBUBUCUDUEUFUE $.
    $( [9-Mar-2005] $) $( [21-Feb-2005] $)

  ${
    mul.1 $e |- A e. CC $.
    mul.2 $e |- B e. CC $.
    mul.3 $e |- C e. CC $.
    $( Commutative/associative law that swaps the first two factors in a triple
       product. $)
    mul12 $p |- ( A x. ( B x. C ) ) = ( B x. ( A x. C ) ) $=
      ( cmul co mulcom opreq1i mulass 3eqtr3 ) ABGHZCGHBAGHZCGHABCGHGHBACGHGHMN
      CGABDEIJABCDEFKBACEDFKL $.
      $( [11-May-1999] $)

    $( Commutative/associative law that swaps the last two factors in a triple
       product. $)
    mul23 $p |- ( ( A x. B ) x. C ) = ( ( A x. C ) x. B ) $=
      ( cc wcel cmul co wceq mul23t mp3an ) AGHBGHCGHABIJCIJACIJBIJKDEFABCLM $.
      $( [11-May-1999] $)
  $}

  ${
    mul4.1 $e |- A e. CC $.
    mul4.2 $e |- B e. CC $.
    mul4.3 $e |- C e. CC $.
    mul4.4 $e |- D e. CC $.
    $( Rearrangement of 4 factors. $)
    mul4 $p |- ( ( A  x. B )  x. ( C  x. D ) ) =
               ( ( A  x. C )  x. ( B  x. D ) ) $=
      ( cc wcel wa cmul co wceq pm3.2i mul4t mp2an ) AIJZBIJZKCIJZDIJZKABLMCDLM
      LMACLMBDLMLMNRSEFOTUAGHOABCDPQ $.
      $( [16-Feb-1995] $)

    $( Product of two sums. $)
    muladd $p |- ( ( A + B ) x. ( C + D ) ) =
        ( ( ( A x. C ) + ( D x. B ) ) + ( ( A x. D ) + ( C x. B ) ) ) $=
      ( cc wcel wa caddc co cmul wceq pm3.2i muladdt mp2an ) AIJZBIJZKCIJZDIJZK
      ABLMCDLMNMACNMDBNMLMADNMCBNMLMLMOSTEFPUAUBGHPABCDQR $.
      $( [17-May-1999] $)
  $}

  $( Distribution of multiplication over subtraction.  Theorem I.5 of
     [Apostol] p. 18. $)
  subdit $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
               ( A x. ( B - C ) ) = ( ( A x. B ) - ( A x. C ) ) ) $=
    ( cc wcel w3a cmul co cmin wceq caddc axdistr 3simp1 3simp3 subclt 3adant1
    syl3anc pncan3t ancoms opreq2d eqtr3d wb subaddt axmulcl 3adant3 3adant2
    wa sylan2 3impb mpbird eqcomd ) ADEZBDEZCDEZFZABGHZACGHZIHZABCIHZGHZUOURUTJ
    ZUQUTKHZUPJZUOACUSKHZGHZVBUPULUNUSDEZVEVBJUOACUSLULUMUNMULUMUNNUMUNVFULBCOZ
    PQUOVDBAGUMUNVDBJZULUNUMVHCBRSPTUAUPDEZUQDEZUTDEZVAVCUBUOUPUQUTUCULUMVIUNAB
    UDUEULUNVJUMACUDUFULUMUNVKULVFVKUMUNUGAUSUDVGUHUIQUJUK $.
    $( [22-Nov-2004] $) $( [18-Nov-2004] $)

  $( Distribution of multiplication over subtraction.  Theorem I.5 of
     [Apostol] p. 18. $)
  subdirt $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
               ( ( A - B ) x. C ) = ( ( A x. C ) - ( B x. C ) ) ) $=
    ( cc wcel w3a cmin co cmul wceq subdit 3coml wa axmulcom subclt sylan
    3impa 3adant2 3adant1 opreq12d 3eqtr4d ) ADEZBDEZCDEZFZCABGHZIHZCAIHZCBIHZG
    HZUFCIHZACIHZBCIHZGHUDUBUCUGUJJCABKLUBUCUDUKUGJZUFDEUDUNUBUCMUFCNABOPQUEULU
    HUMUIGUBUDULUHJUCACNRUCUDUMUIJUBBCNSTUA $.
    $( [31-Dec-2005] $) $( [30-Dec-2005] $)

  ${
    subdi.1 $e |- A e. CC $.
    subdi.2 $e |- B e. CC $.
    subdi.3 $e |- C e. CC $.
    $( Distribution of multiplication over subtraction.  Theorem I.5 of
       [Apostol] p. 18. $)
    subdi $p |- ( A x. ( B - C ) ) = ( ( A x. B ) - ( A x. C ) ) $=
      ( cc wcel cmin co cmul wceq subdit mp3an ) AGHBGHCGHABCIJKJABKJACKJIJLDEF
      ABCMN $.
      $( [26-Nov-1994] $)

    $( Distribution of multiplication over subtraction.  Theorem I.5 of
       [Apostol] p. 18. $)
    subdir $p |- ( ( A - B ) x. C ) = ( ( A x. C ) - ( B x. C ) ) $=
      ( cc wcel cmin co cmul wceq subdirt mp3an ) AGHBGHCGHABIJCKJACKJBCKJIJLDE
      FABCMN $.
      $( [8-May-1999] $)
  $}

  ${
    mulzer.1 $e |- A e. CC $.

    $( Multiplication by 0.  Theorem I.6 of [Apostol] p. 18. $)
    mul01 $p |- ( A x. 0 ) = 0 $=
      ( cc0 cmin co cmul 0cn subdi subid opreq2i mulcl 3eqtr3 ) ACCDEZFEACFEZND
      ENCACCBGGHMCAFCGIJNACBGKIL $.
      $( [23-Nov-1994] $)

    $( Multiplication by 0.  Theorem I.6 of [Apostol] p. 18. $)
    mul02 $p |- ( 0 x. A ) = 0 $=
      ( cc0 cmul co 0cn mulcom mul01 eqtr ) CADEACDECCAFBGABHI $.
      $( [23-Nov-1994] $)

    $( Two times a number. $)
    1p1times $p |- ( ( 1 + 1 ) x. A ) = ( A + A ) $=
      ( c1 caddc co cmul ax1cn adddir mulid2 opreq12i eqtr ) CCDEAFECAFEZLDEAAD
      ECCAGGBHLALADABIZMJK $.
      $( [18-May-1999] $)
  $}

  $( The imaginary unit ` i ` is not zero. $)
  ine0 $p |- i =/= 0 $=
    ( ci cc0 wne wceq wn c1 ax1ne0 df-ne mpbi caddc co cmul opreq2 axicn mul01
    syl6req opreq1d ax1cn addid2 axi2m1 3eqtr3g mto mpbir ) ABCABDZEUDFBDZFBCUE
    EGFBHIUDBFJKAALKZFJKFBUDBUFFJUDUFABLKBABALMANOPQFRSTUAUBABHUC $.
    $( [29-Apr-2005] $) $( [6-May-1999] $)

  ${
    $d x y z $.
    $( 1 is a real number.  (Contributed by Eric Schmidt, 11-Apr-2007.) $)
    1re $p |- 1 e. RR $=
      ( vz vx vy cv cc0 wne cr wrex c1 wcel ci cmul co caddc wceq cc axicn
      axcnre ax-mp wa wo neeq1 rcla4ev adantlr adantll jaodan ex ine0 mpbii wn
      ioran nne anbi12i bitr opreq12 opreq1 mul02 syl6eq sylan2 0cn addid1
      sylbi necon1ai syl syl5 r19.23aivv axrrecex wi axmulrcl adantr wb eleq1
      adantl mpbid r19.23adva mpd r19.23aiva ) ADZEFZAGHZIGJZKBDZCDZKLMZNMZOZCG
      HBGHZVTKPJWGQBCKRSWFVTBCGGWBGJZWCGJZTZWBEFZWCEFZUAZVTWFWJWMVTWJWKVTWLWHWK
      VTWIVSWKAWBGVRWBEUBUCUDWIWLVTWHVSWLAWCGVRWCEUBUCUEUFUGWFWEEFZWMWFKEFWNUHK
