  ${
    mul0or.1 $e |- A e. CC $.
    ${
      mul0or.2 $e |- B e. CC $.
      $( If a product is zero, one of its factors must be zero.  Theorem I.11
         of [Apostol] p. 18. $)
      mul0or $p |- ( ( A x. B ) = 0 <-> ( A = 0 \/ B = 0 ) ) $=
        ( cmulc co cc0 wceq wo wn wne wi df-ne cc wcel w3a wb 0cn 3pm3.2i
        mulcant2 mpan mulzer1 cleq2i syl5bbr biimpd sylbir com12 orrd opreq1
        mulzer2 syl6eq opreq2 jaoi impbi ) ABEFZGHZAGHZBGHZIUPUQURUQJZUPURUSAGK
        ZUPURLAGMUTUPURUTUOAGEFZHZURUPANOZBNOZGNOZPUTVBURQVCVDVECDRSABGTUAVAGUO
        ACUBZUCUDUEUFUGUHUQUPURUQUOGBEFGAGBEUIBDUJUKURUOVAGBGAEULVFUKUMUN $.
        $( [7-Oct-99] $)
    $}

    $( A number is zero iff its square is zero. $)
    sq0 $p |- ( ( A x. A ) = 0 <-> A = 0 ) $=
      ( cmulc co cc0 wceq wo mul0or oridm bitr ) AACDEFAEFZKGKAABBHKIJ $.
      $( [28-Jul-99] $)
  $}

  $( If a product is zero, one of its factors must be zero.  Theorem I.11
     of [Apostol] p. 18. $)
  mul0ort $p |- ( ( A e. CC /\ B e. CC ) ->
                 ( ( A x. B ) = 0 <-> ( A = 0 \/ B = 0 ) ) ) $=
    ( cc wcel cmulc co cc0 wceq wo wb cded opreq1 cleq1d cleq1 birord bi2bid
    opreq2 bilord 0cn elimel mul0or dedth2h ) ACDZBCDZABEFZGHZAGHZBGHZIZJUCAGKZ
    BEFZGHZUJGHZUHIZJUJUDBGKZEFZGHZUMUOGHZIZJABGGAUJHZUFULUIUNUTUEUKGAUJBELMUTU
    GUMUHAUJGNOPBUOHZULUQUNUSVAUKUPGBUOUJEQMVAUHURUMBUOGNRPUJUOAGCSTBGCSTUAUB
    $.
    $( [9-Oct-99] $)

  $( The product of two non-zero numbers is non-zero. $)
  muln0bt $p |- ( ( A e. CC /\ B e. CC ) -> ( ( A =/= 0 /\ B =/= 0 ) <->
               ( A x. B ) =/= 0 ) ) $=
    ( cc wcel wa cc0 wceq wn cmulc co wne wo mul0ort binegd ioran syl6rbb
    df-ne bi2an 3bitr4g ) ACDBCDEZAFGZHZBFGZHZEZABIJZFGZHZAFKZBFKZEUFFKTUHUAUCL
    ZHUETUGUKABMNUAUCOPUIUBUJUDAFQBFQRUFFQS $.
    $( [2-Aug-04] $) $( [1-Aug-04] $)

  ${
    muln0.1 $e |- A e. CC $.
    muln0.2 $e |- B e. CC $.
    muln0.3 $e |- A =/= 0 $.
    muln0.4 $e |- B =/= 0 $.
    $( The product of two non-zero numbers is non-zero. $)
    muln0 $p |- ( A x. B ) =/= 0 $=
      ( cc wcel cmulc co cc0 wne wa pm3.2i muln0bt mpbii mp2an ) AGHZBGHZABIJKL
      ZCDRSMAKLZBKLZMTUAUBEFNABOPQ $.
      $( [15-Feb-95] $)
  $}

  ${
    $d x y A $.  $d x y B $. $( ` y ` is dummy. $)
    receu.1 $e |- A e. CC $.
    receu.2 $e |- B e. CC $.
    receu.3 $e |- A =/= 0 $.
