    $( [5-Jun-2004] $) $( [10-May-2004] $)

  $( Distribution of negative over addition. $)
  negdi2t $p |- ( ( A e. CC /\ B e. CC ) ->
                 -u ( A + B ) = ( -u A - B ) ) $=
    ( cc wcel wa caddc co cneg cmin negdit wceq negsubt negclt sylan eqtrd ) AC
    DZBCDZEABFGHAHZBHFGZRBIGZABJRCDQSTKPRBLAMNO $.
    $( [2-Jan-2006] $) $( [1-Jan-2006] $)

  $( Distribution of negative over subtraction. $)
  negsubdit $p |- ( ( A e. CC /\ B e. CC ) -> -u ( A - B ) = ( -u A + B ) ) $=
    ( cc wcel wa cneg caddc co cmin wceq negdit negclt sylan2 negsubt negeqd
    negnegt adantl opreq2d 3eqtr3d ) ACDZBCDZEZABFZGHZFZAFZUCFZGHZABIHZFUFBGHTU
    CCDUEUHJUAAUCKBLMUBUDUIABNOUBUGBUFGUAUGBJTBPQRS $.
    $( [1-Aug-2006] $) $( [15-Nov-2004] $)

  $( Distribution of negative over subtraction. $)
  negsubdi2t $p |- ( ( A e. CC /\ B e. CC ) -> -u ( A - B ) = ( B - A ) ) $=
    ( cc wcel wa cmin co cneg caddc negsubdit wceq axaddcom negclt sylan
