$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Multiplication
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Identity law for multiplication.  Note:  see ~ ax1id for commuted
     version. $)
  mulid2t $p |- ( A e. CC -> ( 1 x. A ) = A ) $=
    ( cc wcel c1 cmulc co wceq 1cn axmulcom mpan ax1id eqtrd ) ABCZDAEFZADEFZAD
    BCMNOGHDAIJAKL $.
    $( [8-Oct-99] $)

  $( Commutative/associative law. $)
  mul23t $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
               ( ( A x. B ) x. C ) = ( ( A x. C ) x. B ) ) $=
    ( cc wcel w3a cmulc co wceq wa axmulcom opreq2d 3adant1 axmulass 3com23
    3eqtr4d ) ADEZBDEZCDEZFABCGHZGHZACBGHZGHZABGHCGHACGHBGHZRSUAUCIQRSJTUBAGBCK
    LMABCNQSRUDUCIACBNOP $.
    $( [8-Oct-99] $)

  $( Rearrangement of 4 factors. $)
  mul4t $p |- ( ( ( A e. CC /\ B e. CC ) /\ ( C e. CC /\ D e. CC ) ) ->
              ( ( A  x. B )  x. ( C  x. D ) ) =
                 ( ( A  x. C )  x. ( B  x. D ) ) ) $=
    ( cc wcel wa cmulc co wceq w3a mul23t opreq1d 3expa adantrr axmulass 3expb
    axmulcl sylan an4s 3eqtr3d ) AEFZBEFZGZCEFZDEFZGZGABHIZCHIZDHIZACHIZBHIZDHI
    ZUHCDHIHIZUKBDHIHIZUDUEUJUMJZUFUBUCUEUPUBUCUEKUIULDHABCLMNOUHEFZUGUJUNJZUDU
    QUEUFURUHCDPQABRSUBUEUCUFUMUOJZUKEFZUCUFGUSUBUEGUTUCUFUSUKBDPQACRSTUA $.
    $( [8-Oct-99] $)

  ${
    mul.1 $e |- A e. CC $.
    mul.2 $e |- B e. CC $.
    mul.3 $e |- C e. CC $.
    $( Commutative/associative law that swaps the first two factors in a triple
       product. $)
    mul12 $p |- ( A x. ( B x. C ) ) = ( B x. ( A x. C ) ) $=
      ( cmulc co mulcom opreq1i mulass 3eqtr3 ) ABGHZCGHBAGHZCGHABCGHGHBACGHGHM
      NCGABDEIJABCDEFKBACEDFKL $.
      $( [11-May-99] $)

    $( Commutative/associative law that swaps the last two factors in a triple
       product. $)
    mul23 $p |- ( ( A x. B ) x. C ) = ( ( A x. C ) x. B ) $=
      ( cc wcel cmulc co wceq mul23t mp3an ) AGHBGHCGHABIJCIJACIJBIJKDEFABCLM
      $.
      $( [11-May-99] $)
  $}

  ${
    mul4.1 $e |- A e. CC $.
    mul4.2 $e |- B e. CC $.
    mul4.3 $e |- C e. CC $.
    mul4.4 $e |- D e. CC $.
    $( Rearrangement of 4 factors. $)
    mul4 $p |- ( ( A  x. B )  x. ( C  x. D ) ) =
               ( ( A  x. C )  x. ( B  x. D ) ) $=
      ( cc wcel wa cmulc co wceq pm3.2i mul4t mp2an ) AIJZBIJZKCIJZDIJZKABLMCDL
      MLMACLMBDLMLMNRSEFOTUAGHOABCDPQ $.
      $( [16-Feb-95] $)

    $( Product of sums. $)
    muladd $p |- ( ( A + B ) x. ( C + D ) ) =
        ( ( ( A x. C ) + ( D x. B ) ) + ( ( A x. D ) + ( C x. B ) ) ) $=
      ( caddc co cmulc addcl distr1 distr2 opreq12i mulcl add23 mulcom opreq2i
      addass 3eqtr3r 3eqtr ) ABIJZCDIJKJUCCKJZUCDKJZIJACKJZBCKJZIJZADKJZBDKJZIJ
      ZIJZUFDBKJZIJZUICBKJZIJIJZUCCDABEFLGHMUDUHUEUKIABCEFGNABDEFHNOULUFUKIJZUG
      IJUNUIIJZUOIJUPUFUGUKACEGPZBCFGPUIUJADEHPZBDFHPZLQUQURUGUOIUFUIIJZUMIJVBU
      JIJURUQUMUJVBIDBHFRSUFUIUMUSUTDBHFPZQUFUIUJUSUTVATUABCFGROUNUIUOUFUMUSVCL
      UTCBGFPTUBUB $.
      $( [17-May-99] $)
  $}

  ${
    subdist.1 $e |- A e. CC $.
    subdist.2 $e |- B e. CC $.
    subdist.3 $e |- C e. CC $.
    $( Distribution of muliplication over subtraction.  Theorem I.5 of
       [Apostol] p. 18. $)
    subdi $p |- ( A x. ( B - C ) ) = ( ( A x. B ) - ( A x. C ) ) $=
      ( cmulc co cmin wceq caddc subcl distr1 subaddeq opreq2i eqtr3 mulcl
      subadd mpbir cleqcomi ) ABGHZACGHZIHZABCIHZGHZUCUEJUBUEKHZUAJACUDKHZGHUFU
      AACUDDFBCEFLZMUGBAGCBFENOPUAUBUEABDEQACDFQAUDDUHQRST $.
      $( [26-Nov-94] $)

    $( Distribution of muliplication over subtraction.  Theorem I.5 of
       [Apostol] p. 18. $)
    subdistr $p |- ( ( A - B ) x. C ) = ( ( A x. C ) - ( B x. C ) ) $=
      ( cmin co cmulc subdi subcl mulcom opreq12i 3eqtr4 ) CABGHZIHCAIHZCBIHZGH
      OCIHACIHZBCIHZGHCABFDEJOCABDEKFLRPSQGACDFLBCEFLMN $.
      $( [8-May-99] $)
  $}

  ${
    mulzer.1 $e |- A e. CC $.

    $( Multiplication by 0.  Theorem I.6 of [Apostol] p. 18. $)
    mulzer1 $p |- ( A x. 0 ) = 0 $=
      ( cc0 cmin co cmulc 0cn subdi subid opreq2i mulcl 3eqtr3 ) ACCDEZFEACFEZN
      DENCACCBGGHMCAFCGIJNACBGKIL $.
      $( [23-Nov-94] $)

    $( Multiplication by 0.  Theorem I.6 of [Apostol] p. 18. $)
    mulzer2 $p |- ( 0 x. A ) = 0 $=
      ( cc0 cmulc co 0cn mulcom mulzer1 eqtr ) CADEACDECCAFBGABHI $.
      $( [23-Nov-94] $)

    $( Two times a number. $)
    1p1times $p |- ( ( 1 + 1 ) x. A ) = ( A + A ) $=
      ( c1 caddc co cmulc 1cn distr2 mulid2 opreq12i eqtr ) CCDEAFECAFEZLDEAADE
      CCAGGBHLALADABIZMJK $.
      $( [18-May-99] $)
  $}

  $( Multiplication by 0.  Theorem I.6 of [Apostol] p. 18. $)
  mulzer1t $p |- ( A e. CC -> ( A x. 0 ) = 0 ) $=
    ( cc wcel cc0 cmulc co wceq cded opreq1 cleq1d 0cn elimel mulzer1 dedth )
    ABCZADEFZDGOADHZDEFZDGADAQGPRDAQDEIJQADBKLMN $.
    $( [15-May-99] $)

  $( Multiplication by 0.  Theorem I.6 of [Apostol] p. 18. $)
  mulzer2t $p |- ( A e. CC -> ( 0 x. A ) = 0 ) $=
    ( cc wcel cc0 cmulc co wceq 0cn axmulcom mpan mulzer1t eqtrd ) ABCZDAEFZADE
    FZDDBCMNOGHDAIJAKL $.
    $( [10-Aug-99] $)

  ${
    mulneg.1 $e |- A e. CC $.
    mulneg.2 $e |- B e. CC $.

    $( Product with negative is negative of product.  Theorem I.12 of [Apostol]
       p. 18. $)
    mulneg1 $p |- ( -u A x. B ) = -u ( A x. B ) $=
      ( cc0 cmulc co cmin cneg mulzer1 mulcom opreq12i df-neg opreq1i 0cn
      subcl subdi 3eqtr 3eqtr4 ) BEFGZBAFGZHGZEABFGZHGAIZBFGZUCITEUAUCHBDJBADCK
      LUEEAHGZBFGBUFFGUBUDUFBFAMNUFBEAOCPDKBEADOCQRUCMS $.
      $( [10-Feb-95] $)

    $( Product with negative is negative of product. $)
    mulneg2 $p |- ( A x. -u B ) = -u ( A x. B ) $=
      ( cneg cmulc co negcl mulcom mulneg1 negeqi 3eqtr ) ABEZFGMAFGBAFGZEABFGZ
      EAMCBDHIBADCJNOBADCIKL $.
      $( [31-Jul-99] $)

    $( Product of two negatives.  Theorem I.12 of [Apostol] p. 18. $)
    mul2neg $p |- ( -u A x. -u B ) = ( A x. B ) $=
      ( cneg cmulc co negcl mulneg1 mulcom eqtr negeqi mulcl negneg 3eqtr ) AEB
      EZFGZBAFGZABFGQAPFGZEREZERAPCBDHZISTSPAFGTAPCUAJBADCIKLRBADCMNOBADCJK $.
      $( [14-Feb-95] $)

    $( Distribution of negative over addition. $)
    negdi $p |- -u ( A + B ) = ( -u A + -u B ) $=
      ( c1 caddc co cmulc cneg addcl mulid2 negeqi 1cn negcl distr1 mulneg1
      eqtr opreq12i 3eqtr3 eqtr3 ) EABFGZHGZIZUAIAIZBIZFGZUBUAUAABCDJZKLEIZUAHG
      UHAHGZUHBHGZFGUCUFUHABEMNCDOEUAMUGPUIUDUJUEFUIEAHGZIUDEAMCPUKAACKLQUJEBHG
      ZIUEEBMDPULBBDKLQRST $.
      $( [28-Jul-99] $)

    $( Distribution of negative over subtraction. $)
    negdist2 $p |- -u ( A - B ) = ( -u A + B ) $=
      ( cmin co cneg caddc subneg negeqi negcl negdi negneg opreq2i 3eqtr ) ABE
      FZGABGZHFZGAGZQGZHFSBHFPRABCDIJAQCBDKLTBSHBDMNO $.
      $( [6-Aug-99] $)

    $( Distribution of negative over subtraction. $)
    negdist3 $p |- -u ( A - B ) = ( B - A ) $=
      ( cneg caddc co cmin negcl addcom negdist2 subneg 3eqtr4 ) AEZBFGBNFGABHG
      EBAHGNBACIDJABCDKBADCLM $.
      $( [1-Oct-99] $)
  $}

  $( Product with negative is negative of product.  Theorem I.12 of [Apostol]
     p. 18. $)
  mulneg1t $p |- ( ( A e. CC /\ B e. CC ) ->
                 ( -u A x. B ) = -u ( A x. B ) ) $=
    ( cc wcel cneg cmulc co wceq cc0 cded negeq opreq1d opreq1 negeqd cleq12d
    opreq2 0cn elimel mulneg1 dedth2h ) ACDZBCDZAEZBFGZABFGZEZHUAAIJZEZBFGZUGBF
    GZEZHUHUBBIJZFGZUGULFGZEZHABIIAUGHZUDUIUFUKUPUCUHBFAUGKLUPUEUJAUGBFMNOBULHZ
    UIUMUKUOBULUHFPUQUJUNBULUGFPNOUGULAICQRBICQRST $.
    $( [14-May-99] $)

  $( The product with a negative is the negative of the product. $)
  mulneg2t $p |- ( ( A e. CC /\ B e. CC ) ->
                 ( A x. -u B ) = -u ( A x. B ) ) $=
    ( cc wcel wa cneg cmulc co wceq mulneg1t ancoms axmulcom negclt sylan5
    negeqd 3eqtr4d ) ACDZBCDZEZBFZAGHZBAGHZFZATGHZABGHZFRQUAUCIBAJKQTCDUDUAIRAT
    LBMNSUEUBABLOP $.
    $( [25-Aug-04] $) $( [30-Jul-04] $)

  $( Swap the negative sign in a product. $)
  mulneg12t $p |- ( ( A e. CC /\ B e. CC ) ->
                  ( -u A x. B ) = ( A x. -u B ) ) $=
    ( cc wcel wa cneg cmulc co mulneg1t mulneg2t eqtr4d ) ACDBCDEAFBGHABGHFABFG
    HABIABJK $.
    $( [2-Sep-04] $) $( [30-Jul-04] $)

  $( Product of two negatives.  Theorem I.12 of [Apostol] p. 18. $)
  mul2negt $p |- ( ( A e. CC /\ B e. CC ) -> ( -u A x. -u B ) = ( A x. B ) ) $=
    ( cc wcel cneg cmulc co wceq cc0 cded negeq opreq1d opreq1 cleq12d opreq2d
    opreq2 0cn elimel mul2neg dedth2h ) ACDZBCDZAEZBEZFGZABFGZHUAAIJZEZUDFGZUGB
    FGZHUHUBBIJZEZFGZUGUKFGZHABIIAUGHZUEUIUFUJUOUCUHUDFAUGKLAUGBFMNBUKHZUIUMUJU
    NUPUDULUHFBUKKOBUKUGFPNUGUKAICQRBICQRST $.
    $( [30-Aug-04] $) $( [30-Jul-04] $)

  $( Distribution of negative over addition. $)
  negdistt $p |- ( ( A e. CC /\ B e. CC ) ->
                 -u ( A + B ) = ( -u A + -u B ) ) $=
    ( cc wcel caddc co cneg wceq cc0 cded opreq1 negeqd negeq opreq1d cleq12d
    opreq2 opreq2d 0cn elimel negdi dedth2h ) ACDZBCDZABEFZGZAGZBGZEFZHUBAIJZBE
    FZGZUIGZUGEFZHUIUCBIJZEFZGZULUNGZEFZHABIIAUIHZUEUKUHUMUSUDUJAUIBEKLUSUFULUG
    EAUIMNOBUNHZUKUPUMURUTUJUOBUNUIEPLUTUGUQULEBUNMQOUIUNAICRSBICRSTUA $.
    $( [5-Jun-04] $) $( [10-May-04] $)

  $( Distribution of negative over subtraction. $)
  negdist3t $p |- ( ( A e. CC /\ B e. CC ) -> -u ( A - B ) = ( B - A ) ) $=
    ( cc wcel cmin co cneg wceq cc0 cded opreq1 negeqd opreq2 cleq12d 0cn
    elimel negdist3 dedth2h ) ACDZBCDZABEFZGZBAEFZHSAIJZBEFZGZBUDEFZHUDTBIJZEFZ
    GZUHUDEFZHABIIAUDHZUBUFUCUGULUAUEAUDBEKLAUDBEMNBUHHZUFUJUGUKUMUEUIBUHUDEMLB
    UHUDEKNUDUHAICOPBICOPQR $.
    $( [4-Oct-99] $)

  $( Law for double subtraction. $)
  subsubt $p |- ( ( A e. CC /\ B e. CC /\ C e. CC ) ->
                  ( A - ( B - C ) ) = ( ( A - B ) + C ) ) $=
    ( cc wcel w3a cmin co cneg caddc wceq wa subnegt subclt sylan5 3impb
    negdist3t opreq2d 3adant1 addsubasst addsubt eqtr3d 3com23 3eqtrd ) ADEZBDE
    ZCDEZFABCGHZGHZAUHIZJHZACBGHZJHZABGHCJHZUEUFUGUIUKKZUEUHDEUOUFUGLZAUHMBCNOP
    UFUGUKUMKUEUPUJULAJBCQRSUEUGUFUMUNKUEUGUFFACJHBGHUMUNACBTACBUAUBUCUD $.
    $( [6-Jun-04] $) $( [13-May-04] $)

  $( Product with minus one is negative. $)
  mulm1t $p |- ( A e. CC -> ( -u 1 x. A ) = -u A ) $=
    ( cc wcel c1 cneg cmulc co wceq 1cn mulneg1t mpan mulid2t negeqd eqtrd ) AB
    CZDEAFGZDAFGZEZAEDBCOPRHIDAJKOQAALMN $.
    $( [16-Nov-99] $)

  ${
    mulm1.1 $e |- A e. CC $.
    $( Product with minus one is negative. $)
    mulm1 $p |- ( -u 1 x. A ) = -u A $=
      ( cc wcel c1 cneg cmulc co wceq mulm1t ax-mp ) ACDEFAGHAFIBAJK $.
      $( [31-Jul-99] $)
  $}

  ${
    sub4.1 $e |- A e. CC $.
    sub4.2 $e |- B e. CC $.
    sub4.3 $e |- C e. CC $.
    sub4.4 $e |- D e. CC $.
    $( Rearrangement of 4 terms in a mixed addition and subtraction. $)
    sub4 $p |- ( ( A + B ) - ( C + D ) ) =
                 ( ( A - C ) + ( B - D ) ) $=
      ( caddc co cneg cmin negdi opreq2i negcl add4 eqtr4 addcl subneg
      opreq12i 3eqtr4 ) ABIJZCDIJZKZIJZACKZIJZBDKZIJZIJZUBUCLJACLJZBDLJZIJUEUBU
      FUHIJZIJUJUDUMUBICDGHMNAUFBUHECGOFDHOPQUBUCABEFRCDGHRSUKUGULUIIACEGSBDFHS
      TUA $.
      $( [17-Oct-99] $)
