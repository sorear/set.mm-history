    $( Nonnegative integer exponentiation of a reciprocal. $)
    exprec $p |- ( ( A e. CC /\ A =/= 0 /\ N e. NN0 ) ->
                  ( ( 1 / A ) ^ N ) = ( 1 / ( A ^ N ) ) ) $=
      ( vj vk cc wcel cc0 wne cn0 c1 cdiv co cexp wceq wa cv wi caddc opreq2
      opreq2d eqeq12d imbi2d reccl exp0 syl ax1cn div1i syl6eq adantr eqtr4d
      cmul opreq1 ad2antll expp1 sylan an32s adantrr expcl expne0i 3com23
      3expa id adantlr divmuldiv mpanl12 syl21anc mulid1i opreq1i syl6req
