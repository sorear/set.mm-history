    $( Nonnegative integer exponentiation of a reciprocal. $)
    recexpt $p |- ( ( A e. CC /\ N e. NN0 /\ A =/= 0 ) ->
                  ( ( 1 / A ) ^ N ) = ( 1 / ( A ^ N ) ) ) $=
      ( vj vk cc wcel cn0 cc0 wne c1 cdiv co cexp wceq wi wa cv caddc opreq2
