$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Integer powers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ^ $. $( Exponentiation. $)

  $( Extend class notation to include exponentiation of a complex number to an
     integer power. $)
  cexp $a class ^ $.

  ${
    $d x y z $.
    $( Define exponentiation to nonnegative integer powers.  This definition is
       not meant to be used directly; instead, ~ exp0t and ~ expp1t provide a
       the standard recursive definition.  The up-arrow notation is used by
       Donald Knuth for iterated exponentiation (_Science_ 194, 1235-1242,
       1976) and is convenient for us since we don't have superscripts.  See
       ~ expnnvalt for a description of how the recursive sequence builder is
       used.  10-Jun-2005: The definition was extended to include zero
       exponents, so that ` 0 ^ 0 = 1 ` per the convention of Definition
       10-4.1 of [Gleason] p. 134.  (Based on definition contributed by Raph
       Levien, 15-Oct-2004.) $)
    df-exp $a |- ^ = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. NN0 ) /\
          z = if ( y = 0 , 1 , ( ( x. seq1 ( NN X. { x } ) ) ` y ) ) ) }  $.

    $(
    Raph Levien's original definition from set.mm of 15-Oct-2004:
    df-exp $a |- ^ = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. NN0 ) /\
          z = ( ( rec ( { <. w , v >. | v = ( w x. x ) } , 1 ) o.
        `' ( rec ( { <. w , v >. | v = ( w + 1 ) } , 0 ) |` om ) ) ` y ) ) } $.

    Old NN-based version from set.mm of 21-May-2005:
    df-exp $a |- ^ = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. NN ) /\
          z = ( ( x. seq1 ( NN X. { x } ) ) ` y ) ) }  $.
    dfexp2 $p |- ^ = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. NN0 ) /\
        z = ( ( x. seq1 ( { <. 0 , 1 >. } u. ( NN X. { x } ) ) ) ` y ) ) }  $=
          z = ( ( x. seq0 ( NN0 X. { if ( y = 0 , 1 , x ) } ) ) ` y ) ) }  $=
    $)


  $}

  ${
    $d x y z A $.  $d x y z N $.
    $( Value of exponentiation to nonnegative integer powers. $)
    expvalt $p |- ( ( A e. CC /\ N e. NN0 ) -> ( A ^ N ) =
                   if ( N = 0 , 1 , ( ( x. seq1 ( NN X. { A } ) ) ` N ) ) ) $=
      ( vx vy vz cc cn0 cv cc0 wceq c1 cmul cn csn cxp cseq1 co cfv cif cexp
      1nn0 elisseti fvex ifex sneq xpeq2 syl opreq2d fveq1d ifeq2d fveq2 eqeq1
      ifbid eqtrd df-exp oprabval2 ) CDEABFGDHZIJZKUQLMCHZNZOZPQZRZSBIJZKBLMANZ
      OZPQZRZSZTURKUQVGRZSZVDKVHKGUAUBBVGUCUDUSAJZURVCVJKVLUQVBVGVLVAVFLPVLUTVE
      JVAVFJUSAUEUTVEMUFUGUHUIUJUQBJZVKURKVHSVIVMURVJVHKUQBVGUKUJVMURVDKVHUQBIU
      LUMUNCDEUOUP $.
      $( [11-Jun-2005] $) $( [20-May-2004] $)

    $( Value of a complex number raised to the 0th power.  Note that under our
       definition, ` 0 ^ 0 = 1 ` , following the convention used by Gleason.
       Part of Definition 10-4.1 of [Gleason] p. 134. $)
    exp0t $p |- ( A e. CC -> ( A ^ 0 ) = 1 ) $=
      ( cc wcel cc0 cexp co wceq c1 cmul cn csn cxp cseq1 cfv cif cn0 0nn0
      expvalt mpan2 eqid iftrue ax-mp syl6eq ) ABCZADEFZDDGZHDIJAKLMFNZOZHUDDPC
      UEUHGQADRSUFUHHGDTUFHUGUAUBUC $.
      $( [11-Jun-2005] $) $( [20-May-2004] $)
  $}

  ${
    $d x y z A $.  $d x y z B $.
    $( Value of exponentiation to natural number powers.  ` NN X. { A }  `
       is the constant function with value ` A ` . The ` seq1 ` operation
       produces the sequence ` A ` , ` A x. A ` , ` ( A x. A ) x. A ` ,...
       that we evaluate at index ` B ` . $)
    expnnvalt $p |- ( ( A e. CC /\ B e. NN ) -> ( A ^ B ) =
                   ( ( x. seq1 ( NN X. { A } ) ) ` B ) ) $=
      ( cc wcel cn wa cexp co cc0 wceq c1 cmul csn cxp cseq1 cfv cif cn0
      expvalt nnnn0t sylan2 wn wne nnne0t df-ne sylib iffalse syl adantl eqtrd
      ) ACDZBEDZFABGHZBIJZKBLEAMNOHPZQZUOUKBRDUMUPJULABSBTUAULUPUOJZUKULUNUBZUQ
      ULBIUCURBUDBIUEUFUNKUOUGUHUIUJ $.
      $( [22-Oct-2004] $) $( [19-Oct-2004] $)
  $}

  $( Value of a complex number raised to the first power. $)
  exp1t $p |- ( A e. CC -> ( A ^ 1 ) = A ) $=
    ( cc wcel c1 cexp co cmul cn csn cxp cseq1 cfv wceq 1nn expnnvalt mpan2 wf
    fconstg fvconst syl mulex nnex snex xpex seq11 syl5eq eqtrd ) ABCZADEFZDGHA
    IZJZKFLZAUHDHCZUIULMNADOPUHDUKLZAULUHHUJUKQZUNAMZHABRUOUMUPNHADUKSPTGUKUAHU
    JUBAUCUDUEUFUG $.
    $( [22-Oct-2004] $) $( [20-Oct-2004] $)

  $( Value of a complex number raised to a nonnegative integer power plus one.
     Part of Definition 10-4.1 of [Gleason] p. 134. $)
  expp1t $p |- ( ( A e. CC /\ N e. NN0 ) -> ( A ^ ( N + 1 ) ) =
               ( ( A ^ N ) x. A ) ) $=
    ( cc wcel cn0 c1 caddc co cexp cmul wceq cn cc0 wo wa csn cxp cseq1 cfv
    mulex nnex snex xpex seq1p1 adantl wf fvconst fconstg peano2nn syl2an
    opreq2d eqtrd expnnvalt sylan2 opreq1d 3eqtr4d ex opreq1 opreq2 eqeq12d
    mulid2t exp0t exp1t ax1cn addid2 opreq2i syl5eq 3eqtr4rd syl5bir com12
    jaod elnn0 syl5ib imp ) ACDZBEDZABFGHZIHZABIHZAJHZKZVOBLDZBMKZNWAVPVOWBWAWC
    VOWBWAVOWBOZVQJLAPZQZRHZSZBWGSZAJHZVRVTWDWHWIVQWFSZJHZWJWBWHWLKVOBJWFTLWEUA
    AUBUCUDUEWDWKAWIJLWEWFUFVQLDZWKAKVOWBLAVQWFUGLACUHBUIZUJUKULVOWMVRWHKWBAVQU
    MWNUNWDVSWIAJABUMUOUPUQWCVOWAWCWAAMFGHZIHZAMIHZAJHZKVOWCVRWPVTWRWCVQWOAIBMF
    GURUKWCVSWQAJBMAIUSUOUTVOFAJHAWRWPAVAVOWQFAJAVBUOVOAFIHAWPAVCWOFAIFVDVEVFVG
    VHVIVJVKBVLVMVN $.
    $( [27-Jul-2006] $) $( [20-May-2005] $)

  ${
    $d x y z w A $.  $d z B $.  $d x y z w F $.
    expcllem.1 $e |- F (_ CC $.
    expcllem.2 $e |- ( ( x e. F /\ y e. F ) -> ( x x. y ) e. F ) $.
    expcllem.3 $e |- 1 e. F $.
    $( Lemma for proving nonnegative integer exponentiation closure laws. $)
    expcllem $p |- ( ( A e. F /\ B e. NN0 ) -> ( A ^ B ) e. F ) $=
