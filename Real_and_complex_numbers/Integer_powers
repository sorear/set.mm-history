    $( A number is zero iff its square is zero. $)
    sq00 $p |- ( ( A ^ 2 ) = 0 <-> A = 0 ) $=
      ( c2 cexp co cc0 wceq cmul sqval eqeq1i msq0 bitr ) ACDEZFGAAHEZFGAFGMNFA
      BIJABKL $.
      $( [2-Oct-1999] $)

    ${
      sqmul.2 $e |- B e. CC $.
      $( Distribution of square over multiplication. $)
      sqmul $p |- ( ( A x. B ) ^ 2 ) = ( ( A ^ 2 ) x. ( B ^ 2 ) ) $=
        ( cc wcel c2 cn0 cmul co cexp wceq 2nn0 mulexpt mp3an ) AEFBEFGHFABIJGK
        JAGKJBGKJIJLCDMABGNO $.
        $( [31-Jul-2006] $) $( [3-Sep-1999] $)

      ${
        sqdiv.3 $e |- B =/= 0 $.
        $( Distribution of square over division. $)
        sqdiv $p |- ( ( A / B ) ^ 2 ) = ( ( A ^ 2 ) / ( B ^ 2 ) ) $=
          ( cdiv co cmul c2 cexp divmuldiv divcl sqval opreq12i 3eqtr4 ) ABFGZP
          HGAAHGZBBHGZFGPIJGAIJGZBIJGZFGABABCDCDEEKPABCDELMSQTRFACMBDMNO $.
          $( [20-Aug-2001] $)
      $}
    $}

    ${
      sqreci.1 $e |- A =/= 0 $.
      $( Square of reciprocal. $)
      sqreci $p |- ( ( 1 / A ) ^ 2 ) = ( 1 / ( A ^ 2 ) ) $=
        ( c1 cdiv co cmul c2 cexp ax1cn divmuldiv mulid1 opreq1i eqtr reccl
        sqval opreq2i 3eqtr4 ) DAEFZSGFZDAAGFZEFZSHIFDAHIFZEFTDDGFZUAEFUBDADAJB
        JBCCKUDDUAEDJLMNSABCOPUCUADEABPQR $.
        $( [17-Sep-1999] $)
    $}
  $}

  $( A number is zero iff its square is zero. $)
  sq0t $p |- ( A e. CC -> ( ( A ^ 2 ) = 0 <-> A = 0 ) ) $=
    ( cc wcel c2 cexp co cc0 wceq wb cif opreq1 eqeq1d eqeq1 bibi12d 0cn
    elimel sq00 dedth ) ABCZADEFZGHZAGHZISAGJZDEFZGHZUCGHZIAGAUCHZUAUEUBUFUGTUD
    GAUCDEKLAUCGMNUCAGBOPQR $.
    $( [12-Apr-2006] $) $( [11-Mar-2006] $)

  $( A number is nonzero iff its square is nonzero. $)
  sqne0t $p |- ( A e. CC -> ( ( A ^ 2 ) =/= 0 <-> A =/= 0 ) ) $=
    ( cc wcel c2 cexp co cc0 sq0t eqneqd ) ABCADEFGAGAHI $.
    $( [14-Apr-2006] $) $( [11-Mar-2006] $)

  $( Closure of the square of a real number. $)
  resqclt $p |- ( A e. RR -> ( A ^ 2 ) e. RR ) $=
