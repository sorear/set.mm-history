  ${
    $d x y A $.  $d x y N $.
    $( Closure of exponentiation of nonnegative integers. $)
    nnexpclt $p |- ( ( A e. NN /\ N e. NN0 ) -> ( A ^ N ) e. NN ) $=
      ( vx vy cn nnsscn cv nnmulclt 1nn expcllem ) CDABEFCGDGHIJ $.
      $( [28-Jan-2006] $) $( [16-Dec-2005] $)

    $( Closure of exponentiation of nonnegative integers. $)
    nn0expclt $p |- ( ( A e. NN0 /\ N e. NN0 ) -> ( A ^ N ) e. NN0 ) $=
      ( vx vy cn0 nn0sscn cv nn0mulclt 1nn0 expcllem ) CDABEFCGDGHIJ $.
      $( [15-Dec-2005] $) $( [14-Dec-2005] $)

    $( Closure of exponentiation of integers. $)
    zexpclt $p |- ( ( A e. ZZ /\ N e. NN0 ) -> ( A ^ N ) e. ZZ ) $=
      ( vx vy cz zsscn cv zmulclt 1z expcllem ) CDABEFCGDGHIJ $.
      $( [3-May-2006] $) $( [16-Dec-2005] $)

    $( Closure of exponentiation of rationals. $)
    qexpclt $p |- ( ( A e. QQ /\ N e. NN0 ) -> ( A ^ N ) e. QQ ) $=
      ( vx vy cq qsscn cv qmulclt c1 cz wcel 1z zqt ax-mp expcllem ) CDABEFCGDG
      HIJKIEKLIMNO $.
      $( [16-Mar-2006] $) $( [16-Dec-2005] $)

    $( Closure of exponentiation of reals. $)
    reexpclt $p |- ( ( A e. RR /\ N e. NN0 ) -> ( A ^ N ) e. RR ) $=
      ( vx vy cr axresscn cv axmulrcl 1re expcllem ) CDABEFCGDGHIJ $.
      $( [15-Dec-2005] $) $( [14-Dec-2005] $)

    $( Closure law for nonnegative integer exponentiation. $)
    expclt $p |- ( ( A e. CC /\ N e. NN0 ) -> ( A ^ N ) e. CC ) $=
      ( vx vy cc ssid cv axmulcl ax1cn expcllem ) CDABEEFCGDGHIJ $.
      $( [25-Jul-2005] $) $( [26-May-2005] $)
  $}

  $( Exponentiation in terms of predecessor exponent. $)
  expm1t $p |- ( ( A e. CC /\ N e. NN ) -> ( A ^ N ) =
               ( ( A ^ ( N - 1 ) ) x. A ) ) $=
    ( cc wcel cn wa c1 cmin co caddc cexp cmul wceq nncnt ax1cn npcant mpan2
    syl opreq2d adantl cn0 expp1t nnm1nn0t sylan2 eqtr3d ) ACDZBEDZFABGHIZGJIZK
    IZABKIZAUHKIALIZUGUJUKMUFUGUIBAKUGBCDZUIBMZBNUMGCDUNOBGPQRSTUFUHUADUJULMUGA
    UHUBBUCUDUE $.
    $( [26-Jan-2006] $) $( [19-Dec-2005] $)

  ${
    $d j k N $.
    $( Value of one raised to a nonnegative integer power. $)
    1expt $p |- ( N e. NN0 -> ( 1 ^ N ) = 1 ) $=
      ( vj vk c1 cv cexp co wceq cc0 caddc opreq2 eqeq1d cc wcel ax1cn exp0t
      ax-mp cn0 cmul expp1t mpan opreq1 mulid1 syl6eq syl5bir nn0ind ) DBEZFGZD
      HDIFGZDHZDCEZFGZDHZDUKDJGZFGZDHZDAFGZDHBCAUGIHUHUIDUGIDFKLUGUKHUHULDUGUKD
