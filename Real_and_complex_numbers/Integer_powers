$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Integer powers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c ^ $. $( Exponentiation. $)

  $( Extend class notation to include exponentiation of a complex number to an
     integer power. $)
  cexp $a class ^ $.

  ${
    $d x y z $.
    $( Define exponentiation to nonnegative integer powers.  This definition is
       not meant to be used directly; instead, ~ exp0 and ~ expp1 provide a the
       standard recursive definition.  The up-arrow notation is used by Donald
       Knuth for iterated exponentiation (_Science_ 194, 1235-1242, 1976) and
       is convenient for us since we don't have superscripts.  See ~ expnnval
       for a description of how the recursive sequence builder is used.
       10-Jun-2005:  The definition was extended to include zero exponents, so
       that ` 0 ^ 0 = 1 ` per the convention of Definition 10-4.1 of [Gleason]
       p. 134.  (Based on definition contributed by Raph Levien,
       15-Oct-2004.) $)
    df-exp $a |- ^ = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. NN0 ) /\
          z = if ( y = 0 , 1 , ( ( x. seq1 ( NN X. { x } ) ) ` y ) ) ) } $.

    $(
    Raph Levien's original definition from set.mm of 15-Oct-2004:
    df-exp $a |- ^ = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. NN0 ) /\
          z = ( ( rec ( { <. w , v >. | v = ( w x. x ) } , 1 ) o.
        `' ( rec ( { <. w , v >. | v = ( w + 1 ) } , 0 ) |` om ) ) ` y ) ) } $.

    Old NN-based version from set.mm of 21-May-2005:
    df-exp $a |- ^ = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. NN ) /\
          z = ( ( x. seq1 ( NN X. { x } ) ) ` y ) ) }  $.
    dfexp2 $p |- ^ = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. NN0 ) /\
        z = ( ( x. seq1 ( { <. 0 , 1 >. } u. ( NN X. { x } ) ) ) ` y ) ) } $=
          z = ( ( x. seq0 ( NN0 X. { if ( y = 0 , 1 , x ) } ) ) ` y ) ) } $=
    $)
  $}

  ${
    $d x y z A $.  $d x y z N $.
    $( Value of exponentiation to nonnegative integer powers. $)
    expval $p |- ( ( A e. CC /\ N e. NN0 ) -> ( A ^ N ) =
                   if ( N = 0 , 1 , ( ( x. seq1 ( NN X. { A } ) ) ` N ) ) ) $=
      ( vx vy vz cc cn0 cv cc0 wceq c1 cmul cn csn cxp cseq1 co cfv cif cexp
      1nn0 elisseti fvex ifex sneq xpeq2 syl opreq2d fveq1d ifeq2d eqeq1 fveq2
      ifbieq2d df-exp oprabval2 ) CDEABFGDHZIJZKUPLMCHZNZOZPQZRZSBIJZKBLMANZOZP
      QZRZSTUQKUPVFRZSVCKVGKGUAUBBVFUCUDURAJZUQVBVHKVIUPVAVFVIUTVELPVIUSVDJUTVE
      JURAUEUSVDMUFUGUHUIUJUPBJUQVCVHVGKUPBIUKUPBVFULUMCDEUNUO $.
      $( [11-Jun-2005] $) $( [20-May-2004] $)

    $( Value of a complex number raised to the 0th power.  Note that under our
       definition, ` 0 ^ 0 = 1 ` , following the convention used by Gleason.
       Part of Definition 10-4.1 of [Gleason] p. 134. $)
    exp0 $p |- ( A e. CC -> ( A ^ 0 ) = 1 ) $=
      ( cc wcel cc0 cexp co wceq c1 cmul cn csn cxp cseq1 cfv cif cn0 0nn0
      expval mpan2 eqid iftrue ax-mp syl6eq ) ABCZADEFZDDGZHDIJAKLMFNZOZHUDDPCU
      EUHGQADRSUFUHHGDTUFHUGUAUBUC $.
      $( [11-Jun-2005] $) $( [20-May-2004] $)
  $}

  ${
    $d x y z A $.  $d x y z B $.
    $( Value of exponentiation to natural number powers. ` NN X. { A } ` is the
       constant function with value ` A ` .  The ` seq1 ` operation produces
       the sequence (i.e. function on ` NN ` ) with values ` A ` , ` A x. A ` ,
       ` ( A x. A ) x. A ` ,... that we evaluate at index ` B ` . $)
    expnnval $p |- ( ( A e. CC /\ B e. NN ) -> ( A ^ B ) =
                   ( ( x. seq1 ( NN X. { A } ) ) ` B ) ) $=
      ( cc wcel cn wa cexp co cc0 wceq c1 cmul csn cxp cseq1 cfv cif cn0 nnnn0
      expval sylan2 wn wne nnne0 df-ne sylib iffalse syl adantl eqtrd ) ACDZBED
      ZFABGHZBIJZKBLEAMNOHPZQZUOULUKBRDUMUPJBSABTUAULUPUOJZUKULUNUBZUQULBIUCURB
      UDBIUEUFUNKUOUGUHUIUJ $.
      $( [19-Oct-2004] $)
  $}

  $( Value of a complex number raised to the first power. $)
  exp1 $p |- ( A e. CC -> ( A ^ 1 ) = A ) $=
    ( cc wcel c1 cexp co cmul cn csn cxp cseq1 cfv wceq 1nn expnnval mpan2
    mulex nnex snex xpex seq11 wf fconstg fvconst sylancl syl5eq eqtrd ) ABCZAD
    EFZDGHAIZJZKFLZAUHDHCZUIULMNADOPUHULDUKLZAGUKQHUJRASTUAUHHUJUKUBUMUNAMHABUC
    NHADUKUDUEUFUG $.
    $( [20-Oct-2004] $)

  $( Value of a complex number raised to a nonnegative integer power plus one.
     Part of Definition 10-4.1 of [Gleason] p. 134. $)
  expp1 $p |- ( ( A e. CC /\ N e. NN0 ) -> ( A ^ ( N + 1 ) ) =
               ( ( A ^ N ) x. A ) ) $=
    ( cc wcel cn0 c1 caddc co cexp cmul wceq cn cc0 wo elnn0 wa csn cxp cseq1
    cfv mulex nnex snex xpex seq1p1 adantl wf fconstg peano2nn fvconst syl2an
    opreq2d eqtrd expnnval sylan2 opreq1d 3eqtr4d ex mulid2 exp0 ax1cn addid2i
    opreq2i exp1 syl5eq 3eqtr4rd opreq1 opreq2 eqeq12d syl5ibrcom jaod syl5bi
    imp ) ACDZBEDZABFGHZIHZABIHZAJHZKZVOBLDZBMKZNVNVTBOVNWAVTWBVNWAVTVNWAPZVPJL
    AQZRZSHZTZBWFTZAJHZVQVSWCWGWHVPWETZJHZWIWAWGWKKVNBJWEUALWDUBAUCUDUEUFWCWJAW
    HJVNLWDWEUGVPLDZWJAKWALACUHBUIZLAVPWEUJUKULUMWAVNWLVQWGKWMAVPUNUOWCVRWHAJAB
    UNUPUQURVNVTWBAMFGHZIHZAMIHZAJHZKVNFAJHAWQWOAUSVNWPFAJAUTUPVNWOAFIHAWNFAIFV
    AVBVCAVDVEVFWBVQWOVSWQWBVPWNAIBMFGVGULWBVRWPAJBMAIVHUPVIVJVKVLVM $.
    $( [27-Jul-2006] $) $( [20-May-2005] $)

  ${
    $d x y z w A $.  $d z B $.  $d x y z w F $.
    expcllem.1 $e |- F C_ CC $.
    expcllem.2 $e |- ( ( x e. F /\ y e. F ) -> ( x x. y ) e. F ) $.
    expcllem.3 $e |- 1 e. F $.
    $( Lemma for proving nonnegative integer exponentiation closure laws. $)
    expcllem $p |- ( ( A e. F /\ B e. NN0 ) -> ( A ^ B ) e. F ) $=
      ( vz vw cn0 wcel cn cc0 wceq wo cexp co elnn0 cv wi c1 caddc opreq2
      eleq1d imbi2d cc sseli exp1 syl ibir wa cmul opreq1 vtocl2ga ancoms
      adantlr wb nnnn0 expp1 syl2an adantr mpbird exp31 com12 a2d nnind impcom
      exp0 sylan9eqr syl6eqel jaodan sylan2b ) DKLCELZDMLZDNOZPCDQRZELZDSVNVOVR
      VPVOVNVRVNCITZQRZELZUAVNCUBQRZELZUAVNCJTZQRZELZUAVNCWDUBUCRZQRZELZUAVNVRU
      AIJDVSUBOZWAWCVNWJVTWBEVSUBCQUDUEUFVSWDOZWAWFVNWKVTWEEVSWDCQUDUEUFVSWGOZW
      AWIVNWLVTWHEVSWGCQUDUEUFVSDOZWAVRVNWMVTVQEVSDCQUDUEUFVNWCVNWBCEVNCUGLZWBC
      OEUGCFUHZCUIUJUEUKWDMLZVNWFWIVNWPWFWIUAVNWPWFWIVNWPULZWFULWIWECUMRZELZVNW
      FWSWPWFVNWSATZBTZUMRZELWEXAUMRZELWSABWECEEWTWEOXBXCEWTWEXAUMUNUEXACOXCWRE
      XACWEUMUDUEGUOUPUQWQWIWSURWFWQWHWREVNWNWDKLWHWROWPWOWDUSCWDUTVAUEVBVCVDVE
      VFVGVHVNVPULVQUBEVPVNVQCNQRZUBDNCQUDVNWNXDUBOWOCVIUJVJHVKVLVM $.
      $( [14-Dec-2005] $)
  $}

  ${
    $d x y A $.  $d x y N $.
    $( Closure of exponentiation of nonnegative integers. $)
    nnexpcl $p |- ( ( A e. NN /\ N e. NN0 ) -> ( A ^ N ) e. NN ) $=
      ( vx vy cn nnsscn cv nnmulcl 1nn expcllem ) CDABEFCGDGHIJ $.
      $( [16-Dec-2005] $)

    $( Closure of exponentiation of nonnegative integers. $)
    nn0expcl $p |- ( ( A e. NN0 /\ N e. NN0 ) -> ( A ^ N ) e. NN0 ) $=
      ( vx vy cn0 nn0sscn cv nn0mulcl 1nn0 expcllem ) CDABEFCGDGHIJ $.
      $( [14-Dec-2005] $)

    $( Closure of exponentiation of integers. $)
    zexpcl $p |- ( ( A e. ZZ /\ N e. NN0 ) -> ( A ^ N ) e. ZZ ) $=
      ( vx vy cz zsscn cv zmulcl 1z expcllem ) CDABEFCGDGHIJ $.
      $( [16-Dec-2005] $)

    $( Closure of exponentiation of rationals. $)
    qexpcl $p |- ( ( A e. QQ /\ N e. NN0 ) -> ( A ^ N ) e. QQ ) $=
      ( vx vy cq qsscn cv qmulcl c1 cz wcel 1z zq ax-mp expcllem ) CDABEFCGDGHI
      JKIEKLIMNO $.
      $( [16-Dec-2005] $)

    $( Closure of exponentiation of reals. $)
    reexpcl $p |- ( ( A e. RR /\ N e. NN0 ) -> ( A ^ N ) e. RR ) $=
      ( vx vy cr axresscn cv remulcl 1re expcllem ) CDABEFCGDGHIJ $.
      $( [14-Dec-2005] $)

    $( Closure law for nonnegative integer exponentiation. $)
    expcl $p |- ( ( A e. CC /\ N e. NN0 ) -> ( A ^ N ) e. CC ) $=
      ( vx vy cc ssid cv mulcl ax1cn expcllem ) CDABEEFCGDGHIJ $.
      $( [26-May-2005] $)

    $( Closure law for exponentiation of positive reals. $)
    rpexpcl $p |- ( ( A e. RR+ /\ N e. NN0 ) -> ( A ^ N ) e. RR+ ) $=
      ( vx vy crp cr cc rpssre axresscn sstri cv rpmulcl 1rp expcllem ) CDABEEF
      GHIJCKDKLMN $.
      $( [24-Feb-2008] $)
  $}

  $( Exponentiation in terms of predecessor exponent. $)
  expm1t $p |- ( ( A e. CC /\ N e. NN ) -> ( A ^ N ) =
               ( ( A ^ ( N - 1 ) ) x. A ) ) $=
    ( cc wcel cn wa c1 cmin co caddc cexp cmul wceq nncn ax1cn npcan sylancl
    opreq2d adantl cn0 nnm1nn0 expp1 sylan2 eqtr3d ) ACDZBEDZFABGHIZGJIZKIZABKI
    ZAUGKIALIZUFUIUJMUEUFUHBAKUFBCDGCDUHBMBNOBGPQRSUFUEUGTDUIUKMBUAAUGUBUCUD $.
    $( [19-Dec-2005] $)

  ${
    $d j k N $.
    $( Value of one raised to a nonnegative integer power. $)
    1exp $p |- ( N e. NN0 -> ( 1 ^ N ) = 1 ) $=
      ( vj vk c1 cv cexp co wceq cc0 caddc opreq2 eqeq1d cc wcel ax1cn exp0
      ax-mp cn0 cmul opreq1 mulid1i syl6eq expp1 mpan syl5ibr nn0ind ) DBEZFGZD
      HDIFGZDHZDCEZFGZDHZDUKDJGZFGZDHZDAFGZDHBCAUGIHUHUIDUGIDFKLUGUKHUHULDUGUKD
