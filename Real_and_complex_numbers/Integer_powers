    $( Natural number exponentiation is nonzero iff its mantissa is nonzero. $)
    expne0 $p |- ( ( A e. CC /\ N e. NN ) ->
                  ( ( A ^ N ) =/= 0 <-> A =/= 0 ) ) $=
      ( cc wcel cn wa cexp co cc0 expeq0 necon3bid ) ACDBEDFABGHIAIABJK $.
      $( [10-Jul-2008] $) $( [6-May-2005] $)

    $( Nonnegative integer exponentiation is nonzero if its mantissa is
       nonzero. $)
    expne0i $p |- ( ( A e. CC /\ A =/= 0 /\ N e. NN0 )
                -> ( A ^ N ) =/= 0 ) $=
      ( cc wcel cc0 wne cn0 cexp co wa cn wceq wo elnn0 expne0 biimpar an32s c1
      ax-1ne0 opreq2 exp0 sylan9eqr neeq1d mpbiri adantlr jaodan sylan2b 3impa
      ) ACDZAEFZBGDZABHIZEFZUKUIUJJZBKDZBELZMUMBNUNUOUMUPUIUOUJUMUIUOJUMUJABOPQ
