    expeq0 $p |- ( ( A e. CC /\ N e. NN ) -> ( ( A ^ N ) = 0 <-> A = 0 ) ) $=
      ( vj vk cn wcel cc cexp co cc0 wceq wb cv wi c1 caddc opreq2 eqeq1d
      bibi1d imbi2d exp1 wa wo cn0 nnnn0 cmul expp1 expcl simpl mul0or
      syl11anc bitrd sylan2 bi1 idd jaod olc impbid1 sylan9bb exp31 com12 a2d
