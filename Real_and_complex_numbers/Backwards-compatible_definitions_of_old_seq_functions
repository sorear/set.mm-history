$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
            Backwards-compatible definitions of old "seq" functions
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c seq1 $.  $( Symbol for recursive sequence builder starting at index 1. $)
  $c seq0 $.  $( Symbol for recursive sequence builder starting at index 0. $)
  $c seqz $.  $( Symbol for recursive sequence builder starting at an arbitrary
                 index. $)

  $( Extend class notation with 1-based recursive sequence builder. $)
  cseq1 $a class seq1 $.
  $( Extend class notation with 0-based recursive sequence builder. $)
  cseq0 $a class seq0 $.
  $( Extend class notation with arbitrarily-based recursive sequence
     builder. $)
  cseqz $a class seqz $.

  ${
    $d f g p $.
    $( Define the 1-based sequence in terms of a general sequence. $)
    df-seq1 $a |- seq1 = ( p e. _V , f e. _V |-> seq 1 ( p , f ) ) $.

    $( Define the 0-based sequence in terms of a general sequence. $)
    df-seq0 $a |- seq0 = ( p e. _V , f e. _V |-> seq 0 ( p , f ) ) $.

    $( Define the old general sequence from the new definition. $)
    df-seqz $a |- seqz = ( g e. _V , f e. _V |->
      seq ( 1st ` g ) ( ( 2nd ` g ) , f ) ) $.
  $}

  ${
    $d F y $.  $d S x y $.
    seq1val.1 $e |- S e. _V $.
    seq1val.2 $e |- F e. _V $.
