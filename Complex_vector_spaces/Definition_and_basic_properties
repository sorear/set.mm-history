  ${
    $d x y z G $.  $d x y z W $.
    vcabl.1 $e |- G = ( 1st ` W ) $.
    $( Vector addition is an Abelian group operation. $)
    vcabl $p |- ( W e. CVec -> G e. Abel ) $=
      ( vx vy vz cvc wcel cabl cc crn cxp c2nd cfv wf c1 cv co wceq wral caddc
      cmul wa eqid vci 3simp1d ) BGHAIHJAKZLUGBMNZOPDQZUHRUISEQZUIFQZARUHRUJUIU
      HRZUJUKUHRARSFUGTUJUKUARUIUHRULUKUIUHRZARSUJUKUBRUIUHRUJUMUHRSUCFJTUCEJTU
      CDUGTDEFUHABUGCUHUDUGUDUEUF $.
      $( [12-Nov-2006] $) $( [3-Nov-2006] $)

    $( Vector addition is a group operation. $)
    vcgrp $p |- ( W e. CVec -> G e. Grp ) $=
      ( cvc wcel cabl cgr vcabl ablgrp syl ) BDEAFEAGEABCHAIJ $.
      $( [14-Nov-2006] $) $( [4-Nov-2006] $)
  $}

  ${
    vcgcl.1 $e |- G = ( 1st ` W ) $.
    vcgcl.2 $e |- X = ran G $.
    $( Closure law for the vector addition (group) operation of a complex
       vector space. $)
    vcgcl $p |- ( ( W e. CVec /\ A e. X /\ B e. X ) -> ( A G B ) e. X ) $=
      ( cgr wcel co cvc grpcl vcgrp syl3an1 ) CHIAEIBEIABCJEIDKIABCEGLCDFMN $.
      $( [5-Feb-2007] $) $( [1-Feb-2007] $)
  $}

  ${
    vccom.1 $e |- G = ( 1st ` W ) $.
    vccom.2 $e |- X = ran G $.
    $( Vector addition is commutative. $)
    vccom $p |- ( ( W e. CVec /\ A e. X /\ B e. X ) ->
                ( A G B ) = ( B G A ) ) $=
      ( cabl wcel co wceq cvc ablcom vcabl syl3an1 ) CHIAEIBEIABCJBACJKDLIABCEG
      MCDFNO $.
      $( [29-Nov-2006] $) $( [4-Nov-2006] $)

    $( Vector addition is associative. $)
    vcaass $p |- ( ( W e. CVec /\ ( A e. X /\ B e. X /\ C e. X ) ) ->
                ( ( A G B ) G C ) = ( A G ( B G C ) ) ) $=
      ( cgr wcel w3a co wceq cvc grpass vcgrp sylan ) DIJAFJBFJCFJKABDLCDLABCDL
      DLMENJABCDFHODEGPQ $.
      $( [8-Dec-2006] $) $( [4-Nov-2006] $)

    $( Right cancellation law for vector addition. $)
    vcrcan $p |- ( ( W e. CVec /\ ( A e. X /\ B e. X /\ C e. X ) ) ->
                  ( ( A G C ) = ( B G C ) <-> A = B ) ) $=
      ( cgr wcel w3a co wceq wb cvc grprcan vcgrp sylan ) DIJAFJBFJCFJKACDLBCDL
      MABMNEOJABCDFHPDEGQR $.
      $( [11-Apr-2007] $) $( [4-Nov-2006] $)

    $( Left cancellation law for vector addition. $)
    vclcan $p |- ( ( W e. CVec /\ ( A e. X /\ B e. X /\ C e. X ) ) ->
                  ( ( C G A ) = ( C G B ) <-> A = B ) ) $=
      ( cgr wcel w3a co wceq wb cvc grplcan vcgrp sylan ) DIJAFJBFJCFJKCADLCBDL
      MABMNEOJABCDFHPDEGQR $.
      $( [14-Nov-2006] $) $( [4-Nov-2006] $)
  $}

  ${
    vc0cl.1 $e |- G = ( 1st ` W ) $.
    vc0cl.2 $e |- X = ran G $.
    vc0cl.3 $e |- Z = ( Id ` G ) $.
    $( The zero vector is a vector. $)
    vc0cl $p |- ( W e. CVec -> Z e. X ) $=
      ( cvc wcel cgr vcgrp grpidcl syl ) BHIAJIDCIABEKDACFGLM $.
      $( [15-Nov-2006] $) $( [4-Nov-2006] $)

    $( The zero vector is a right identity element. $)
    vc0rid $p |- ( ( W e. CVec /\ A e. X ) -> ( A G Z ) = A ) $=
      ( cgr wcel co wceq cvc grprid vcgrp sylan ) BIJADJAEBKALCMJAEBDGHNBCFOP
      $.
      $( [15-Nov-2006] $) $( [4-Nov-2006] $)

    $( The zero vector is a left identity element. $)
    vc0lid $p |- ( ( W e. CVec /\ A e. X ) -> ( Z G A ) = A ) $=
      ( cgr wcel co wceq cvc grplid vcgrp sylan ) BIJADJEABKALCMJAEBDGHNBCFOP
      $.
      $( [?] $) $( [26-Apr-2007] $)
  $}

  ${
    vc0.1 $e |- G = ( 1st ` W ) $.
    vc0.2 $e |- S = ( 2nd ` W ) $.
