    $( Functionality of th scalar product of a complex vector space. $)
    vcsm $p |- ( W e. CVec -> S : ( CC X. X ) --> X ) $=
      ( vx vy vz cvc wcel cabl cc cxp cv co wceq wral wa wf c1 caddc vci simp2d
      cmul ) CKLBMLNDODAUAUBHPZAQUGRIPZUGJPZBQAQUHUGAQZUHUIAQBQRJDSUHUIUCQUGAQU
      JUIUGAQZBQRUHUIUFQUGAQUHUKAQRTJNSTINSTHDSHIJABCDEFGUDUE $.
      $( [3-Nov-2006] $)

    $( Closure of the scalar product of a complex vector space. $)
    vccl $p |- ( ( W e. CVec /\ A e. CC /\ B e. X ) -> ( A S B ) e. X ) $=
      ( cvc wcel cc cxp wf co vcsm foprrn syl3an1 ) EJKLFMFCNALKBFKABCOFKCDEFGH
      IPABFLFCQR $.
      $( [3-Nov-2006] $)

    $( Identity element for the scalar product of a complex vector space. $)
    vcid $p |- ( ( W e. CVec /\ A e. X ) -> ( 1 S A ) = A ) $=
      ( vx vy vz cvc wcel c1 cv co wceq wral cc wa cabl cxp wf caddc cmul simpl
      w3a vci ralimi 3ad2ant3 syl opreq2 id eqeq12d rcla4cva sylan ) DLMZNIOZBP
      ZURQZIERZAEMNABPZAQZUQCUAMZSEUBEBUCZUTJOZURKOZCPBPVFURBPZVFVGBPCPQKERVFVG
