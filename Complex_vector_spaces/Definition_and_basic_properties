$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                     Definition and basic properties
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c CVec $.

  $( Extend class notation with the class of all complex vector spaces. $)
  cvc $a class CVec $.

  ${
    $d g s x y z $.
    $( Define the class of all complex vector spaces. $)
    df-vc $a |- CVec = { <. g , s >. | ( g e. Abel /\
     s :  ( CC X. ran g ) --> ran g /\ A. x e. ran g ( ( 1 s x ) = x /\
       A. y e. CC ( A. z e. ran g ( y s ( x g z ) ) = ( ( y s x ) g ( y s z ) )
         /\ A. z e. CC ( ( ( y + z ) s x ) = ( ( y s x ) g ( z s x ) ) /\
           ( ( y x. z ) s x ) = ( y s ( z s x ) ) ) ) ) ) } $.

    $( The class of all complex vector spaces is a relation. $)
    vcrel $p |- Rel CVec $=
      ( vg vs vx vy vz cvc wrel cv cabl wcel cc crn cxp wf c1 co wceq wral
      caddc cmul wa w3a copab relopab wb df-vc releq ax-mp mpbir ) FGZAHZIJKUKL
      ZMULBHZNOCHZUMPUNQDHZUNEHZUKPUMPUOUNUMPZUOUPUMPUKPQEULRUOUPSPUNUMPUQUPUNU
      MPZUKPQUOUPTPUNUMPUOURUMPQUAEKRUADKRUACULRUBZABUCZGZUSABUDFUTQUJVAUECDEAB
      UFFUTUGUHUI $.
      $( [6-Apr-2007] $) $( [17-Mar-2007] $)
  $}

