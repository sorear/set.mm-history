      OBSZXAVJXBVLXEWTVICDVOBAQUNURXEWRVKADVOBCDUQUOUPUSVBVCVD $.
      $( [3-Nov-2006] $)

    $( A vector plus itself is two times the vector. $)
    vc2 $p |- ( ( W e. CVec /\ A e. X ) -> ( A G A ) = ( 2 S A ) ) $=
      ( cvc wcel wa c1 co c2 vcid opreq12d caddc df-2 opreq1i ax-1cn wceq vcdir
      cc mp3anr1 mpanr1 syl5req eqtr3d ) DIJZAEJZKZLABMZUKCMZAACMNABMZUJUKAUKAC
      ABCDEFGHOZUNPUJUMLLQMZABMZULNUOABRSUHLUCJZUIUPULUAZTUHUQUQUIURTLLABCDEFGH
      UBUDUEUFUG $.
      $( [1-Feb-2007] $)

    $( Subtractive distributive law for the scalar product of a complex vector
       space. $)
    vcsubdir $p |- ( ( W e. CVec /\ ( A e. CC /\ B e. CC /\ C e. X ) ) ->
      ( ( A - B ) S C ) = ( ( A S C ) G ( -u 1 S ( B S C ) ) ) ) $=
      ( cvc wcel cc w3a wa cneg co c1 wceq opreq1d caddc negcl syl3anr2 3adant3
      cmin vcdir negsub adantl cmul mulm1 ad2antrl ax-1cn negcli mp3anr1 eqtr3d
      vcass 3adantr1 opreq2d 3eqtr3d ) FKLZAMLZBMLZCGLZNZOZABPZUAQZCDQZACDQZVFC
      DQZEQZABUEQZCDQVIRPZBCDQDQZEQVBVAUTVFMLVCVHVKSBUBAVFCDEFGHIJUFUCVEVGVLCDV
      DVGVLSZUTVAVBVOVCABUGUDUHTVEVJVNVIEUTVBVCVJVNSVAUTVBVCOOZVMBUIQZCDQZVJVNV
      PVQVFCDVBVQVFSUTVCBUJUKTUTVMMLVBVCVRVNSRULUMVMBCDEFGHIJUPUNUOUQURUS $.
      $( [31-Jul-2007] $)
  $}

  ${
    $d x y z G $.  $d x y z W $.
    vcabl.1 $e |- G = ( 1st ` W ) $.
    $( Vector addition is an Abelian group operation. $)
    vcabl $p |- ( W e. CVec -> G e. AbelOp ) $=
      ( vx vy vz cvc wcel cabl cc crn cxp c2nd cfv cv co wceq wral wa eqid cmul
      wf c1 caddc vci simp1d ) BGHAIHJAKZLUGBMNZUBUCDOZUHPUIQEOZUIFOZAPUHPUJUIU
      HPZUJUKUHPAPQFUGRUJUKUDPUIUHPULUKUIUHPZAPQUJUKUAPUIUHPUJUMUHPQSFJRSEJRSDU
      GRDEFUHABUGCUHTUGTUEUF $.
      $( [3-Nov-2006] $)

    $( Vector addition is a group operation. $)
    vcgrp $p |- ( W e. CVec -> G e. GrpOp ) $=
      ( cvc wcel cabl cgr vcabl ablogrpo syl ) BDEAFEAGEABCHAIJ $.
      $( [4-Nov-2006] $)
  $}

  ${
    vcgcl.1 $e |- G = ( 1st ` W ) $.
    vcgcl.2 $e |- X = ran G $.
    $( Closure law for the vector addition (group) operation of a complex
       vector space. $)
    vcgcl $p |- ( ( W e. CVec /\ A e. X /\ B e. X ) -> ( A G B ) e. X ) $=
      ( cvc wcel cgr co vcgrp grpocl syl3an1 ) DHICJIAEIBEIABCKEICDFLABCEGMN $.
      $( [1-Feb-2007] $)
  $}

  ${
    vccom.1 $e |- G = ( 1st ` W ) $.
    vccom.2 $e |- X = ran G $.
    $( Vector addition is commutative. $)
    vccom $p |- ( ( W e. CVec /\ A e. X /\ B e. X ) ->
                ( A G B ) = ( B G A ) ) $=
      ( cvc wcel cabl co wceq vcabl ablocom syl3an1 ) DHICJIAEIBEIABCKBACKLCDFM
      ABCEGNO $.
      $( [4-Nov-2006] $)

    $( Vector addition is associative. $)
    vcaass $p |- ( ( W e. CVec /\ ( A e. X /\ B e. X /\ C e. X ) ) ->
                ( ( A G B ) G C ) = ( A G ( B G C ) ) ) $=
      ( cvc wcel cgr w3a co wceq vcgrp grpoass sylan ) EIJDKJAFJBFJCFJLABDMCDMA
      BCDMDMNDEGOABCDFHPQ $.
      $( [4-Nov-2006] $)

    $( Commutative/associative law that swaps the last two terms in a triple
       vector sum. $)
    vca32 $p |- ( ( W e. CVec /\ ( A e. X /\ B e. X /\ C e. X ) ) ->
     ( ( A G B ) G C ) = ( ( A G C ) G B ) ) $=
      ( cvc wcel cabl w3a co wceq vcabl ablo32 sylan ) EIJDKJAFJBFJCFJLABDMCDMA
      CDMBDMNDEGOABCDFHPQ $.
      $( [5-Aug-2007] $)

    $( Rearrangement of 4 terms in a vector sum. $)
    vca4 $p |- ( ( W e. CVec /\ ( A e. X /\ B e. X ) /\ ( C e. X /\ D e. X )
      ) -> ( ( A G B ) G ( C G D ) ) = ( ( A G C ) G ( B G D ) ) ) $=
      ( cvc wcel cabl wa co wceq vcabl ablo4 syl3an1 ) FJKELKAGKBGKMCGKDGKMABEN
      CDENENACENBDENENOEFHPABCDEGIQR $.
      $( [5-Aug-2007] $)

    $( Right cancellation law for vector addition. $)
    vcrcan $p |- ( ( W e. CVec /\ ( A e. X /\ B e. X /\ C e. X ) ) ->
                  ( ( A G C ) = ( B G C ) <-> A = B ) ) $=
      ( cvc wcel cgr w3a co wceq wb vcgrp grporcan sylan ) EIJDKJAFJBFJCFJLACDM
      BCDMNABNODEGPABCDFHQR $.
      $( [4-Nov-2006] $)

    $( Left cancellation law for vector addition. $)
    vclcan $p |- ( ( W e. CVec /\ ( A e. X /\ B e. X /\ C e. X ) ) ->
                  ( ( C G A ) = ( C G B ) <-> A = B ) ) $=
      ( cvc wcel cgr w3a co wceq wb vcgrp grpolcan sylan ) EIJDKJAFJBFJCFJLCADM
      CBDMNABNODEGPABCDFHQR $.
      $( [4-Nov-2006] $)
  $}

  ${
    vczcl.1 $e |- G = ( 1st ` W ) $.
    vczcl.2 $e |- X = ran G $.
    vczcl.3 $e |- Z = ( Id ` G ) $.
    $( The zero vector is a vector. $)
    vczcl $p |- ( W e. CVec -> Z e. X ) $=
      ( cvc wcel cgr vcgrp grpoidcl syl ) BHIAJIDCIABEKDACFGLM $.
      $( [4-Nov-2006] $)

    $( The zero vector is a right identity element. $)
    vc0rid $p |- ( ( W e. CVec /\ A e. X ) -> ( A G Z ) = A ) $=
      ( cvc wcel cgr co wceq vcgrp grporid sylan ) CIJBKJADJAEBLAMBCFNAEBDGHOP
      $.
      $( [4-Nov-2006] $)

    $( The zero vector is a left identity element. $)
    vc0lid $p |- ( ( W e. CVec /\ A e. X ) -> ( Z G A ) = A ) $=
      ( cvc wcel cgr co wceq vcgrp grpolid sylan ) CIJBKJADJEABLAMBCFNAEBDGHOP
      $.
      $( [26-Apr-2007] $)
  $}

  ${
    vc0.1 $e |- G = ( 1st ` W ) $.
    vc0.2 $e |- S = ( 2nd ` W ) $.
    vc0.3 $e |- X = ran G $.
    vc0.4 $e |- Z = ( Id ` G ) $.
    $( Zero times a vector is the zero vector.  Equation 1a of [Kreyszig]
       p. 51. $)
    vc0 $p |- ( ( W e. CVec /\ A e. X ) -> ( 0 S A ) = Z ) $=
      ( cvc wcel wa cc0 co wceq c1 ax-1cn cc 0cn vc0rid caddc addid1i a1i vcdir
      opreq1i mp3anr1 mpanr1 vcid 3eqtr3d opreq1d 3eqtr2rd wb vccl mp3an2 vczcl
      w3a adantr simpr 3jca vclcan syldan mpbid ) DKLZAELZMZANABOZCOZAFCOZPZVGF
