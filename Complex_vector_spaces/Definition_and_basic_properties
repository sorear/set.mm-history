    $( Functionality of th scalar product of a complex vector space. $)
    vcsm $p |- ( W e. CVec -> S : ( CC X. X ) --> X ) $=
      ( vx vy vz cvc wcel cabl cc cxp wf c1 cv co wceq wral caddc cmul wa vci
      simp2d ) CKLBMLNDODAPQHRZASUGTIRZUGJRZBSASUHUGASZUHUIASBSTJDUAUHUIUBSUGAS
      UJUIUGASZBSTUHUIUCSUGASUHUKASTUDJNUAUDINUAUDHDUAHIJABCDEFGUEUF $.
      $( [3-Nov-2006] $)

    $( Closure of the scalar product of a complex vector space. $)
    vccl $p |- ( ( W e. CVec /\ A e. CC /\ B e. X ) -> ( A S B ) e. X ) $=
      ( cc cxp wf wcel co cvc foprrn vcsm syl3an1 ) JFKFCLAJMBFMABCNFMEOMABFJFC
      PCDEFGHIQR $.
      $( [3-Nov-2006] $)

    $( Identity element for the scalar product of a complex vector space. $)
    vcid $p |- ( ( W e. CVec /\ A e. X ) -> ( 1 S A ) = A ) $=
      ( vx vy vz cvc wcel c1 cv co wceq wral cabl cc cxp wf caddc cmul wa w3a
      vci simpl ralimi 3ad2ant3 syl opreq2 id eqeq12d rcla4cva sylan ) DLMZNIOZ
