$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                Group actions
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c GrpAct $.
  $( Extend class definition to include the class of group actions. $)
  cga $a class GrpAct $.

  ${
    $d g m s x y z $.
    $( Define the class of all group actions. $)
    df-ga $a |- GrpAct = { <. g , m >. | ( g e. GrpOp /\ E. s ( m : ( ran g X.
  s ) --> s /\ A. x e. s ( ( ( Id ` g ) m x ) = x /\ A. y e. ran g A. z e. ran
    g ( ( y g z ) m x ) = ( y m ( z m x ) ) ) ) ) } $.
  $}

  ${
    $d g m s x y z G $.  $d g m s x y z M $.  $d g m U $.  $d g m y z X $.
    isgalem.1 $e |- X = ran G $.
    isgalem.2 $e |- U = ( Id ` G ) $.
    $( Lemma for ~ isga . $)
    isgalem $p |- ( M e. A -> ( <. G , M >. e. GrpAct <-> ( G e. GrpOp /\ E. s
  ( M : ( X X. s ) --> s /\ A. x e. s ( ( U M x ) = x /\ A. y e. X A. z e. X (
    ( y G z ) M x ) = ( y M ( z M x ) ) ) ) ) ) ) $=
      ( vg vm wcel cga cv co wceq wral wa cop cgr cxp wf wex cvv wbr df-br wrel
      crn cgi cfv copab relopab df-ga releqi brrelexi sylbir anim1i ancoms elex
      mpbir adantr eleq2i eleq1 wb rneq syl6eqr xpeq1 feq2 fveq2 opreq1d eqeq1d
      3syl opreq raleqbidv anbi12d ralbidv exbidv feq1 opreq2d eqeq12d 2ralbidv
