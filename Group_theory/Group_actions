$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                Group actions
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c GrpAct $.
  $( Extend class definition to include the class of group actions. $)
  cga $a class GrpAct $.

  ${
    $d g m s x y z $.
    $( Define the class of all group actions. $)
    df-ga $a |- GrpAct = { <. g , m >. | ( g e. Grp /\ E. s ( m : ( ran g X. s
    ) --> s /\ A. x e. s ( ( ( Id ` g ) m x ) = x /\ A. y e. ran g A. z e. ran
    g ( ( y g z ) m x ) = ( y m ( z m x ) ) ) ) ) } $.
  $}

  ${
