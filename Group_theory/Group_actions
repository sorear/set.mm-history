    isgalem.1 $e |- X = ran G $.
    isgalem.2 $e |- U = ( Id ` G ) $.
    $( Lemma for ~ isga . $)
    isgalem $p |- ( M e. A -> ( <. G , M >. e. GrpAct <-> ( G e. Grp /\ E. s (
    M : ( X X. s ) --> s /\ A. x e. s ( ( U M x ) = x /\ A. y e. X A. z e. X (
