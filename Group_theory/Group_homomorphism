    ${
      $d C w $.  $d F w $.  $d X w $.  $d ch w $.  $d ph w $.
      ghgrpilem2.8 $e |- ( ( w e. X /\ ph ) -> ps ) $.
      ghgrpilem2.9 $e |- ( ( F ` w ) = C -> ( ps <-> ch ) ) $.
      $( Lemma for ~ ghgrpi . $)
      ghgrpilem2 $p |- ( ( ph /\ C e. Y ) -> ch ) $=
        ( wcel cv cfv wceq wrex crn wfn wfo wa df-fo mpbi simpri eleq2i fvelrnb
        wb simpli ax-mp bitr3i ancoms syl5ibcom rexlimdva syl5bi imp ) AHNUDZCV
