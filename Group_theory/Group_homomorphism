      ghgrpilem3.9 $e |- N = ( inv ` G ) $.
      ghgrpilem3.10 $e |- D = ( /g ` G ) $.
      $( Lemma for ~ ghgrpi . $)
      ghgrpilem3 $p |- ( ( a e. Y /\ b e. Y ) ->
