    $( Lemma for ~ grpoidinv . $)
    grpoidinvlem1 $p |- ( ( ( G e. GrpOp /\ ( Y e. X /\ A e. X ) ) /\
                  ( ( Y G A ) = U /\ ( A G A ) = A ) ) -> ( U G A ) = U ) $=
      ( cgr wcel wa co wceq opreq1 ad2antrl w3a id grpoass sylan2 adantr eqtr3d
