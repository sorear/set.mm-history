      $( [7-Nov-2006] $)

    $( The group identity element of nonzero complex number multiplication is
       one.  (Contributed by Steve Rodriguez, 23-Feb-2007.)  (Revised by Mario
       Carneiro, 17-Dec-2013.) $)
    mulid $p |- ( Id ` ( x. |` ( ( CC \ { 0 } ) X. ( CC \ { 0 } ) ) ) ) = 1 $=
      ( vy vx vz cmul cc cc0 csn cdif cxp cres cgi cfv cv co wceq wral crio c1
      cgr wcel cabl ablmul ablogrpo ax-mp mulnzcnopr fdmi grprn eqid grpoidval
      wne eldifsn ax-1cn ax-1ne0 mpbir2an oprvres mpan eldifi mulid2 syl eqtrd
      rgen wreu wb grpoideu ax-17 a17d opreq1 eqeq1d ralbidv riota2f mp2an
      mpbi eqtri ) DEFGZHZVOIZJZKLZAMZBMZVQNZVTOZBVOPZAVOQZRVQSTZVRWDOVQUATWEUB
      VQUCUDZBAVRVQVOVQVOWFVPVOVQUEUFUGZVRUHUIUDRVTVQNZVTOZBVOPZWDROZWIBVOVTVOT
      ZWHRVTDNZVTRVOTZWLWHWMOWNRETRFUJREFUKULUMUNZRVTVOVODUOUPWLVTETWMVTOVTEVNU
      QVTURUSUTVAWNWCAVOVBZWJWKVCWOWEWPWFBAVQVOWGVDUDWCWJACVORCMRTAVEWNWJAVFVSR
      OZWBWIBVOWQWAWHVTVSRVTVQVGVHVIVJVKVLVM $.
      $( [17-Dec-2013] $) $( [3-Dec-2006] $)
  $}


