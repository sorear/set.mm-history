  $c SetCat $.
  $( Extend class notation to include the category Set. $)
  ccaset $a class SetCat $.

  ${
    $( Definition of the category Set.  (We should say "the categories Set"
       since there is such a category per universe but for our purpose they are
       equivalent obviously.)  Experimental. $)
    df-catset $a |- SetCat = ( x e. Univ |->
     <. <. ( domSetCat ` x ) , ( codSetCat ` x ) >. ,
        <. ( IdSetCat ` x ) , ( roSetCat ` x ) >. >. ) $.
  $}

  ${
    $d x U $.
    $( The category Set. $)
    iscatset $p |- ( U e. Univ -> ( SetCat ` U ) =
      <. <. ( domSetCat ` U ) , ( codSetCat ` U ) >. ,
         <. ( IdSetCat ` U ) , ( roSetCat ` U ) >. >. ) $=
      ( vx cv cdomcase cfv ccodcase cop cidcase crocase cgru wceq fveq2 opeq12d
      ccaset df-catset opex fvmpt ) BABCZDEZRFEZGZRHEZRIEZGZGADEZAFEZGZAHEZAIEZ
      GZGJNRAKZUAUGUDUJUKSUETUFRADLRAFLMUKUBUHUCUIRAHLRAILMMBOUGUJPQ $.
      $( [8-Nov-2013] $)
  $}

  ${
    $d f g U h a $.
    $( The category set is a category.  (Contributed by FL, 6-Nov-2013.) $)
    setiscat $p |- ( U e. Univ -> ( SetCat ` U ) e. Cat ) $=
      ( vh vg vf va wcel cfv cop cv wceq wa co wi wral w3a wf 3jca cvv eqid syl
      cgru ccaset cdomcase ccodcase cidcase crocase ccat iscatset cded cdm calg
      wb wfun cxp wss ccmrcase domcatfun domdomcatfun domidcat feq23d codcatfun
