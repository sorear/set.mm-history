  $( The members of a 'less than or equal' relationship are extended reals.
     (Contributed by FL, 31-Jul-2009.) $)
  mlteqer $p |- ( B e. C -> ( A <_ B -> ( A e. RR* /\ B e. RR* ) ) ) $=
    ( cle wbr wcel cxr wa cop wi df-br cxp clt ccnv cdif wn eldif opelxpg sylbi
    biimpcd adantr df-le eleq2s com12 ) ABDEZBCFZAGFBGFHZUEABIZDFUFUGJZABDKUIUH
    GGLZMNZOZDUHULFUHUJFZUHUKFPZHUIUHUJUKQUMUIUNUFUMUGABGGCRTUASUBUCSUD $.
    $( [31-Jul-2009] $)

  $( Transitive law for ordering on extended reals ( compare ~ xrletr ).
     (Contributed by FL, 2-Aug-2009.) $)
  xrletr2 $p |- ( C e. D ->
             ( ( A <_ B /\ B <_ C ) -> A <_ C ) ) $=
    ( cle wbr wa wcel wi cxr mlteqer w3a xrletr exp3a 3expia com23 syl6 pm2.43i
    com3l com4l imp impcom com12 ) ABEFZBCEFZGCDHZACEFZUEUDUFUGIZUEUDUHIUFUEUEU
