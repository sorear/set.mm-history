  $c Intvl $.

  $( Extend class notation to include intervals. $)
  cintvl $a class Intvl $.

  $( The intervals of ` RR ` . $)
  df-intvl $a |- Intvl =
    ( ( ran (,) u. ( ran (,] u. ( ran [,) u. ran [,] ) ) ) i^i ~P RR ) $.

  $( The set of intervals is a set.  (Contributed by FL, 29-May-2014.) $)
  intvlset $p |- Intvl e. _V $=
    ( cintvl cioo crn cioc cico cicc cun cpw cin cvv df-intvl reex pwex eqeltri
    cr inex2 ) ABCDCECFCGGGZOHZIJKRQOLMPN $.
    $( [29-May-2014] $)

  $( An interval is a part of ` RR ` .  (Contributed by FL, 29-May-2014.) $)
  intrr $p |- ( I e. Intvl -> I C_ RR ) $=
    ( cr wss cioo crn cioc cico cicc cun cpw cin cintvl wcel elpwi adantl sylbi
    wa elin df-intvl eleq2s ) ABCZADEFEGEHEIIIZBJZKZLAUDMAUBMZAUCMZQUAAUBUCRUFU
    AUEABNOPST $.
    $( [29-May-2014] $)

