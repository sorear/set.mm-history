$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              Order theory
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c mxl $.
  $c mnl $.
  $c ub $.
  $c lb $.
  $c PresetRel $.
  $c ge $.
  $c se $.
  $c AntiDir $.

  $( Extend class notation with the class of all the presets. $)
  cpresetrel $a class PresetRel $.

  $( Extend class notation with a function that returns the maximal elements of
     a preset. $)
  cmxl $a class mxl $.

  $( Extend class notation with a function that returns the minimal elements of
     a preset. $)
  cmnl $a class mnl $.

  $( Extend class notation with a function that returns the upper bounds of a
     part of a preset. $)
  cub $a class ub $.

  $( Extend class notation with a function that returns the lower bounds of a
     part of a preset. $)
  clb $a class lb $.

  $( Extend class notation with a function that returns the greatest element of
     a poset. $)
  cge $a class ge $.

  $( Extend class notation with a function that returns the smallest element of
     a poset. $)
  cse $a class se $.

  $( Extend class notation with the class of all the anti-directions. $)
  cantidir $a class AntiDir $.

  $( Define the class of all presets.  A preset is a transitive and reflexive
     relation.  ("preset" is the short for preordered set.) $)
  df-prs $a |- PresetRel = { r | ( Rel r /\ ( r o. r ) C_ r /\
              ( _I |` U. U. r ) C_ r ) } $.

  ${
    $d R r $.
    $( The predicate "is a preset".  (Contributed by FL, 1-May-2011.) $)
    isprsr $p |- ( R e. A -> ( R e. PresetRel <->
       ( Rel R /\ ( R o. R ) C_ R /\ ( _I |` U. U. R ) C_ R ) ) ) $=
      ( vr cv wrel ccom wss cid cuni cres w3a cpresetrel wceq releq coeq1 coeq2
      wb eqtrd sseq12 mpancom unieq unieqd reseq2d 3anbi123d df-prs elab2g ) CD
      ZEZUGUGFZUGGZHUGIZIZJZUGGZKBEZBBFZBGZHBIZIZJZBGZKCBLAUGBMZUHUOUJUQUNVAUGB
      NUIUPMVBUJUQQVBUIBUGFUPUGBUGOUGBBPRUIUPUGBSTUMUTMVBUNVAQVBULUSHVBUKURUGBU
      AUBUCUMUTUGBSTUDCUEUF $.
      $( [1-May-2011] $)
  $}

  $( A preset is a relation.  (Contributed by FL, 18-May-2011.) $)
  preorel $p |- ( R e. PresetRel -> Rel R ) $=
    ( cpresetrel wcel wrel ccom wss cid cuni cres w3a isprsr ibi simp1d ) ABCZA
    DZAAEAFZGAHHIAFZNOPQJBAKLM $.
    $( [18-May-2011] $)

  ${
    $d R x $.
    $( The domain of a preset equals its field.  (Contributed by FL,
       22-May-2011.) $)
    preodom2 $p |- ( R e. PresetRel -> dom R = U. U. R ) $=
      ( vx cpresetrel wcel cdm cuni wceq wrel ccom wss cid cres w3a isprsr wral
      cv wbr ref4w domfldref sylan2br 3adant2 syl6bi pm2.43i ) ACDZAEAFFZGZUDUD
      AHZAAIAJZKUELAJZMUFCANUGUIUFUHUIUGBPZUJAQBUEOUFBUEARBASTUAUBUC $.
      $( [22-May-2011] $)
  $}

  ${
    preoref12.1 $e |- X = dom R $.
    $( The field of a preset is a set.  (Contributed by FL, 19-Sep-2011.) $)
    ppldrels $p |- ( R e. PresetRel -> X e. _V ) $=
      ( cpresetrel wcel cdm cvv dmexg syl5eqel ) ADEBAFGCADHI $.
      $( [19-Sep-2011] $)

    $( A preset is reflexive.  (Contributed by FL, 18-May-2011.) $)
    preoref12 $p |- ( R e. PresetRel -> ( _I |` X ) C_ R ) $=
      ( cpresetrel wcel cid cres cuni cdm preodom2 syl5eq reseq2d wrel ccom wss
      w3a isprsr ibi simp3d eqsstrd ) ADEZFBGFAHHZGZAUABUBFUABAIUBCAJKLUAAMZAAN
      AOZUCAOZUAUDUEUFPDAQRST $.
      $( [18-May-2011] $)

    ${
      $d A x $.  $d R x $.
      $( A preset is reflexive.  (Contributed by FL, 22-May-2011.) $)
      preoref22 $p |- ( ( R e. PresetRel /\ A e. X ) -> A R A ) $=
        ( vx cpresetrel wcel wbr cdm cuni wceq cid wss wi preodom2 preoref12 wa
        cres eqtr cv ref4w wb breq12 anidms rcla4cv sylbir reseq2 sseq1d imbi1d
        wral eleq2 imbi12d mpbiri syl mpan sylc imp ) BFGZACGZAABHZURBIZBJJZKZL
        CRZBMZUSUTNZBOBCDPCVAKZVCVEVFNZDVGVCQCVBKZVHCVAVBSVIVHLVBRZBMZAVBGZUTNZ
        NVKETZVNBHZEVBUJVMEVBBUAVOUTEAVBVNAKVOUTUBVNAVNABUCUDUEUFVIVEVKVFVMVIVD
        VJBCVBLUGUHVIUSVLUTCVBAUKUIULUMUNUOUPUQ $.
        $( [22-May-2011] $)
    $}

    ${
      $d R x $.
      $( The range of a preset equals its field.  (Contributed by FL,
         22-May-2011.) $)
      preoran2 $p |- ( R e. PresetRel -> ran R = X ) $=
        ( vx cpresetrel wcel crn wceq wrel ccom wss cid cuni cres w3a isprsr cv
        wbr wral ref4w wa cdm domrngref syl5req sylan2br 3adant2 syl6bi pm2.43i
        ) AEFZAGZBHZUIUIAIZAAJAKZLAMMZNAKZOUKEAPULUOUKUMUOULDQZUPARDUNSZUKDUNAT
        ULUQUABAUBUJCDAUCUDUEUFUGUH $.
        $( [22-May-2011] $)
    $}

    $( If ` A <_ B ` then ` A ` belongs to the field of the preset.
       (Contributed by FL, 23-May-2011.) $)
    pre1befi2 $p |- ( ( R e. PresetRel /\ A R B ) -> A e. X ) $=
      ( cpresetrel wcel wbr wa cdm wrel preorel releldm sylan syl6eleqr ) CFGZA
      BCHZIACJZDPCKQARGCLABCMNEO $.
      $( [23-May-2011] $)

    $( If ` A <_ B ` then ` B ` belongs to the field of the preset.
       (Contributed by FL, 23-May-2011.) $)
    pre2befi2 $p |- ( ( R e. PresetRel /\ B e. C /\ A R B ) -> B e. X ) $=
      ( cpresetrel wcel wbr w3a crn wrel preorel relelrng 3exp syl5com preoran2
      wi 3imp wb eqcomd eleq2d 3ad2ant1 mpbird ) DGHZBCHZABDIZJBEHZBDKZHZUEUFUG
      UJUEDLZUFUGUJRDMUFUKUGUJABCDNOPSUEUFUHUJTUGUEEUIBUEUIEDEFQUAUBUCUD $.
      $( [23-May-2011] $)
  $}

  ${
    domcprere.1 $e |- X = dom R $.
    $( If ` R ` is a preset, its domain and the domain of its converse are
       equal.  (Contributed by FL, 19-Sep-2011.) $)
    domcnvpre $p |- ( R e. PresetRel -> X = dom `' R ) $=
      ( cpresetrel wcel ccnv cdm crn df-rn preoran2 syl5reqr ) ADEAFGAHBAIABCJK
      $.
      $( [19-Sep-2011] $)
  $}

  $( A preset is transitive.  (Contributed by FL, 22-May-2011.) $)
  preotr1 $p |- ( R e. PresetRel -> ( R o. R ) C_ R ) $=
    ( cpresetrel wcel wrel ccom wss cid cuni cres w3a isprsr ibi simp2d ) ABCZA
    DZAAEAFZGAHHIAFZNOPQJBAKLM $.
    $( [22-May-2011] $)

  ${
    $d A x y z $.  $d B x y z $.  $d C x y z $.  $d R x y z $.
    $( A preset is transitive.  (Contributed by FL, 23-May-2011.) $)
    preotr2 $p |- ( ( R e. PresetRel /\ ( A R B /\ B R C /\ C e. D ) )
      -> A R C ) $=
      ( vx vy vz wcel wbr w3a wi cv wa wal cvv wceq breq12d ex pm2.43i ccom wss
      cpresetrel preotr1 cotr preorel brrelex simp1 simp2 simp3 anbi12d imbi12d
      wrel cla43gv com23 3exp com12 com4r com13 a1dd syl com4l com3l com14 3imp
      sylbi mpcom imp ) EUCIZABEJZBCEJZCDIZKZACEJZEEUAEUBZVIVMVNLZEUDVOFMZGMZEJ
      ZVRHMZEJZNZVQVTEJZLZHOGOFOZVIVPLFGHEUEVIWEVPVIEUMZWEVPLEUFVMWFWEVNVJVKVLW
      FWEVNLZLWFVJVKVLWGWFVJVKVLWGLZLZLVJWFWFWIVJWFWFWILZLWFVJVJWFWIWFVJVJWJLZW
      FVJNAPIZWKABEUGVKVJWFWLWHWFVKVJWLWHLZWFVKVJWMLZLVKWFWFWNVKWFWFWNLZLWFVKVK
      WFWNWFVKVKWOLZWFVKNBPIZWPBCEUGWQVKWNWFVJVKWQWMVJVKWQWMLWQWLVLVJVKNZWGWLWQ
      VLWRWGLZLWLWQVLWSWLWQVLKWEWRVNWDWRVNLFGHABCPPDVQAQZVRBQZVTCQZKZWBWRWCVNXC
      VSVJWAVKXCVQAVRBEWTXAXBUHZWTXAXBUIZRXCVRBVTCEXEWTXAXBUJZRUKXCVQAVTCEXDXFR
      ULUNUOUPUQURSUSUTVASVBTUSTVCVDVASVBTVCTVBVEVCVAUQVFVGVH $.
