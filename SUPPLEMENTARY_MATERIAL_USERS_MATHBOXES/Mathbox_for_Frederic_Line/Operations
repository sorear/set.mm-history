  ${
    $d ch u $.  $d ph u x z $.  $d ph u y z $.  $d ps u $.
    ssoprab2g.1 $e |- ( ph -> ( ps -> ch ) ) $.
    $( Inference of operation class abstraction subclass from implication.
       (Contributed by FL, 24-Jan-2010.) $)
    ssoprab2g $p |- ( ph -> { <. <. x , y >. , z >. | ps } C_
        { <. <. x , y >. , z >. | ch } ) $=
      ( vu cv cop wa wex copab copab2 anim2d 2eximdv ssopab2dv dfoprab2 3sstr4g
      wceq ) AHIDIEIJTZBKZELDLZHFMUACKZELDLZHFMBDEFNCDEFNAUCUEHFAUBUDDEABCUAGOP
      QBDEFHRCDEFHRS $.
      $( [24-Jan-2010] $)
  $}

  ${
    $d A x y z $.  $d B x y z $.
    $( The domain of an operation class abstraction.  Compare ~ dmoprabss .
       (Contributed by FL, 24-Jan-2010.) $)
    dmoprabsss $p |- dom { <. <. x , y >. , z >. |
    ( <. x , y >. e. ( A X. B ) /\ ph ) } C_ ( A X. B ) $=
      ( cv cop cxp wcel wa copab2 cdm oprab4 dmeqi dmoprabss eqsstri ) BGZCGZHE
      FIZJAKBCDLZMREJSFJKAKBCDLZMTUAUBABCDEFNOABCDEFPQ $.
      $( [24-Jan-2010] $)
  $}

  $( Value returned by the operation ` G ` in terms of the value returned by
     the "super"-operation ` F ` .  (A version of ~ oprssov adapted to partial
     operations.)  (Contributed by FL, 5-Oct-2009.) $)
  oprssopvg $p |- ( ( Fun F /\ G C_ F /\ <. A , B >. e. dom G )
    -> ( A F B ) = ( A G B ) ) $=
    ( wfun wss cop cdm wcel w3a cres wceq fvres df-ov 3eqtr4g 3ad2ant3 funssres
    co cfv wa oveqd 3adant3 eqtr3d ) CEZDCFZABGZDHZIZJABCUGKZRZABCRZABDRZUHUDUJ
    UKLUEUHUFUISUFCSUJUKUFUGCMABUINABCNOPUDUEUJULLUHUDUETUIDABCDQUAUBUC $.
    $( [5-Oct-2009] $)

  ${
    $d A x y z $.  $d B z $.
    dmoprabss5.1 $e |- B e. C $.
    $( The domain of an operation class abstraction.  (A version of ~ dmoprabss
       adapted to partial operations.)  (Contributed by FL, 18-Apr-2010.) $)
    dmoprabss6 $p |- ( Rel A ->
     dom { <. <. x , y >. , z >. | ( <. x , y >. e. A /\ z = B ) } = A ) $=
      ( wrel cv cop wcel wceq wa copab2 cdm wex copab dmoprab opabbii syl5eq
      19.42v simpl elexi isseti jctr impbii eqtri opabid2 ) DHZAIBIJDKZCIELZMZA
      BCNOULCPZABQZDULABCRUIUNUJABQZDUNUJUKCPZMZABQUOUMUQABUJUKCUASUQUJABUQUJUJ
