  $c -ucv $. $( Negative of a complex vector. $)

  $( Extends class notation with the negative of a complex vector. $)
  cnegcv $a class -ucv $.

  ${
    $d n u $.
    $( Negative of a complex vector.  Experimental. $)
    df-ucv $a |- -ucv = ( n e. NN |-> ( u e. ( CC ^m ( 1 ... n ) )
      |-> ( ( 0cv ` n ) ( -cv ` n ) u ) ) ) $.
  $}

  ${
    $d u U $.  $d u n N $.
    isucv.1 $e |- ~w = ( -ucv ` N ) $.
    isucv.2 $e |- 0w = ( 0cv ` N ) $.
    isucv.3 $e |- -w = ( -cv ` N ) $.
    $( Negative of a complex vector.  (Contributed by FL, 15-Sep-2013.) $)
    isucv $p |- ( ( N e. NN /\ U e. ( CC ^m ( 1 ... N ) ) ) ->
       ( ~w ` U ) = ( 0w -w U ) ) $=
