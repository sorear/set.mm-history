  $c 0cv $.

  $( Extends class notation with null vector. $)
  c0cv $a class 0cv $.

  ${
    $d n x $.
    $( The null vector in a space of dimension ` n ` .  Experimental. $)
    df-nullcv $a |- 0cv = ( n e. NN |-> ( x e. ( 1 ... n ) |-> 0 ) ) $.
  $}

  ${
    isnullcv.1 $e |- 0w = ( 0cv ` N ) $.
    ${
      $( The null vector. $)
      $d x n N $.
      $( The null vector in a space of dimension ` n ` .  (Contributed by FL,
         15-Sep-2013.) $)
      isnullcv $p |- ( N e. NN -> 0w = ( x e. ( 1 ... N ) |-> 0 ) ) $=
        ( vn cn wcel c1 cfz co cc0 cmpt cvv wceq ovex mptex wa c0cv cfv cv
        oveq2 eqidd mpteq12dv df-nullcv fvmptg syl5eq mpan2 ) BFGZAHBIJZKLZMGZC
        UJNAUIKHBIOPUHUKQCBRSUJDEBAHETZIJZKLUJFMRULBNZAUMKUIKULBHIUAUNKUBUCAEUD
        UEUFUG $.
        $( [15-Sep-2013] $)
    $}

    ${
      $d x N $.
      $( The null vector is a complex vector.  (Contributed by FL,
         15-Sep-2013.) $)
      zernpl $p |- ( N e. NN -> 0w e. ( CC ^m ( 1 ... N ) ) ) $=
        ( vx cn wcel c1 cfz co cc0 cmpt cc cmap isnullcv wf cv wa 0cn a1i eqid
        fmptd cnex ovex elmap sylibr eqeltrd ) AEFZBDGAHIZJKZLUHMIZDABCNUGUHLUI
        OUIUJFUGDUHJLUIJLFUGDPUHFQRSUITUALUHUIUBGAHUCUDUEUF $.
        $( [15-Sep-2013] $)
    $}

    ${
      $d x A $.  $d x n N $.
      $( Value of the complex vector at a specific coordinate.  (Contributed by
         FL, 15-Sep-2013.) $)
      valvze $p |- ( ( N e. NN /\ A e. ( 1 ... N ) ) -> ( 0w ` A ) = 0 ) $=
        ( vn vx cn wcel c1 cfz co cfv c0cv cc0 cv cmpt wceq a1i fveq1d cvv ovex
        wa fveq1i df-nullcv mptexg syl oveq2 eqidd mpteq12dv eqid fvmptg adantr
        mpdan cc simpr 0cn sylancl 3eqtrd syl5eq ) BGHZAIBJKZHZUBZACLABMLZLZNAC
        VDDUCVCVEABEGFIEOZJKZNPZPZLZLAFVANPZLZNVCAVDVJVCBMVIMVIQVCFEUDRSSVCAVJV
        KUTVJVKQZVBUTVKTHZVMUTVATHZVNVOUTIBJUARFVANTUEUFEBVHVKGTVIVFBQZFVGNVANV
        FBIJUGVPNUHUIVIUJUKUMULSVCVBNUNHVLNQUTVBUOUPFANNVAUNVKFOAQNUHVKUJUKUQUR
        US $.
        $( [15-Sep-2013] $)
    $}
  $}

  ${
    addcomv.1 $e |- +w = ( +cv ` N ) $.
    ${
      $d x A $.  $d x B $.  $d x N $.
      $( Vector addition is commutative.  (Contributed by FL, 15-Sep-2013.) $)
      addcomv $p |- ( ( N e. NN /\ A e. ( CC ^m ( 1 ... N ) )
      /\ B e. ( CC ^m ( 1 ... N ) ) )
      -> ( A +w B ) = ( B +w A ) ) $=
        ( vx wcel cc c1 cfz co w3a cfv caddc cmpt wa wceq wi wf cnex cn cmap cv
        ovex elmap ffvelrn 3adant1 3adant2 jca sylbi com12 imp addcom mpteq2dva
        3exp syl isaddrv 3com23 3eqtr4d ) CUAGZAHICJKZUBKZGZBVBGZLZFVAFUCZAMZVF
        BMZNKZOFVAVHVGNKZOZABDKBADKZVEFVAVIVJVEVFVAGZPVGHGZVHHGZPZVIVJQVEVMVPVC
        VDVMVPRZUTVCVDVQVCVAHASZVDVQRHVAATICJUDZUEVDVRVQVDVAHBSZVRVQRHVABTVSUEV
        TVRVMVPVTVRVMLVNVOVRVMVNVTVAHVFAUFUGVTVMVOVRVAHVFBUFUHUIUOUJUKUJULUGULV
        GVHUMUPUNFACBDEUQUTVDVCVLVKQFBCADEUQURUS $.
        $( [15-Sep-2013] $)
    $}

    ${
      $d x y A $.  $d x y B $.  $d x y C $.  $d x y N $.
      $( Vector addition is associative.  (Contributed by FL, 15-Sep-2013.) $)
      addassv $p |- ( ( N e. NN /\ ( A e. ( CC ^m ( 1 ... N ) ) /\
        B e. ( CC ^m ( 1 ... N ) ) /\ C e. ( CC ^m ( 1 ... N ) ) ) ) ->
        ( ( A +w B ) +w C ) = ( A +w ( B +w C ) ) ) $=
        ( vx vy wcel cc co wa cfv caddc wceq cvv wi cnex ffvelrnb mp2an cn cmap
        c1 cfz w3a cv cmpt isaddrv 3adant3r3 oveq1d simpl wf simpr1 ovex simpr2
        sylan addcl syl2anc eqid wb pm3.2i a1i elmapg syl mpbird simpr3 syl3anc
        fmptd 3adant3r1 oveq2d ex 3ad2ant1 adantl 3ad2ant2 3ad2ant3 simpr fveq2
        imp addass oveq12d fvmptg sylancl 3eqtr4d mpteq2dva 3eqtr4rd 3eqtrd ) D
