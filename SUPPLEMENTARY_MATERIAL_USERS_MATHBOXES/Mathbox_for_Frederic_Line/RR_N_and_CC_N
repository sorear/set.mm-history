$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        ( RR ^ N ) and ( CC ^ N )
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c +cv $. $( Addition of complex vectors. $)

  $( Extends class notation with addition of complex vectors. $)
  cplcv $a class +cv $.

  ${
    $d n u v x $.
    $( Addition of complex vectors in a space of dimension ` n ` .
       Experimental. $)
    df-addcv $a |- +cv = ( n e. NN |-> ( u e. ( CC ^m ( 1 ... n ) ) ,
        v e. ( CC ^m ( 1 ... n ) ) |->
        ( x e. ( 1 ... n ) |-> ( ( u ` x ) + ( v ` x ) ) ) ) ) $.
  $}

  ${
    isaddrv.1 $e |- +w = ( +cv ` N ) $.
    ${
      $d x v u U $.  $d x v u n N $.  $d x u v V $.
      $( Addition of complex vectors.  Experimental.  (Contributed by FL,
         14-Sep-2013.) $)
      isaddrv $p |- ( ( N e. NN /\ U e. ( CC ^m ( 1 ... N ) ) /\
      V e. ( CC ^m ( 1 ... N ) ) ) -> ( U +w V ) =
      ( x e. ( 1 ... N ) |-> ( ( U ` x ) + ( V ` x ) ) ) ) $=
        ( vu vv vn wcel cc c1 cfz co cmap cv cfv caddc cmpt wceq cn cmpt2 cplcv
        w3a oveq2 oveq2d eqidd mpteq12dv mpt2eq123dv df-addcv ovex mpt2ex fvmpt
        syl5eq 3ad2ant1 oveqd fveq1 oveq1d mpteq2dv mptex ovmpt2 3adant1 eqtrd
