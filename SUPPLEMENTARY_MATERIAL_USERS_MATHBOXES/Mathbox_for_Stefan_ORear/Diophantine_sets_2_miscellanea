    $( ~ fresaun transposed to mappings.  (Contributed by Stefan O'Rear,
       9-Oct-2014.) $)
    elmapresaun $p |- ( ( F e. ( C ^m A ) /\ G e. ( C ^m B ) /\ ( F |` ( A i^i
        B ) ) = ( G |` ( A i^i B ) ) ) -> ( F u. G ) e. ( C ^m ( A u. B ) ) )
