    $( Lemma for ~ irrapx1 .  Two multiples in the same bucket means they are
       very close mod 1. $)
    irrapxlem2 $p |- ( ( A e. RR+ /\ B e. NN ) -> E. x e. ( 0 ... B ) E. y e. (
