        ssv fndmdif sylancl fnniniseg2 eleq1d cpws rlmbase adantr oveq1d pwsbas
        csca pwsval rlmsca eqtr4d 3eqtrd rabeq eqtr3d frlmval ) BGOZEHOZUAZCUBZ
