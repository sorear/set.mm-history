    ${
      issubrngd.o $e |- ( ph -> O = ( 1r ` I ) ) $.
      issubrngd.t $e |- ( ph -> T = ( .r ` I ) ) $.
      issubrngd.ocl $e |- ( ph -> O e. D ) $.
      issubrngd.tcl $e |- ( ( ph /\ x e. D /\ y e. D ) -> ( x T y ) e. D ) $.

