  ${
    imp5OLD.1 $e |- ( ph -> ( ps -> ( ch -> ( th -> ( ta -> et ) ) ) ) ) $.
    $( An importation inference.  (Moved into main set.mm as ~ imp5g and may be
       deleted by mathbox owner, JGH. --NM 29-May-2014.) $)
    imp5gOLD $p |- ( ( ph /\ ps ) -> ( ( ( ch /\ th ) /\ ta ) -> et ) ) $=
      ( imp5g ) ABCDEFGH $.
      $( [7-Jul-2009] $)

    $( An importation inference.  (Moved into main set.mm as ~ imp55 and may be
       deleted by mathbox owner, JGH. --NM 29-May-2014.) $)
    imp55OLD $p |- ( ( ( ph /\ ( ps /\ ( ch /\ th ) ) ) /\ ta ) -> et ) $=
      ( imp55 ) ABCDEFGH $.
      $( [7-Jul-2009] $)

    $( An importation inference.  (Moved into main set.mm as ~ imp511 and may
       be deleted by mathbox owner, JGH. --NM 29-May-2014.) $)
    imp511OLD $p |- ( ( ph /\ ( ( ps /\ ( ch /\ th ) ) /\ ta ) ) -> et ) $=
      ( imp511 ) ABCDEFGH $.
      $( [7-Jul-2009] $)
  $}

  ${
