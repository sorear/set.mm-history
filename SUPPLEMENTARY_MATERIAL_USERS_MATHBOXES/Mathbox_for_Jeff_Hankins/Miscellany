  ${
    3com12d.1 $e |- ( ph -> ( ps /\ ch /\ th ) ) $.
    $( Commutation in consequent.  Swap 1st and 2nd.  (Contributed by Jeff
       Hankins, 17-Nov-2009.) $)
    3com12d $p |- ( ph -> ( ch /\ ps /\ th ) ) $=
      ( w3a id 3com12 syl ) ABCDFCBDFZECBDJJGHI $.
      $( [17-Nov-2009] $)
  $}

  ${
    3imp5.1 $e |- ( ph -> ( ps -> ( ch -> ( th -> ( ta -> et ) ) ) ) ) $.
    $( A triple importation inference.  (Contributed by Jeff Hankins,
       8-Jul-2009.) $)
    imp5p $p |- ( ph -> ( ps -> ( ( ch /\ th /\ ta ) -> et ) ) ) $=
      ( w3a wi com52l 3imp com3l ) CDEHABFCDEABFIIABCDEFGJKL $.
      $( [8-Jul-2009] $)

    $( A triple importation inference.  (Contributed by Jeff Hankins,
       8-Jul-2009.) $)
    imp5q $p |- ( ( ph /\ ps ) -> ( ( ch /\ th /\ ta ) -> et ) ) $=
      ( wa wi imp 3impd ) ABHCDEFABCDEFIIIGJK $.
      $( [8-Jul-2009] $)
  $}

  ${
    ecase13d.1 $e |- ( ph -> -. ch ) $.
    ecase13d.2 $e |- ( ph -> -. th ) $.
    ecase13d.3 $e |- ( ph -> ( ch \/ ps \/ th ) ) $.
    $( Deduction for elimination by cases.  (Contributed by Jeff Hankins,
       18-Aug-2009.) $)
    ecase13d $p |- ( ph -> ps ) $=
      ( wn wo wi w3o 3orass df-or bitri sylib mpd orcom ) ADHZBFABDIZRBJZACHZSE
      ACBDKZUASJZGUBCSIUCCBDLCSMNOPSDBITBDQDBMNOP $.
      $( [18-Aug-2009] $)
  $}

  ${
    $d y ph $.  $d x y ps $.  $d x y A $.
    eqeuOLD.1 $e |- ( x = A -> ( ph <-> ps ) ) $.
    $( A condition which implies existential uniqueness.  (Moved into main
       set.mm as ~ eqeu and may be deleted by mathbox owner, JGH. --NM
       29-May-2014.) $)
    eqeuOLD $p |- ( ( A e. B /\ ps /\ A. x ( ph -> x = A ) ) -> E! x ph ) $=
      ( eqeu ) ABCDEFG $.
      $( [8-Sep-2009] $)
  $}

  ${
    $d z w ph $.  $d z w ps $.  $d z w ch $.  $d y z w A $.  $d y z w B $.
    $d y z w X $.  $d y z w Y $.  $d y z w Z $.  $d x y z w $.
    subtr.1 $e |- ( y e. A -> A. x y e. A ) $.
    subtr.2 $e |- ( y e. B -> A. x y e. B ) $.
    ${
      subtr.3 $e |- ( y e. Y -> A. x y e. Y ) $.
      subtr.4 $e |- ( y e. Z -> A. x y e. Z ) $.
      subtr.5 $e |- ( x = A -> X = Y ) $.
      subtr.6 $e |- ( x = B -> X = Z ) $.
      $( Transitivity of implicit substitution.  (Contributed by Jeff Hankins,
         13-Sep-2009.) $)
      subtr $p |- ( ( A e. C /\ B e. D ) -> ( A = B -> Y = Z ) ) $=
        ( vz vw cv wceq wi csb eqeq1 csbhypf eqeq1d imbi12d eqeq2 eqeq2d csbeq1
        vtocl2g ) PRZQRZSZAUJGUAZAUKGUAZSZTCUKSZHUNSZTCDSZHISZTPQCDEFUJCSZULUPU
        OUQUJCUKUBUTUMHUNAPBCGHJLNUCUDUEUKDSZUPURUQUSUKDCUFVAUNIHAQBDGIKMOUCUGU
        EAUJUKGUHUI $.
        $( [13-Sep-2009] $)
    $}

    ${
      subtr2.3 $e |- ( ps -> A. x ps ) $.
      subtr2.4 $e |- ( ch -> A. x ch ) $.
      subtr2.5 $e |- ( x = A -> ( ph <-> ps ) ) $.
      subtr2.6 $e |- ( x = B -> ( ph <-> ch ) ) $.
      $( Transitivity of implicit substitution into a wff.  (Contributed by
         Jeff Hankins, 19-Sep-2009.) $)
      subtr2 $p |- ( ( A e. C /\ B e. D ) -> ( A = B -> ( ps <-> ch ) ) ) $=
        ( vz vw wceq wb wi cv wsbc eqeq1 wcel ax-17 hbeq hbs1 hbbi hbim sbceq1a
        vex bicomd sylan9bb sylbird vtoclef bibi1d imbi12d eqeq2 bibi2d vtocl2g
        ex dfsbcq ) PUAZQUAZRZADVCUBZADVDUBZSZTFVDRZBVGSZTFGRZBCSZTPQFGHIVCFRZV
        EVIVHVJVCFVDUCVMVFBVGVMVFBSZTDVCVMVNDDEEVCFEUAZVCUDDUEJUFVFBDADPUGLUHUI
        PUKDUAZVCRZVMVPFRZVNVPVCFUCVQVRVNVQVFAVRBVQAVFADVCUJULNUMVAUNUOUPUQVDGR
        ZVIVKVJVLVDGFURVSVGCBVSVGCSZTDVDVSVTDDEEVDGVOVDUDDUEKUFVGCDADQUGMUHUIQU
        KVPVDRZVSVPGRZVTVPVDGUCWAWBVTWAVGAWBCWAAVGADVDUJULOUMVAUNUOUSUQADVCVDVB
        UT $.
        $( [19-Sep-2009] $)
    $}
  $}

  ${
    $d w A $.  $d z w C $.  $d z w D $.  $d z w x y $.
    cbvcsb.1 $e |- ( z e. C -> A. y z e. C ) $.
    cbvcsb.2 $e |- ( z e. D -> A. x z e. D ) $.
    cbvcsb.3 $e |- ( x = y -> C = D ) $.
    $( Change bound variables in a class substitution.  Interestingly, this
       does not require any bound variable conditions on ` A ` .  (Contributed
       by Jeff Hankins, 13-Sep-2009.) $)
    cbvcsb $p |- ( A e. B -> [_ A / x ]_ C = [_ A / y ]_ D ) $=
      ( vw cv csb wceq csbeq1 eqeq12d vex wcel ax-17 cvv hbcsb wi hbcsb1 eqcomd
      eqcoms csbeq1a subtr mp2an csbief vtoclg ) AKLZFMZBUKGMZNADFMZBDGMZNKDEUK
      DNULUNUMUOAUKDFOBUKDGOPACUKFUMKQZABCUKGCLZUKRZASIUAALZTRUKTRUSUKNFUMNUBAQ
      UPBCUSUKTTGFUMUQUSRBSURBSZHBCUKGUTUCGFNUSBLZUSVANFGJUDUEBUKGUFUGUHUIUJ $.
      $( [13-Sep-2009] $)
  $}

  ${
    $d x A $.  $d x B $.  $d x C $.
    $( A subset of a difference does not intersect the subtrahend.
