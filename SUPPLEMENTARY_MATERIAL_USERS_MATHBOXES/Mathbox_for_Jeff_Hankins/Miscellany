  $( Two equal reduced fractions have the same numerator and denominator.
     (Contributed by Jeff Hankins, 29-Sep-2013.)  (Moved into main set.mm as
     ~ qredeq and may be deleted by mathbox owner, JGH. --NM 13-Oct-2014.) $)
  qredeqOLD $p |- ( ( ( M e. ZZ /\ N e. NN /\ ( M gcd N ) = 1 ) /\ ( P e. ZZ
    /\ Q e. NN /\ ( P gcd Q ) = 1 ) /\ ( M / N ) = ( P / Q ) ) -> ( M = P /\
    N = Q ) ) $=
    ( qredeq ) ABCDE $.
    $( [29-Sep-2013] $)

  ${
    $d k n x y z A $.
    $( Every rational number has a unique reduced form.  (Contributed by Jeff
       Hankins, 29-Sep-2013.)  (Moved into main set.mm as ~ qredeq and may be
       deleted by mathbox owner, JGH. --NM 13-Oct-2014.) $)
    qredeuOLD $p |- ( A e. QQ -> E! x e. ( ZZ X. NN ) ( ( ( 1st ` x ) gcd ( 2nd
        `
    x ) ) = 1 /\ A = ( ( 1st ` x ) / ( 2nd ` x ) ) ) ) $=
      ( qredeu ) ABC $.
      $( [29-Sep-2013] $)
  $}

