$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Boundedness
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c TotBnd $.
  $c Bnd $.
  $( Extend class notation with the class of totally bounded metric spaces. $)
  ctotbnd $a class TotBnd $.
  $( Extend class notation with the class of bounded metric spaces. $)
  cbnd $a class Bnd $.

  ${
    $d m d v b x $.
    $( Define the class of totally bounded metrics.  A metric space is totally
       bounded iff it can be covered by a finite number of balls of any given
       radius. $)
    df-totbnd $a |- TotBnd = { m e. Met | A. d e. RR+ E. v e. Fin ( U. v =
      dom dom m /\ A. b e. v E. x e. dom dom m b = ( x ( ball ` m ) d ) ) } $.
  $}

  ${
    $d M m d v b x $.  $d X m d v b x $.
    istotbnd.1 $e |- X = dom dom M $.
    $( The predicate "is a totally bounded metric space".  (Contributed by Jeff
       Madsen, 2-Sep-2009.) $)
    istotbnd $p |- ( M e. TotBnd <-> ( M e. Met /\ A. d e. RR+ E. v e. Fin
        ( U. v = X /\ A. b e. v E. x e. X b = ( x ( ball ` M ) d ) ) ) ) $=
      ( vm cv cdm wceq cbl cfv co wrex wral wa cfn crp eqeq2d cuni ctotbnd dmeq
      cme dmeqd syl6eqr fveq2 oveqd rexeqbidv ralbidv anbi12d rexbidv df-totbnd
      elrab2 ) BIZUAZHIZJZJZKZEIZAIZFIZUQLMZNZKZAUSOZEUOPZQZBROZFSPUPDKZVAVBVCC
      LMZNZKZADOZEUOPZQZBROZFSPHCUDUBUQCKZVJVRFSVSVIVQBRVSUTVKVHVPVSUSDUPVSUSCJ
      ZJDVSURVTUQCUCUEGUFZTVSVGVOEUOVSVFVNAUSDWAVSVEVMVAVSVDVLVBVCUQCLUGUHTUIUJ
      UKULUJABHEFUMUN $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d M d v b x $.  $d X d v b x $.
    istotbnd2.1 $e |- X = dom dom M $.
    $( The predicate "is a totally bounded metric space."  (Contributed by Jeff
       Madsen, 2-Sep-2009.) $)
    istotbnd2 $p |- ( M e. Met -> ( M e. TotBnd <-> A. d e. RR+ E. v e. Fin
        ( U. v = X /\ A. b e. v E. x e. X b = ( x ( ball ` M ) d ) ) ) ) $=
      ( ctotbnd wcel cme cv cuni wceq cbl cfv co wrex wral wa cfn istotbnd baib
      crp ) CHICJIBKZLDMEKAKFKCNOPMADQEUDRSBTQFUCRABCDEFGUAUB $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d M d v b x $.
    $( The predicate "totally bounded" implies ` M ` is a metric space.
       (Contributed by Jeff Madsen, 2-Sep-2009.) $)
    totbndmet $p |- ( M e. TotBnd -> M e. Met ) $=
      ( vv vb vx vd ctotbnd wcel cme cv cuni cdm wceq cbl cfv wrex wral cfn crp
      co wa eqid istotbnd simplbi ) AFGAHGBIZJAKKZLCIDIEIAMNSLDUEOCUDPTBQOERPDB
      AUECEUEUAUBUC $.
      $( [2-Sep-2009] $)
  $}

  ${
    $d X a b c d r s t u v w x y z $.  $d M a b c d r s t u v w x y z $.
    $d Y a b c d r s t u v w x y z $.  $d N a b c d r s t u v w x y z $.
    sstotbnd.1 $e |- X = dom dom M $.
    sstotbnd.2 $e |- N = ( M |` ( Y X. Y ) ) $.
    $( Condition for a subset of a metric space to be totally bounded.
       (Contributed by Jeff Madsen, 2-Sep-2009.) $)
    sstotbnd $p |- ( ( M e. Met /\ Y C_ X ) -> ( N e. TotBnd <->
                        A. d e. RR+ E. v e. Fin ( Y C_ U. v /\
