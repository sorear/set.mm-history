  $( Quotient of two negatives.  (Moved into main set.mm as ~ div2neg and may
     be deleted by mathbox owner, PC. --NM 20-Sep-2013.) $)
  div2negOLD $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                  ( -u A / -u B ) = ( A / B ) ) $=
    ( div2neg ) ABC $.
    $( [10-Nov-2012] $)

  ${
    $d k m $.
    $( Utility lemma to convert between ` m <_ k ` and ` k e. ( ZZ>= `` m ) `
       in limit theorems.  (Contributed by Paul Chapman, 10-Nov-2012.) $)
    climuzcnv $p |- ( m e. NN -> ( ( k e. ( ZZ>= ` m ) -> ph ) <->
                                   ( k e. NN -> ( m <_ k -> ph ) ) ) ) $=
      ( cv cn wcel cuz cfv wi cle wbr wa elnnuz uztrn sylan2b sylibr expcom nnz
      c1 cz eluzle a1i jcad biimpri syl3an1 syl3an2 3expib impbid imbi1d impexp
      w3a eluz2 syl6bb ) CDZEFZBDZUNGHFZAIUPEFZUNUPJKZLZAIURUSAIIUOUQUTAUOUQUTU
      OUQURUSUQUOURUQUOLUPSGHZFZURUOUQUNVAFVBUNMUNUPSNOUPMPQUQUSIUOUNUPUAUBUCUO
      URUSUQURUOUPTFZUSUQUPRUOUNTFZVCUSUQUNRUQVDVCUSUKUNUPULUDUEUFUGUHUIURUSAUJ
      UM $.
      $( [10-Nov-2012] $)
  $}

  ${
    $d k w x y z F $.  $d k w y z H $.  $d k z M $.  $d k w y z ph $.
    $d k G $.
    sinccvg.1 $e |- ( ph -> F : NN --> ( RR \ { 0 } ) ) $.
    sinccvg.2 $e |- ( ph -> F ~~> 0 ) $.
    sinccvg.3 $e |- G = ( x e. ( RR \ { 0 } ) |-> ( ( sin ` x ) / x ) ) $.
    sinccvg.4 $e |- H = ( x e. CC |-> ( 1 - ( ( x ^ 2 ) / 3 ) ) ) $.
    sinccvg.5 $e |- ( ph -> M e. NN ) $.
    sinccvg.6 $e |- ( ( ph /\ k e. ( ZZ>= ` M ) ) ->
                      ( abs ` ( F ` k ) ) < 1 ) $.
    $( ` ( ( sin `` x ) / x ) ~~> 1 ` as (real) ` x ~~> 0 ` .  (Contributed by
       Paul Chapman, 10-Nov-2011.)  (Revised by Mario Carneiro,
       21-May-2014.) $)
    sinccvglem $p |- ( ph -> ( G o. F ) ~~> 1 ) $=
      ( c1 wcel cc0 cc co c3 cdiv vy vz vw ccom cz cvv cuz cfv eqid nnz syl a1i
      cn 1z cli 0cn wfun wf cv c2 cexp cmin wral ax-1cn sqcl wne 3cn 3ne0 divcl
      mp3an23 subcl sylancr rgen fmpt mpbi ffun ax-mp csn cdif nnex fex sylancl
      cr cofunexg adantr nnuz uztrn2 sylan ffvelrn syl2anc eldifsn sylib simpld
      wa recnd wss ccncf crp cabs clt wbr wrex ssid copn ccn cmpt wtru cme cuni
      wceq cnmet cnmetba uniopn2 mp2an oveq1 fvco3 syl3anc oveq1d syl6eq oveq2d
      wi fvmpt csin ovex eqtrd 1re eqeltrd fveq2 id oveq12d resincl simprd cmul
      cle syl112anc ltle mpd wb eqbrtrd cneg ctop opntop cnmptc sqcn divccn ctx
      cnmpt11 subcn cnmpt12f eqeltri cncfmet1 eleqtrri cncfi mpanl12 climcn1lem
      trud sq0i div0i subid1i elexi syl6breq funmpt funeqi mpbir resqcl nndivre
      3nn resubcl redivcl abscl subdir mulid2 caddc df-3 cn0 2nn0 expp1 absresq
      oveq2i syl5eq div23 eqtr2d cioc absrpcl rpgt0 cxr w3a 0xr elioc2 sin01bnd
      syl3anbrc ltmuldiv mpbid wo sinneg div2neg eqeq1d syl5ibrcom jaod breqtrd
      absor 3brtr4d mulid1 breqtrrd ltdivmul mpbird eqbrtrrd climsqueeze ) ANCF
