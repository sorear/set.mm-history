  ${
    pm5.21nddOLDOLD.1 $e |- ( ph -> ( ch -> ps ) ) $.
    pm5.21nddOLDOLD.2 $e |- ( ph -> ( th -> ps ) ) $.
    pm5.21nddOLDOLD.3 $e |- ( ph -> ( ps -> ( ch <-> th ) ) ) $.
    $( Eliminate an antecedent implied by each side of a biconditional,
       deduction version.  (Moved into main set.mm as ~ pm5.21ndd and may be
       deleted by mathbox owner, PC. --NM 20-Sep-2013.) $)
    pm5.21nddOLDOLD $p |- ( ph -> ( ch <-> th ) ) $=
      ( pm5.21ndd ) ABCDEFGH $.
      $( [21-Nov-2012] $)
  $}

  $( One less than an integer greater than or equal to 2 is a positive
     integer.  (Contributed by Paul Chapman, 17-Nov-2012.)  (Moved into main
     set.mm as ~ uz2m1nn and may be deleted by mathbox owner, PC. --NM
     2-Mar-2014.) $)
  uz2m1nnOLD $p |- ( N e. ( ZZ>= ` 2 ) -> ( N - 1 ) e. NN ) $=
    ( uz2m1nn ) AB $.
    $( [2-Mar-2014] $) $( [17-Nov-2012] $)

  $( Membership in a curtailed finite sequence of integers.  (Contributed by
     Paul Chapman, 17-Nov-2012.) $)
  elfzm12 $p |- ( N e. NN ->
                  ( M e. ( 1 ... ( N - 1 ) ) -> M e. ( 1 ... N ) ) ) $=
    ( cn wcel c1 cmin co cfz cuz cfv wss cz nnz cle wbr cr zre wb mpbird syl
    caddc lep1 1re lesubadd mp3an2 anidms peano2zm eluz mpancom fzss2 sseld ) B
    CDZEBEFGZHGZEBHGZAULBUMIJDZUNUOKULBLDZUPBMUQUPUMBNOZUQBPDZURBQUSURBBEUAGNOZ
    BUBUSURUTRZUSEPDUSVAUCBEBUDUEUFSTUMLDUQUPURRBUGUMBUHUISTUMEBUJTUK $.
    $( [17-Nov-2012] $)

  ${
    $d F k $.  $d N k $.
    nn0seqcvg.1 $e |- F : NN0 --> NN0 $.
    nn0seqcvg.2 $e |- N = ( F ` 0 ) $.
    nn0seqcvg.3 $e |- ( k e. NN0 -> ( ( F ` ( k + 1 ) ) =/= 0 ->
                                      ( F ` ( k + 1 ) ) < ( F ` k ) ) ) $.
    $( A strictly-decreasing nonnegative integer sequence with initial term
       ` N ` reaches zero by the ` N ` th term.  Inference version.
       (Contributed by Paul Chapman, 31-Mar-2011.) $)
    nn0seqcvg $p |- ( F ` N ) = 0 $=
      ( c1 wceq cfv cc0 eqid cn0 wf a1i cv wcel caddc co wne clt wbr nn0seqcvgd
      wi adantl ax-mp ) GGHZCBIJHGKUFABCLLBMUFDNCJBIHUFENAOZLPUGGQRBIZJSUHUGBIT
      UAUCUFFUDUBUE $.
      $( [31-Mar-2011] $)
  $}

  $( An integer modulo 1 is 0.  (Contributed by Paul Chapman, 22-Jun-2011.) $)
  zmod10 $p |- ( N e. ZZ -> ( N mod 1 ) = 0 ) $=
    ( cz wcel c1 cmo co cfl cfv cmin cc0 cr wceq zre modfrac syl flid oveq2d cc
    zcn subid 3eqtrd ) ABCZADEFZAAGHZIFZAAIFZJUBAKCUCUELAMANOUBUDAAIAPQUBARCUFJ
    LASATOUA $.
    $( [26-Jun-2011] $)

  $( Identity law for modulo restricted to integers.  (Contributed by Paul
     Chapman, 22-Jun-2011.) $)
  zmodid2 $p |- ( ( M e. ZZ /\ N e. NN ) -> ( ( M mod N ) = M <->
                                              M e. ( 0 ... ( N - 1 ) ) ) ) $=
    ( cz wcel cn wa cmo co wceq cc0 cle wbr clt c1 cmin cfz cr crp wb zre nnrp
    modid2 syl2an nnz w3a 0z elfzm11 mpan 3anass syl6bb bicomd sylan9bbr bitr4d
    syl ibar ) ACDZBEDZFABGHAIZJAKLZABMLZFZAJBNOHPHDZUPAQDBRDURVASUQATBUAABUBUC
    UQVBUPVAFZUPVAUQBCDZVBVCSBUDVDVBUPUSUTUEZVCJCDVDVBVESUFAJBUGUHUPUSUTUIUJUNU
    PVAVCUPVAUOUKULUM $.
    $( [22-Jun-2011] $)

  $( Absorbtion law for modulo.  (Contributed by Paul Chapman, 22-Jun-2011.) $)
  modaddabs $p |- ( ( A e. RR /\ B e. RR /\ C e. RR+ ) ->
            ( ( ( A mod C ) + ( B mod C ) ) mod C ) = ( ( A + B ) mod C ) ) $=
    ( cr wcel cmo co caddc cc wceq wa modcl recnd 3adant2 3adant1 addcom oveq1d
    syl2anc simpl jca crp modabs2 modadd1 syl3anc recn 3ad2ant2 3simpc 3eqtr2rd
    w3a simpr eqtr4d ) ADEZBDEZCUAEZUIZACFGZBCFGZHGZCFGUQUPHGZCFGZABHGCFGZUOURU
    SCFUOUPIEZUQIEZURUSJULUNVBUMULUNKZUPACLZMNZUMUNVCULUMUNKZUQBCLZMOUPUQPRQUOU
    TBUPHGZCFGZUPBHGZCFGZVAUOUQDEZUMKZUPDEZUNKZUQCFGUQJZUTVJJUMUNVNULVGVMUMVHUM
    UNSTOULUNVPUMVDVOUNVEULUNUJTNUMUNVQULBCUBOUQBUPCUCUDUOVKVICFUOVBBIEZVKVIJVF
    UMULVRUNBUEUFUPBPRQUOVOULKZVGUPCFGUPJZVLVAJULUNVSUMVDVOULVEULUNSTNULUMUNUGU
    LUNVTUMACUBNUPABCUCUDUHUK $.
    $( [22-Jun-2011] $)

  $( An integer is a member of a 0-based finite set of sequential integers iff
     its successor is a member of the corresponding 1-based set.  (Contributed
     by Paul Chapman, 22-Jun-2011.) $)
  elfzp1b $p |- ( ( K e. ZZ /\ N e. ZZ ) ->
                ( K e. ( 0 ... ( N - 1 ) ) <-> ( K + 1 ) e. ( 1 ... N ) ) ) $=
    ( cz wcel wa c1 caddc co cfz cmin cc0 wb peano2z fzsubel mpanl1 mpanr2 wceq
    1z cc ax-1cn sylan2 ancoms zcn sylancl subidi oveq1i eleq12d adantr bitr2d
    pncan a1i ) ACDZBCDZEAFGHZFBIHDZUNFJHZFFJHZBFJHZIHZDZAKURIHZDZUMULUOUTLZULU
    MUNCDZVCAMUMVDFCDZVCRVEUMVDVEEVCRUNFFBNOPUAUBULUTVBLUMULUPAUSVAULASDFSDUPAQ
    AUCTAFUJUDUSVAQULUQKURIFTUEUFUKUGUHUI $.
    $( [22-Jun-2011] $)

  ${
    $( Division of both sides of 'less than or equal to' by a nonnegative
       number.  (Contributed by Paul Chapman, 7-Sep-2007.) $)
    lediv2aALT $p |- ( ( ( A e. RR /\ 0 < A ) /\ ( B e. RR /\ 0 < B ) /\
                       ( C e. RR /\ 0 <_ C ) ) ->
                     ( A <_ B -> ( C / B ) <_ ( C / A ) ) ) $=
      ( cr wcel cc0 clt wbr wa cle w3a c1 cdiv co anim12i ancoms cc recn adantr
      syl wi wne gt0ne0 rereccl syldan 3adant3 simp3 df-3an sylanbrc lemul2a ex
      cmul lerec wceq jca 3anass sylibr divrec 3adant1 3adant2 breq12d 3imtr4d
      wb ) ADEZFAGHZIZBDEZFBGHZIZCDEZFCJHZIZKZLBMNZLAMNZJHZCVNULNZCVOULNZJHZABJ
      HZCBMNZCAMNZJHVMVNDEZVODEZVLKZVPVSUAVMWCWDIZVLWEVFVIWFVLVIVFWFVIWCVFWDVGV
      HBFUBZWCBUCZBUDUEVDVEAFUBZWDAUCZAUDUEOPUFVFVIVLUGWCWDVLUHUIWEVPVSVNVOCUJU
      KTVFVIVTVPVCVLABUMUFVMWAVQWBVRJVIVLWAVQUNZVFVLVIWKVLVIIZCQEZBQEZWGKZWKWLW
      MWNWGIZIWOVLWMVIWPVJWMVKCRSZVIWNWGVGWNVHBRSWHUOOWMWNWGUPUQCBURTPUSVFVLWBV
      RUNZVIVLVFWRVLVFIZWMAQEZWIKZWRWSWMWTWIIZIXAVLWMVFXBWQVFWTWIVDWTVEARSWJUOO
      WMWTWIUPUQCAURTPUTVAVB $.
      $( [7-Sep-2007] $)
  $}

  ${
    abs2sqlei.1 $e |- A e. CC $.
    abs2sqlei.2 $e |- B e. CC $.
    $( The absolute values of two numbers compare as their squares.
       (Contributed by Paul Chapman, 7-Sep-2007.) $)
    abs2sqlei $p |- ( ( abs ` A ) <_ ( abs ` B ) <->
                     ( ( abs ` A ) ^ 2 ) <_ ( ( abs ` B ) ^ 2 ) ) $=
      ( cc0 cabs cfv cle wbr c2 cexp co wb absge0i abscli le2sqi mp2an ) EAFGZH
      IEBFGZHIRSHIRJKLSJKLHIMACNBDNRSACOBDOPQ $.
      $( [7-Sep-2007] $)
  $}

  ${
    abs2sqlti.1 $e |- A e. CC $.
    abs2sqlti.2 $e |- B e. CC $.
    $( The absolute values of two numbers compare as their squares.
       (Contributed by Paul Chapman, 7-Sep-2007.) $)
    abs2sqlti $p |- ( ( abs ` A ) < ( abs ` B ) <->
                     ( ( abs ` A ) ^ 2 ) < ( ( abs ` B ) ^ 2 ) ) $=
      ( cc0 cabs cfv cle wbr clt c2 cexp co wb absge0i abscli lt2sqi mp2an ) EA
      FGZHIEBFGZHISTJISKLMTKLMJINACOBDOSTACPBDPQR $.
      $( [7-Sep-2007] $)
  $}

  ${
    $( The absolute values of two numbers compare as their squares.
       (Contributed by Paul Chapman, 7-Sep-2007.) $)
    abs2sqle $p |- ( ( A e. CC /\ B e. CC ) ->
                      ( ( abs ` A ) <_ ( abs ` B ) <->
                        ( ( abs ` A ) ^ 2 ) <_ ( ( abs ` B ) ^ 2 ) ) ) $=
      ( cc wcel cabs cfv cle wbr c2 cexp co wb cc0 cif wceq fveq2 breq1d breq2d
      bibi12d 0cn oveq1d oveq1 syl elimel abs2sqlei dedth2h ) ACDZBCDZAEFZBEFZG
      HZUIIJKZUJIJKZGHZLUGAMNZEFZUJGHZUPIJKZUMGHZLUPUHBMNZEFZGHZURVAIJKZGHZLABM
      MAUOOZUKUQUNUSVEUIUPUJGAUOEPZQVEULURUMGVEUIUPIJVFUAQSBUTOZUQVBUSVDVGUJVAU
      PGBUTEPZRVGUJVAOZUSVDLVHVIUMVCURGUJVAIJUBRUCSUOUTAMCTUDBMCTUDUEUF $.
      $( [7-Sep-2007] $)
  $}

  ${
    $( The absolute values of two numbers compare as their squares.
       (Contributed by Paul Chapman, 7-Sep-2007.) $)
    abs2sqlt $p |- ( ( A e. CC /\ B e. CC ) ->
                      ( ( abs ` A ) < ( abs ` B ) <->
                        ( ( abs ` A ) ^ 2 ) < ( ( abs ` B ) ^ 2 ) ) ) $=
      ( cc wcel cabs cfv clt wbr c2 cexp co wb cc0 cif wceq fveq2 breq1d breq2d
      bibi12d 0cn oveq1d oveq1 syl elimel abs2sqlti dedth2h ) ACDZBCDZAEFZBEFZG
      HZUIIJKZUJIJKZGHZLUGAMNZEFZUJGHZUPIJKZUMGHZLUPUHBMNZEFZGHZURVAIJKZGHZLABM
      MAUOOZUKUQUNUSVEUIUPUJGAUOEPZQVEULURUMGVEUIUPIJVFUAQSBUTOZUQVBUSVDVGUJVAU
      PGBUTEPZRVGUJVAOZUSVDLVHVIUMVCURGUJVAIJUBRUCSUOUTAMCTUDBMCTUDUEUF $.
      $( [7-Sep-2007] $)
  $}

  ${
    abs2difi.1 $e |- A e. CC $.
    abs2difi.2 $e |- B e. CC $.
    $( Difference of absolute values.  (Contributed by Paul Chapman,
       7-Sep-2007.) $)
    abs2difi $p |- ( ( abs ` A ) - ( abs ` B ) ) <_ ( abs ` ( A - B ) ) $=
      ( cc wcel cabs cfv cmin co cle wbr abs2dif mp2an ) AEFBEFAGHBGHIJABIJGHKL
      CDABMN $.
      $( [7-Sep-2007] $)
  $}

  ${
    abs2difabsi.1 $e |- A e. CC $.
    abs2difabsi.2 $e |- B e. CC $.
    $( Absolute value of difference of absolute values.  (Contributed by Paul
       Chapman, 7-Sep-2007.) $)
    abs2difabsi $p |- ( abs ` ( ( abs ` A ) - ( abs ` B ) ) ) <_
                     ( abs ` ( A - B ) ) $=
      ( cc wcel cabs cfv cmin co cle wbr abs2difabs mp2an ) AEFBEFAGHBGHIJGHABI
      JGHKLCDABMN $.
      $( [7-Sep-2007] $)
  $}

