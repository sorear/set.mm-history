  ${
    $d A d $.  $d B f g $.  $d G f g $.  $d Y g $.  $d Z f $.  $d d f g $.
    $d f g x $.
    bnj1244.1 $e |- B = { d | ( d C_ A /\ A. x e. d _pred ( x , A , R ) C_ d )
        } $.
    bnj1244.2 $e |- Y = <. x , ( f |` _pred ( x , A , R ) ) >. $.
    bnj1244.3 $e |- C = { f | E. d e. B ( f Fn d /\ A. x e. d ( f ` x ) = ( G `
        Y ) ) } $.
    bnj1244.4 $e |- D = ( dom g i^i dom h ) $.
    bnj1244.5 $e |- E = { x e. D | ( g ` x ) =/= ( h ` x ) } $.
    bnj1244.6 $e |- ( ph <-> ( R _FrSe A /\ g e. C /\ h e. C /\ ( g |` D ) =/=
        ( h |` D ) ) ) $.
    bnj1244.7 $e |- ( ps <-> ( ph /\ x e. E /\ A. y e. E -. y R x ) ) $.
    bnj1244.8 $e |- Z = <. x , ( g |` _pred ( x , A , R ) ) >. $.
    bnj1244.9 $e |- K = { g | E. d e. B ( g Fn d /\ A. x e. d ( g ` x ) = ( G `
        Z ) ) } $.
