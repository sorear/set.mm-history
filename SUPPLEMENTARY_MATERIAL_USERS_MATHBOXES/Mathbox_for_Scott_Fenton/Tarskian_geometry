  $( Lemma for ~ colinearalg .  Expand out a multiplication. $)
  colinearalglem1 $p |- (
    ( ( A e. CC /\ B e. CC /\ C e. CC ) /\
      ( D e. CC /\ E e. CC /\ F e. CC ) ) ->
    ( ( ( B - A ) x. ( F - D ) ) = ( ( E - D ) x. ( C - A ) ) <->
      ( ( B x. F ) - ( ( A x. F ) + ( B x. D ) ) ) =
      ( ( C x. E ) - ( ( A x. E ) + ( C x. D ) ) ) ) ) $=
    ( cc wcel cmin co cmul wceq caddc subcl syl2anc syl3anc mulcl syl2an subdir
    w3a wa simpl2 simpl1 simpr3 simpr1 subdi simp2 simp3 addsub oveq12d subsub3
    simp1 eqtr2d subsub4 oveq1d 3eqtr3d eqtrd simpr2 simpl3 mulcom 3eqtrd addcl
    eqeq12d wb addcan2 bitrd ) AGHZBGHZCGHZTZDGHZEGHZFGHZTZUAZBAIJZFDIJKJZEDIJZ
    CAIJZKJZLBFKJZAFKJZBDKJZMJZIJZADKJZMJZCEKJZAEKJZCDKJZMJZIJZWFMJZLZWEWLLZVOV
    QWGVTWMVOVQVPFKJZVPDKJZIJZWGVOVPGHZVMVKVQWRLVOVHVGWSVGVHVIVNUBZVGVHVIVNUCZB
    ANOVJVKVLVMUDZVJVKVLVMUEZVPFDUFPVOWAWBIJZWFMJWCIJZXDWCIJZWFMJZWRWGVOXDGHZWF
    GHZWCGHZXEXGLVOWAGHZWBGHZXHVJVHVMXKVNVGVHVIUGZVKVLVMUHZBFQRZVJVGVMXLVNVGVHV
    IULZXNAFQRZWAWBNOZVJVGVKXIVNXPVKVLVMULZADQRZVJVHVKXJVNXMXSBDQRZXDWFWCUIPVOW
    RXDWCWFIJZIJZXEVOWPXDWQYBIVOVHVGVMWPXDLWTXAXBBAFSPVOVHVGVKWQYBLWTXAXCBADSPU
    JVOXHXJXIYCXELXRYAXTXDWCWFUKPUMVOXFWEWFMVOXKXLXJXFWELXOXQYAWAWBWCUNPUOUPUQV
    OVTVSVRKJZVSEKJZVSDKJZIJZWMVOVRGHZVSGHZVTYDLVOVLVKYHVJVKVLVMURZXCEDNOVOVIVG
