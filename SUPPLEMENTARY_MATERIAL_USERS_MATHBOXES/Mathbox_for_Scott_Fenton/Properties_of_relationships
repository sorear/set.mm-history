  ${
    $d A a b c d e f g h p q x $.  $d a ps $.  $d a rh $.
    $d B a b c d e f g h p q x $.  $d b ch $.  $d b rh $.
    $d C a b c d e f g h p q x $.  $d c th $.  $d c rh $.
    $d D a b c d e f g h p q x $.  $d d ta $.  $d d rh $.
    $d E a b c d e f g h p q x $.  $d e et $.  $d e rh $.
    $d F a b c d e f g h p q x $.  $d f ze $.  $d f rh $.
    $d G a b c d e f g h p q x $.  $d g si $.  $d g rh $.
    $d H a b c d e f g h p q x $.  $d h rh $.  $d P a b c d e f g h p q $.
    $d p ph $.  $d q ph $.  $d Q a b c d e f g h x $.
    $d S a b c d e f g h p q x $.  $d X a b c d e f g h x $.  $d x ph $.
    $d x rh $.
    br8.1 $e |- ( a = A -> ( ph <-> ps ) ) $.
    br8.2 $e |- ( b = B -> ( ps <-> ch ) ) $.
    br8.3 $e |- ( c = C -> ( ch <-> th ) ) $.
    br8.4 $e |- ( d = D -> ( th <-> ta ) ) $.
    br8.5 $e |- ( e = E -> ( ta <-> et ) ) $.
    br8.6 $e |- ( f = F -> ( et <-> ze ) ) $.
    br8.7 $e |- ( g = G -> ( ze <-> si ) ) $.
    br8.8 $e |- ( h = H -> ( si <-> rh ) ) $.
    br8.9 $e |- ( x = X -> P = Q ) $.
    br8.10 $e |- R = { <. p , q >. |
       E. x e. S E. a e. P E. b e. P E. c e. P E. d e. P E. e e. P
       E. f e. P E. g e. P E. h e. P ( p = <. <. a , b >. , <. c , d >. >. /\
       q = <. <. e , f >. , <. g , h >. >. /\ ph ) } $.
    $( Substitution for an eight-place predicate.  (Contributed by Scott
       Fenton, 26-Sep-2013.) $)
    br8 $p |- ( ( ( X e. S /\ A e. Q /\ B e. Q ) /\
                   ( C e. Q /\ D e. Q /\ E e. Q ) /\
                   ( F e. Q /\ G e. Q /\ H e. Q ) ) ->
      ( <. <. A , B >. , <. C , D >. >. R <. <. E , F >. , <. G , H >. >. <->
        rh ) ) $=
      ( cop wbr wceq wrex wcel opex eqeq1 3anbi1d rexbidv 2rexbidv 3anbi2d brab
      cv w3a wi wa eqcom opth bitri anbi12i simp13 vex opthg syl simp22 anbi12d
      wb simp31 simp33 syl5bb ad3antrrr simpllr simplrl simplrr simprll simprlr
      bitrd simprrl simprrr 3bitrd biimpd exp3a rexlimdvw a1d rexlimdvv simpl11
      3impd simpl12 simpl13 simpl21 simpl22 simpl23 simpl31 simpl32 eqidd simpr
      syl6bi simpl33 opeq1 opeq2d 3anbi23d rcla42ev syl113anc 3anbi13d rcla43ev
      eqeq2d opeq2 opeq1d syl31anc rexeqdv rexeqbidv rcla4ev syl2anc ex impbid
      ) KLVDZMNVDZVDZUCUDVDZUEUFVDZVDZQVEUUAUJVPZUKVPZVDZULVPZUMVPZVDZVDZVFZUUD
      SVPZTVPZVDZUAVPZUBVPZVDZVDZVFZAVQZUBOVGZUAOVGZTOVGZSOVGZUMOVGZULOVGZUKOVG
