    $( An element less than or equal to zero equals zero.  ( ~ chle0
       analog.) $)
    ople0 $p |- ( ( K e. OP /\ X e. B ) -> ( X .<_ Z <-> X = Z ) ) $=
      ( cops wcel wa wbr wceq op0le biantrud cpo wb opposet adantr simpr op0cl
      posasymb syl3anc bitrd ) BIJZDAJZKZDECLZUHEDCLZKZDEMZUGUIUHABCDEFGHNOUGBP
