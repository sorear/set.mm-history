    $( Lemma for ~ cdlemkfid3 . $)
    cdlemkfid1 $p |- ( ( ( K e. HL /\ W e. H )
        /\ ( F e. T /\ F =/= ( _I |` B ) /\ G e. T )
        /\ ( ( R ` G ) =/= ( R ` F ) /\ ( P e. A /\ -. P .<_ W ) ) )
