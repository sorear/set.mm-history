  $c +P $.

  $( Extend class notation with projective subspace sum. $)
  cpadd $a class +P $.

  ${
    $d l m n p q r $.
    $( Def. 8.6.6, p. 230. $)
    $( Define projective sum of two subspaces (or more generally two sets of
       atoms), which is the union of all lines generated by pairs of atoms from
       each subspace.  Lemma 16.2 of [MaedaMaeda] p. 68.  For convenience, our
       definition is generalized to apply to empty sets. $)
    df-padd $a |- +P = ( l e. _V
        |-> ( m e. ~P ( Atoms ` l ) , n e. ~P ( Atoms ` l )
           |-> ( ( m u. n ) u. { p e. ( Atoms ` l )
             | E. q e. m E. r e. n p ( le ` l ) ( q ( join ` l ) r ) } ) ) ) $.
  $}

  ${
    $d h m n p s A $.  $d h .\/ $.  $d h m n p q r s K $.  $d h .<_ $.
    paddfval.l $e |- .<_ = ( le ` K ) $.
    paddfval.j $e |- .\/ = ( join ` K ) $.
    paddfval.a $e |- A = ( Atoms ` K ) $.
    paddfval.p $e |- .+ = ( +P ` K ) $.
    $( Projective subspace sum operation. $)
    paddfval $p |- ( K e. B -> .+ = ( m e. ~P A , n e. ~P A
             |-> ( ( m u. n ) u. { p e. A
               | E. q e. m E. r e. n p .<_ ( q .\/ r ) } ) ) ) $=
      ( vh cv wrex cfv catm wcel cvv cpw cun wbr crab cmpt2 wceq elex cpadd cjn
      co cple fveq2 syl6eqr pweqd eqidd breq123d 2rexbidv rabeqbidv mpt2eq123dv
      oveqd uneq2d df-padd fvex eqeltri pwex mpt2ex fvmpt syl5eq syl ) GBUAGUBU
      AZCDEAUCZVMDQZEQZUDZKQZJQZIQZFULZHUEZIVORJVNRZKAUFZUDZUGZUHGBUIVLCGUJSWEO
