    $( This version of ~ cdlemg16 uses ~ cdlemg15a instead of ~ cdlemg15 , in
       case ~ cdlemg15 ends up not being needed.  TODO:  FIX COMMENT $)
    cdlemg16ALT $p |- ( ( ( K e. HL /\ W e. H
            /\ ( F e. T /\ G e. T ) )
         /\ ( ( P e. A /\ -. P .<_ W ) /\ ( Q e. A /\ -. Q .<_ W ) /\ P =/= Q )
         /\ ( ( ( F ` ( G ` P ) ) .\/ ( F ` ( G ` Q ) ) ) =/= ( P .\/ Q )
