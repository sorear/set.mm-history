$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
     Construction of a vector space from a Hilbert lattice
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d r s A $.  $d r s B $.  $d r s .\/ $.  $d r s K $.  $d r s .<_ $.
    $d r s ./\ $.  $d r s N $.  $d r s P $.  $d r s Q $.  $d r s X $.
    cdlema1.b $e |- B = ( Base ` K ) $.
    cdlema1.l $e |- .<_ = ( le ` K ) $.
    cdlema1.j $e |- .\/ = ( join ` K ) $.
    cdlema1.m $e |- ./\ = ( meet ` K ) $.
    cdlema1.a $e |- A = ( Atoms ` K ) $.
    cdlema1.n $e |- N = ( Lines ` K ) $.
    cdlema1.f $e |- F = ( pmap ` K ) $.
    $( A condition for required for proof of Lemma A in [Crawley] p. 112. $)
    cdlema1 $p |- ( ( ( K e. HL /\ X e. B /\ Y e. B ) /\ ( P e. A /\ Q e. A
     /\ R e. A ) /\ ( ( R =/= P /\ R .<_ ( P .\/ Q ) ) /\ ( P .<_ X /\ Q .<_ Y
        )
         /\ ( ( F ` Y ) e. N /\ ( X ./\ Y ) e. A /\ -. Q .<_ X ) ) )
              -> ( X .\/ R ) = ( X .\/ Y ) ) $=
      ( chlt wcel w3a wne co wbr wa cfv wn clat simp11 syl simp12 simp23 atbase
      hllat latjcl syl3anc simp13 latlej1 simp21 simp22 simp31r simp32l simp32r
      latjlej12 syl122anc mp2and lattrd latjle12 syl13anc mpbi2and wceq simp331
      wi wb simp332 simp333 latmle1 breq1 syl5ibrcom necon3bd latmle2 syl332anc
      mpd lneq2at 3jca simp31l hlatexch1 sylc latjlej1 latmlej11 latmcl eqbrtrd
      jca latasymd ) HUAUBZLBUBZMBUBZUCZCAUBZDAUBZEAUBZUCZECUDZECDGUEZIUFZUGZCL
      IUFZDMIUFZUGZMFUHKUBZLMJUEZAUBZDLIUFZUIZUCZUCZUCZBHILEGUEZLMGUEZNOXSWQHUJ
