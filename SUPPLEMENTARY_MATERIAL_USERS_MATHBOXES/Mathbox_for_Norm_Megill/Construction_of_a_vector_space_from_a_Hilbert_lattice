  ${
    cdlemb.b $e |- B = ( Base ` K ) $.
    cdlemb.l $e |- .<_ = ( le ` K ) $.
    cdlemb.j $e |- .\/ = ( join ` K ) $.
    cdlemb.u $e |- U = ( 1. ` K ) $.
    cdlemb.c $e |- C = ( <o ` K ) $.
    cdlemb.a $e |- A = ( Atoms ` K ) $.
    ${
      cdlemblem.s $e |- S = ( lt ` K ) $.
      cdlemblem.m $e |- ./\ = ( meet ` K ) $.
      cdlemblem.v $e |- V = ( ( P .\/ Q ) ./\ X ) $.
      $( Lemma for ~ cdlemb . $)
      cdlemblem $p |- ( ( ( ( K e. HL /\ P e. A /\ Q e. A )
           /\ ( X e. B /\ P =/= Q ) /\ ( X C U /\ -. P .<_ X /\ -. Q .<_ X ) )
        /\ ( u e. A /\ ( u =/= V /\ u S X ) )
        /\ ( r e. A /\ ( r =/= P /\ r =/= u /\ r .<_ ( P .\/ u ) ) ) )
      -> ( -. r .<_ X /\ -. r .<_ ( P .\/ Q ) ) ) $=
        ( chlt wcel w3a wne wa wbr wn cv co simp132 simp111 simp2l simp12l 3jca
