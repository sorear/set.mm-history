    $( Utility lemma. $)
    cdlemkoatnle-2 $p |- ( ( ( K e. HL /\ W e. H
                  /\ ( R ` F ) = ( R ` N ) )
           /\ ( F e. T /\ C e. T /\ N e. T )
           /\ ( ( R ` C ) =/= ( R ` F )
             /\ ( F =/= ( _I |` B ) /\ C =/= ( _I |` B ) )
             /\ ( P e. A /\ -. P .<_ W ) ) )
