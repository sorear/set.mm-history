  $c LDil $.
  $c LTrn $.
  $c Dil $.
  $c Trn $.

  $( Extend class notation with set of all lattice dilations. $)
  cldil $a class LDil $.

  $( Extend class notation with set of all lattice translations. $)
  cltrn $a class LTrn $.

  $( Extend class notation with set of all dilations. $)
  cdil $a class Dil $.

  $( Extend class notation with set of all translations. $)
  ctrn $a class Trn $.

  ${
    $d k w f x y p q $.
    $( Define set of all lattice dilations.  Similar to definition of dilation
       in [Crawley] p. 111. $)
    df-ldil $a |- LDil = ( k e. _V |-> ( w e. ( LHyp ` k )
       |-> { f e. ( LAut ` k ) | A. x e. ( Base ` k )
               ( x ( le ` k ) w -> ( f ` x ) = x ) } ) ) $.

    $( Define set of all lattice translations.  Similar to definition of
       translation in [Crawley] p. 111. $)
    df-ltrn $a |- LTrn = ( k e. _V |-> ( w e. ( LHyp ` k )
   |-> { f e. ( ( LDil ` k ) ` w ) |
     A. p e. ( Atoms ` k ) A. q e. ( Atoms ` k )
      ( ( -. p ( le ` k ) w /\ -. q ( le ` k ) w ) ->
           ( ( p ( join ` k ) ( f ` p ) ) ( meet ` k ) w )
        = ( ( q ( join ` k ) ( f ` q ) ) ( meet ` k ) w ) ) } ) ) $.
  $}

  ${
    $d k d f x q r $.
    $( Def. 8.6.17, p. 234. $)
    $( Define set of all dilations.  Definition of dilation in [Crawley]
       p. 111. $)
    df-dil $a |- Dil = ( k e. _V |-> ( d e. ( Atoms ` k )
       |-> { f e. ( PAut ` k ) | A. x e. ( PSubSp ` k )
               ( x C_ ( ( WAtoms ` k ) ` d ) -> ( f ` x ) = x ) } ) ) $.

    $( Def. 8.6.18, p. 234. $)
    $( Define set of all translations.  Definition of translation in [Crawley]
       p. 111. $)
    df-trn $a |- Trn = ( k e. _V |-> ( d e. ( Atoms ` k )
   |-> { f e. ( ( Dil ` k ) ` d )
      | A. q e. ( ( WAtoms ` k ) ` d ) A. r e. ( ( WAtoms ` k ) ` d )
           ( ( q ( +P ` k ) ( f ` q ) ) i^i ( ( _|_P ` k ) ` { d } ) )
         = ( ( r ( +P ` k ) ( f ` r ) ) i^i ( ( _|_P ` k ) ` { d } ) ) } ) ) $.
  $}

