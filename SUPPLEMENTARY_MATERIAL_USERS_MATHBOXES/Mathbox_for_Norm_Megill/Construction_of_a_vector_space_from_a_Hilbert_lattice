    $( Lemma for ~ 4atexlem7 . $)
    4atexlemex6 $p |- ( ( ( ( K e. HL /\ W e. H )
                /\ ( P e. A /\ -. P .<_ W ) /\ ( Q e. A /\ -. Q .<_ W ) )
            /\ ( ( R e. A /\ -. R .<_ W ) /\ S e. A )
