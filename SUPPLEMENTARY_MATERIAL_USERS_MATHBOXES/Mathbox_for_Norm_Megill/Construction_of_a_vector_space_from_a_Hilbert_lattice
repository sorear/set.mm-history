    $( Condition implying membership in a projective subspace sum with a
       point. $)
    elpaddatri $p |- ( ( ( K e. Lat /\ X C_ A /\ Q e. A ) /\
       ( R e. X /\ S e. A /\ S .<_ ( R .\/ Q ) ) ) ->
