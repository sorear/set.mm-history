  $c map1dimOLD $.

  $( Extend class notation with one-dimensional subspace map. $)
  cmp1dOLD $a class map1dimOLD $.

  ${
    $d q k w f $.
    $( Def. 8.6.49, p. 245. $)
    $( THIS DEFINITION WILL BE DISCARDED. Define a mapping from a
       one-dimensional subspace of the vector space generated by isomorphism H,
       to a one-dimensional subspace (ray) of functionals in the dual vector
       space.  The ` C_ ` relation (rather than ` = ` ) causes the zero
       functional to be included, since its kernel is the full vector space.
       This function corresponds to a projectivity in Baer, _Linear Algebra and
       Projective Geometry_, p. 40.  (Ideally, this would be a pure vector
       space definition, but it seems we need the orthocomplement from the
       Hilbert lattice ` k ` .) $)
    df-map1dimOLD $a |- map1dimOLD = ( k e. _V |-> ( w e. ( LHyp ` k ) |->
          ( q e. ( LSAtoms ` ( ( DVecH ` k ) ` w ) ) |->
  { f e. ( LFnl ` ( ( DVecH ` k ) ` w ) ) | ( ( ( ocH ` k ) ` w ) ` q )
        C_ ( ( LKer ` ( ( DVecH ` k ) ` w ) ) ` f ) } ) ) ) $.
  $}

  ${
    $d k w H $.  $d f k q w K $.
    map1dimOLDval.h $e |- H = ( LHyp ` K ) $.
    $( Mapping from vector space H atoms to rays of functionals. $)
    map1dimOLDffval $p |- ( K e. V -> ( map1dimOLD ` K ) = ( w e. H
  |-> ( q e. ( LSAtoms ` ( ( DVecH ` K ) ` w ) )
      |-> { f e. ( LFnl ` ( ( DVecH ` K ) ` w ) ) | ( ( ( ocH ` K ) ` w ) ` q )
          C_ ( ( LKer ` ( ( DVecH ` K ) ` w ) ) ` f ) } ) ) ) $=
      ( vk cvv cfv cv cdvh clsa coch clk clfn cmpt clh fveq2 fveq1d wss syl6eqr
      wcel crab wceq elex fveq2d sseq12d rabeqbidv mpteq12dv df-map1dimOLD fvex
      cmp1dOLD eqeltri mptex fvmpt syl ) DEUCDIUCDUMJACFAKZDLJZJZMJZFKZURDNJZJZ
      JZBKZUTOJZJZUAZBUTPJZUDZQZQZUEDEUFHDAHKZRJZFURVNLJZJZMJZVBURVNNJZJZJZVFVQ
      OJZJZUAZBVQPJZUDZQZQVMIUMVNDUEZAVOWGCVLWHVODRJZCVNDRSGUBWHFVRWFVAVKWHVQUT
      MWHURVPUSVNDLSTZUGWHWDVIBWEVJWHVQUTPWJUGWHWAVEWCVHWHVBVTVDWHURVSVCVNDNSTT
      WHVFWBVGWHVQUTOWJUGTUHUIUJUJABHFUKACVLCWIIGDRULUNUOUPUQ $.
      $( [6-Oct-2014] $)

    $d q w A $.  $d f w F $.  $d w L $.  $d w O $.  $d f q w W $.
    map1dimOLDval.u $e |- U = ( ( DVecH ` K ) ` W ) $.
    map1dimOLDval.a $e |- A = ( LSAtoms ` U ) $.
    map1dimOLDval.f $e |- F = ( LFnl ` U ) $.
    map1dimOLDval.l $e |- L = ( LKer ` U ) $.
    map1dimOLDval.o $e |- O = ( ( ocH ` K ) ` W ) $.
    map1dimOLDval.m $e |- M = ( ( map1dimOLD ` K ) ` W ) $.
    $( Mapping from vector space H atoms to rays of functionals. $)
    map1dimOLDfval $p |- ( ( K e. V /\ W e. H )
     -> M = ( q e. A |-> { f e. F | ( O ` q ) C_ ( L ` f ) } ) ) $=
      ( cfv wcel cdvh clsa coch clk wss clfn crab cmpt cmp1dOLD map1dimOLDffval
      vw cv fveq1d syl5eq wceq fveq2 syl6eqr fveq2d sseq12d rabeqbidv mpteq12dv
      eqid cvv fvex eqeltri mptex fvmpt sylan9eq ) FJUAZKEUAHKULELULUMZFUBTZTZU
      CTZLUMZVKFUDTZTZTZCUMZVMUETZTZUFZCVMUGTZUHZUIZUIZTZLAVOITZVSGTZUFZCDUHZUI
      ZVJHKFUJTZTWGSVJKWMWFULCEFJLMUKUNUOULKWEWLEWFVKKUPZLVNWDAWKWNVNBUCTZAWNVM
      BUCWNVMKVLTBVKKVLUQNURZUSOURWNWBWJCWCDWNWCBUGTDWNVMBUGWPUSPURWNVRWHWAWIWN
      VOVQIWNVQKVPTIVKKVPUQRURUNWNVSVTGWNVTBUETGWNVMBUEWPUSQURUNUTVAVBWFVCLAWKA
      WOVDOBUCVEVFVGVHVI $.
      $( [6-Oct-2014] $)

    $d q F $.  $d q L $.  $d q O $.  $d f q Q $.
    map1dimOLDval.k $e |- ( ph -> ( K e. V /\ W e. H ) ) $.
    map1dimOLDval.q $e |- ( ph -> Q e. A ) $.
    $( Mapping from vector space H atoms to rays of functionals. $)
    map1dimOLDval $p |- ( ph
           -> ( M ` Q ) = { f e. F | ( O ` Q ) C_ ( L ` f ) } ) $=
      ( vq cfv cv wss crab cmpt wcel wa wceq map1dimOLDfval syl fveq1d cvv clfn
      fvex eqeltri rabex fveq2 sseq1d rabbidv eqid fvmptg sylancl eqtrd ) ACJUD
      CUCBUCUEZKUDZEUEIUDZUFZEFUGZUHZUDZCKUDZVIUFZEFUGZACJVLAHLUIMGUIUJJVLUKUAB
      DEFGHIJKLMUCNOPQRSTULUMUNACBUIVPUOUIVMVPUKUBVOEFFDUPUDUOQDUPUQURUSUCCVKVP
      BUOVLVGCUKZVJVOEFVQVHVNVIVGCKUTVAVBVLVCVDVEVF $.
      $( [6-Oct-2014] $)
  $}

$(
  @{
    map1dimOLDfray.h @e |- H = ( LHyp ` K ) @.
    map1dimOLDfray.u @e |- U = ( ( DVecH ` K ) ` W ) @.
    map1dimOLDfray.a @e |- A = ( LSAtoms ` U ) @.
    map1dimOLDfray.m @e |- M = ( ( map1dimOLD ` K ) ` W ) @.
    map1dimOLDfray.r @e |- R = ( fRay ` U ) @.
    map1dimOLDfray.k @e |- ( ph -> ( K e. HL /\ W e. H ) ) @.
    map1dimOLDfray.q @e |- ( ph -> Q e. A ) @.
    @( The value of the D map is a ray of functionals. @)
    map1dimOLDfray @p |- ( ph -> ( M ` Q ) e. ran R ) @=
? @.
      @( [12-Oct-2014] @)
  @}

  @{
    map1dimOLDrnfray.h @e |- H = ( LHyp ` K ) @.
    map1dimOLDrnfray.u @e |- U = ( ( DVecH ` K ) ` W ) @.
    map1dimOLDrnfray.m @e |- M = ( ( map1dimOLD ` K ) ` W ) @.
    map1dimOLDrnfray.r @e |- R = ( fRay ` U ) @.
    map1dimOLDrnfray.k @e |- ( ph -> ( K e. HL /\ W e. H ) ) @.
    @( The value of the D map is a ray of functionals. @)
