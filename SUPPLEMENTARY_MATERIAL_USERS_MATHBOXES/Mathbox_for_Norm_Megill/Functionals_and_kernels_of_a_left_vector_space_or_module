  ${
    $d x y z G $.  $d x y z H $.  $d x y z P $.  $d x y z R $.  $d x y z W $.
    $d x y z ph $.
    lfladdcl.r $e |- R = ( Scalar ` W ) $.
    lfladdcl.p $e |- P = ( +g ` R ) $.
    lfladdcl.f $e |- F = ( LFnl ` W ) $.
    lfladdcl.w $e |- ( ph -> W e. LMod ) $.
    lfladdcl.g $e |- ( ph -> G e. F ) $.
    lfladdcl.h $e |- ( ph -> H e. F ) $.
