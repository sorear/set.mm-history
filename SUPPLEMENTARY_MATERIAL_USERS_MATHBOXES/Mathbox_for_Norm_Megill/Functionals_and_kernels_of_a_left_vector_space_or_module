$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
     Functionals and kernels of a left vector space (or module)
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c LFnl $.

  $( Extend class notation with all linear functionals of a left module or left
     vector space. $)
  clfn $a class LFnl $.

  ${
    $d w x y r f $.
    $( Define the set of all linear functionals (maps from vectors to to the
       ring) of a left module or left vector space. $)
    df-lfl $a |- LFnl = ( w e. _V |->
     { f e. ( ( Base ` ( Scalar ` w ) ) ^m ( Base ` w ) )
       | A. r e. ( Base ` ( Scalar ` w ) )
         A. x e. ( Base ` w ) A. y e. ( Base ` w )
          ( f ` ( ( r ( vsca ` w ) x ) ( +g ` w ) y ) )
             = ( ( r ( .r ` ( Scalar ` w ) ) ( f ` x ) )
                 ( +g ` ( Scalar ` w ) ) ( f ` y ) ) } ) $.
  $}

  ${
    $d w A $.  $d f r w K $.  $d w P $.  $d w S $.  $d w T $.  $d f w x y V $.
    $d f r w x y W $.
    lflset.v $e |- V = ( Base ` W ) $.
    lflset.a $e |- A = ( +g ` W ) $.
    lflset.d $e |- D = ( Scalar ` W ) $.
    lflset.s $e |- S = ( vsca ` W ) $.
    lflset.k $e |- K = ( Base ` D ) $.
    lflset.p $e |- P = ( +g ` D ) $.
    lflset.t $e |- T = ( .r ` D ) $.
    lflset.f $e |- F = ( LFnl ` W ) $.
    $( The set of linear functionals in a left module or left vector space. $)
    lflset $p |- ( W e. X -> F = { f e. ( K ^m V )
       | A. r e. K A. x e. V A. y e. V ( f ` ( ( r S x ) A y ) )
             = ( ( r T ( f ` x ) ) P ( f ` y ) ) } ) $=
      ( vw wcel cvv cv co wceq wral cmap crab elex clfn cvsca cplusg csca cmulr
      cfv fveq2 syl6eqr fveq2d oveq12d oveqd eqidd oveq123d raleqbidv rabeqbidv
      cbs eqeq12d df-lfl ovex rabex fvmpt syl5eq syl ) LMUDLUEUDZINUFZAUFZFUGZB
