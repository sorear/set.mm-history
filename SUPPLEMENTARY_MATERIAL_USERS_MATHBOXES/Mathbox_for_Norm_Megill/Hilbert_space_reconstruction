          /\ ( U e. E /\ V e. E /\ ( U ` F ) = ( V ` F ) )
          /\ ( F e. T /\ F =/= ( _I |` B ) /\ h e. T ) )
       /\ ( h =/= ( _I |` B ) /\ g e. T
          /\ g =/= ( _I |` B ) )
