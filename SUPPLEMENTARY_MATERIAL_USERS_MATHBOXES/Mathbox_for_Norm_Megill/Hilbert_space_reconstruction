$(
  @c LSLat @.

  @( Extend class notation with lattice of subspaces of a left module
     or left vector space. @)
  clsl @a class LSLat @.

  @{
    @d w x y @.
    df-lsl @a |- LSLat = ( w e. _V |-> { <. ( Base ` ndx ) ,
        ( LSubSp ` w ) >. , <. ( le ` ndx ) ,
     { <. x , y >. | ( x e. ( LSubSp ` w ) /\ y e. ( LSubSp ` w )
        /\ x C_ y ) } >. } ) @.
  @}

  @c mapd @.

  @( Extend class notation with one-dimensional subspace map. @)
  cmpd @a class mapd @.

@(

  @{
    @( Def. 8.6.49, p. 245. @)
    @( Define one-dimensional subspace map. @)
    df-mapd @a |- mapd = ( k e. _V , p e. _V |->
        ( s e. ( LSAtoms ` w ) |-> { f e. ( LFnl ` w ) |
           ( ( ker ` w ) ` f ) =

{ a | E. v E. w
     ( v = ( l DVecH p ) /\ w = ( Vldual ` v )
       /\ a e. ( s e. { r | E. x e. ( vbase ` v )
          r = ( ( span ` v ) ` { x } ) } |->
               ( iota t A. x e. s A. f e. ( vbase ` w )
   ( ( ( ker ` w ) ` f ) = s /\ t = ( ( span ` w ) ` { f } ) ) ) ) ) } ) @.
  @}
$)

$(
    @( Def. 8.6.49, p. 245. @)
    @( Define one-dimensional subspace map. @)
    df-mapd @a |- mapd = ( l e. _V , p e. _V |-> { a | E. v E. w
     ( v = ( l DVecH p ) /\ w = ( Vldual ` v )
       /\ a e. ( s e. { r | E. x e. ( vbase ` v )
          r = ( ( span ` v ) ` { x } ) } |->
               ( iota t A. x e. s A. f e. ( vbase ` w )
   ( ( ( ker ` w ) ` f ) = s /\ t = ( ( span ` w ) ` { f } ) ) ) ) ) } ) @.
$)


