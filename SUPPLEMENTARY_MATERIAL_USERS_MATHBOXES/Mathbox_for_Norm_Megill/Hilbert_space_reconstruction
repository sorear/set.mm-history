    ${
      dihglblem5a.l $e |- .<_ = ( le ` K ) $.
      dihglblem5a.j $e |- .\/ = ( join ` K ) $.
      dihglblem5a.a $e |- A = ( Atoms ` K ) $.
      dihglblem5a.p $e |- P = ( ( oc ` K ) ` W ) $.
      dihglblem5a.t $e |- T = ( ( LTrn ` K ) ` W ) $.
