  $( An unordered pair has at most two elements. $)
  hashprlei $p |- ( { A , B } e. Fin /\ ( # ` { A , B } ) <_ 2 ) $=
    ( csn cpr c1 c2 df-pr hashsnlei 1nn0 df-2 hashunlei ) ACBCABDEEFABGAHBHIIJK
    $.
    $( [11-Feb-2015] $)

  $( An unordered triple has at most three elements. $)
  hashtplei $p |- ( { A , B , C } e. Fin /\ ( # ` { A , B , C } ) <_ 3 ) $=
    ( cpr csn ctp c2 c1 c3 df-tp hashprlei hashsnlei 2nn0 1nn0 df-3 hashunlei )
    ABDCEABCFGHIABCJABKCLMNOP $.
    $( [11-Feb-2015] $)

