    @( Lemma for ~ gch-ac . The limit step of the transfinite induction in the
       proof of ~ gch-ac . @)
    gch-aclem2 @p |- ( ( Lim r /\
      A. x e. On ( aleph ` suc x ) ~~ ~P ( aleph ` x ) /\
      A. s e. r E. t t We ( R1 ` s ) ) -> E. t t We ( R1 ` r ) ) @=
      ? @.
  @}

