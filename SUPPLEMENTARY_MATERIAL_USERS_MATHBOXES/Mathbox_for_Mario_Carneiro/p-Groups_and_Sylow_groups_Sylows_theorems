$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
              p-Groups and Sylow groups; Sylow's theorems
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c od $.
  $c pGrp $.
  $c pSyl $.

  $( Extend class notation to include the order function on the elements of a
     group. $)
  cod $a class od $.

  $( Extend class notation to include the class of all p-groups. $)
  cpgp $a class pGrp $.

  $( Extend class notation to include the class of all Sylow p-subgroups of a
     group. $)
  cslw $a class pSyl $.

  ${
    $d g h k n p x $.
    $( Define the order of an element in a group. $)
    df-od $a |- od = ( g e. _V |-> ( x e. ( Base ` g ) |->
      sup ( { n e. NN | ( n ( .g ` g ) x ) = ( 0g ` g ) } , RR* , `' < ) ) ) $.

    $( Define the set of p-groups, which are groups such that every element has
       a power of ` p ` as its order. $)
    df-pgp $a |- pGrp = { <. p , g >. | ( ( p e. Prime /\ g e. Grp ) /\
      A. x e. ( Base ` g ) E. n e. NN0 ( ( od ` g ) ` x ) = ( p ^ n ) ) } $.

    $( Define the set of Sylow p-subgroups of a group ` g ` .  A Sylow
       p-subgroup is a p-group that is not a subgroup of any other p-groups in
       ` g ` . $)
    df-slw $a |- pSyl = ( p e. Prime , g e. Grp |-> { h e. ( SubGrp ` g ) |
   A. k e. ( SubGrp ` g ) ( ( h C_ k /\ p pGrp ( g |`s k ) ) <-> h = k ) } ) $.
  $}

  ${
    $d n x y A $.  $d g n x G $.  $d g n x y T $.  $d g x X $.  $d g n x y Z $.
    $d n N $.
    odval.1 $e |- X = ( Base ` G ) $.
    odval.2 $e |- T = ( .g ` G ) $.
    odval.3 $e |- Z = ( 0g ` G ) $.
    odval.4 $e |- O = ( od ` G ) $.
    $( Value of the order function. $)
    odfval $p |- ( G e. V -> O = ( x e. X |->
      sup ( { n e. NN | ( n T x ) = Z } , RR* , `' < ) ) ) $=
      ( vg cfv cv wceq cn cxr cvv cbs wcel cod crab clt ccnv csup cmpt elex cmg
