    $( The order of the group identity is one. $)
    od1 $p |- ( G e. Grp -> ( O ` Z ) = 1 ) $=
      ( cgrp wcel cfv c1 wceq cle wbr cbs cn cmg co eqid syl cpnf cxr 1nn mulg1
      grpidcl a1i odlem3 syl12anc wo odcl nnge1 cr ressxr sselii pnfge ax-mp id
