    $( A filter has the same convergence and clustering properties as some
       net.  (Contributed by Jeff Hankins, 12-Dec-2009.) $)
    filnet $p |- ( F e. Fil -> E. d e. DirRel E. f ( f : dom d --> X
       /\ F = ( ( X FilMap ran ( tail ` d ) ) ` f ) ) ) $=
      ( vx vy vn vm cv wcel wa copab c2nd cfv wss eqid filnetlem4 ) FGDAFJZHJZB
      KIJTKLIHMZKGJZUAKLUBNOSNOPLFGMZIHBUACEUAQUCQR $.
      $( [24-Nov-2013] $) $( [12-Dec-2009] $)
  $}

$( (End of Jeffrey Hankins' mathbox.) $)


