$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                Directed sets, nets
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

$c Dir $.
  $( Extend class notation with the class of all directed sets. $)
  cdir $a class Dir $.

$c tail $.
  $( Extend class notation with the tail function. $)
  ctail $a class tail $.

  ${
    $d d x y z $.
    $( Define the class of all directed sets/directions. $)
    df-dir $a |- Dir = { d | ( ( Rel d /\ ( I |` U. U. d ) (_ d ) /\ ( ( d o. d
    ) (_ d /\ A. x e. U. U. d A. y e. U. U. d E. z e. U. U. d ( x d z /\ y d z
    ) ) ) } $.
  $}

  ${
    $d t w x y z $.
    $( Define the tail function for directed sets. $)
    df-tail $a |- tail = { <. x , y >. | ( x e. Dir /\ y = { <. z , w >. | ( z
    e. U. U. x /\ w = { t | z x t } ) } ) } $.
  $}

  ${
    $d d x y z A $. $d d B $. $d d x y z D $.
    isdir.1 $e |- A = U. U. D $.
    $( A condition for a relation to be a direction. $)
    isdir $p |- ( D e. B -> ( D e. Dir <-> ( ( Rel D /\ ( I |` A ) (_ D ) /\ (
    ( D o. D ) (_ D /\ A. x e. A A. y e. A E. z e. A ( x D z /\ y D z ) ) ) ) )
    $=
      ( vd cv wrel cid cuni cres wss wa ccom wbr wrex wral cdir wceq releq
      unieq unieqd syl6eqr reseq2 syl id sseq12d anbi12d coeq1 coeq2 eqtrd
      breq rexeq12d raleq12d df-dir elab2g ) HIZJZKUSLZLZMZUSNZOZUSUSPZUSNZAIZC
