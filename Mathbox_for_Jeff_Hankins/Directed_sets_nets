  ${
    $d d f m n t v x y w z F $.  $d d f m n t w z X $.
    filnet.1 $e |- X = U. F $.
    ${
      $d t v w x y z H $.  $d t v w z D $.
      filnet.2 $e |- H = { <. m , n >. | ( n e. F /\ m e. n ) } $.
      filnet.3 $e |- D = { <. x , y >. |
        ( ( x e. H /\ y e. H ) /\ ( 2nd ` y ) C_ ( 2nd ` x ) ) } $.
      ${
        $d A x y $.  $d B x y $.
        filnetlem1.4 $e |- A e. _V $.
        filnetlem1.5 $e |- B e. _V $.
        $( Lemma for ~ filnet .  Change variables. $)
        filnetlem1 $p |- ( A D B <-> ( ( A e. H /\ B e. H ) /\
          ( 2nd ` B ) C_ ( 2nd ` A ) ) ) $=
          ( wcel wa c2nd cfv wss wceq eleq1 anbi1d sseq2d anbi12d anbi2d sseq1d
          cv fveq2 brab ) AUHZIPZBUHZIPZQZUMRSZUKRSZTZQCIPZUNQZUPCRSZTZQUSDIPZQ
