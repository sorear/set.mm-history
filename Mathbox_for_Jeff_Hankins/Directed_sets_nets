      UPUBUCAEUDUEUFULAUOUQCUQULCBUGUODBUHUIULUQUJUCUK $.
      $( [24-Nov-2013] $) $( [25-Nov-2009] $)

    $( The tail of an element in a directed set.  (Contributed by Jeff Hankins,
       25-Nov-2009.) $)
    tailval $p |- ( ( D e. DirRel /\ A e. X ) ->
                   ( ( tail ` D ) ` A ) = ( D " { A } ) ) $=
      ( vx cdir wcel wa ctail cfv cv csn cima cmpt wceq tailfval fveq1d adantr
      cvv id imaexg sneq imaeq2d eqid fvmptg syl2anc eqtrd ) BFGZACGZHABIJZJZAE
      CBEKZLZMZNZJZBALZMZUHUKUPOUIUHAUJUOEBCDPQRUIUIURSGUPUROUHUITBUQFUAEAUNURC
      SUOULAOUMUQBULAUBUCUOUDUEUFUG $.
      $( [24-Nov-2013] $) $( [25-Nov-2009] $)

    $( An element of a tail.  (Contributed by Jeff Hankins, 25-Nov-2009.) $)
    eltail $p |- ( ( D e. DirRel /\ A e. X /\ B e. C ) ->
                   ( B e. ( ( tail ` D ) ` A ) <-> A D B ) ) $=
      ( cdir wcel w3a ctail cfv csn cima wbr wb wa tailval eleq2d 3adant3 cop
      elimasng df-br syl6bbr 3adant1 bitrd ) DGHZAEHZBCHZIBADJKKZHZBDALMZHZABDN
      ZUFUGUJULOUHUFUGPUIUKBADEFQRSUGUHULUMOUFUGUHPULABTDHUMDABECUAABDUBUCUDUE
