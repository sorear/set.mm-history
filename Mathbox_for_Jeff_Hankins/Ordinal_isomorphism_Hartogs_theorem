      $( Lemma for ~ hartog .  (Moved to ~ hartoglem in main set.mm and may be
         deleted by mathbox owner, JGH. --NM 26-Aug-2011.) $)
      hartoglemOLD $p |- ( ( ( A e. B /\ g : t -1-1-> A ) /\ t e. On )
          -> g Isom _E , R ( t , ( g " t ) ) ) $=
        ( vr vs cv cima cep wiso wf1o wbr cfv wb wral wcel wf1 wa con0 df-iso
        wfo df-f1o wf ccnv wfun df-f1 crn wfn f1f ffn dffn4 fof sylbi 3syl
