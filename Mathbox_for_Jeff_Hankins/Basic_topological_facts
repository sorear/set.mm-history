    $( Lemma for ~ alexsub .  Every subset of a base which has no finite
       subcover is a subset of a maximal such collection. $)
    alexsublem2 $p |- ( ( ( J = ( topGen ` ( fi ` x ) ) /\ A. c e. ~P x ( X =
    U. c -> E. d e. ( ~P c i^i Fin ) X = U. d ) /\ a e. ~P ( fi ` x ) ) /\ A. b
    e. ( ~P a i^i Fin ) -. X = U. b ) -> E. u e. ( { z e. ~P ( fi ` x ) | ( a
    C_ z /\ A. b e. ( ~P z i^i Fin ) -. X = U. b ) } u. { (/) } ) A. v e. ( { z
    e. ~P ( fi ` x ) | ( a C_ z /\ A. b e. ( ~P z i^i Fin ) -. X = U. b ) } u.
    { (/) } ) -. u C. v ) $=
