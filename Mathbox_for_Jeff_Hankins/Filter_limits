    $( Lemma for ~ fmfnfm . $)
    fmfnfmlem2 $p |- ( ph -> ( E. x e. L s = ( `' F " x ) ->
                       ( ( F " s ) C_ t -> ( t C_ X -> t e. L ) ) ) ) $=
      ( cv wceq wss wcel wi wa ad2antrr vy ccnv cima cfil crn cin cfbas w3a cfm
      vz wf co cfv simp1d simp2d simplr wfo simp3d wfn ffn dffn4 sylib 3syl cfg
