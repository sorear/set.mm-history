  ${
    $d b s t w x y z B $.  $d b s t w x y z F $.  $d b s t w x y z L $.
    $d b s t w x y z X $.  $d s t w x y z ph $.  $d b s t x y z Y $.
    $d s t x y Z $.
    fmfnfm.1 $e |- Y = U. B $.
    fmfnfm.2 $e |- Z = U. L $.
    fmfnfm.3 $e |- ( ph -> ( ( B e. fBas /\ L e. Fil /\ F : Y --> X ) /\
                             ( ( X FilMap B ) ` F ) C_ L /\ X = Z ) ) $.
