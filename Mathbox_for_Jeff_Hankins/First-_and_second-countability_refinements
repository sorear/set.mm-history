$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                First- and second-countability, refinements
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c 1stc $.
  $( Extend class definition to include the class of all first-countable
     topologies. $)
  c1stc $a class 1stc $.

  $c 2ndc $.
  $( Extend class definition to include the class of all second-countable
     topologies. $)
  c2ndc $a class 2ndc $.

  $c Fne $.
  $( Extend class definition to include the "finer than" relation. $)
  cfne $a class Fne $.

  $c Ref $.
  $( Extend class definition to include the refinement relation. $)
  cref $a class Ref $.

  $c PtFin $.
  $( Extend class definition to include the class of point-finite covers. $)
  cptfin $a class PtFin $.

  $c LocFin $.
  $( Extend class definition to include the class of locally finite covers. $)
  clocfin $a class LocFin $.

  ${
    $d w x y z $.
    $( Define the class of all first-countable topologies. $)
    df-1stc $a |- 1stc = { x e. Top | A. y e. U. x E. z e. ~P x ( z ~<_ om /\
    A. w e. x ( y e. w -> y e. U. ( z i^i ~P w ) ) ) } $.
  $}

  ${
    $d x y $.
    $( Define the class of all second-countable topologies. $)
    df-2ndc $a |- 2ndc = { x e. Top | E. y e. TopBases ( y ~<_ om
       /\ ( topGen ` y ) = x ) } $.
  $}

  ${
    $d x y z $.
    $( Define the fineness relation for covers. $)
    df-fne $a |- Fne = { <. x , y >. | ( U. x = U. y /\ A. z e. x z C_ U. ( y
    i^i ~P z ) ) } $.
  $}

  ${
    $d w x y z $.
    $( Define the refinement relation. $)
    df-ref $a |- Ref = { <. x , y >. | ( U. x = U. y /\ A. z e. y E. w e. x z
    C_ w ) } $.
  $}

  ${
    $d x y z $.
    $( Define "point-finite." $)
    df-ptfin $a |- PtFin = { x | A. y e. U. x { z e. x | y e. z } e. Fin } $.
  $}

  ${
    $d n p s x y $.
    $( Define "locally finite." $)
    df-locfin $a |- LocFin = { <. x , y >. | ( x e. Top /\ U. x = U. y /\ A. p
    e. U. x E. n e. ( ( nei ` x ) ` { p } ) { s e. y | ( s i^i n ) =/= (/) } e.
    Fin ) } $.
  $}

  ${
    $d w x y z J $.  $d w x X $.  $d w x y z $.
    is1stc.1 $e |- X = U. J $.
    $( The predicate "is a first-countable topology."  This can be described as
       "every point has a countable local basis"-that is, every point has a
       countable collection of open sets containing it such that every open set
       containing the point has an open set from this collection as a subset.
       (Contributed by Jeff Hankins, 22-Aug-2009.) $)
    is1stc $p |- ( J e. 1stc <-> ( J e. Top /\ A. x e. X E. y e. ~P J ( y ~<_
    om /\ A. z e. J ( x e. z -> x e. U. ( y i^i ~P z ) ) ) ) ) $=
      ( vw cv com cdom wbr wcel cpw cin cuni wi wral wa wrex ctop c1stc syl6eqr
      wceq unieq pweq raleq anbi2d rexeqbidv raleqbidv df-1stc elrab2 ) BHZIJKZ
      AHZCHZLUNULUOMNOLPZCGHZQZRZBUQMZSZAUQOZQUMUPCDQZRZBDMZSZAEQGDTUAUQDUCZVAV
      FAVBEVGVBDOEUQDUDFUBVGUSVDBUTVEUQDUEVGURVCUMUPCUQDUFUGUHUIGABCUJUK $.
      $( [22-Aug-2009] $)

    $( The predicate "is a first-countable topology."  (Contributed by Jeff
       Hankins, 22-Aug-2009.) $)
    is1stc2 $p |- ( J e. Top -> ( J e. 1stc <-> A. x e. X E. y e. ~P J ( y ~<_
    om /\ A. z e. J ( x e. z -> x e. U. ( y i^i ~P z ) ) ) ) ) $=
      ( c1stc wcel ctop cv com cdom wbr cpw cin cuni wi wral wa wrex is1stc
      baib ) DGHDIHBJZKLMAJZCJZHUDUCUENOPHQCDRSBDNTAERABCDEFUAUB $.
      $( [22-Aug-2009] $)
  $}

  ${
    $d x y z J $.  $d x X $.  $d w x y z $.
    is1stc3.1 $e |- X = U. J $.
    $( An equivalent way of saying "is a first-countable topology."
       (Contributed by Jeff Hankins, 22-Aug-2009.) $)
    is1stc3 $p |- ( J e. Top -> ( J e. 1stc <-> A. x e. X E. y e. ~P J ( y ~<_
    om /\ A. z e. J ( x e. z -> E. w e. y ( x e. w /\ w C_ z ) ) ) ) ) $=
      ( ctop wcel c1stc cv cpw wi wral wa wrex wex anbi2i anass ralbii com cdom
      wbr cin cuni wss is1stc2 elin ancom visset elpw anbi12i bitri exbii eluni
      3bitr2i df-rex 3bitr4i imbi2i rexbii syl6bb ) EHIEJIBKZUAUBUCZAKZCKZIZVDV
      BVELZUDZUEIZMZCENZOZBELZPZAFNVCVFVDDKZIZVOVEUFZOZDVBPZMZCENZOZBVMPZAFNABC
      EFGUGVNWCAFVLWBBVMVKWAVCVJVTCEVIVSVFVPVOVHIZOZDQVOVBIZVROZDQVIVSWEWGDWEVP
      WFVOVGIZOZOVPWFOZWHOZWGWDWIVPVOVBVGUHRVPWFWHSWKWFVPOZVQOWGWJWLWHVQVPWFUIV
      OVEDUJUKULWFVPVQSUMUPUNDVDVHUOVRDVBUQURUSTRUTTVA $.
      $( [22-Aug-2009] $)
  $}

  ${
    $d x y z J $.
    $( A first-countable topology is a topology.  (Contributed by Jeff Hankins,
       22-Aug-2009.) $)
    1stctop $p |- ( J e. 1stc -> J e. Top ) $=
      ( vy vx vz c1stc wcel ctop cv com cdom wbr cpw cin cuni wi wral wrex eqid
      wa is1stc simplbi ) AEFAGFBHZIJKCHZDHZFUCUBUDLMNFODAPSBALQCANZPCBDAUEUERT
      UA $.
      $( [22-Aug-2009] $)
  $}

  ${
    $d w x y z A $.  $d w x y J $.  $d w X $.
    1stcclb.1 $e |- X = U. J $.
    $( A property of points in a first-countable topology.  (Contributed by
       Jeff Hankins, 22-Aug-2009.) $)
    1stcclb $p |- ( ( J e. 1stc /\ A e. X ) -> E. x e. ~P J ( x ~<_ om /\ A. y
    e. J ( A e. y -> E. z e. x ( A e. z /\ z C_ y ) ) ) ) $=
      ( vw c1stc wcel cv com cdom wbr wa wrex wi wral eleq1 rexbidv wss 1stctop
      cpw ctop is1stc3 biimpcd wceq anbi1d imbi12d ralbidv anbi2d rcla4v mpan9
      mpd ) EIJZAKZLMNZHKZBKZJZURCKZJZVAUSUAZOZCUPPZQZBERZOZAEUCZPZHFRZDFJUQDUS
      JZDVAJZVCOZCUPPZQZBERZOZAVIPZUOEUDJZVKEUBVTUOVKHABCEFGUEUFUNVJVSHDFURDUGZ
      VHVRAVIWAVGVQUQWAVFVPBEWAUTVLVEVOURDUSSWAVDVNCUPWAVBVMVCURDVASUHTUIUJUKTU
      LUM $.
      $( [22-Aug-2009] $)
  $}

  ${
    $d x y J $.
    $( The property of being second-countable.  (Contributed by Jeff Hankins,
       17-Jan-2010.) $)
    is2ndc $p |- ( J e. 2ndc <-> ( J e. Top /\ E. x e. TopBases ( x ~<_ om /\ (
    topGen ` x ) = J ) ) ) $=
      ( vy cv com cdom wbr ctg cfv wceq wa wrex ctop c2ndc eqeq2 anbi2d rexbidv
      ctb df-2ndc elrab2 ) ADZEFGZUAHIZCDZJZKZARLUBUCBJZKZARLCBMNUDBJZUFUHARUIU
      EUGUBUDBUCOPQCAST $.
      $( [17-Jan-2010] $)
  $}

  ${
    $d x J $.
    $( The property of being second-countable with the condition that ` J ` is
       a topology removed.  (Contributed by Jeff Hankins, 17-Jan-2010.) $)
    is2ndc2 $p |- ( J e. 2ndc <-> E. x e. TopBases ( x ~<_ om /\ ( topGen ` x )
      = J ) ) $=
      ( c2ndc wcel ctop cv com cdom wbr ctg cfv wceq ctb wrex is2ndc simpr tgcl
      wa eleq1 syl5ibcom adantld r19.23aiv ancri impbii bitri ) BCDBEDZAFZGHIZU
      GJKZBLZRZAMNZRZULABOUMULUFULPULUFUKUFAMUGMDZUJUFUHUNUIEDUJUFUGQUIBESTUAUB
      UCUDUE $.
      $( [17-Jan-2010] $)
  $}

  ${
    $d x J $.
    $( A second-countable topology is a topology.  (Contributed by Jeff
       Hankins, 17-Jan-2010.) $)
    2ndctop $p |- ( J e. 2ndc -> J e. Top ) $=
      ( vx c2ndc wcel ctop cv com cdom wbr ctg cfv wceq ctb wrex is2ndc simplbi
      wa ) ACDAEDBFZGHIRJKALQBMNBAOP $.
      $( [17-Jan-2010] $)
  $}

  ${
    $d x J $.
    $( A second-countable topology has a countable base.  (Contributed by Jeff
       Hankins, 17-Jan-2010.) $)
    2ndccb $p |- ( J e. 2ndc -> E. x e. TopBases ( x ~<_ om /\ ( topGen ` x )
     = J ) ) $=
      ( c2ndc wcel ctop cv com cdom wbr ctg cfv wceq wa ctb wrex is2ndc simprbi
      ) BCDBEDAFZGHIRJKBLMANOABPQ $.
      $( [17-Jan-2010] $)
  $}

  ${
    $d x y J $.
    $( Having a countable subbase is a sufficient condition for
       second-countability.  (Contributed by Jeff Hankins, 17-Jan-2010.) $)
    2ndcsb $p |- ( J e. Top -> ( J e. 2ndc <-> E. x ( x ~<_ om /\ ( topGen ` (
    fi ` x ) ) = J ) ) ) $=
      ( vy ctop wcel cv com cdom wbr ctg cfv wceq wa ctb cfi wss fibas ad2antrr
