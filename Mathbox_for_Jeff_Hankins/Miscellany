  ${
    $d z w ph $. $d z w ps $. $d z w ch $. $d y z w A $. $d y z w B $. $d y z w
    X $. $d y z w Y $. $d y z w Z $. $d x y z w $.
    subtr.1 $e |- ( y e. A -> A. x y e. A ) $.
    subtr.2 $e |- ( y e. B -> A. x y e. B ) $.
    ${
    subtr.3 $e |- ( y e. Y -> A. x y e. Y ) $.
    subtr.4 $e |- ( y e. Z -> A. x y e. Z ) $.
    subtr.5 $e |- ( x = A -> X = Y ) $.
    subtr.6 $e |- ( x = B -> X = Z ) $.
    $( Transitivity of implicit substitution. $)
    subtr $p |- ( ( A e. C /\ B e. D ) -> ( A = B -> Y = Z ) ) $=
      ( vz vw cv wceq csb wi eqeq1 wex a9e wcel ax-17 hbeq visset hbcsb1 hbim
      csbeq1a sylan9req ex sylbird 19.23ai ax-mp eqeq1d imbi12d eqeq2 eqeq2d
      csbeq1 vtocl2g ) PRZQRZSZAVCGTZAVDGTZSZUACVDSZHVGSZUACDSZHISZUAPQCDEFVCCS
      ZVEVIVHVJVCCVDUBVMVFHVGARZVCSZAUCVMVFHSZUAZAPUDVOVQAVMVPAABBVCCBRZVCUEAUF
      ZJUGABBVFHABVCGPUHVSUILUGUJVOVMVNCSZVPVNVCCUBVOVTVPVOVTVFGHAVCGUKNULUMUNU
      OUPUQURVDDSZVIVKVJVLVDDCUSWAVGIHVNVDSZAUCWAVGISZUAZAQUDWBWDAWAWCAABBVDDVR
      VDUEAUFZKUGABBVGIABVDGQUHWEUIMUGUJWBWAVNDSZWCVNVDDUBWBWFWCWBWFVGGIAVDGUKO
      ULUMUNUOUPUTURAVCVDGVAVB $.
      $( [22-Sep-2009] $) $( [13-Sep-2009] $)
    $}

    ${
      subtr2.3 $e |- ( ps -> A. x ps ) $.
      subtr2.4 $e |- ( ch -> A. x ch ) $.
      subtr2.5 $e |- ( x = A -> ( ph <-> ps ) ) $.
      subtr2.6 $e |- ( x = B -> ( ph <-> ch ) ) $.
      $( Transitivity of implicit substitution into a wff. $)
      subtr2 $p |- ( ( A e. C /\ B e. D ) -> ( A = B -> ( ps <-> ch ) ) ) $=
