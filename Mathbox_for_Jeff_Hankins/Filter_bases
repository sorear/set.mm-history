$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                Filter bases
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c fBas $.
  $( Extend class definition to include the class of filter bases. $)
  cfbas $a class fBas $.

  $c filGen $.
  $( Extend class definition to include the filter generating function. $)
  cfg $a class filGen $.

  ${
    $d x y z $.
    $( Define the class of all filter bases. $)
    df-fbas $a |- fBas = { x | ( x =/= (/) /\ (/) e/ x /\ A. y e. x A. z e. x (
    x i^i P~ ( y i^i z ) ) =/= (/) ) } $.
  $}

  ${
    $d x y z $.
    $( Define the filter generating function. $)
    df-fg $a |- filGen = { <. x , y >. | ( x e. fBas /\ y = { z e. P~ U. x | (
    x i^i P~ z ) =/= (/) } ) } $.
  $}

  ${
    $d x y z F $.
    $( The predicate " ` F ` is a filter base." Note that some authors require
    filter bases to be closed under pairwise intersections, but that is not
    necessary under our definition. One advantage of this definition is that
    tails in a directed set form a filter base under our meaning. $)
    isfbas $p |- ( F e. A -> ( F e. fBas <-> ( F =/= (/) /\ (/) e/ F /\ A. x e.
    F A. y e. F ( F i^i P~ ( x i^i y ) ) =/= (/) ) ) ) $=
      ( vz cv c0 wne wnel cin cpw wral w3a cfbas wceq neeq1 neleq2 ineq1
      neeq1d raleqd 3anbi123d df-fbas elab2g ) EFZGHZGUDIZUDAFBFJKZJZGHZBUDLZAU
      DLZMDGHZGDIZDUGJZGHZBDLZADLZMEDNCUDDOZUEULUFUMUKUQUDDGPUDDGQUJUPAUDDUIUOB
      UDDURUHUNGUDDUGRSTTUAEABUBUC $.
      $( [1-Sep-2009] $) $( [1-Sep-2009] $)
  $}

  ${
    $d x y z F $.
    $( The predicate " ` F ` is a filter base." $)
    isfbas2 $p |- ( F e. A -> ( F e. fBas <-> ( F =/= (/) /\ (/) e/ F /\ A. x
    e. F A. y e. F E. z e. F z (_ ( x i^i y ) ) ) ) $=
      ( wcel cfbas c0 wne wnel cv cin cpw wral w3a wss wrex isfbas wex wa elin
      visset elpw anbi2i bitri exbii n0 df-rex 3bitr4i 2ralbii 3anbi3i syl6bb
      ) EDFEGFEHIZHEJZEAKBKLZMZLZHIZBENAENZOUMUNCKZUOPZCEQZBENAENZOABDERUSVCUMU
      NURVBABEEUTUQFZCSUTEFZVATZCSURVBVDVFCVDVEUTUPFZTVFUTEUPUAVGVAVEUTUOCUBUCU
      DUEUFCUQUGVACEUHUIUJUKUL $.
      $( [1-Sep-2009] $) $( [1-Sep-2009] $)
  $}

  ${
    $d x y F $.
    $( There are no empty filter bases. $)
    fbasne0 $p |- ( F e. fBas -> F =/= (/) ) $=
      ( vx vy cfbas wcel c0 wne wnel cv cin cpw wral w3a isfbas ibi 3simp1d )
      ADEZAFGZFAHZABICIJKJFGCALBALZQRSTMBCDANOP $.
      $( [1-Sep-2009] $) $( [1-Sep-2009] $)
  $}

  ${
    $d x y F $.
    $( No filter base contains the empty set. $)
    0nelfb $p |- ( F e. fBas -> (/) e/ F ) $=
      ( vx vy cfbas wcel c0 wne wnel cv cin cpw wral w3a isfbas ibi 3simp2d )
      ADEZAFGZFAHZABICIJKJFGCALBALZQRSTMBCDANOP $.
      $( [1-Sep-2009] $) $( [1-Sep-2009] $)
  $}

  ${
    $d x y z A $. $d x y z B $. $d x y z F $.
    $( A filter base contains subsets of its pairwise intersections. $)
    fbasssin $p |- ( ( F e. fBas /\ A e. F /\ B e. F ) -> E. x e. F x (_ ( A
    i^i B ) ) $=
      ( vy vz cfbas wcel cv cin wss wrex wa wral wceq ineq1 sseq2d rexbidv
      ineq2 rcla42v c0 wne wnel w3a isfbas2 ibi 3simp3d syl5com 3impib ) DGHZBD
      HZCDHZAIZBCJZKZADLZUKULMUMEIZFIZJZKZADLZFDNEDNZUPUJVAUPUMBURJZKZADLEFBCDD
      UQBOZUTVDADVEUSVCUMUQBURPQRURCOZVDUOADVFVCUNUMURCBSQRTUJDUAUBZUADUCZVBUJV
      GVHVBUDEFAGDUEUFUGUHUI $.
      $( [1-Sep-2009] $) $( [1-Sep-2009] $)
  $}

