  ${
    $d j n x y z F $.  $d k n w x y J $.  $d f j k n w x y z ph $.  $d w x R $.
    $d f j k n u w x y z A $.  $d f j u w x y z M $.  $d f j k n u w x y z Y $.
    $d f j k n w x y z D $.  $d w x y z K $.  $d f j u w x y z N $.  $d n T $.
    $d w x y z L $.  $d f k n w x y z S $.  $d w x y z U $.  $d w x y z W $.
    minvec.x $e |- X = ( BaseSet ` U ) $.
    minvec.m $e |- M = ( -v ` U ) $.
    minvec.n $e |- N = ( norm ` U ) $.
    minvec.y $e |- Y = ( BaseSet ` W ) $.
    minvec.u $e |- ( ph -> U e. CPreHil ) $.
    minvec.w $e |- ( ph -> W e. ( ( SubSp ` U ) i^i CBan ) ) $.
    minvec.a $e |- ( ph -> A e. X ) $.
