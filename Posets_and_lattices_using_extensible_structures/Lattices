$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                   Lattices
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Extend class notation with the class of all lattices. $)
  clat $a class Lat $.

  ${
    $d p s x y $.
    $( Define the class of all lattices.  A lattice is a poset in which the
       join and meet of any two elements always exists. $)
    df-lat $a |- Lat = { p e. Poset | A. x e. ( Base ` p )
       A. y e. ( Base ` p ) ( ( x ( join ` p ) y ) e. ( Base ` p )
          /\ ( x ( meet ` p ) y ) e. ( Base ` p ) ) } $.
  $}

  ${
    $d l x y B $.  $d l J $.  $d l x y K $.  $d l M $.
    islat.b $e |- B = ( Base ` K ) $.
    islat.j $e |- J = ( join ` K ) $.
    islat.m $e |- M = ( meet ` K ) $.
    $( The predicate "is a lattice." $)
    islat $p |- ( K e. Lat <-> ( K e. Poset /\
             A. x e. B A. y e. B ( ( x J y ) e. B /\ ( x M y ) e. B ) ) ) $=
      ( vl cv cjn cfv co cbs wcel cmee wa wral cpo clat wceq fveq2 syl6eqr
      raleqdv raleqbidv opreqd eleq12d anbi12d 2ralbidv bitrd df-lat elrab2 )
      AKZBKZJKZLMZNZUPOMZPZUNUOUPQMZNZUSPZRZBUSSZAUSSZUNUODNZCPZUNUOFNZCPZRZBCS
      ACSZJETUAUPEUBZVFVDBCSZACSVLVMVEVNAUSCVMUSEOMCUPEOUCGUDZVMVDBUSCVOUEUFVMV
      DVKABCCVMUTVHVCVJVMURVGUSCVMUQDUNUOVMUQELMDUPELUCHUDUGVOUHVMVBVIUSCVMVAFU
      NUOVMVAEQMFUPEQUCIUDUGVOUHUIUJUKABJULUM $.
      $( [18-Oct-2012] $)
  $}

  ${
    $d x y B $.  $d x y J $.  $d x y K $.  $d x y M $.  $d x y X $.  $d y Y $.
    latlem.b $e |- B = ( Base ` K ) $.
    latlem.j $e |- J = ( join ` K ) $.
    latlem.m $e |- M = ( meet ` K ) $.
    $( Lemma for lattice properties. $)
    latlem $p |- ( ( K e. Lat /\ X e. B /\ Y e. B ) ->
           ( ( X J Y ) e. B /\ ( X M Y ) e. B ) ) $=
      ( vx vy clat wcel co wa cv wral cpo islat simprbi wceq opreq1 eleq1d
      anbi12d opreq2 rcla42v syl5com 3impib ) CLMZEAMZFAMZEFBNZAMZEFDNZAMZOZUIJ
      PZKPZBNZAMZUQURDNZAMZOZKAQJAQZUJUKOUPUICRMVDJKABCDGHISTVCUPEURBNZAMZEURDN
      ZAMZOJKEFAAUQEUAZUTVFVBVHVIUSVEAUQEURBUBUCVIVAVGAUQEURDUBUCUDURFUAZVFUMVH
      UOVJVEULAURFEBUEUCVJVGUNAURFEDUEUCUDUFUGUH $.
      $( [14-Sep-2011] $)
  $}

  ${
    $d x y K $.
    $( A lattice is a poset. $)
    latpos $p |- ( K e. Lat -> K e. Poset ) $=
      ( vx vy clat wcel cpo cv cjn cfv co cbs cmee wa wral eqid islat simplbi
      ) ADEAFEBGZCGZAHIZJAKIZERSALIZJUAEMCUANBUANBCUATAUBUAOTOUBOPQ $.
      $( [17-Sep-2011] $)
  $}

  ${
    latjcl.b $e |- B = ( Base ` K ) $.
    latjcl.j $e |- J = ( join ` K ) $.
    $( Closure of join operation in a lattice.  (Th. ~ chjcom analog.) $)
    latjcl $p |- ( ( K e. Lat /\ X e. B /\ Y e. B ) -> ( X J Y ) e. B ) $=
      ( clat wcel w3a co cmee cfv eqid latlem simpld ) CHIDAIEAIJDEBKAIDECLMZKA
      IABCQDEFGQNOP $.
      $( [14-Sep-2011] $)
  $}

  ${
    latmcl.b $e |- B = ( Base ` K ) $.
    latmcl.m $e |- M = ( meet ` K ) $.
    $( Closure of meet operation in a lattice.  (Th. ~ incom analog.) $)
    latmcl $p |- ( ( K e. Lat /\ X e. B /\ Y e. B ) -> ( X M Y ) e. B ) $=
      ( clat wcel w3a cjn cfv co eqid latlem simprd ) BHIDAIEAIJDEBKLZMAIDECMAI
      AQBCDEFQNGOP $.
      $( [14-Sep-2011] $)
  $}

  ${
    $d x y B $.  $d x y K $.
    islati.1 $e |- K e. Poset $.
    islati.b $e |- B = ( Base ` K ) $.
    islati.j $e |- J = ( join ` K ) $.
    islati.m $e |- M = ( meet ` K ) $.
    islati.5 $e |- ( ( x e. B /\ y e. B ) -> ( x J y ) e. B ) $.
    islati.6 $e |- ( ( x e. B /\ y e. B ) -> ( x M y ) e. B ) $.
    $( Properties that determine a lattice. $)
    islati $p |- K e. Lat $=
      ( clat wcel cpo cv co wa wral islat jca rgen2a mpbir2an ) EMNEONAPZBPZDQC
      NZUDUEFQCNZRZBCSACSABCDEFHIJTGUHABCUDCNUECNRUFUGKLUAUBUC $.
      $( [12-Sep-2011] $)
  $}

  ${
    latref.b $e |- B = ( Base ` K ) $.
    latref.l $e |- L = ( le ` K ) $.
    $( A lattice ordering is reflexive.  (Th. ~ ssid analog.) $)
    latref $p |- ( ( K e. Lat /\ X e. B ) -> X L X ) $=
      ( clat wcel cpo wbr latpos posref sylan ) BGHBIHDAHDDCJBKABCDEFLM $.
      $( [8-Oct-2011] $)

    $( A lattice ordering is asymetric.  (Th. ~ eqss analog.) $)
    latasymb $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
                -> ( ( X L Y /\ Y L X ) <-> X = Y ) ) $=
      ( clat wcel cpo wbr wa wceq wb latpos posasymb syl3an1 ) BHIBJIDAIEAIDECK
      EDCKLDEMNBOABCDEFGPQ $.
      $( [22-Oct-2011] $)

    $( A lattice ordering is asymetric.  (Th. ~ eqss analog.) $)
    latasym $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
                -> ( ( X L Y /\ Y L X ) -> X = Y ) ) $=
      ( clat wcel w3a wbr wa wceq latasymb biimpd ) BHIDAIEAIJDECKEDCKLDEMABCDE
      FGNO $.
      $( [8-Oct-2011] $)

    $( A lattice ordering is transitive.  (Th. ~ sstr analog.) $)
    lattr $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                -> ( ( X L Y /\ Y L Z ) -> X L Z ) ) $=
      ( clat wcel cpo w3a wbr wa wi latpos postr sylan ) BIJBKJDAJEAJFAJLDECMEF
      CMNDFCMOBPABCDEFGHQR $.
      $( [17-Nov-2011] $)
  $}

  ${
    latasymd.b $e |- B = ( Base ` K ) $.
    latasymd.l $e |- L = ( le ` K ) $.
    latasymd.3 $e |- ( ph -> K e. Lat ) $.
    latasymd.4 $e |- ( ph -> X e. B ) $.
    latasymd.5 $e |- ( ph -> Y e. B ) $.
    latasymd.6 $e |- ( ph -> X L Y ) $.
    latasymd.7 $e |- ( ph -> Y L X ) $.
    $( Deduce equality from lattice ordering.  (Th. ~ eqssd analog.) $)
    latasymd $p |- ( ph -> X = Y ) $=
      ( wbr wceq clat wcel wa wb latasymb syl111anc mpbi2and ) AEFDNZFEDNZEFOZA
      CPQEBQFBQUCUDRUESIJKBCDEFGHTUALMUB $.
      $( [18-Nov-2011] $)
  $}

  ${
    lattrd.b $e |- B = ( Base ` K ) $.
    lattrd.l $e |- L = ( le ` K ) $.
    lattrd.1 $e |- ( ph -> K e. Lat ) $.
    lattrd.2 $e |- ( ph -> X e. B ) $.
    lattrd.3 $e |- ( ph -> Y e. B ) $.
    lattrd.4 $e |- ( ph -> Z e. B ) $.
    lattrd.5 $e |- ( ph -> X L Y ) $.
    lattrd.6 $e |- ( ph -> Y L Z ) $.
    $( A lattice ordering is transitive.  Deduction version of ~ lattr . $)
    lattrd $p |- ( ph -> X L Z ) $=
      ( wbr clat wcel wa wi lattr syl13anc mp2and ) AEFDPZFGDPZEGDPZNOACQREBRFB
      RGBRUDUESUFTJKLMBCDEFGHIUAUBUC $.
      $( [3-Sep-2012] $)
  $}

  ${
    latjcom.b $e |- B = ( Base ` K ) $.
    latjcom.j $e |- J = ( join ` K ) $.
    $( The join of a lattice commutes.  (Th. ~ chjcom analog.) $)
    latjcom $p |- ( ( K e. Lat /\ X e. B /\ Y e. B ) -> ( X J Y ) =
              ( Y J X ) ) $=
      ( clat wcel w3a co wa wceq latjcl 3com23 jca cpo latpos joincom syl3anl1
      mpdan ) CHIZDAIZEAIZJZDEBKZAIZEDBKZAIZLZUFUHMZUEUGUIABCDEFGNUBUDUCUIABCED
      FGNOPUBCQIUCUDUJUKCRABCDEFGSTUA $.
      $( [16-Sep-2011] $)
  $}

  ${
    latlej.b $e |- B = ( Base ` K ) $.
    latlej.l $e |- L = ( le ` K ) $.
    latlej.j $e |- J = ( join ` K ) $.
    $( A join's first argument is less than or equal to the join.  (Th. ~ chub1
       analog.) $)
    latlej1 $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
            -> X L ( X J Y ) ) $=
      ( clat wcel w3a co wbr latjcl lejoin1 mpdan ) CJKEAKFAKLEFBMZAKERDNABCEFG
      IOJABCDEFGHIPQ $.
      $( [17-Sep-2011] $)

    $( A join's second argument is less than or equal to the join.  (Th.
       ~ chub2 analog.) $)
    latlej2 $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
            -> Y L ( X J Y ) ) $=
      ( clat wcel w3a co wbr latjcl lejoin2 mpdan ) CJKEAKFAKLEFBMZAKFRDNABCEFG
      IOJABCDEFGHIPQ $.
      $( [17-Sep-2011] $)

    $( A join is less than or equal to a third value iff each argument is less
       than or equal to the third value.  (Th. ~ chlub analog.) $)
    latjle12 $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                      -> ( ( X L Z /\ Y L Z ) <-> ( X J Y ) L Z ) ) $=
      ( clat wcel w3a wa co wbr wb latjcl 3adant3r3 cpo latpos joinle syl3an1
      3expa mpdan ) CKLZEALZFALZGALZMZNEFBOZALZEGDPFGDPNUKGDPQZUFUGUHULUIABCEFH
      JRSUFUJULUMUFCTLUJULUMCUAABCDEFGHIJUBUCUDUE $.
      $( [17-Sep-2011] $)

    $( Less-than-or-equal-to in terms of join.  (Th. ~ chlejb1 analog.) $)
    latleeqj1 $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
            -> ( X L Y <-> ( X J Y ) = Y ) ) $=
      ( clat wcel w3a wbr co wa wceq latref 3adant2 biantrud wb simp1 simp2
      simp3 latjle12 syl13anc bitrd latlej2 cpo latpos 3ad2ant1 latjcl
      posasymb syl111anc 3bitrd ) CJKZEAKZFAKZLZEFDMZEFBNZFDMZVAFUTDMZOZUTFPZUR
      USUSFFDMZOZVAURVEUSUOUQVEUPACDFGHQRSURUOUPUQUQVFVATUOUPUQUAUOUPUQUBUOUPUQ
      UCZVGABCDEFFGHIUDUEUFURVBVAABCDEFGHIUGSURCUHKZUTAKUQVCVDTUOUPVHUQCUIUJABC
      EFGIUKVGACDUTFGHULUMUN $.
      $( [21-Oct-2011] $)

    $( Less-than-or-equal-to in terms of join.  (Th. ~ chlejb2 analog.) $)
    latleeqj2 $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
            -> ( X L Y <-> ( Y J X ) = Y ) ) $=
      ( clat wcel w3a wbr co wceq latleeqj1 latjcom eqeq1d bitrd ) CJKEAKFAKLZE
      FDMEFBNZFOFEBNZFOABCDEFGHIPTUAUBFABCEFGIQRS $.
      $( [14-Nov-2011] $)

    $( Add join to both sides of a lattice ordering.  (Th. ~ chlej1i
       analog.) $)
    latjlej1 $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B ) )
       -> ( X L Y -> ( X J Z ) L ( Y J Z ) ) ) $=
      ( clat wcel w3a wa wbr co latlej1 3adant3r1 wi simpl simpr1 simpr2
      latjcl lattr syl13anc mpan2d latlej2 jctird wb simpr3 3jca latjle12
      syldan sylibd ) CKLZEALZFALZGALZMZNZEFDOZEFGBPZDOZGVBDOZNZEGBPVBDOZUTVAVC
      VDUTVAFVBDOZVCUOUQURVGUPABCDFGHIJQRUTUOUPUQVBALZVAVGNVCSUOUSTUOUPUQURUAZU
      OUPUQURUBUOUQURVHUPABCFGHJUCRZACDEFVBHIUDUEUFUOUQURVDUPABCDFGHIJUGRUHUOUS
      UPURVHMVEVFUIUTUPURVHVIUOUPUQURUJVJUKABCDEGVBHIJULUMUN $.
      $( [8-Nov-2011] $)

    $( Add join to both sides of a lattice ordering.  (Th. ~ chlej2i
       analog.) $)
    latjlej2 $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B ) )
       -> ( X L Y -> ( Z J X ) L ( Z J Y ) ) ) $=
      ( clat wcel w3a wa wbr co latjlej1 wceq latjcom 3adant3r2 3adant3r1
      breq12d sylibd ) CKLZEALZFALZGALZMNZEFDOEGBPZFGBPZDOGEBPZGFBPZDOABCDEFGHI
      JQUHUIUKUJULDUDUEUGUIUKRUFABCEGHJSTUDUFUGUJULRUEABCFGHJSUAUBUC $.
      $( [8-Nov-2011] $)

    $( Add join to both sides of a lattice ordering.  (Th. ~ chlej12i
       analog.) $)
    latjlej12 $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B )
          /\ ( Z e. B /\ W e. B ) ) -> ( ( X L Y /\ Z L W )
             -> ( X J Z ) L ( Y J W ) ) ) $=
      ( clat wcel wa w3a wbr co wi simp1 simp2l simp2r simp3l latjlej1
      syl13anc simp3r latjlej2 latjcl syl111anc lattr syl2and ) CLMZFAMZGAMZNZH
      AMZEAMZNZOZFGDPZFHBQZGHBQZDPZHEDPZVAGEBQZDPZUTVDDPZURUKULUMUOUSVBRUKUNUQS
      ZUKULUMUQTZUKULUMUQUAZUKUNUOUPUBZABCDFGHIJKUCUDURUKUOUPUMVCVERVGVJUKUNUOU
      PUEZVIABCDHEGIJKUFUDURUKUTAMZVAAMZVDAMZVBVENVFRVGURUKULUOVLVGVHVJABCFHIKU
      GUHURUKUMUOVMVGVIVJABCGHIKUGUHURUKUMUPVNVGVIVKABCGEIKUGUHACDUTVAVDIJUIUDU
      J $.
      $( [8-Nov-2011] $)

    $( An idiom to express that a lattice element differs from two others. $)
    latnlej $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B )
         /\ -. X L ( Y J Z ) ) -> ( X =/= Y /\ X =/= Z ) ) $=
      ( clat wcel w3a co wbr wn wne wa wceq latlej1 3adant3r1 breq1 syl5ibrcom
      necon3bd latlej2 jcad 3impia ) CKLZEALZFALZGALZMZEFGBNZDOZPZEFQZEGQZRUHUL
      RZUOUPUQURUNEFURUNEFSFUMDOZUHUJUKUSUIABCDFGHIJTUAEFUMDUBUCUDURUNEGURUNEGS
      GUMDOZUHUJUKUTUIABCDFGHIJUEUAEGUMDUBUCUDUFUG $.
      $( [28-May-2012] $)

    $( An idiom to express that a lattice element differs from two others. $)
    latnlej1l $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B )
         /\ -. X L ( Y J Z ) ) -> X =/= Y ) $=
      ( clat wcel w3a co wbr wn wne latnlej simpld ) CKLEALFALGALMEFGBNDOPMEFQE
      GQABCDEFGHIJRS $.
      $( [19-Jul-2012] $)

    $( An idiom to express that a lattice element differs from two others. $)
    latnlej1r $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B )
         /\ -. X L ( Y J Z ) ) -> X =/= Z ) $=
      ( clat wcel w3a co wbr wn wne latnlej simprd ) CKLEALFALGALMEFGBNDOPMEFQE
      GQABCDEFGHIJRS $.
      $( [19-Jul-2012] $)

    $( An idiom to express that a lattice element differs from two others. $)
    latnlej2 $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B )
         /\ -. X L ( Y J Z ) ) -> ( -. X L Y /\ -. X L Z ) ) $=
      ( clat wcel w3a co wbr wn wa latlej1 3adant3r1 wi simpl simpr1 simpr2
      latjcl lattr syl13anc mpan2d con3d latlej2 simpr3 jcad 3impia ) CKLZEALZF
      ALZGALZMZEFGBNZDOZPZEFDOZPZEGDOZPZQUMUQQZUTVBVDVEVAUSVEVAFURDOZUSUMUOUPVF
      UNABCDFGHIJRSVEUMUNUOURALZVAVFQUSTUMUQUAZUMUNUOUPUBZUMUNUOUPUCUMUOUPVGUNA
      BCFGHJUDSZACDEFURHIUEUFUGUHVEVCUSVEVCGURDOZUSUMUOUPVKUNABCDFGHIJUISVEUMUN
      UPVGVCVKQUSTVHVIUMUNUOUPUJVJACDEGURHIUEUFUGUHUKUL $.
      $( [10-Jul-2012] $)

    $( An idiom to express that a lattice element differs from two others. $)
    latnlej2l $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B )
         /\ -. X L ( Y J Z ) ) -> -. X L Y ) $=
      ( clat wcel w3a co wbr wn latnlej2 simpld ) CKLEALFALGALMEFGBNDOPMEFDOPEG
      DOPABCDEFGHIJQR $.
      $( [19-Jul-2012] $)

    $( An idiom to express that a lattice element differs from two others. $)
    latnlej2r $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B )
         /\ -. X L ( Y J Z ) ) -> -. X L Z ) $=
      ( clat wcel w3a co wbr wn latnlej2 simprd ) CKLEALFALGALMEFGBNDOPMEFDOPEG
      DOPABCDEFGHIJQR $.
      $( [19-Jul-2012] $)
  $}

  ${
    latidm.b $e |- B = ( Base ` K ) $.
    latidm.j $e |- J = ( join ` K ) $.
    $( Lattice join is idempotent.  (Th. ~ chjidm analog.) $)
    latjidm $p |- ( ( K e. Lat /\ X e. B ) -> ( X J X ) = X ) $=
      ( clat wcel wa cple cfv co eqid simpl latjcl 3anidm23 simpr wbr wb
      latjle12 syl13anc latref mpbi2and latlej1 latasymd ) CGHZDAHZIZACCJKZDDBL
      ZDEUIMZUFUGNZUFUGUJAHABCDDEFOPUFUGQZUHDDUIRZUNUJDUIRZUHUFUGUGUGUNUNIUOSUL
      UMUMUMABCUIDDDEUKFTUAACUIDEUKUBZUPUCUFUGDUJUIRABCUIDDEUKFUDPUE $.
      $( [8-Oct-2011] $)
  $}

  ${
    latmcom.b $e |- B = ( Base ` K ) $.
    latmcom.m $e |- M = ( meet ` K ) $.
    $( The join of a lattice commutes.  (Th. ~ incom analog.) $)
    latmcom $p |- ( ( K e. Lat /\ X e. B /\ Y e. B ) -> ( X M Y ) =
              ( Y M X ) ) $=
      ( clat wcel w3a co wa wceq latmcl 3com23 jca cpo latpos meetcom syl3anl1
      mpdan ) BHIZDAIZEAIZJZDECKZAIZEDCKZAIZLZUFUHMZUEUGUIABCDEFGNUBUDUCUIABCED
      FGNOPUBBQIUCUDUJUKBRABCDEFGSTUA $.
      $( [6-Nov-2011] $)
  $}

  ${
    latmle.b $e |- B = ( Base ` K ) $.
    latmle.l $e |- L = ( le ` K ) $.
    latmle.m $e |- M = ( meet ` K ) $.
    $( A meet is less than or equal to its first argument.  (Th. ~ inss1
       analog.) $)
    latmle1 $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
            -> ( X M Y ) L X ) $=
      ( clat wcel w3a co wbr latmcl lemeet1 mpdan ) BJKEAKFAKLEFDMZAKRECNABDEFG
      IOJABCDEFGHIPQ $.
      $( [21-Oct-2011] $)

    $( A meet is less than or equal to its second argument.  (Th. ~ inss2
       analog.) $)
    latmle2 $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
            -> ( X M Y ) L Y ) $=
      ( clat wcel w3a co wbr latmcl lemeet2 mpdan ) BJKEAKFAKLEFDMZAKRFCNABDEFG
      IOJABCDEFGHIPQ $.
      $( [21-Oct-2011] $)

    $( An element is less than or equal to a meet iff the element is less than
       or equal to each argument of the meet.  (Th. ~ ssin analog.) $)
    latlem12 $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B ) )
                      -> ( ( X L Y /\ X L Z ) <-> X L ( Y M Z ) ) ) $=
      ( wcel w3a clat wbr wa co wb 3anrot latmcl 3adant3r3 cpo latpos meetle
      bicomd syl3an1 mpd3an3 sylan2b ) EAKZFAKZGAKZLBMKZUIUJUHLZEFCNEGCNOZEFGDP
      ZCNZQZUHUIUJRUKULUNAKZUPUKUIUJUQUHABDFGHJSTUKBUAKZULUQUPBUBURULUQLUOUMABC
      DFGEHIJUCUDUEUFUG $.
      $( [21-Oct-2011] $)

    $( Less-than-or-equal-to in terms of meet.  (Th. ~ df-ss analog.) $)
    latleeqm1 $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
            -> ( X L Y <-> ( X M Y ) = X ) ) $=
      ( clat wcel w3a wbr co wa wceq latref 3adant3 biantrurd wb simp1 simp2
      simp3 latlem12 syl13anc bitrd latmle1 cpo latpos 3ad2ant1 latmcl
      posasymb syl111anc 3bitrd ) BJKZEAKZFAKZLZEFCMZEEFDNZCMZUTECMZVAOZUTEPZUR
      USEECMZUSOZVAURVEUSUOUPVEUQABCEGHQRSURUOUPUPUQVFVATUOUPUQUAUOUPUQUBZVGUOU
      PUQUCABCDEEFGHIUDUEUFURVBVAABCDEFGHIUGSURBUHKZUTAKUPVCVDTUOUPVHUQBUIUJABD
      EFGIUKVGABCUTEGHULUMUN $.
      $( [7-Nov-2011] $)

    $( Less-than-or-equal-to in terms of meet.  (Th. ~ sseqin2 analog.) $)
    latleeqm2 $p |- ( ( K e. Lat /\ X e. B /\ Y e. B )
            -> ( X L Y <-> ( Y M X ) = X ) ) $=
      ( clat wcel w3a wbr co wceq latleeqm1 latmcom eqeq1d bitrd ) BJKEAKFAKLZE
      FCMEFDNZEOFEDNZEOABCDEFGHIPTUAUBEABDEFGIQRS $.
      $( [7-Nov-2011] $)

    $( Add meet to both sides of a lattice ordering.  (Th. ~ ssrin analog.) $)
    latmlem1 $p |- ( ( K e. Lat /\ ( X e. B /\ Y e. B /\ Z e. B ) )
       -> ( X L Y -> ( X M Z ) L ( Y M Z ) ) ) $=
      ( clat wcel w3a wa wbr co latmle1 3adant3r2 wi simpl latmcl simpr1
      simpr2 lattr syl13anc mpand latmle2 jctird wb simpr3 3jca latlem12
      syldan sylibd ) BKLZEALZFALZGALZMZNZEFCOZEGDPZFCOZVBGCOZNZVBFGDPCOZUTVAVC
