  $}

  $c CLat $.

  $( Extend class notation with complete lattices. $)
  ccla $a class CLat $.

  ${
    $d p s x y $.
    $( Define the class of all complete lattices. $)
    df-clat $a |- CLat = { p e. Poset | A. s ( s C_ ( Base ` p )
      -> ( ( ( lub ` p ) ` s ) e. ( Base ` p ) /\
           ( ( glb ` p ) ` s ) e. ( Base ` p ) ) ) } $.
  $}

  ${
    $d l B $.  $d l G $.  $d l s K $.  $d l U $.
    isclat.b $e |- B = ( Base ` K ) $.
    isclat.u $e |- U = ( lub ` K ) $.
    isclat.g $e |- G = ( glb ` K ) $.
    $( The predicate "is an complete lattice." $)
    isclat $p |- ( K e. CLat <-> ( K e. Poset /\
             A. s ( s C_ B -> ( ( U ` s ) e. B /\ ( G ` s ) e. B ) ) ) ) $=
      ( vl cv cbs cfv wss club wcel cglb wa wi wal cpo ccla wceq fveq2 syl6eqr
      sseq2d fveq1d eleq12d anbi12d imbi12d albidv df-clat elrab2 ) EJZIJZKLZMZ
      UMUNNLZLZUOOZUMUNPLZLZUOOZQZRZESUMAMZUMBLZAOZUMCLZAOZQZRZESIDTUAUNDUBZVDV
