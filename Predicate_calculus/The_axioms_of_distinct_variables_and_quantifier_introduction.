  ${
    $d x ps $.  $d y ps $.  $d z ps $.
    $( Theorem 19.41 of [Margaris] p. 90 with 3 quantifiers. $)
    19.41vvv $p |- ( E. x E. y E. z ( ph /\ ps ) <->
                     ( E. x E. y E. z ph /\ ps ) ) $=
      ( wa wex 19.41vv biex 19.41v bitr ) ABFEGDGZCGAEGDGZBFZCGMCGBFLNCABDEHIMB
      CJK $.
      $( [30-Apr-95] $)
  $}

  ${
    $d x ph $.
    $( Special case of Theorem 19.42 of [Margaris] p. 90. $)
    19.42v $p |- ( E. x ( ph /\ ps ) <-> ( ph /\ E. x ps ) ) $=
      ( ax-17 19.42 ) ABCACDE $.
      $( [5-Aug-93] $)
  $}

  ${
    $d y ph $.
    $( Distribution of existential quantifiers. $)
    exdistr $p |- ( E. x E. y ( ph /\ ps ) <-> E. x ( ph /\ E. y ps ) ) $=
      ( wa wex 19.42v biex ) ABEDFABDFECABDGH $.
      $( [9-Mar-95] $)
  $}

  ${
    $d x ph $.  $d y ph $.
    $( Theorem 19.42 of [Margaris] p. 90 with 2 quantifiers. $)
    19.42vv $p |- ( E. x E. y ( ph /\ ps ) <-> ( ph /\ E. x E. y ps ) ) $=
      ( wa wex exdistr 19.42v bitr ) ABEDFCFABDFZECFAJCFEABCDGAJCHI $.
      $( [16-Mar-95] $)
  $}

  ${
    $d y ph $.  $d z ph $.
    $( Distribution of existential quantifiers. $)
    exdistr2 $p |- ( E. x E. y E. z ( ph /\ ps ) <->
                   E. x ( ph /\ E. y E. z ps ) ) $=
      ( wa wex 19.42vv biex ) ABFEGDGABEGDGFCABDEHI $.
      $( [17-Mar-95] $)
  $}

  ${
    $d y ph $.  $d z ph $.  $d z ps $.
    $( Distribution of existential quantifiers. $)
    3exdistr $p |- ( E. x E. y E. z ( ph /\ ps /\ ch ) <->
                E. x ( ph /\ E. y ( ps /\ E. z ch ) ) ) $=
      ( w3a wex wa 3anass biex 19.42v bilan 3bitr bitr ) ABCGZFHZEHZABCFHIZEHIZ
      DRASIZEHTQUAEQABCIZIZFHAUBFHZIUAPUCFABCJKAUBFLUDSABCFLMNKASELOK $.
      $( [9-Mar-95] $)
  $}

  ${
    $d y ph $.  $d z ph $.  $d w ph $.  $d z ps $.  $d w ps $.  $d w ch $.
    $( Distribution of existential quantifiers. $)
    4exdistr $p |- ( E. x E. y E. z E. w ( ( ph /\ ps ) /\ ( ch /\ th ) ) <->
                E. x ( ph /\ E. y ( ps /\ E. z ( ch /\ E. w th ) ) ) ) $=
      ( wa wex anass biex 19.42v bilan 3bitr bitr ) ABICDIZIZHJZGJZFJZABCDHJIZG
      JIZFJIZEUAAUCIZFJUDTUEFTABUBIZIZGJAUFGJZIUESUGGSABQIZIZHJZUGRUJHABQKLUKAU
      IHJZIABQHJZIZIUGAUIHMULUNABQHMNUNUFAUMUBBCDHMNNOPLAUFGMUHUCABUBGMNOLAUCFM
      PL $.
      $( [9-Mar-95] $)
  $}

  ${
    $d y ph $. $d x ps $.
    cbvalv.1 $e |- ( x = y -> ( ph <-> ps ) ) $.
    $( Rule used to change bound variables with implicit substitution. $)
    cbvalv $p |- ( A. x ph <-> A. y ps ) $=
      ( ax-17 cbval ) ABCDADFBCFEG $.
      $( [5-Aug-93] $)

    $( Rule used to change bound variables with implicit substitution. $)
    cbvexv $p |- ( E. x ph <-> E. y ps ) $=
      ( ax-17 cbvex ) ABCDADFBCFEG $.
      $( [5-Aug-93] $)
  $}

  ${
    $d y x $. $d y z $. $d w x $. $d w z $.
    cbval2.1 $e |- ( ph -> A. z ph ) $.
    cbval2.2 $e |- ( ph -> A. w ph ) $.
    cbval2.3 $e |- ( ps -> A. x ps ) $.
    cbval2.4 $e |- ( ps -> A. y ps ) $.
    cbval2.5 $e |- ( ( x = z /\ y = w ) -> ( ph <-> ps ) ) $.
    $( Rule used to change bound variables with implicit substitution. $)
    cbval2 $p |- ( A. x A. y ph <-> A. z A. w ps ) $=
      ( wal hbal weq wb wi wa ax-17 hban exp com12 pm5.32d cbval 19.28 3bitr3
      pm5.32 mpbir ) ADLZBFLZCEAEDGMBCFIMCENZUHUIOPUJUHQZUJUIQZOUJAQZDLUJBQZFLU
      KULUMUNDFUJAFUJFRZHSUJBDUJDRZJSDFNZUJABUJUQABOZUJUQURKTUAUBUCUJADUPUDUJBF
      UOUDUEUJUHUIUFUGUC $.
      $( [22-Dec-03] $) $( [22-Dec-03] $)

    $( Rule used to change bound variables with implicit substitution. $)
    cbvex2 $p |- ( E. x E. y ph <-> E. z E. w ps ) $=
      ( wex hbex weq wb wi wa ax-17 hban exp com12 pm5.32d cbvex 19.42 3bitr3
      pm5.32 mpbir ) ADLZBFLZCEAEDGMBCFIMCENZUHUIOPUJUHQZUJUIQZOUJAQZDLUJBQZFLU
      KULUMUNDFUJAFUJFRZHSUJBDUJDRZJSDFNZUJABUJUQABOZUJUQURKTUAUBUCUJADUPUDUJBF
      UOUDUEUJUHUIUFUGUC $.
      $( [14-Sep-03] $)
  $}

  ${
    $d z w ph $.  $d x y ps $.  $d x w $.  $d z y $.
    cbvex2v.1 $e |- ( ( x = z /\ y = w ) -> ( ph <-> ps ) ) $.
    $( Rule used to change bound variables with implicit substitution. $)
    cbvex2v $p |- ( E. x E. y ph <-> E. z E. w ps ) $=
      ( ax-17 cbvex2 ) ABCDEFAEHAFHBCHBDHGI $.
      $( [26-Jul-95] $)
  $}

  ${
    $d x ph $.  $d x ch $.
    cbvald.1 $e |- ( ph -> A. y ph ) $.
    cbvald.2 $e |- ( ph -> ( ps -> A. y ps ) ) $.
    cbvald.3 $e |- ( ph -> ( x = y -> ( ps <-> ch ) ) ) $.
    $( Deduction used to change bound variables with implicit substitution,
       particularly useful in conjuction with ~ ddelim . $)
    cbvald $p |- ( ph -> ( A. x ps <-> A. y ch ) ) $=
      ( wal wi hbim1 ax-17 weq wb com12 pm5.74d cbval 19.21v 19.21 3bitr3
      pm5.74ri ) ABDIZCEIZABJZDIACJZEIAUBJAUCJUDUEDEABEFGKUEDLDEMZABCAUFBCNHOPQ
      ABDRACEFSTUA $.
      $( [2-Jan-02] $)

    $( Deduction used to change bound variables with implicit substitution,
       particularly useful in conjuction with ~ ddelim . $)
    cbvexd $p |- ( ph -> ( E. x ps <-> E. y ch ) ) $=
      ( wn wal wex hbnd weq wb pm4.11 syl6ib cbvald binegd df-ex 3bitr4g ) ABIZ
      DJZICIZEJZIBDKCEKAUBUDAUAUCDEFABEFGLADEMBCNUAUCNHBCOPQRBDSCEST $.
      $( [2-Jan-02] $)
  $}

  ${
    $v u $.
    $v f $.
    $v g $.
    $( Define temporary individual variables. $)
    cbvex4v.vu $f set u $.
    cbvex4v.vf $f set f $.
    cbvex4v.vg $f set g $.
    $d v u ph $.  $d f g ph $.  $d x y ch $.  $d z w ch $.
    $d x y ps $.  $d f g ps $.
    $d x z w u $.  $d y z w v $.  $d z g $.  $d w f $.
    cbvex4v.1 $e |- ( ( x = v /\ y = u ) -> ( ph <-> ps ) ) $.
    cbvex4v.2 $e |- ( ( z = f /\ w = g ) -> ( ps <-> ch ) ) $.
    $( Rule used to change bound variables with implicit substitution. $)
    cbvex4v $p |- ( E. x E. y E. z E. w ph <-> E. v E. u E. f E. g ch ) $=
      ( wex weq wa bi2exdv cbvex2v bi2ex bitr ) AGNFNZENDNBGNFNZINHNCKNJNZINHNU
      AUBDEHIDHOEIOPABFGLQRUBUCHIBCFGJKMRST $.
      $( [26-Jul-95] $)
  $}

  ${
    $d y ph $.  $d x ps $.
    $( Rearrange existential quantifiers. $)
    eeanv $p |- ( E. x E. y ( ph /\ ps ) <-> ( E. x ph /\ E. y ps ) ) $=
      ( wa wex exdistr ax-17 hbex 19.41 bitr ) ABEDFCFABDFZECFACFLEABCDGALCBCDB
      CHIJK $.
      $( [26-Jul-95] $)
  $}

  ${
    $d y z ph $.  $d x z ps $.  $d x y ch $.
    $( Rearrange existential quantifiers. $)
    eeeanv $p |- ( E. x E. y E. z ( ph /\ ps /\ ch ) <->
                 ( E. x ph /\ E. y ps /\ E. z ch ) ) $=
      ( wa wex w3a 19.42vv eeanv bilan bitr biex 19.41v 3anass bi3ex 3bitr4 )
      ABCGZGZFHEHZDHZADHZBEHZCFHZGZGZABCIZFHEHDHUCUDUEIUBAUFGZDHUGUAUIDUAASFHEH
      ZGUIASEFJUJUFABCEFKLMNAUFDOMUHTDEFABCPQUCUDUEPR $.
      $( [26-Jul-95] $)
  $}

  ${
    $d z ph $.  $d w ph $.  $d x ps $.  $d y ps $.  $d y z $.  $d w x $.
    $( Rearrange existential quantifiers. $)
    ee4anv $p |- ( E. x E. y E. z E. w ( ph /\ ps ) <->
                  ( E. x E. y ph /\ E. z E. w ps ) ) $=
      ( wa wex excom biex eeanv bi2ex 3bitr ) ABGFHZEHDHZCHNDHZEHZCHADHZBFHZGZE
      HCHRCHSEHGOQCNDEIJPTCEABDFKLRSCEKM $.
      $( [31-Jul-95] $)
  $}

  ${
    $d x ph $.
    nexdv.1 $e |- ( ph -> -. ps ) $.
    $( Deduction for generalization rule for negated wff. $)
    nexdv $p |- ( ph -> -. E. x ps ) $=
      ( ax-17 nexd ) ABCACEDF $.
      $( [5-Aug-93] $)
  $}

  ${
    $d x ps $.
    chv.1 $e |- ( x = y -> ( ph <-> ps ) ) $.
    chv.2 $e |- ph $.
    $( Implicit substitution of ` y ` for ` x ` into a theorem. $)
    chv $p |- ps $=
      ( a4b1 mpg ) ABCABCDEGFH $.
      $( [20-Apr-94] $)
  $}

  ${
    $d x z $.  $d y z $.
    $( When the class variables in definition ~ df-clel are replaced with set
       variables, this theorem of predicate calculus is the result.  This
       theorem provides part of the justification for the consistency of that
       definition, which "overloads" the set variables in ~ wel with the class
       variables in ~ wcel . $)
    cleljust $p |- ( x e. y <-> E. z ( z = x /\ z e. y ) ) $=
      ( weq wel wa wex ax-17 a13b eqsex bicom ) CADCBEZFCGABEZLMCAMCHCABIJK $.
      $( [28-Jan-04] $) $( [28-Jan-04] $)
  $}


