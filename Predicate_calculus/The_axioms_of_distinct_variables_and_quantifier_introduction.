  ${
    $d x y $.  $d x ps $.  $d y ph $.
    $( Special case of ~ 19.12 where its converse holds. $)
    19.12vv $p |- ( E. x A. y ( ph -> ps ) <-> A. y E. x ( ph -> ps ) ) $=
      ( wi wal wex 19.21v biex 19.36v bial bitr2 3bitr ) ABEZDFZCGABDFZEZCGACFZ
      PEZNCGZDFZOQCABDHIAPCJUARBEZDFSTUBDABCJKRBDHLM $.
      $( [18-Jul-01] $)
