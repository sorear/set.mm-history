      VSJVQEUOUPUQVTVIDBWAVHDABCURUCUSUTVAVBVCVB $.
      $( [5-Aug-93] $)
  $}

  $( A variable not free remains so after substitution with a distinct
     variable (closed form of ~ hbsb4 ). $)
  hbsb4t $p |- ( A. x A. z ( ph -> A. z ph ) ->
               ( -. A. z z = y -> ( [ y / x ] ph -> A. z [ y / x ] ph ) ) ) $=
    ( wal wi wsb weq wn wb wa ax-4 biantru bi bitr4 bi2al sbba4 a4s hba1 biald
    bi2imd a7s sylbi hbsb4 syl5bir ) AADEZFZDEBEZABCGZUIDEZFZUFBCGZULDEZFZDCHDE
    IUHAUFJZDEBEUKUNJZUGUOBDUGUGUFAFZKUOUQUGADLMAUFNOPUOUPDBUOBEZDEZUIULUJUMURU
    IULJDAUFBCQRZUSUIULDURDSUTTUAUBUCUFBCDADSUDUE $.
    $( [14-Apr-04] $) $( [7-Apr-04] $)

  ${
    ddelimf2.1 $e |- ( ph -> A. x ph ) $.
    ddelimf2.2 $e |- ( ps -> A. z ps ) $.
    ddelimf2.3 $e |- ( z = y -> ( ph <-> ps ) ) $.
    $( Proof of ~ ddelimf without using ~ ax-11 .  This may be useful in a
       study to determine whether ~ ax-11 can be derived from the others,
       which is currently unknown. $)
    ddelimf2 $p |- ( -. A. x x = y -> ( ps -> A. x ps ) ) $=
      ( weq wal wn wi ax-10 eq4s hba1 syl5 a1d wa eq6 hban ax-12 imp a1i hbimd
      hbald exp pm2.61i eqsal bial 3imtr3g ) CDICJKZEDIZALZEJZUNCJZBBCJCEICJZUK
      UNUOLZLUPUQUKUPUNEJZUOUNURUOLECUNECMNUMEOPQUPKZUKUQUSUKRZUMCEUSUKECEESCDE
      STUTULACUSUKCCECSCDCSTUSUKULULCJLEDCUAUBAACJLUTFUCUDUEUFUGABEDGHUHZUNBCVA
      UIUJ $.
      $( [12-Nov-02] $)
  $}

  ${
    ddelimf.1 $e |- ( ph -> A. x ph ) $.
    ddelimf.2 $e |- ( ps -> A. z ps ) $.
    ddelimf.3 $e |- ( z = y -> ( ph <-> ps ) ) $.
    $( Version of ~ ddelim without any variable restrictions. $)
    ddelimf $p |- ( -. A. x x = y -> ( ps -> A. x ps ) ) $=
      ( weq wal wn wsb hbsb4 sbie bial 3imtr3g ) CDICJKAEDLZQCJBBCJAEDCFMABEDGH
      NZQBCROP $.
      $( [1-Oct-02] $)
  $}

  ${
    ddelimdf.1 $e |- ( ph -> A. x ph ) $.
    ddelimdf.2 $e |- ( ph -> A. z ph ) $.
    ddelimdf.3 $e |- ( ph -> ( ps -> A. x ps ) ) $.
    ddelimdf.4 $e |- ( ph -> ( ch -> A. z ch ) ) $.
    ddelimdf.5 $e |- ( ph -> ( z = y -> ( ps <-> ch ) ) ) $.
    $( Deduction form of ~ ddelimf .  This version may be useful if we want to
       avoid ~ ax-17 and use ~ ax-16 instead. $)
    ddelimdf $p |- ( ph -> ( -. A. x x = y -> ( ch -> A. x ch ) ) ) $=
      ( weq wal wn wi wa wsb 19.21ai 19.20i hbsb4t 3syl imp wb sbied adantr
      biald 3imtr3d exp ) ADELDMNZCCDMZOAUIPBFEQZUKDMZCUJAUIUKULOZAADMZFMBBDMOZ
      DMZFMUIUMOAUNFHGRUNUPFAUODISSBFEDTUAUBAUKCUCUIABCFEHJKUDZUEAULUJUCUIAUKCD
      GUQUFUEUGUH $.
      $( [15-Apr-04] $) $( [7-Apr-04] $)
  $}

  $( A composition law for substitution. $)
  sbco $p |- ( [ y / x ] [ x / y ] ph <-> [ y / x ] ph ) $=
    ( wsb wb weq sbeq2 sbequ12 bicomd sbimi ax-mp sbbi mpbi ) ACBDZAEZBCDZNBCDA
    BCDECBFZBCDPBCGQOBCQANACBHIJKNABCLM $.
    $( [5-Aug-93] $)

  ${
    sbid2.1 $e |- ( ph -> A. x ph ) $.
    $( An identity law for substitution. $)
    sbid2 $p |- ( [ y / x ] [ x / y ] ph <-> ph ) $=
      ( wsb sbco sbf1 bitr4 ) ACBEBCEABCEAABCFABCDGH $.
      $( [5-Aug-93] $)
  $}

  $( An idempotent law for substitution. $)
  sbidm $p |- ( [ y / x ] [ y / x ] ph <-> [ y / x ] ph ) $=
    ( weq wal wsb wb sbequ12 bicomd a4s wn eq6 hbsb2 wi pm4.2i a1i sbied
    pm2.61i ) BCDZBEZABCFZBCFZUAGZSUCBSUAUBUABCHIJTKZUAUABCBCBLABCMSUAUAGNUDSUA
    OPQR $.
    $( [30-Jun-94] $)

  ${
    sbco2.1 $e |- ( ph -> A. z ph ) $.
    $( A composition law for substitution. $)
    sbco2 $p |- ( [ y / z ] [ z / x ] ph <-> [ y / x ] ph ) $=
      ( weq wal wsb wb sbequ sbid2 syl5bbr sbequ12 bitr3d a4s wn eq6 hbsb3
      hbsb4 wi a1i sbied bicomd pm2.61i ) BCFZBGZABDHZDCHZABCHZIZUEUJBUEAUHUIUE
      UGDBHUHAUGBCDJADBEKLZABCMNOUFPZUIUHULAUHBCBCBQUGDCBABDERSUEAUHITULUKUAUBU
      CUD $.
      $( [30-Jun-94] $)
