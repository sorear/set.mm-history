  ${
    $d A u v w x y z $.  $d B u v w x y z $.  $d C u v w z $.
    $d D u v w x y z $.  $d F u v $.  $d G u v z $.  $d H u v w x y z $.
    oprabco.1 $e |- ( ( x e. A /\ y e. B ) -> C e. D ) $.
    oprabco.2 $e |- F = { <. <. x , y >. , z >. |
                                ( ( x e. A /\ y e. B ) /\ z = C ) } $.
    oprabco.3 $e |- G = { <. <. x , y >. , w >. |
                                ( ( x e. A /\ y e. B ) /\ w = ( H ` C ) ) } $.
