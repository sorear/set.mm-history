    $( The base set of an extended filter.  (Contributed by Jeff Hankins,
       7-Sep-2009.) $)
    extbas2 $p |- ( ( X C_ A /\ A e. B ) -> U. ( F u. { A } ) =
      A ) $=
      ( wss wcel wa csn cun cuni uniun uneq1i eqtr4i unisng uneq2d ssid jctr
      unss sylib ssun2 a1i eqssd sylan9eqr syl5eq ) DAFZABGZHCAIZJKZDUHKZJZAUIC
      KZUJJUKCUHLDULUJEMNUGUFUKDAJZAUGUJADABOPUFUMAUFUFAAFZHUMAFUFUNAQRDAASTAUM
      FUFADUAUBUCUDUE $.
      $( [1-Dec-2010] $) $( [7-Sep-2009] $)
