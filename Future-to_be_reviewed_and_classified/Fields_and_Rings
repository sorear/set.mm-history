        $( [18-Feb-2010] $)

      $( The unit of a ring is an identity element for the multiplication.
         (Contributed by FL, 18-Apr-2010.) $)
      ringolidm $p |- ( ( R e. RingOps /\ A e. X ) -> ( U H A ) = A ) $=
        ( cring wcel wa co wceq ringoidmlem simpld ) BIJAEJKCADLAMACDLAMABCDEFG
        HNO $.
        $( [18-Apr-2010] $)
