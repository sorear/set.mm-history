    ssfii $p |- ( A e. V -> A C_ ( fi ` A ) ) $=
      ( vx vy wcel cfi cfv cv cint wceq cpw cfn cin wrex csn snelpwi snfi a1i
      elin sylanbrc visset intsn eqcomi inteq eqeq2d rcla4ev sylancl cvv wb
      elfi mpan syl5ibr ssrdv ) ABEZCAAFGZCHZAEZUPUOEZUNUPDHZIZJZDAKZLMZNZUQUPO
