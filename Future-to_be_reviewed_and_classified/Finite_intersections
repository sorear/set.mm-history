  ${
    $d A x y $.
    $( If ` A ` is not empty, the class of all the finite intersections of
       ` A ` is not empty either.  (Contributed by FL, 2-Sep-2008.) $)
    fine2 $p |- ( A e. B -> ( A =/= (/) -> ( fi ` A ) =/= (/) ) ) $=
      ( vy vx c0 wne cv wss cfn wcel cint wceq w3a wex cab cfi cfv fine fiv
      eqcomd neeq1d syl5ib ) AEFCGZAHUCIJDGUCKLMCNDOZEFABJZAPQZEFDCARUEUDUFEUEU
      FUDCDABSTUAUB $.
      $( [1-Dec-2010] $) $( [27-Apr-2008] $)
  $}

