  ${
    $d A a x y $.
    $( Condition required for a nonempty finite intersection.  (Contributed by
       FL, 2-Sep-2008.) $)
    fine $p |- ( A =/= (/)
               -> { x | E. y ( y C_ A /\ y e. Fin /\ x = |^| y ) } =/= (/) ) $=
      ( va cv wcel wex wss cfn cint wceq w3a c0 wne cab ax-17 csn wa cvv wi
      snex sseq1 eleq1 inteq eqeq2d 3anbi123d cla4egv ax-mp snssi adantl snfi
      a1i visset intsn eqcomi eqeq2i biimpi adantr syl3anc ex a4ime 19.23aiv
      n0 abn0 3imtr4i ) DEZCFZDGBEZCHZVHIFZAEZVHJZKZLZBGZAGZCMNVOAOMNVGVPDVGVOA
      DVGAPVKVFKZVGVOVFQZCHZVRIFZVKVRJZKZVOVQVGRZVRSFVSVTWBLZVOTVFUAVNWDBVRSVHV
      RKZVIVSVJVTVMWBVHVRCUBVHVRIUCWEVLWAVKVHVRUDUEUFUGUHVGVSVQVFCUIUJVTWCVFUKU
      LVQWBVGVQWBVFWAVKWAVFVFDUMUNUOUPUQURUSUTVAVBDCVCVOAVDVE $.
      $( [1-Dec-2010] $) $( [16-Nov-2007] $)
  $}

  ${
    $d A x y $.
    $( If ` A ` is not empty, the class of all the finite intersections of
       ` A ` is not empty either.  (Contributed by FL, 2-Sep-2008.) $)
    fine2 $p |- ( A e. B -> ( A =/= (/) -> ( fi ` A ) =/= (/) ) ) $=
      ( vy vx wcel cv wss cfn cint wceq w3a wex cab c0 wne cfi cfv fiv eqcomd
      neeq1d fine syl5bi ) ABEZCFZAGUDHEDFUDIJKCLDMZNOAPQZNOANOUCUEUFNUCUFUECDA
      BRSTDCAUAUB $.
      $( [1-Dec-2010] $) $( [27-Apr-2008] $)
  $}

  ${
    $d A x y $.
    $( Any element of ` A ` is the intersection of a finite subclass of
       ` A ` .  (Contributed by FL, 2-Sep-2008.) $)
    abfi $p |- A
               C_ { x | E. y ( y C_ A /\ y e. Fin /\ x = |^| y ) } $=
      ( cv wss cfn wcel cint wceq w3a wex cab wi ssab csn cpw wa wrex snfi
      visset intsn eqcomi eleq1 inteq eqeq2d anbi12d rcla4ev ex mp2ani snelpw
      df-rex elpw anbi1i 3anass bitr4i exbii bitr2i 3imtr4i mpgbir ) CBDZCEZUTF
      GZADZUTHZIZJZBKZALEVCCGZVGMAVGACNVCOZCPZGZVBVEQZBVJRZVHVGVKVIFGZVCVIHZIZV
