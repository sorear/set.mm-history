    $( Structure product base set. $)
    prdmbaslem $p |- ( ( S e. W /\ R e. Z ) -> B = ( Base ` ( S Xm_ R ) ) ) $=
      ( vr wcel wa cbs cfv cprdm wceq cvv prdmbasex prdmvalex prdmvalfun baseid
      elex cnx cop cplusg cmulr ctp csca cvsca cpr cun cts cple cds tpid1 elun1
      co opex mp2b eleqtrri strfv 3syl adantl prdmval fveq2d eqtr4d ) FNUEZEOUE
      ZUFZCHUGUHZFEUIVKZUGUHWBCWDUJZWAWBEUKUECUKUEWFEOUPACEUKPULCHUGUKUDABCDEFG
      HIJKLMPQRSTUAUBUCUMABCDEFGHIJKLMPQRSTUAUBUCUNUDUOUQUGUHZCURZWHUQUSUHBURZU
