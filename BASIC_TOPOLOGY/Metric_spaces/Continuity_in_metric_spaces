      $( [1-Aug-2007] $)

    $( A constant function is continuous (metric space version of
       ~ cnconst ). $)
    metcnconst $p |- ( ( ( C e. Met /\ D e. Met ) /\ ( B e. Y /\
             F : X --> { B } ) ) -> F e. ( J Cn K ) ) $=
      ( cme wcel wa wf ctop cuni opntop uniopn2 csn ccn co anim12i adantr feq2d
      eleq2d bi2anan9r biimpa eqid cnconst syl2anc ) BMNZCMNZOZAHNZGAUAZDPZOZOE
