$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                        Open sets of a metric space
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y z A $.  $d d x y z D $.  $d x y z X $.
    opnval.1 $e |- J = ( MetOpen ` D ) $.
    $( An open set is a subset of a metric space which includes a ball around
       each of its points.  Definition 1.3-2 of [Kreyszig] p. 18.  The object
       ` ( MetOpen `` D ) ` is the family of all open sets in the metric space
       determined by the metric ` D ` .  By ~ opntop , the open sets of a
       metric space form a topology ` J ` , whose base set is ` U. J ` by
       ~ uniopn2 . $)
    opnval $p |- ( D e. Met -> J = ( topGen ` ran ( ball ` D ) ) ) $=
      ( vd cme wcel copn cfv cbl crn ctg cv wceq fveq2 rneqd fveq2d df-opn fvex
      fvmpt syl5eq ) AEFBAGHAIHZJZKHZCDADLZIHZJZKHUCEGUDAMZUFUBKUGUEUAUDAINOPDQ
      UBKRST $.
      $( [12-Nov-2013] $) $( [1-Sep-2006] $)

    $( The set of open sets of a metric space is a topology.  Remark in
       [Kreyszig] p. 19.  This theorem connects the two concepts and makes
       available the theorems for topologies for use with metric spaces. $)
    opntop $p |- ( D e. Met -> J e. Top ) $=
      ( cme wcel cbl cfv crn ctg ctop opnval ctb blbas tgcl syl eqeltrd ) ADEZB
      AFGHZIGZJABCKQRLESJEAMRNOP $.
      $( [12-Nov-2013] $) $( [28-Aug-2006] $)
  $}

  ${
    $d x y z A $.  $d d x y z D $.  $d x y z X $.
    uniopn2.1 $e |- X = dom dom D $.
    uniopn2.2 $e |- J = ( MetOpen ` D ) $.
    $( The union of all open sets in a metric space is its underlying set. $)
    uniopn2 $p |- ( D e. Met -> X = U. J ) $=
      ( cme wcel cuni cbl cfv crn ctg opnval unieqd ctb blbas unitg syl unirnbl
      wceq 3eqtrrd ) AFGZBHAIJKZLJZHZUCHZCUBBUDABEMNUBUCOGUEUFTAPUCQRACDSUA $.
      $( [12-Nov-2013] $) $( [4-Sep-2006] $)

    $( The defining property of an open set of a metric space. $)
    isopn $p |- ( D e. Met -> ( A e. J <->
   ( A C_ X /\ A. x e. A E. y e. ran ( ball ` D ) ( x e. y /\ y C_ A ) ) ) ) $=
      ( cme wcel cbl cfv crn ctg cuni wss cv wa wrex wral opnval wb blbas eltg2
      eleq2d ctb syl unirnbl sseq2d anbi1d 3bitrd ) DIJZCEJCDKLMZNLZJZCUMOZPZAQ
      BQZJURCPRBUMSACTZRZCFPZUSRULEUNCDEHUAUEULUMUFJUOUTUBDUCABCUMUDUGULUQVAUSU
      LUPFCDFGUHUIUJUK $.
      $( [12-Nov-2013] $) $( [1-Sep-2006] $)

    $( The family of open sets of a metric space is a collection of subsets of
       the base set. $)
    opnfss $p |- ( D e. Met -> J C_ ~P X ) $=
      ( cme wcel cuni cpw pwuni uniopn2 pweqd syl5sseqr ) AFGZBHZIBCIBJNCOABCDE
      KLM $.
      $( [12-Nov-2013] $) $( [3-Sep-2006] $)

    $( The base set of a metric space is open.  Part of Theorem T1 of
       [Kreyszig] p. 19. $)
    opnm $p |- ( D e. Met -> X e. J ) $=
      ( cme wcel cuni uniopn2 ctop opntop eqid topopn syl eqeltrd ) AFGZCBHZBAB
      CDEIPBJGQBGABEKBQQLMNO $.
      $( [12-Nov-2013] $) $( [4-Sep-2006] $)

    $( A defining property of an open set of a metric space. $)
    isopn4 $p |- ( D e. Met -> ( A e. J <-> ( A C_ X /\
           A. x e. A E. y e. RR+ ( x ( ball ` D ) y ) C_ A ) ) ) $=
