$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                        Topology on the Reals
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d t u v w x y z S $.
    qtopbas.1 $e |- S C_ RR* $.
    $( The set of open intervals with endpoints in a subset forms a basis for a
       topology. $)
    qtopbaslem $p |- ( (,) " ( S X. S ) ) e. TopBases $=
      ( vx vy vz vw vv vu vt cioo wcel cv cin wral wa cxr wceq sseli cfv mp2an
      cxp cima cvv ctb crn iooex rnex imassrn ssexi co cle wbr cif iooin syl2an
      anim12i ifcl ancoms cop df-ov opelxpi wfun cdm wss wi cr cpw wf ioof ffun
      ax-mp xpss12 fdmi sseqtr4i funfvima2 syl syl5eqel an4s eqeltrd ralrimivva
      rgen2a wfn wb ffn ineq1 eleq1d ralbidv raleqfn fveq2 syl6eqr ineq1d ineq2
      ineq2d ralxp bitr3i syl6bb mpbir fiinbas ) JAAUAZUBZUCKCLZDLZMZWTKZDWTNZC
      WTNZWTUDKWTJUEJUFUGJWSUHUIXFELZFLZJUJZGLZHLZJUJZMZWTKZHANGANZFANEANZXOEFA
      XGAKZXHAKZOZXNGHAAXSXJAKZXKAKZOZOXMXGXJUKULZXJXGUMZXHXKUKULZXHXKUMZJUJZWT
      XSXGPKZXHPKZOXJPKZXKPKZOXMYGQYBXQYHXRYIAPXGBRAPXHBRUPXTYJYAYKAPXJBRAPXKBR
      UPXGXHXJXKUNUOXQXTXRYAYGWTKZXQXTOYDAKZYFAKZYLXRYAOXTXQYMYCXJXGAUQURYEXHXK
      AUQYMYNOZYGYDYFUSZJSZWTYDYFJUTYOYPWSKZYQWTKZYDYFAAVAJVBZWSJVCZVDYRYSVEPPU
      AZVFVGZJVHZYTVIUUBUUCJVJVKWSUUBUUAAPVDZUUEWSUUBVDZBBAPAPVLTZUUBUUCJVIVMVN
      WSYPJVOTVPVQUOVRVSVTWAXFILZJSZXBMZWTKZDWTNZIWSNZXPJUUBWBZUUFUUMXFWCUUDUUN
      VIUUBUUCJWDVKZUUGXEUULICUUBWSJXAUUIQZXDUUKDWTUUPXCUUJWTXAUUIXBWEWFWGWHTUU
      LXOIEFAAUUHXGXHUSZQZUULXIXBMZWTKZDWTNZXOUURUUKUUTDWTUURUUJUUSWTUURUUIXIXB
      UURUUIUUQJSXIUUHUUQJWIXGXHJUTWJWKWFWGUVAXIUUIMZWTKZIWSNZXOUUNUUFUVDUVAWCU
      UOUUGUUTUVCIDUUBWSJXBUUIQUUSUVBWTXBUUIXIWLWFWHTUVCXNIGHAAUUHXJXKUSZQZUVBX
      MWTUVFUUIXLXIUVFUUIUVEJSXLUUHUVEJWIXJXKJUTWJWMWFWNWOWPWNWOWQCDWTUCWRT $.
      $( [17-Jun-2014] $)
  $}

  $( The set of open intervals with rational endpoints forms a basis for a
     topology. $)
  qtopbas $p |- ( (,) " ( QQ X. QQ ) ) e. TopBases $=
    ( cq cr cxr qssre ressxr sstri qtopbaslem ) AABCDEFG $.
    $( [28-Jun-2008] $) $( [8-Mar-2007] $)

  $( A basis for the standard topology on the reals.  (The proof was shortened
     by Mario Carneiro, 17-Jun-2014.) $)
  retopbas $p |- ran (,) e. TopBases $=
    ( cioo cxr cxp cima crn ctb cdm cr cpw ioof fdmi imaeq2i imadmrn qtopbaslem
    eqtr3i ssid eqeltrri ) ABBCZDZAEZFAAGZDSTUARARHIAJKLAMOBBPNQ $.
    $( [17-Jun-2014] $) $( [6-Feb-2007] $)

  $( The standard topology on the reals.  (Contributed by FL, 4-Jun-2007.) $)
  retop $p |- ( topGen ` ran (,) ) e. Top $=
    ( cioo crn ctb wcel ctg cfv ctop retopbas tgcl ax-mp ) ABZCDKEFGDHKIJ $.
    $( [29-May-2007] $)

  $( The underlying set of the standard topology on the reals is the reals.
     (Contributed by FL, 4-Jun-2007.) $)
  uniretop $p |- RR = U. ( topGen ` ran (,) ) $=
    ( cioo crn cuni ctg cfv unirnioo ctb wcel wceq retopbas unitg ax-mp eqtr4i
    cr ) NABZCZODECZFOGHQPIJOKLM $.
    $( [29-May-2007] $)

  $( The standard topological space on the reals. $)
  retpsOLD $p |- <. RR , ( topGen ` ran (,) ) >. e. TopSpOLD $=
    ( cr cioo crn ctg cfv cop ctpsOLD wcel ctop cuni uniretop istpsOLD mpbir2an
    wceq retop ) ABCDEZFGHPIHAPJNOKAPLM $.
    $( [10-Feb-2007] $)

  ${
    retps.k $e |- K = { <. ( Base ` ndx ) , RR >. ,
                      <. ( TopOpen ` ndx ) , ( topGen ` ran (,) ) >. } $.
    $( The standard topological space on the reals. $)
    retps $p |- K e. TopSp $=
      ( cr cioo crn ctg cfv uniretop retop eltpsi ) CDEFGABHIJ $.
      $( [19-Oct-2012] $)
  $}

  $( Open intervals are open sets of the standard topology on the reals .
     (Contributed by FL, 18-Jun-2007.) $)
  iooretop $p |- ( A (,) B ) e. ( topGen ` ran (,) ) $=
    ( cxr wcel wa co crn ctg cfv ctb wss retopbas bastg ax-mp cxp wfn cr cpw c0
    cioo wf ioof ffn fnovrn mp3an1 sseldi wn ndmioo ctop retop syl6eqel pm2.61i
    0opn ) ACDZBCDZEZABTFZTGZHIZDUPURUSUQURJDURUSKLURMNTCCOZPZUNUOUQURDUTQRZTUA
    VAUBUTVBTUCNCCABTUDUEUFUPUGUQSUSABUHUSUIDSUSDUJUSUMNUKUL $.
    $( [21-Jun-2007] $)

  $( Closed intervals are closed sets of the standard topology on ` RR ` .
     (Contributed by FL, 14-Sep-2007.) $)
  icccld $p |- ( ( A e. RR /\ B e. RR ) -> ( A [,] B ) e.
        ( Clsd ` ( topGen ` ran (,) ) ) ) $=
    ( cr wcel wa cicc co cioo crn ctg cfv ccld cdif cmnf cpnf cun difreicc ctop
    retop iooretop unopn syl6eqel wss wb iccssre uniretop iscld2 sylancr mpbird
    mp3an ) ACDBCDEZABFGZHIJKZLKDZCULMZUMDZUKUONAHGZBOHGZPZUMABQUMRDZUQUMDURUMD
    USUMDSNATBOTUQURUMUAUJUBUKUTULCUCUNUPUDSABUEULUMCUFUGUHUI $.
    $( [14-Sep-2007] $)

  ${
    $d v w x y z $.
    $( ` QQ ` is dense in the standard topology on ` RR ` . $)
    qdensere $p |- ( ( cls ` ( topGen ` ran (,) ) ) ` QQ ) = RR $=
      ( vx vy vz vv vw cq cioo cfv cr wcel wss cv c0 wne clt wbr wceq cxr wrex
      wa crn ctg ccl ctop retop qssre uniretop clsss3 mp2an cin wral crab rabex
      wi reex dfioo2 elrnoprab co iooval2 eqeq2d anbi1d ne0i neeq1 syl5ib ioon0
      imp qbtwnxr 3expia sylbid syl5 sylbird wb eleq2 qre biantrurd breq2 breq1
      anbi12d elrab syl6rbbr sylan9bb rexbidva ad2antrl mpbird disjr necon2abii
      wn ralnex bitri sylib exp32 rexlimivv sylbi rgen ctb retopbas eqid elcls3
      mp3an12 mpbiri ssriv eqssi ) FGUAZUBHZUCHHZIXDUDJFIKZXEIKUEUFFXDIUGUHUIAI
      XEALZIJZXGXEJZXGBLZJZXJFUJZMNZUNZBXCUKZXNBXCXJXCJXJCLZDLZOPZXQELZOPZTZDIU
      LZQZERSCRSXNCEARRYBXJGYADIUOUMCEADUPUQYCXNCERRXPRJZXSRJZTZYCXKXMYFYCXKTZT
      ZXKAFSZXMYHYIXPXGOPZXGXSOPZTZAFSZYFYGYMYFYGXJXPXSGURZQZXKTZYMYFYOYCXKYFYN
      YBXJDXPXSUSUTVAYPYNMNZYFYMYOXKYQXKXJMNYOYQXJXGVBXJYNMVCVDVFYFYQXPXSOPZYMX
      PXSVEYDYEYRYMAXPXSVGVHVIVJVKVFYCYIYMVLYFXKYCXKYLAFYCXKXGYBJZXGFJZYLXJYBXG
      VMYTYLXHYLTYSYTXHYLXGVNVOYAYLDXGIXQXGQXRYJXTYKXQXGXPOVPXQXGXSOVQVRVSVTWAW
      BWCWDYIXLMXLMQXKWGAFUKYIWGAXJFWEXKAFWHWIWFWJWKWLWMWNXCWOJXFXHXIXOVLWPUFBX
      CXGFXDIXDWQUGWRWSWTXAXB $.
      $( [1-Mar-2007] $)
  $}

  ${
    $d D x y z $.
    cnmet.1 $e |- D = ( abs o. - ) $.
    $( Value of the distance function of the metric space of complex
       numbers. $)
    cnmetdval $p |- ( ( A e. CC /\ B e. CC ) ->
                  ( A D B ) = ( abs ` ( A - B ) ) ) $=
      ( cc wcel wa cop cabs cmin ccom cfv co wf wceq subopr opelxpi fvco3 df-ov
      cxp sylancr fveq1i eqtri fveq2i 3eqtr4g ) AEFBEFGZABHZIJKZLZUGJLZILZABCMZ
      ABJMZILUFEETZEJNUGUNFUIUKOPABEEQUNEUGIJRUAULUGCLUIABCSUGCUHDUBUCUMUJIABJS
      UDUE $.
      $( [27-Dec-2014] $)  $( [9-Dec-2006] $)

    $( The base set of the metric for complex numbers. $)
    cnmetba $p |- CC = dom dom D $=
      ( cdm cc cxp cabs cmin ccom dmeqi cr wf absf subopr fco mp2an fdmi dmxpid
      eqtri eqtr2i ) ACZCDDEZCDTUATFGHZCUAAUBBIUAJUBDJFKUADGKUAJUBKLMUADJFGNOPR
      IDQS $.
      $( [30-Aug-2007] $)

    $( The absolute value metric determines a metric space on the complex
       numbers.  This theorem provides a link between complex numbers and
       metrics spaces, making metric space theorems available for use with
       complex numbers.  (Contributed by FL, 9-Oct-2006.) $)
    cnmet $p |- D e. Met $=
      ( vx vy vz cc cr wf cabs cmin cv wcel wa cfv cc0 wceq cnmetdval w3a caddc
      co cnex cxp ccom absf subopr fco mp2an feq1i mpbir subcl abs00 syl eqcomd
      wb eqeq1d subeq0 3bitr3d cle abs3dif abssub 3adant2 breqtrd 3adant3 3coml
      oveq1d oveq12d 3brtr4d ismeti ) CDEAFUAFFUBZGAHVIGIJUCZHZFGIHVIFJHVKUDUEV
      IFGIJUFUGVIGAVJBUHUICKZFLZDKZFLZMZVLVNJTZINZOPZVQOPZVLVNATZOPVLVNPVPVQFLV
      SVTUNVLVNUJVQUKULVPVRWAOVPWAVRVLVNABQZUMUOVLVNUPUQVMVOEKZFLZRZVRWCVLJTINZ
      WCVNJTINZSTZWAWCVLATZWCVNATZSTZURWEVRVLWCJTINZWGSTZWHURVLVNWCUSVMWDWMWHPV
      OVMWDMWLWFWGSVLWCUTVEVAVBVMVOWAVRPWDWBVCWDVMVOWKWHPWDVMVORWIWFWJWGSWDVMWI
      WFPVOWCVLABQVCWDVOWJWGPVMWCVNABQVAVFVDVGVH $.
      $( [7-Sep-2007] $) $( [6-Oct-2006] $)
  $}

  ${
    $d x y z C $.
    cn2met.1 $e |- C = ( abs o. - ) $.
    cn2met.2 $e |- D = ( x e. ( CC X. CC ) , y e. ( CC X. CC ) |->
                     sup ( { ( ( 1st ` x ) C ( 1st ` y ) ) ,
                      ( ( 2nd ` x ) C ( 2nd ` y ) ) } , RR , < ) ) $.
    $( The standard metric space on ` CC X. CC ` . $)
    cn2met $p |- D e. Met $=
      ( cc cnmetba cnmet metxp ) ABCCDGGCEHZKCEIZLFJ $.
      $( [21-Jun-2007] $)

    txcc.3 $e |- J = ( MetOpen ` C ) $.
    $( The product of two copies of ` CC ` , in terms of the sup norm.
       (Contributed by Jeff Madsen, 11-Jun-2010.)  (Revised by Mario Carneiro,
       5-May-2014.) $)
    txcc $p |- ( J tX J ) = ( MetOpen ` D ) $=
      ( cc cnmetba cnmet txmet ) EEDCCIIABHHCFJZMCFKZNGL $.
      $( [5-May-2014] $) $( [11-Jun-2010] $)
  $}

  ${
    $d r x y z D $.  $d x A $.  $d x B $.
    remet.1 $e |- D = ( ( abs o. - ) |` ( RR X. RR ) ) $.
    $( Value of the distance function of the metric space of real numbers. $)
    remetdval $p |- ( ( A e. RR /\ B e. RR ) ->
                  ( A D B ) = ( abs ` ( A - B ) ) ) $=
      ( cr wcel wa co cop cabs cmin ccom cxp cres cfv df-ov fveq1i wceq cc recn
      eqtri opelxpi fvres syl eqid cnmetdval syl2an syl5eqr eqtrd syl5eq ) AEFZ
      BEFZGZABCHZABIZJKLZEEMZNZOZABKHJOZUNUOCOUSABCPUOCURDQUAUMUSUOUPOZUTUMUOUQ
      FUSVARABEEUBUOUQUPUCUDUMVAABUPHZUTABUPPUKASFBSFVBUTRULATBTABUPUPUEUFUGUHU
      IUJ $.
      $( [16-May-2007] $)

    $( The base set for the metric for real numbers. $)
    remetba $p |- RR = dom dom D $=
      ( cdm cr cxp cabs cmin ccom cres cin dmeqi dmres wceq cc ax-resscn xpss12
      wss mp2an wf absf subopr fdmi sseqtr4i df-ss mpbi 3eqtri dmxpid eqtr2i
      fco ) ACZCDDEZCDUJUKUJFGHZUKIZCUKULCZJZUKAUMBKULUKLUKUNQUOUKMUKNNEZUNDNQZ
      UQUKUPQOODNDNPRUPDULNDFSUPNGSUPDULSTUAUPNDFGUIRUBUCUKUNUDUEUFKDUGUH $.
      $( [30-Aug-2007] $)

    $( The absolute value metric determines a metric space on the reals. $)
    remet $p |- D e. Met $=
      ( cabs cmin ccom cr cxp cres cme wcel eqid cnmet metres ax-mp eqeltri ) A
      CDEZFFGHZIBPIJQIJPPKLPFMNO $.
      $( [10-Feb-2007] $)

    $( A ball in terms of an open interval of reals. $)
    bl2ioo $p |- ( ( A e. RR /\ B e. RR+ ) ->
                   ( A ( ball ` D ) B ) = ( ( A - B ) (,) ( A + B ) ) ) $=
      ( vx cr wcel wa cfv co cmin clt wbr wb cabs cc recn syl2an sylan2 cxr crp
      cbl caddc cioo cv w3a rpre remetdval abssub eqtrd breq1d adantlr absdiflt
      wceq 3expb ancoms bitrd pm5.32da 3anass syl6bbr remet remetba elbl mp3an1
      cme resubcl readdcl rexr elioo2 syl2anc 3bitr4d eqrdv ) AFGZBUAGZHZEABCUB
      IJZABKJZABUCJZUDJZVOEUEZFGZAVTCJZBLMZHZWAVQVTLMZVTVRLMZUFZVTVPGZVTVSGZVOW
      DWAWEWFHZHZWGVNVMBFGZWDWKNBUGZVMWLHZWAWCWJWNWAHWCVTAKJOIZBLMZWJVMWAWCWPNW
      LVMWAHZWBWOBLWQWBAVTKJOIZWOAVTCDUHVMAPGVTPGWRWOUNWAAQVTQAVTUIRUJUKULWAWNW
      PWJNZWAVMWLWSVTABUMUOUPUQURSWAWEWFUSUTCVEGVMVNWHWDNCDVAVTCABFCDVBVCVDVNVM
      WLWIWGNZWMWNVQFGZVRFGZWTABVFABVGXAVQTGVRTGWTXBVQVHVRVHVQVRVTVIRVJSVKVL $.
      $( [13-Nov-2013] $) $( [18-May-2007] $)

    $( An open interval of reals in terms of a ball. $)
    ioo2bl $p |- ( ( A e. RR /\ B e. RR /\ A < B ) -> ( A (,) B ) =
     ( ( ( A + B ) / 2 ) ( ball ` D ) ( ( B - A ) / 2 ) ) ) $=
      ( cr wcel clt wbr caddc co c2 cdiv cmin cioo crp wceq wa ancoms 3adant3
      cc w3a cbl cfv readdcl rehalfcl syl cc0 resubcl biimp3a elrp sylanbrc 2re
      posdif 2pos elrpii rpdivcl sylancl bl2ioo syl2anc recn syl2anr halfaddsub
      addcom oveq1d simprd simpld oveq12d 3eqtr3rd ) AEFZBEFZABGHZUAZBAIJZKLJZB
      AMJZKLJZCUBUCZJZVNVPMJZVNVPIJZNJZABIJZKLJZVPVQJABNJVLVNEFZVPOFZVRWAPVIVJW
      DVKVIVJQVMEFZWDVJVIWFBAUDRVMUEUFSVLVOOFZKOFWEVLVOEFZUGVOGHZWGVIVJWHVKVJVI
      WHBAUHRSVIVJVKWIABUMUIVOUJUKKULUNUOVOKUPUQVNVPCDURUSVLVNWCVPVQVLVMWBKLVIV
      JVMWBPZVKVJBTFZATFZWJVIBUTZAUTZBAVCVASVDVDVLVSAVTBNVLVTBPZVSAPZVIVJWOWPQZ
      VKVJWKWLWQVIWMWNBAVBVASZVEVLWOWPWRVFVGVH $.
      $( [13-Nov-2013] $) $( [18-May-2007] $)

    $( An open interval of reals in terms of a ball. $)
    ioo2blex $p |- ( ( A e. RR /\ B e. RR /\ A < B ) ->
                     ( A (,) B ) e. ran ( ball ` D ) ) $=
      ( cr wcel clt wbr w3a cioo co caddc c2 cdiv cmin cbl cfv crn crp 3adant3
      ioo2bl wa readdcl rehalfcl syl cc0 resubcl ancoms posdif biimp3a sylanbrc
      elrp 2re 2pos elrpii rpdivcl sylancl cxp wfn cme cpw wf remet remetba blf
      ffn mp2b fnovrn mp3an1 syl2anc eqeltrd ) AEFZBEFZABGHZIZABJKABLKZMNKZBAOK
      ZMNKZCPQZKZVTRZABCDUAVOVQEFZVSSFZWAWBFZVLVMWCVNVLVMUBVPEFWCABUCVPUDUETVOV
      RSFZMSFWDVOVREFZUFVRGHZWFVLVMWGVNVMVLWGBAUGUHTVLVMVNWHABUIUJVRULUKMUMUNUO
      VRMUPUQVTESURZUSZWCWDWECUTFWIEVAZVTVBWJCDVCCECDVDVEWIWKVTVFVGESVQVSVTVHVI
      VJVK $.
      $( [14-Nov-2013] $)

    $( The balls of the standard real metric space are included in the open
       real intervals. $)
    blssioo $p |- ran ( ball ` D ) C_ ran (,) $=
      ( vz vy vr cbl cfv crn cioo cv wcel co wceq crp wrex cr ax-mp wa cxr rexr
      cme remet remetba blrn cmin caddc bl2ioo rpre resubcl readdcl cxp wfn cpw
      ioof ffn fnovrn mp3an1 syl2an syl2anc sylan2 eqeltrd syl5ibrcom rexlimivv
      wb wf eleq1 sylbi ssriv ) CAFGZHZIHZCJZVJKZVLDJZEJZVILZMZENODPOZVLVKKZAUA
      KVMVRVDABUBDVLAPEABUCUDQVQVSDEPNVNPKZVONKZRZVSVQVPVKKWBVPVNVOUELZVNVOUFLZ
      ILZVKVNVOABUGWAVTVOPKZWEVKKZVOUHVTWFRWCPKZWDPKZWGVNVOUIVNVOUJWHWCSKZWDSKZ
      WGWIWCTWDTISSUKZULZWJWKWGWLPUMZIVEWMUNWLWNIUOQSSWCWDIUPUQURUSUTVAVLVPVKVF
      VBVCVGVH $.
      $( [13-Nov-2013] $) $( [8-May-2007] $)

    ${
      $d a b v x y z $.  $d a b x y z D $.  $d v J $.
      tgioo.2 $e |- J = ( MetOpen ` D ) $.
      $( The topology generated by open intervals of reals is the same as the
