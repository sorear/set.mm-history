      CLURABCTUPCUQDUJUKVBUSIABJTULUN $.
      $( [7-Sep-2007] $) $( [9-Dec-2006] $)

    $( The base set of the metric for complex numbers. $)
    cnmetba $p |- CC = dom dom D $=
      ( cdm cc cxp cabs cmin ccom dmeqi cr wf absf subopr fco mp2an fdmi dmxpid
      eqtri eqtr2i ) ACZCDDEZCDTUATFGHZCUAAUBBIUAJUBDJFKUADGKUAJUBKLMUADJFGNOPR
      IDQS $.
      $( [30-Aug-2007] $)

    $( The absolute value metric determines a metric space on the complex
       numbers.  This theorem provides a link between complex numbers and
       metrics spaces, making metric space theorems available for use with
       complex numbers.  (Contributed by FL, 9-Oct-2006.) $)
    cnmet $p |- D e. Met $=
      ( vx vy vz cc cr wf cabs cmin cv wcel wa cfv cc0 wceq cnmetdval w3a caddc
      co cnex cxp ccom absf subopr fco mp2an feq1i mpbir subcl abs00 syl eqcomd
      wb eqeq1d subeq0 3bitr3d cle abs3dif abssub 3adant2 breqtrd 3adant3 3coml
      oveq1d oveq12d 3brtr4d ismeti ) CDEAFUAFFUBZGAHVIGIJUCZHZFGIHVIFJHVKUDUEV
      IFGIJUFUGVIGAVJBUHUICKZFLZDKZFLZMZVLVNJTZINZOPZVQOPZVLVNATZOPVLVNPVPVQFLV
      SVTUNVLVNUJVQUKULVPVRWAOVPWAVRVLVNABQZUMUOVLVNUPUQVMVOEKZFLZRZVRWCVLJTINZ
      WCVNJTINZSTZWAWCVLATZWCVNATZSTZURWEVRVLWCJTINZWGSTZWHURVLVNWCUSVMWDWMWHPV
      OVMWDMWLWFWGSVLWCUTVEVAVBVMVOWAVRPWDWBVCWDVMVOWKWHPWDVMVORWIWFWJWGSWDVMWI
      WFPVOWCVLABQVCWDVOWJWGPVMWCVNABQVAVFVDVGVH $.
      $( [7-Sep-2007] $) $( [6-Oct-2006] $)
  $}

  ${
    $d A f w x y z $.  $d B f w x y z $.  $d C w x y z $.  $d D w x y z $.
    $d J f w x y z $.  $d K f w x y $.
    cncfmet.1 $e |- C = ( ( abs o. - ) |` ( A X. A ) ) $.
    cncfmet.2 $e |- D = ( ( abs o. - ) |` ( B X. B ) ) $.
    cncfmet.3 $e |- J = ( MetOpen ` C ) $.
    cncfmet.4 $e |- K = ( MetOpen ` D ) $.
    $( Relate complex function continuity to metric space continuity.
       (Contributed by Paul Chapman, 26-Nov-2007.) $)
    cncfmet $p |- ( ( A C_ CC /\ B C_ CC ) ->
                       ( A -cn-> B ) = ( J Cn K ) ) $=
