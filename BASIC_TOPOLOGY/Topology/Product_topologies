      $( [2-Sep-2009] $)
  $}

  $( The product of two closed sets is closed in the product topology.
     (Contributed by Jeff Madsen, 2-Sep-2009.) $)
  txcld $p |- ( ( ( R e. Top /\ S e. Top ) /\ ( A e. ( Clsd ` R )
           /\ B e. ( Clsd ` S ) ) ) -> ( A X. B ) e. ( Clsd ` ( R tX S ) ) ) $=
