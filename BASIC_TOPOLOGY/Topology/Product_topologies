  ${
    $d f k r s t u w y z A $.  $d f k r s t u w x y z R $.  $d f u v w x z X $.
    $d f t w x y z ph $.  $d f r s t w y z S $.  $d f k r s t u v w x y z W $.
    $d f t u v w x y z Y $.
    txcmp.x $e |- X = U. R $.
    txcmp.y $e |- Y = U. S $.
    txcmp.r $e |- ( ph -> R e. Comp ) $.
    txcmp.s $e |- ( ph -> S e. Comp ) $.
    txcmp.w $e |- ( ph -> W C_ ( R tX S ) ) $.
    txcmp.u $e |- ( ph -> ( X X. Y ) = U. W ) $.
    ${
      txcmp.a $e |- ( ph -> A e. X ) $.
      txcmp.z $e |- ( ph -> Y =/= (/) ) $.
