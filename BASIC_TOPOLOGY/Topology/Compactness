      USVCUTUKUGUHULUIUJUMUNBCAUOUP $.
      $( [13-Jun-2011] $)
  $}

  $( The singleton of the empty set is compact.  (Contributed by FL,
     2-Aug-2009.) $)
  0cmp $p |- { (/) } e. Comp $=
    ( c0 csn ctop cfn cin wcel ccmp elin sn0top snfi mpbir2an fincmp ax-mp ) AB
