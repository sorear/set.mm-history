    $( A continuous function is a mapping.  (Contributed by FL,
       15-Dec-2006.) $)
    cnf $p |- ( ( J e. Top /\ K e. Top /\ F e. ( J Cn K ) ) ->
                F : X --> Y ) $=
      ( vx ctop wcel ccn co wf wa ccnv cv cima wral iscn simprbda 3impa ) BIJZC
      IJZABCKLJZDEAMZUBUCNUDUEAOHPQBJHCRHABCDEFGSTUA $.
      $( [8-Dec-2006] $)

    $( A continuous function at point ` P ` is a mapping.  (Contributed by FL,
       31-Dec-2006.) $)
    cnpf $p |- ( ( ( J e. Top /\ K e. Top /\ P e. X ) /\
        F e. ( ( J CnP K ) ` P ) ) -> F : X --> Y ) $=
      ( vx vy ctop wcel w3a ccnp co cfv wf cv cima wss wa wrex wi wral simprbda
      iscnp ) CKLDKLAELMBACDNOPLEFBQABPIRZLAJRZLBUHSUGTUAJCUBUCIDUDJIABCDEFGHUF
      UE $.
      $( [17-Nov-2006] $)

    $( The value of a continuous function from ` J ` to ` K ` at point ` P `
       belongs to the underlying set of topology ` K ` .  (Contributed by FL,
       31-Dec-2006.) $)
    cnpcl $p |- ( ( ( J e. Top /\ K e. Top /\ P e. X ) /\
        ( F e. ( ( J CnP K ) ` P ) /\ A e. X ) ) -> ( F ` A ) e. Y ) $=
      ( ctop wcel w3a ccnp co cfv wa wf cnpf adantrr simprr ffvelrn syl2anc ) D
      JKEJKBFKLZCBDEMNOKZAFKZPPFGCQZUEACOGKUCUDUFUEBCDEFGHIRSUCUDUETFGACUAUB $.
      $( [27-Dec-2006] $)
  $}

  ${
    $d x y A $.  $d F y x $.  $d J x y $.  $d K y $.  $d P x y $.  $d X y $.
    cnpimaex.1 $e |- X = U. J $.
    $( Property of a function continuous at a point.  (Contributed by FL,
       31-Dec-2006.) $)
    cnpimaex $p |- ( ( ( J e. Top /\ K e. Top /\ P e. X ) /\ (
                   F e. ( ( J CnP K ) ` P ) /\ A e. K /\ ( F ` P ) e. A ) ) ->
                    E. x e. J ( P e. x /\ ( F " x ) C_ A ) ) $=
      ( vy ctop wcel w3a ccnp co cfv cv wss wa wrex wi cima wral iscnp simplbda
      cuni wf eqid wceq eleq2 sseq2 anbi2d rexbidv imbi12d rcla4cv syl6 3imp2
      ex ) EJKFJKCGKLZDCEFMNOKZBFKZCDOZBKZCAPZKZDVCUAZBQZRZAESZURUSVAIPZKZVDVEV
      IQZRZAESZTZIFUBZUTVBVHTZTURUSVOURUSGFUEZDUFVOAICDEFGVQHVQUGUCUDUQVNVPIBFV
      IBUHZVJVBVMVHVIBVAUIVRVLVGAEVRVKVFVDVIBVEUJUKULUMUNUOUP $.
      $( [22-Dec-2006] $)

    $( A restricted identity function is a continuous function.  (Contributed
       by FL, 31-Dec-2006.) $)
    idcn $p |- ( J e. Top -> ( _I |` X ) e. ( J Cn J ) ) $=
      ( vy ctop wcel cid cres ccnv cima wa wb wss a1i wfun wceq ax-mp eqtrd syl
      cnvi ccn co wf cv wral iscn anidms wfn rnresi eqimssi fnresi jctil sylibr
      df-f funi eqcomi funeq sylib funcnvres imai reseq2d reseq1 syl6eq imaeq1d
      crn eltopss resiima simpr eqeltrd ralrimiva mpbir2and ) AEFZGBHZAAUAUBFZB
      BVMUCZVMIZDUDZJZAFZDAUEZVLVNVOVTKLDVMAABBCCUFUGVLVMBUHZVMVEZBMZKVOVLWCWAW
