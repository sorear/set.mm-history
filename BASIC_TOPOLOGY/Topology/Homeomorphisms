      BVPVAVBVCWTWBWMADWTWAWLCWTVTWKVPVNBVGVDVBVCVEVHTVFVI $.
      $( [30-May-2014] $) $( [14-Feb-2007] $)
  $}

  ${
    $d F x $.  $d J x $.  $d K x $.
    hmeomap.1 $e |- X = U. J $.
    hmeomap.2 $e |- Y = U. K $.
    $( A homeomorphism is a 1-1-onto mapping.  (Contributed by FL,
       5-Mar-2007.) $)
    hmeomap $p |- ( ( J e. Top /\ K e. Top ) ->
      ( F e. ( J Homeo K ) -> F : X -1-1-onto-> Y ) ) $=
      ( vx ctop wcel wa chomeosm co wf1o cv cima wral ccnv w3a ishomeo syl6bi
      simp1 ) BIJCIJKABCLMJDEANZAHOZPCJHBQZARUDPBJHCQZSUCHABCDEFGTUCUEUFUBUA $.
      $( [30-May-2014] $) $( [5-Mar-2007] $)
  $}

  ${
    $d F x $.  $d J x $.  $d K x $.
    $( The image of an open set by the converse of a homeomorphism is an open
       set.  (Contributed by FL, 5-Mar-2007.) $)
    hmeocna $p |- ( ( J e. Top /\ K e. Top ) ->
      ( F e. ( J Homeo K ) -> A. x e. K ( `' F " x ) e. J ) ) $=
      ( ctop wcel wa chomeosm co cuni wf1o cv cima wral ccnv eqid ishomeo simp3
      w3a syl6bi ) CEFDEFGBCDHIFCJZDJZBKZBALZMDFACNZBOUDMCFADNZSUFABCDUAUBUAPUB
      PQUCUEUFRT $.
      $( [1-Dec-2010] $) $( [5-Mar-2007] $)
  $}

  ${
    $d F x $.  $d J x $.  $d K x $.
    $( The image of an open set by a homeomorphism is an open set.
       (Contributed by FL, 5-Mar-2007.) $)
    hmeocnb $p |- ( ( J e. Top /\ K e. Top ) ->
      ( F e. ( J Homeo K ) -> A. x e. J ( F " x ) e. K ) ) $=
      ( ctop wcel wa chomeosm co cuni wf1o cv cima wral ccnv eqid ishomeo simp2
      w3a syl6bi ) CEFDEFGBCDHIFCJZDJZBKZBALZMDFACNZBOUDMCFADNZSUEABCDUAUBUAPUB
