  $( An integer is a member of a 0-based finite set of sequential integers iff
     its successor is a member of the corresponding 1-based set.  (Contributed
     by Paul Chapman, 22-Jun-2011.) $)
  elfzp1b $p |- ( ( K e. ZZ /\ N e. ZZ ) ->
                ( K e. ( 0 ... ( N - 1 ) ) <-> ( K + 1 ) e. ( 1 ... N ) ) ) $=
    ( cz wcel wa c1 caddc co cfz cmin cc0 wb peano2z fzsubel mpanl1 mpanr2 wceq
    1z cc ax-1cn sylan2 ancoms zcn sylancl subidi opreq1i eleq12d adantr bitr2d
    pncan a1i ) ACDZBCDZEAFGHZFBIHDZUNFJHZFFJHZBFJHZIHZDZAKURIHZDZUMULUOUTLZULU
    MUNCDZVCAMUMVDFCDZVCRVEUMVDVEEVCRUNFFBNOPUAUBULUTVBLUMULUPAUSVAULASDFSDUPAQ
    AUCTAFUJUDUSVAQULUQKURIFTUEUFUKUGUHUI $.
    $( [22-Jun-2011] $)

  ${
    $( Division of both sides of 'less than or equal to' by a nonnegative
       number.  (Contributed by Paul Chapman, 7-Sep-2007.) $)
    lediv2aALT $p |- ( ( ( A e. RR /\ 0 < A ) /\ ( B e. RR /\ 0 < B ) /\
                       ( C e. RR /\ 0 <_ C ) ) ->
                     ( A <_ B -> ( C / B ) <_ ( C / A ) ) ) $=
