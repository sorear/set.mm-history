  ${
    pm5.21nddOLDOLD.1 $e |- ( ph -> ( ch -> ps ) ) $.
    pm5.21nddOLDOLD.2 $e |- ( ph -> ( th -> ps ) ) $.
    pm5.21nddOLDOLD.3 $e |- ( ph -> ( ps -> ( ch <-> th ) ) ) $.
    $( Eliminate an antecedent implied by each side of a biconditional,
       deduction version.  (Moved into main set.mm as ~ pm5.21ndd and may be
       deleted by mathbox owner, PC. --NM 20-Sep-2013.) $)
    pm5.21nddOLDOLD $p |- ( ph -> ( ch <-> th ) ) $=
      ( pm5.21ndd ) ABCDEFGH $.
      $( [21-Nov-2012] $)
  $}

  $( One less than an integer greater than or equal to 2 is a positive
     integer.  (Contributed by Paul Chapman, 17-Nov-2012.) $)
  uz2m1nn $p |- ( N e. ( ZZ>= ` 2 ) -> ( N - 1 ) e. NN ) $=
    ( c2 cuz cfv wcel cz c1 clt wbr wa cmin co cn eluz2b1 wb znnsub mpan biimpa
    1z sylbi ) ABCDEAFEZGAHIZJAGKLMEZANUAUBUCGFEUAUBUCOSGAPQRT $.
    $( [23-Nov-2012] $) $( [17-Nov-2012] $)

  $( Membership in a curtailed finite sequence of integers.  (Contributed by
     Paul Chapman, 17-Nov-2012.) $)
  elfzm12 $p |- ( N e. NN ->
                  ( M e. ( 1 ... ( N - 1 ) ) -> M e. ( 1 ... N ) ) ) $=
    ( cn wcel c1 cmin co cfz cuz cfv wss cz nnz cle wbr cr zre wb mpbird syl
    caddc lep1 1re lesubadd mp3an2 anidms peano2zm eluz mpancom fzss2 sseld ) B
    CDZEBEFGZHGZEBHGZAULBUMIJDZUNUOKULBLDZUPBMUQUPUMBNOZUQBPDZURBQUSURBBEUAGNOZ
    BUBUSURUTRZUSEPDUSVAUCBEBUDUEUFSTUMLDUQUPURRBUGUMBUHUISTUMEBUJTUK $.
    $( [17-Nov-2012] $)

  ${
    $d F k $.  $d N k $.
    nn0seqcvg.1 $e |- F : NN0 --> NN0 $.
    nn0seqcvg.2 $e |- N = ( F ` 0 ) $.
    nn0seqcvg.3 $e |- ( k e. NN0 -> ( ( F ` ( k + 1 ) ) =/= 0 ->
                                      ( F ` ( k + 1 ) ) < ( F ` k ) ) ) $.
    $( A strictly-decreasing nonnegative integer sequence with initial term
       ` N ` reaches zero by the ` N ` th term.  Inference version.
       (Contributed by Paul Chapman, 31-Mar-2011.) $)
    nn0seqcvg $p |- ( F ` N ) = 0 $=
      ( c1 wceq cfv cc0 eqid cn0 wf a1i cv wcel caddc co wne clt wbr nn0seqcvgd
      wi adantl ax-mp ) GGHZCBIJHGKUFABCLLBMUFDNCJBIHUFENAOZLPUGGQRBIZJSUHUGBIT
      UAUCUFFUDUBUE $.
      $( [31-Mar-2011] $)
  $}

