$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Some properties of specific numbers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( The square of 1 is 1. $)
  sq1 $p |- ( 1 ^ 2 ) = 1 $=
    ( c1 c2 cexp co cmulc 1cn sqval mulid1 eqtr ) ABCDAAEDAAFGAFHI $.
    $([22-Aug-99]$)

  $( The square of 2 is 4. $)
  sq2 $p |- ( 2 ^ 2 ) = 4 $=
    ( c2 cexp co cmulc c4 2re recn sqval 2t2e4 eqtr ) AABCAADCEAAFGHIJ $.
    $([22-Aug-99]$)

  $( The square root of 1 is 1. $)
  sqr1 $p |- ( sqr ` 1 ) = 1 $=
    ( c1 cmulc co csqr cfv 1cn mulid1 fveq2i cc0 cle wbr wceq ax0re ax1re lt01
    ltlei sqrsqid ax-mp eqtr3 ) AABCZDEZADEATADAFGHIAJKUAALIAMNOPANQRS $.
    $([31-Jul-99]$)

  $( The square root of 4 is 2. $)
  sqr4 $p |- ( sqr ` 4 ) = 2 $=
    ( c4 csqr cfv c2 wceq cmulc co 2t2e4 cleqcomi cc0 clt wbr wb 4pos 2pos cle
    4re 2re sqrsq ax0re ltle syl2an mp2an mpbir ) ABCDEZADDFGZEZUFAHIJAKLZJDKLZ
    UEUGMZNOJAPLJDPLUJUHUIADQRSJATQUAJDTRUAUBUCUD $.
    $([3-Aug-99]$)

  $( The square root of 9 is 3. $)
  sqr9 $p |- ( sqr ` 9 ) = 3 $=
    ( c9 csqr cfv c3 wceq cmulc co 3t3e9 cleqcomi cc0 clt wbr wb 9pos 3pos cle
    9re 3re sqrsq ax0re ltle syl2an mp2an mpbir ) ABCDEZADDFGZEZUFAHIJAKLZJDKLZ
    UEUGMZNOJAPLJDPLUJUHUIADQRSJATQUAJDTRUAUBUCUD $.
    $([?]$)

  ${
    binom.1 $e |- A e. CC $.
    binom.2 $e |- B e. CC $.
    $( Binomial expansion. $)
    binom $p |- ( ( A + B ) ^ 2 ) = ( ( ( A ^ 2 ) + ( 2 x.
                ( A x. B ) ) ) + ( B ^ 2 ) ) $=
      ( caddc co cmulc c2 cexp addcl distr1 distr2 mulcom opreq2i eqtr
      opreq12i mulcl addass opreq1i eqtr3 3eqtr sqval 2times 3eqtr4 ) ABEFZUEGF
      ZAAGFZABGFZUHEFZEFZBBGFZEFZUEHIFAHIFZHUHGFZEFZBHIFZEFUFUEAGFZUEBGFZEFUGUH
      EFZUHUKEFZEFZULUEABABCDJZCDKUQUSURUTEUQUGBAGFZEFUSABACDCLVCUHUGEBADCMNOAB
      BCDDLPUSUHEFZUKEFVAULUSUHUKUGUHAACCQZABCDQZJVFBBDDQRVDUJUKEUGUHUHVEVFVFRS
      TUAUEVBUBUOUJUPUKEUMUGUNUIEACUBUHVFUCPBDUBPUD $.
      $([11-Aug-99]$)
  $}

  ${
    discrlem.1 $e |- A e. RR $.
    discrlem.2 $e |- B e. RR $.
    discrlem.3 $e |- C e. RR $.
    ${
      discrlem1.4 $e |- D = -u ( B / ( 2 x. A ) ) $.
      ${
        discrlem1.5 $e |- 0 < A $.
        $( Lemma for discriminant theorem. $)
        discrlem1 $p |- ( 0 <_
                     ( ( ( A x. ( D ^ 2 ) ) + ( B x. D ) ) + C ) <->
                     ( ( B ^ 2 ) - ( 4 x. ( A x. C ) ) ) <_ 0 ) $=
          ( cc0 c2 cexp co cmulc caddc cle wbr cneg c4 cmin 4re recn mulcl
