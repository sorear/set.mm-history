    divrecz $p |- ( A =/= 0 -> ( B / A ) = ( B x. ( 1 / A ) ) ) $=
      ( cc0 wne cdiv co c1 cmulc wceq cded opreq2 opreq2d cleq12d cc 1cn
      keepel elimne0 divrec dedth ) AEFZBAGHZBIAGHZJHZKBUBAILZGHZBIUFGHZJHZKAIA
