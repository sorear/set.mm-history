  $( Multiplication of a number and its reciprocal. $)
  recidt $p |- ( A e. CC -> ( A =/= 0_10 -> ( A x. ( 1_10 / A ) ) = 1_10 ) ) $=
    ( cc wcel cn0 wne cn1 cdiv co cmulc wceq wi cded neeq1 id opreq2 opreq12d
    cleq1d bi2imd 0cn elimel recidz dedth ) ABCZADEZAFAGHZIHZFJZKUCADLZDEZUHFUH
