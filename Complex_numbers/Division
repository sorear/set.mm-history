$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Division
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y z w $.
    $( Define division.  Theorem ~ divmul relates it to multiplication, and
       ~ divcl and ~ divrcl prove its closure laws. $)
    df-div $a |- / = { <. <. x , y >. , z >. |
                   ( ( x e. CC /\ y e. ( CC \ { 0_10 } ) ) /\
                   z = U. { w e. CC | ( y x. w ) = x } ) } $.
    $(
    Future simpler alternative?
    df-div $a |- / = { <. <. x , y >. , z >. |
                ( ( y e. CC /\ z e. CC ) /\  x = ( y x. z ) ) } $.
    $)
  $}


  ${
    $d x y z w A $.  $d x y z w B $.  $( ` y z w ` are dummy. $)
    divval.1 $e |- A e. CC $.
    divval.2 $e |- B e. CC $.
    divval.3 $e |- B =/= 0_10 $.
    $( Value of division: the (unique) element ` x ` such that
        ` ( B x. x ) = A `.  This is meaningful only when ` B ` is nonzero. $)
    divval $p |- ( A / B ) = U. { x e. CC | ( B x. x ) = A } $=
      ( vy vz vw cc wcel cn0 csn cdif cdiv co cv cmulc wceq crab cuni wn wa
      wne df-ne mpbi elisseti elsnc bineg mpbir pm3.2i eldif axcnex zfausrab
      zfun2 cleq2 rbiabdv unieqd opreq1 cleq1d df-div oprabval2 mp2an ) BJKCJLM
      ZNZKZBCOPCAQZRPZBSZAJTZUAZSDVFCJKZCVDKZUBZUCVLVNEVNCLSZUBZCLUDVPFCLUEUFVM
      VOCLCJEUGUHUIUJUKCJVDULUJGHIBCJVEHQZVGRPZGQZSZAJTZUAVKOVRBSZAJTZUAVJVIAJU
      MUNUOVSBSZWAWCWDVTWBAJVSBVRUPUQURVQCSZWCVJWEWBVIAJWEVRVHBVQCVGRUSUTUQURGH
      IAVAVBVC $.
      $([8-May-99]$)
  $}

  ${
    $d x A $.  $d x B $.  $d x C $.  $( ` x ` is dummy. $)
    divmul.1 $e |- A e. CC $.
    divmul.2 $e |- B e. CC $.
    divmul.3 $e |- C e. CC $.
    divmul.4 $e |- B =/= 0_10 $.
    $( Relationship between division and multiplication. $)
    divmul   $p |- ( ( A / B ) = C <-> ( B x. C ) = A ) $=
      ( vx cc wcel cdiv co wceq cmulc wb cv cleq2 opreq2 cleq1d bi2bid crab
      cuni wreu receux reuuni mpan2 cleqcom divval cleq2i bitr syl6rbbr
      vtoclga ax-mp ) CIJABKLZCMZBCNLZAMZOZFUNHPZMZBUSNLZAMZOURHCIUSCMZUTUOVBUQ
      USCUNQVCVAUPAUSCBNRSTUSIJZVBUSVBHIUAUBZMZUTVDVBHIUCVBVFOHBAEDGUDVBHIUEUFU
      TUSUNMVFUNUSUGUNVEUSHABDEGUHUIUJUKULUM $.
      $([2-Feb-95]$)
  $}

  ${
    divmulz.1 $e |- A e. CC $.
    divmulz.2 $e |- B e. CC $.
    divmulz.3 $e |- C e. CC $.
    $( Relationship between division and multiplication. $)
    divmulz   $p |- ( B =/= 0_10 -> ( ( A / B ) = C <-> ( B x. C ) = A ) ) $=
      ( cn0 wne cdiv co wceq cmulc wb cn1 cded opreq2 cleq1d opreq1 bi2bid cc
      1cn keepel elimne0 divmul dedth ) BGHZABIJZCKZBCLJZAKZMAUFBNOZIJZCKZUKCLJ
      ZAKZMBNBUKKZUHUMUJUOUPUGULCBUKAIPQUPUIUNABUKCLRQSAUKCDUFBNTEUAUBFBUCUDUE
      $.
      $([8-May-99]$)
  $}

  ${
    $d x A $.  $d x B $.  $( ` x ` is dummy. $)
    divcl.1 $e |- A e. CC $.
    divcl.2 $e |- B e. CC $.
    divcl.3 $e |- B =/= 0_10 $.
    $( Closure law for division. $)
    divcl   $p |- ( A / B ) e. CC $=
      ( vx cdiv co cv cmulc wceq cc crab cuni divval wreu wcel receux reucl
      ax-mp eqeltr ) ABGHBFIJHAKZFLMNZLFABCDEOUBFLPUCLQFBADCERUBFLSTUA $.
      $([2-Feb-95]$)
  $}

  ${
    divclz.1 $e |- A e. CC $.
    divclz.2 $e |- B e. CC $.
    $( Closure law for division. $)
    divclz   $p |- ( B =/= 0_10 -> ( A / B ) e. CC ) $=
      ( cn0 wne cdiv co cc wcel cn1 cded wceq opreq2 eleq1d 1cn keepel elimne0
      divcl dedth ) BEFZABGHZIJAUABKLZGHZIJBKBUCMUBUDIBUCAGNOAUCCUABKIDPQBRST
      $.
      $([7-May-99]$)
  $}

  $( Closure law for division. $)
  divclt   $p |- ( ( A e. CC /\ B e. CC ) ->
                 ( B =/= 0_10 -> ( A / B ) e. CC ) ) $=
    ( cc wcel cn0 wne cdiv co wi cded wceq opreq1 eleq1d bilimd neeq1 opreq2
    bi2imd 0cn elimel divclz dedth2h ) ACDZBCDZBEFZABGHZCDZIUDUBAEJZBGHZCDZIUCB
    EJZEFZUGUJGHZCDZIABEEAUGKZUFUIUDUNUEUHCAUGBGLMNBUJKZUDUKUIUMBUJEOUOUHULCBUJ
    UGGPMQUGUJAECRSBECRSTUA $.
    $([21-Jul-01]$)

  ${
    divcan.1 $e |- A e. CC $.
    divcan.2 $e |- B e. CC $.
    ${
      divcan.3 $e |- A =/= 0_10 $.
      $( A cancellation law for division. $)
      divcan2   $p |- ( A x. ( B / A ) ) = B $=
        ( cdiv co wceq cmulc cleqid divcl divmul mpbi ) BAFGZNHANIGBHNJBANDCBAD
        CEKELM $.
        $([9-Feb-95]$)

      $( A cancellation law for division. $)
      divcan1   $p |- ( ( B / A ) x. A ) = B $=
        ( cdiv co divcl divcan2 mulcoml ) ABAFGBCBADCEHDABCDEIJ $.
        $([18-May-99]$)
    $}

    $( A cancellation law for division. $)
    divcan1z $p |- ( A =/= 0_10 -> ( ( B / A ) x. A ) = B ) $=
      ( cn0 wne cdiv co cmulc wceq cn1 cded opreq2 id opreq12d cleq1d cc 1cn
      keepel elimne0 divcan1 dedth ) AEFZBAGHZAIHZBJBUCAKLZGHZUFIHZBJAKAUFJZUEU
      HBUIUDUGAUFIAUFBGMUINOPUFBUCAKQCRSDATUAUB $.
      $([2-Oct-99]$)

    $( A cancellation law for division. $)
    divcan2z $p |- ( A =/= 0_10 -> ( A x. ( B / A ) ) = B ) $=
      ( cn0 wne cdiv co cmulc wceq cn1 cded id opreq2 opreq12d cleq1d cc 1cn
      keepel elimne0 divcan2 dedth ) AEFZABAGHZIHZBJUCAKLZBUFGHZIHZBJAKAUFJZUEU
      HBUIAUFUDUGIUIMAUFBGNOPUFBUCAKQCRSDATUAUB $.
      $([10-Aug-99]$)
  $}

  ${
    divneq0.1 $e |- A e. CC $.
    divneq0.2 $e |- B e. CC $.
    divneq0.3 $e |- A =/= 0_10 $.
    divneq0.4 $e |- B =/= 0_10 $.
    $( The ratio of non-zero numbers is non-zero. $)
    divneq0   $p |- ( A / B ) =/= 0_10 $=
      ( cdiv co cn0 wne wceq wn cmulc 0cn divmul mulzer1 cleq1i cleqcom 3bitr
      bineg df-ne 3bitr4 mpbir ) ABGHZIJZAIJZEUDIKZLAIKZLUEUFUGUHUGBIMHZAKIAKUH
      ABICDNFOUIIABDPQIARSTUDIUAAIUAUBUC $.
      $([9-Feb-95]$)
  $}

  ${
    recneq0z.1 $e |- A e. CC $.
    $( The reciprocal of a non-zero number is non-zero. $)
    recneq0z $p |- ( A =/= 0_10 -> ( 1_10 / A ) =/= 0_10 ) $=
      ( cn0 wne cn1 cdiv co cded wceq opreq2 neeq1d 1cn cc keepel ax1ne0
      elimne0 divneq0 dedth ) ACDZEAFGZCDESAEHZFGZCDAEAUAITUBCAUAEFJKEUALSAEMBL
      NOAPQR $.
      $([14-May-99]$)
  $}

  ${
    recid.1 $e |- A e. CC $.
    recid.2 $e |- A =/= 0_10 $.
    $( Multiplication of a number and its reciprocal. $)
    recid   $p |- ( A x. ( 1_10 / A ) ) = 1_10 $=
      ( cn1 1cn divcan2 ) ADBECF $.
      $([9-Feb-95]$)
  $}

  ${
    recidz.1 $e |- A e. CC $.
