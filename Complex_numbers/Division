  ${
    divneq0.1 $e |- A e. CC $.
    divneq0.2 $e |- B e. CC $.
    divneq0.3 $e |- A =/= 0 $.
    divneq0.4 $e |- B =/= 0 $.
    $( The ratio of non-zero numbers is non-zero. $)
    divneq0   $p |- ( A / B ) =/= 0 $=
      ( cdiv co cc0 wne wceq wn cmulc 0cn divmul mulzer1 cleq1i cleqcom 3bitr
      bineg df-ne 3bitr4 mpbir ) ABGHZIJZAIJZEUDIKZLAIKZLUEUFUGUHUGBIMHZAKIAKUH
      ABICDNFOUIIABDPQIARSTUDIUAAIUAUBUC $.
      $([9-Feb-95]$)
  $}

  ${
    recneq0z.1 $e |- A e. CC $.
    $( The reciprocal of a non-zero number is non-zero. $)
    recneq0z $p |- ( A =/= 0 -> ( 1 / A ) =/= 0 ) $=
      ( cc0 wne c1 cdiv co cded wceq opreq2 neeq1d 1cn cc keepel ax1ne0
      elimne0 divneq0 dedth ) ACDZEAFGZCDESAEHZFGZCDAEAUAITUBCAUAEFJKEUALSAEMBL
      NOAPQR $.
      $([14-May-99]$)
  $}

  ${
    recid.1 $e |- A e. CC $.
    recid.2 $e |- A =/= 0 $.
    $( Multiplication of a number and its reciprocal. $)
    recid   $p |- ( A x. ( 1 / A ) ) = 1 $=
      ( c1 1cn divcan2 ) ADBECF $.
      $([9-Feb-95]$)
  $}

  ${
    recidz.1 $e |- A e. CC $.
