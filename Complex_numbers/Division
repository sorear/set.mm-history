$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Division
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y z w $.
    $( Define division.  Theorem ~ divmul relates it to multiplication, and
       ~ divcl and ~ divrcl prove its closure laws. $)
    df-div $a |- / = { <. <. x , y >. , z >. |
                   ( ( x e. CC /\ y e. ( CC \ { 0 } ) ) /\
                   z = U. { w e. CC | ( y x. w ) = x } ) } $.
    $(
    Future simpler alternative?
    df-div $a |- / = { <. <. x , y >. , z >. |
                ( ( y e. CC /\ z e. CC ) /\  x = ( y x. z ) ) } $.
    $)
  $}


  ${
    $d x y z w A $.  $d x y z w B $.  $( ` y z w ` are dummy. $)
    divval.1 $e |- A e. CC $.
    divval.2 $e |- B e. CC $.
    divval.3 $e |- B =/= 0 $.
    $( Value of division: the (unique) element ` x ` such that
        ` ( B x. x ) = A ` .  This is meaningful only when ` B ` is nonzero. $)
    divval $p |- ( A / B ) = U. { x e. CC | ( B x. x ) = A } $=
      ( vy vz vw cc wcel cc0 csn cdif cdiv co cv cmulc wceq crab cuni wn wa
      wne df-ne mpbi elisseti elsnc mtbir pm3.2i eldif mpbir axcnex zfausrab
      zfun2 cleq2 birabsdv unieqd opreq1 cleq1d df-div oprabval2 mp2an ) BJKCJL
      MZNZKZBCOPCAQZRPZBSZAJTZUAZSDVFCJKZCVDKZUBZUCVLVNEVMCLSZCLUDVOUBFCLUEUFCL
      CJEUGUHUIUJCJVDUKULGHIBCJVEHQZVGRPZGQZSZAJTZUAVKOVQBSZAJTZUAVJVIAJUMUNUOV
      RBSZVTWBWCVSWAAJVRBVQUPUQURVPCSZWBVJWDWAVIAJWDVQVHBVPCVGRUSUTUQURGHIAVAVB
      VC $.
      $([8-May-99]$)
  $}

  ${
    $d x A $.  $d x B $.  $d x C $.  $( ` x ` is dummy. $)
    divmul.1 $e |- A e. CC $.
    divmul.2 $e |- B e. CC $.
    divmul.3 $e |- C e. CC $.
    divmul.4 $e |- B =/= 0 $.
    $( Relationship between division and multiplication. $)
    divmul   $p |- ( ( A / B ) = C <-> ( B x. C ) = A ) $=
      ( vx cc wcel cdiv co wceq cmulc wb cv cleq2 opreq2 cleq1d bi2bid crab
      cuni wreu receu reuuni mpan2 cleqcom divval cleq2i bitr syl6rbbr vtoclga
      ax-mp ) CIJABKLZCMZBCNLZAMZOZFUNHPZMZBUSNLZAMZOURHCIUSCMZUTUOVBUQUSCUNQVC
      VAUPAUSCBNRSTUSIJZVBUSVBHIUAUBZMZUTVDVBHIUCVBVFOHBAEDGUDVBHIUEUFUTUSUNMVF
      UNUSUGUNVEUSHABDEGUHUIUJUKULUM $.
      $([2-Feb-95]$)
  $}

  ${
    divmulz.1 $e |- A e. CC $.
    divmulz.2 $e |- B e. CC $.
    divmulz.3 $e |- C e. CC $.
    $( Relationship between division and multiplication. $)
    divmulz   $p |- ( B =/= 0 -> ( ( A / B ) = C <-> ( B x. C ) = A ) ) $=
      ( cc0 wne cdiv co wceq cmulc wb c1 cded opreq2 cleq1d opreq1 bi2bid cc
      1cn keepel elimne0 divmul dedth ) BGHZABIJZCKZBCLJZAKZMAUFBNOZIJZCKZUKCLJ
      ZAKZMBNBUKKZUHUMUJUOUPUGULCBUKAIPQUPUIUNABUKCLRQSAUKCDUFBNTEUAUBFBUCUDUE
      $.
      $([8-May-99]$)
  $}

  ${
    $d x A $.  $d x B $.  $( ` x ` is dummy. $)
    divcl.1 $e |- A e. CC $.
    divcl.2 $e |- B e. CC $.
    divcl.3 $e |- B =/= 0 $.
    $( Closure law for division. $)
    divcl   $p |- ( A / B ) e. CC $=
      ( vx cdiv co cv cmulc wceq cc crab cuni divval wreu wcel receu reucl
      ax-mp eqeltr ) ABGHBFIJHAKZFLMNZLFABCDEOUBFLPUCLQFBADCERUBFLSTUA $.
      $([2-Feb-95]$)
  $}

  ${
    divclz.1 $e |- A e. CC $.
    divclz.2 $e |- B e. CC $.
    $( Closure law for division. $)
    divclz   $p |- ( B =/= 0 -> ( A / B ) e. CC ) $=
      ( cc0 wne cdiv co cc wcel c1 cded wceq opreq2 eleq1d 1cn keepel elimne0
      divcl dedth ) BEFZABGHZIJAUABKLZGHZIJBKBUCMUBUDIBUCAGNOAUCCUABKIDPQBRST
      $.
      $([7-May-99]$)
  $}

  $( Closure law for division. $)
  divclt   $p |- ( ( A e. CC /\ B e. CC ) ->
                 ( B =/= 0 -> ( A / B ) e. CC ) ) $=
    ( cc wcel cc0 wne cdiv co wi cded wceq opreq1 eleq1d bilimd neeq1 opreq2
    bi2imd 0cn elimel divclz dedth2h ) ACDZBCDZBEFZABGHZCDZIUDUBAEJZBGHZCDZIUCB
    EJZEFZUGUJGHZCDZIABEEAUGKZUFUIUDUNUEUHCAUGBGLMNBUJKZUDUKUIUMBUJEOUOUHULCBUJ
    UGGPMQUGUJAECRSBECRSTUA $.
    $([21-Jul-01]$)

  ${
    divcan.1 $e |- A e. CC $.
    divcan.2 $e |- B e. CC $.
    ${
      divcan.3 $e |- A =/= 0 $.
      $( A cancellation law for division. $)
      divcan2   $p |- ( A x. ( B / A ) ) = B $=
        ( cdiv co wceq cmulc cleqid divcl divmul mpbi ) BAFGZNHANIGBHNJBANDCBAD
        CEKELM $.
        $([9-Feb-95]$)

      $( A cancellation law for division. $)
      divcan1   $p |- ( ( B / A ) x. A ) = B $=
        ( cdiv co cmulc divcl mulcom divcan2 eqtr ) BAFGZAHGAMHGBMABADCEICJABCD
        EKL $.
        $([18-May-99]$)
    $}

    $( A cancellation law for division. $)
    divcan1z $p |- ( A =/= 0 -> ( ( B / A ) x. A ) = B ) $=
      ( cc0 wne cdiv co cmulc wceq c1 cded opreq2 id opreq12d cleq1d cc 1cn
      keepel elimne0 divcan1 dedth ) AEFZBAGHZAIHZBJBUCAKLZGHZUFIHZBJAKAUFJZUEU
      HBUIUDUGAUFIAUFBGMUINOPUFBUCAKQCRSDATUAUB $.
      $([2-Oct-99]$)

    $( A cancellation law for division. $)
    divcan2z $p |- ( A =/= 0 -> ( A x. ( B / A ) ) = B ) $=
      ( cc0 wne cdiv co cmulc wceq c1 cded id opreq2 opreq12d cleq1d cc 1cn
      keepel elimne0 divcan2 dedth ) AEFZABAGHZIHZBJUCAKLZBUFGHZIHZBJAKAUFJZUEU
      HBUIAUFUDUGIUIMAUFBGNOPUFBUCAKQCRSDATUAUB $.
      $([10-Aug-99]$)
  $}

  $( A cancellation law for division. $)
  divcan2t $p |- ( ( A e. CC /\ B e. CC ) ->
                 ( A =/= 0 -> ( A x. ( B / A ) ) = B ) ) $=
    ( cc wcel cc0 wne cdiv co cmulc wceq wi cded neeq1 id opreq2 opreq12d
    cleq1d bi2imd opreq1 opreq2d cleq12d bilimd 0cn elimel divcan2z dedth2h )
    ACDZBCDZAEFZABAGHZIHZBJZKUGAELZEFZUMBUMGHZIHZBJZKUNUMUHBELZUMGHZIHZURJZKABE
    EAUMJZUIUNULUQAUMEMVBUKUPBVBAUMUJUOIVBNAUMBGOPQRBURJZUQVAUNVCUPUTBURVCUOUSU
    MIBURUMGSTVCNUAUBUMURAECUCUDBECUCUDUEUF $.
