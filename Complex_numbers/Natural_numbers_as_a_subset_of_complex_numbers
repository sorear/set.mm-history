  $( Subtraction of natural numbers. $)
  nnsubt $p |- ( ( A e. NN /\ B e. NN ) -> ( A < B <-> ( B - A ) e. NN ) ) $=
    ( cn wcel clt wbr cmin co wb cn1 cded wceq breq1 opreq2 eleq1d bi2bid
    breq2 opreq1 peano1c elimel nnsub dedth2h ) ACDZBCDZABEFZBAGHZCDZIUCAJKZBEF
