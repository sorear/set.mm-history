$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Natural numbers (as a subset of complex numbers)
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y $.
    $( The natural numbers of analysis start at one (unlike the natural
       numbers of set theory, i.e. the members of set omega, which start at
       zero).  This is the convention used by most analysis books.  See
       ~ ind for the principle of mathematical induction. $)
    df-n  $a |- NN =
       |^| { x | ( 1_10 e. x /\ A. y ( y e. x -> ( y + 1_10 ) e. x ) ) } $.
  $}

  ${
    $d x y A $.  $( ` y ` is dummy. $)
    peano5c.1 $e |- A e. V $.
    $( Peano's inductive postulate. $)
    peano5c   $p |- ( ( 1_10 e. A /\
                 A. x ( x e. A -> ( x + 1_10 ) e. A ) ) -> NN (_ A ) $=
      ( vy cn1 wcel cv caddc co wi wal wa wel cab cint wss cn wceq eleq2
      bi2imd bialdv bi2and abcl intss sylbir df-n sseq1i sylibr ) EBFZAGZBFZUJE
      HIZBFZJZAKZLZEDGZFZADMZULUQFZJZAKZLZDNZOZBPZQBPUPBVDFVFVCUPDBCUQBRZURUIVB
      UOUQBESVGVAUNAVGUSUKUTUMUQBUJSUQBULSTUAUBUCBVDUDUEQVEBDAUFUGUH $.
      $([10-Jan-97]$)
  $}

  $( ` x ` is dummy. $)
  $( The natural numbers are a subset of the reals. $)
  nnssre $p |- NN (_ RR $=
    ( vx cn1 cr wcel cv caddc co wi wal cn wss ax1re axaddrcl mpan2 ax-gen cc
    axcnex axresscn ssexi peano5c mp2an ) BCDZAEZCDZUCBFGCDZHZAIJCKLUFAUDUBUELU
    CBMNOACCPQRSTUA $.
    $([10-Jan-97]$)

  $( A natural number is a real number. $)
  nnret $p |- ( A e. NN -> A e. RR ) $=
    ( cn cr nnssre ssi ) BCADE $.
    $([18-Aug-99]$)

  $( A natural number is a complex number. $)
  nncnt $p |- ( A e. NN -> A e. CC ) $=
    ( cn wcel nnret recnd ) ABCAADE $.
    $([18-Aug-99]$)

  ${
    nnre.1 $e |- A e. NN $.
    $( A natural number is a real number. $)
    nnre $p |- A e. RR $=
      ( cn wcel cr nnret ax-mp ) ACDAEDBAFG $.
      $([18-Aug-99]$)

    $( A natural number is a complex number. $)
    nncn $p |- A e. CC $=
      ( cn wcel cc nncnt ax-mp ) ACDAEDBAFG $.
      $([18-Aug-99]$)
  $}

  $( The set of natural numbers exists. $)
  nnex $p |- NN e. V $=
    ( cn cr cc axcnex axresscn ssexi nnssre ) ABBCDEFGF $.
    $([3-Oct-99]$)

  ${
    $d x y z A $.  $( ` x y z ` are dummy. $)
    $( Peano postulate: 1 is a natural number. $)
    peano1c $p |- 1_10 e. NN $=
      ( vx vy cn1 cn wcel cv wel caddc co wi wal wa cab cint cc 1cn elisseti
      inteqab pm3.26 mpgbir df-n eleq2i mpbir ) CDECCAFZEZBAGBFCHIUDEJBKZLZAMNZ
      EZUIUGUEJAUGACCOPQRUEUFSTDUHCABUAUBUC $.
      $([11-Jan-97]$)

    $( Peano postulate: a successor of a natural number is a natural number. $)
    peano2c $p |- ( A e. NN -> ( A + 1_10 ) e. NN ) $=
      ( vz vx vy cv cn1 caddc co cn wcel wceq opreq1 eleq1d wel wi wal wa cab
      cint weq a13b bi2imd a4b1 adantl a2i 19.20i visset inteqab oprex 3imtr4
      df-n eleq2i vtoclga ) BEZFGHZIJZAFGHZIJBAIUNAKUOUQIUNAFGLMUNFCEZJZDCNZDEZ
      FGHZURJZOZDPZQZCRSZJZUOVGJZUNIJUPVFBCNZOZCPVFUOURJZOZCPVHVIVKVMCVFVJVLVEV
      JVLOZUSVDVNDBDBTZUTVJVCVLDBCUAVOVBUOURVAUNFGLMUBUCUDUEUFVFCUNBUGUHVFCUOUN
      FGUIUHUJIVGUNCDUKZULIVGUOVPULUJUM $.
      $([11-Jan-97]$)

  $}

  ${
    $d x y $.  $d x A $.  $d x ps $.  $d x ch $.  $d x th $.  $d x ta $.
    $d y ph $.
    $( Substitutions. $)
    ind.1 $e |- ( x = 1_10 -> ( ph <-> ps ) ) $.
    ind.2 $e |- ( x = y -> ( ph <-> ch ) ) $.
    ind.3 $e |- ( x = ( y + 1_10 ) -> ( ph <-> th ) ) $.
    ind.4 $e |- ( x = A -> ( ph <-> ta ) ) $.
    $( Basis. $)
    ind.5 $e |- ps $.
    $( Induction hypothesis. $)
    ind.6 $e |- ( y e. NN -> ( ch -> th ) ) $.
    $( Principle of Mathematical Induction (inference schema).  The first
       four hypotheses give us the substitution instances we need; the last
       two are the basis and the induction hypothesis.  See ~ nnaddclt for
       an example of its use. $)
    ind $p |- ( A e. NN -> ta ) $=
      ( cn wcel cv wa cab cn1 caddc co wi wal wss peano1c pm3.2i cc 1cn
      elisseti wceq eleq1 bi2and abcl mpbir pm3.26 ssab2i ssi abid2 eleq2i
      peano2c a1i im2and visset weq oprex 3imtr4g sylbir mpcom ax-gen nnex
      zfausab peano5c mp2an abclg mpbid pm3.27d ) HOPZVREVRHFQZOPZARZFSZPVRERZO
      WBHTWBPZGQZWBPZWETUAUBZWBPZUCZGUDOWBUEWDTOPZBRZWJBUFMUGWAWKFTTUHUIUJVSTUK
      VTWJABVSTOULIUMUNUOWIGWEVTFSZPZWFWHWBWLWEWAVTFVTAUPUQURWMWEOPZWIOWLWEFOUS
      UTWNWNCRZWGOPZDRZWFWHWNWNWPCDWNWPUCWNWEVAVBNVCWAWOFWEGVDFGVEVTWNACVSWEOUL
      JUMUNWAWQFWGWETUAVFVSWGUKVTWPADVSWGOULKUMUNVGVHVIVJGWBAFOVKVLVMVNURWAWCFH
      OVSHUKVTVRAEVSHOULLUMVOVPVQ $.
      $([10-Jan-97]$)
  $}

  ${
    $d x y A $.  $d x y B $. $( ` x y ` are dummy. $)
    $( Closure of addition of natural numbers, proved by induction on the
       second addend. $)
    nnaddclt $p |- ( ( A e. NN /\ B e. NN ) -> ( A + B ) e. NN ) $=
      ( vx vy cn wcel caddc co cv wi cn1 wceq opreq2 eleq1d bilimd weq peano2c
      wa cc 1cn axaddass mp3an3 nncnt syl2an syl5bi exp comm a2d ind imp ) AEFZ
      BEFZABGHZEFZULUKUNUKACIZGHZEFZJUKAKGHZEFZJUKADIZGHZEFZJUKAUTKGHZGHZEFZJUK
      UNJCDBUOKLZUQUSUKVFUPUREUOKAGMNOCDPZUQVBUKVGUPVAEUOUTAGMNOUOVCLZUQVEUKVHU
      PVDEUOVCAGMNOUOBLZUQUNUKVIUPUMEUOBAGMNOAQUTEFZUKVBVEUKVJVBVEJZUKVJVKUKVJR
      ZVAKGHZEFVEVBVLVMVDEASFZUTSFZVMVDLZUKVJVNVOKSFVPTAUTKUAUBAUCUTUCUDNVAQUEU
      FUGUHUIUGUJ $.
      $([12-Jan-97]$)
  $}

  ${
    $d x y A $.  $d x y B $. $( ` x y ` are dummy. $)
    $( Closure of multiplication of natural numbers. $)
    nnmulclt $p |- ( ( A e. NN /\ B e. NN ) -> ( A x. B ) e. NN ) $=
      ( vx vy cn wcel cmulc co cv wi cn1 caddc wceq opreq2 eleq1d bilimd weq
      cc nncnt ax1id biimprd mpcom wa 1cn axdistr mp3an3 opreq2d adantr eqtrd
      syl2an nnaddclt ancoms syl5bir exp4b pm2.43b a2d ind imp ) BEFZAEFZABGHZE
      FZUSUTVBUTACIZGHZEFZJUTAKGHZEFZJUTADIZGHZEFZJUTAVHKLHZGHZEFZJUTVBJCDBVCKM
      ZVEVGUTVNVDVFEVCKAGNOPCDQZVEVJUTVOVDVIEVCVHAGNOPVCVKMZVEVMUTVPVDVLEVCVKAG
      NOPVCBMZVEVBUTVQVDVAEVCBAGNOPARFZUTVGASZVRVGUTVRVFAEATZOUAUBVHEFZUTVJVMWA
      UTVJVMJUTWAUTVJVMUTWAUCZVMVIALHZEFZUTVJUCWBVLWCEVRVHRFZVLWCMUTWAVRWEUCVLV
      IVFLHZWCVRWEKRFVLWFMUDAVHKUEUFVRWFWCMWEVRVFAVILVTUGUHUIVSVHSUJOVJUTWDVIAU
      KULUMUNUOUPUQURUL $.
      $([12-Jan-97]$)
  $}

  ${
    $d x A $.  $d x B $.
    $( There exists a natural number g.e. than any two others. $)
    nn2get $p |- ( ( A e. NN /\ B e. NN ) ->
                E. x e. NN ( A <_ x /\ B <_ x ) ) $=
      ( cn wcel wa cle wbr wo cv wrex cr letrit nnret syl2an wi leidt biantrud
      biimpd syl anc2li wceq breq2 bi2and rcla4ev syl6 adantl biantrurd adantr
      jaod mpd ) BDEZCDEZFZBCGHZCBGHZIZBAJZGHZCURGHZFZADKZBLEZCLEZUQULUMBCMBNZC
      NZOUNUOVBUPUMUOVBPULUMUOUMUOCCGHZFZFVBUMUOVHUMVDUOVHPVFVDUOVHVDVGUOCQRSTU
      AVAVHACDURCUBUSUOUTVGURCBGUCURCCGUCUDUEUFUGULUPVBPUMULUPULBBGHZUPFZFVBULU
      PVJULVCUPVJPVEVCUPVJVCVIUPBQUHSTUAVAVJABDURBUBUSVIUTUPURBBGUCURBCGUCUDUEU
      FUIUJUK $.
      $([18-Aug-99]$)
  $}

  ${
    $d x y A $.
    $( A natural number is one or greater. $)
    nnge1t $p |- ( A e. NN -> 1_10 <_ A ) $=
      ( vx vy cn1 cv cle wbr caddc co breq2 wceq cleqid ax1re eqle ax-mp cn
      wcel cr wi nnret cn0 cc recnt ax0id syl breq2d clt wn lt01 wa jctl ax0re
      axltadd mp3an1 mpi axlttrn mp3an3 axaddrcl mpan2 sylan13 exp3a mpd
      pm2.16d wb jctil leltt 3imtr4d sylbird ind ) DBEZFGDDFGZDCEZFGZDVLDHIZFGZ
      DAFGBCAVJDDFJVJVLDFJVJVNDFJVJADFJDDKVKDLDDMMNOVLPQVLRQZVMVOSVLTVPVMDVLUAH
      IZFGZVOVPVQVLDFVPVLUBQVQVLKVLUCVLUDUEUFVPVQDUGGZUHZVNDUGGZUHZVRVOVPWAVSVP
