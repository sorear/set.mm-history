  $( Natural number ordering relation. $)
  nnltle1 $p |- ( ( A e. NN /\ B e. NN ) ->
                ( A < B <-> ( A + 1_10 ) <_ B ) ) $=
    ( cn wcel wa clt wbr cn1 caddc co cle cr wb ax1re ltadd1t mp3an3 nnret
    syl2an nnlelt1 peano2c sylan bitr4d ) ACDZBCDZEABFGZAHIJZBHIJFGZUFBKGZALDZB
