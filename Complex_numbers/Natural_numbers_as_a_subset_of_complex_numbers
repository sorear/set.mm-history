      EDVNVFUEVGVHUEVI $.
      $([25-Aug-99]$)
  $}

  $( A natural number is positive. $)
  nngt0t $p |- ( A e. NN -> 0_10 < A ) $=
    ( cr wcel cn1 cle wbr cn0 clt cn lt01 wa wi ax1re ax0re ltletrt mp3an1
    mpan mpani nnret nnge1t syl13 ) ABCZDAEFZGAHFZAICUBGDHFZUCUDJDBCZUBUEUCKUDL
    ZMGBCUFUBUGNGDAOPQRASATUA $.
    $([26-Sep-99]$)

  $( Zero is not a natural number. $)
  0nnn $p |- -. 0_10 e. NN $=
    ( cn0 cn wcel cn1 cle wbr clt wn lt01 ax1re ax0re lelt bicon2 mpbi nnge1t
    mto ) ABCDAEFZADGFZQHIQRDAJKLMNAOP $.
    $([25-Aug-99]$)

  $( A natural number is non-zero. $)
  nnne0t $p |- ( A e. NN -> A =/= 0_10 ) $=
    ( cn wcel cn0 wceq wn wne 0nnn eleq1 mtbiri con2 df-ne sylibr ) ABCZADEZFAD
    GONONDBCHADBIJKADLM $.
    $([27-Sep-99]$)

  ${
    nngt0.1 $e |- A e. NN $.
    $( A natural number is positive. $)
    nngt0 $p |- 0_10 < A $=
      ( cn wcel cn0 clt wbr nngt0t ax-mp ) ACDEAFGBAHI $.
      $([17-Sep-99]$)

    $( A natural number is non-zero. $)
    nnne0 $p |- A =/= 0_10 $=
      ( nnre nngt0 gt0ne0i ) AABCABDE $.
      $([25-Aug-99]$)
  $}

