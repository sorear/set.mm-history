$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Ordering on reals
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Define "less than or equal to" on real subset of compex numbers. $)
  df-le $a |- <_ = ( ( RR X. RR ) \ `' < ) $.

  $( "L.e." expressed in terms of "less than". $)
  leltt $p |- ( ( A e. RR /\ B e. RR ) -> ( A <_ B <-> -. B < A ) ) $=
    ( cr wcel wa cle wbr cop clt ccnv wn cxp wb opelxpi cdif df-le eleq2i
    eldif bitr baib syl df-br syl5bb opelcnvg syl6rbbr binegd bitr4d ) ACDBCDEZ
    ABFGZABHZIJZDZKZBAIGZKUHUJFDZUMUIUHUJCCLZDZUOUMMABCCNUOUQUMUOUJUPUKOZDUQUME
    FURUJPQUJUPUKRSTUAABFUBUCUHUNULUHULBAHIDUNABCCIUDBAIUBUEUFUG $.
    $([13-May-99]$)

  ${
    $d x y z $. $( ` x y z ` are dummy. $)
    $( "Less than" is a strict ordering. $)
    ltso $p |- < Or RR $=
      ( vx vy vz cr clt cv wcel w3a wbr weq wo wn wb wa wi axlttri 3adant3
      axlttrn jca so ) ABCDEAFZDGZBFZDGZCFZDGZHUAUCEIZABJUCUAEIKLMZUGUCUEEINUAU
      EEIOUBUDUHUFUAUCPQUAUCUERST $.
      $([19-Jan-97]$)
  $}

  $( Trichotomy law. $)
  lttri3t $p |- ( ( A e. RR /\ B e. RR ) ->
                ( A = B <-> ( -. A < B /\ -. B < A ) ) ) $=
    ( cr clt wor wcel wa wceq wbr wn wb sotrieq2 ltso mpan ) CDEACFBCFGABHABDIJ
    BADIJGKCABDLMN $.
    $([5-May-99]$)

  $( Trichotomy law. $)
  letri3t $p |- ( ( A e. RR /\ B e. RR ) ->
                ( A = B <-> ( A <_ B /\ B <_ A ) ) ) $=
    ( cr wcel wa wceq clt wbr wn cle lttri3t ancom syl6bbr leltt wb ancoms
    bi2and bitr4d ) ACDZBCDZEZABFZBAGHIZABGHIZEZABJHZBAJHZEUAUBUDUCEUEABKUCUDLM
    UAUFUCUGUDABNTSUGUDOBANPQR $.
    $([14-May-99]$)

  $( "L.e." expressed in terms of less than or equals. $)
  leloet $p |- ( ( A e. RR /\ B e. RR ) ->
               ( A <_ B <-> ( A < B \/ A = B ) ) ) $=
    ( cr wcel wa cle wbr clt wn wceq wo leltt wb wor sotric ltsor mpan ancoms
    bicon2d cleqcom biror orcom bitr syl5rbbr bitrd ) ACDZBCDZEZABFGBAHGZIZABHG
    ZABJZKZABLUHBAJZUKKZUJUMUHUIUOUGUFUIUOIMZCHNUGUFEUPCBAHOPQRSUOULUKKUMUNULUK
    BATUAULUKUBUCUDUE $.
    $([13-May-99]$)

  $( L.e. implies less than is not equals. $)
  leltne $p |- ( ( A e. RR /\ B e. RR ) ->
               ( A <_ B -> ( A < B <-> -. A = B ) ) ) $=
    ( cr wcel wa cle wbr clt wceq wn wb wi lttri3t pm3.26 syl6bi adantr wo
    leloet biimpa ord impbid bicon2d exp ) ACDBCDEZABFGZABHGZABIZJKUDUEEZUGUFUH
    UGUFJZUDUGUILUEUDUGUIBAHGJZEUIABMUIUJNOPUHUFUGUDUEUFUGQABRSTUAUBUC $.
    $([27-Jul-99]$)

  $( Transitive law. $)
  lelttrt $p |- ( ( A e. RR /\ B e. RR /\ C e. RR ) ->
             ( ( A <_ B /\ B < C ) -> A < C ) ) $=
    ( cr wcel w3a cle wbr clt wceq wo wi wb leloet 3adant3 axlttrn exp3a breq1
    biimprd a1i jaod sylbid imp3a ) ADEZBDEZCDEZFZABGHZBCIHZACIHZUGUHABIHZABJZK
    ZUIUJLZUDUEUHUMMUFABNOUGUKUNULUGUKUIUJABCPQULUNLUGULUJUIABCIRSTUAUBUC $.
    $([23-May-99]$)

  $( "Less than" is irreflexive. $)
  ltnrt $p |- ( A e. RR -> -. A < A ) $=
    ( cr clt wor wcel wbr wn sonr ltsor mpan ) BCDABEAACFGBACHIJ $.
    $([18-Aug-99]$)

  $( "Less than or equals" is reflexive. $)
  leidt $p |- ( A e. RR -> A <_ A ) $=
    ( cr wcel cle wbr wa clt wceq wo leloet wn cleqid a1i orri mpbiri anabss )
    ABCZAADEZQQFRAAGEZAAHZIAAJSTTSKALMNOP $.
    $([18-Aug-99]$)

  ${
    lt.1 $e |- A e. RR $.

    ${
      lt.2 $e |- B e. RR $.

      $( Consequence of trichotomy. $)
      lttri2 $p |- ( -. A = B <-> ( A < B \/ B < A ) ) $=
        ( clt wbr wo wceq wn cr wor wcel wa wb sotrieq ltso pm3.2i mp2an bicom
        bicon1 ) ABEFBAEFGZABHZUBUAIZJEKAJLZBJLZMUBUCNJABEOPUDUECDQRST $.
        $([19-Jan-97]$)

