      letri3 $p |- ( A = B <-> ( A <_ B /\ B <_ A ) ) $=
        ( cr wcel wceq cle wbr wa wb letri3t mp2an ) AEFBEFABGABHIBAHIJKABLCDM
        $.
        $([14-May-99]$)

