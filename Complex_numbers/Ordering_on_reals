        ltlei $p |- A <_ B $=
          ( clt wbr cle ltle ax-mp ) ABFGABHGEABCDIJ $.
          $([22-Aug-99]$)
      $}

      $( Equality implies l.e. $)
      eqle $p |- ( A = B -> A <_ B ) $=
        ( wceq cle wbr wa letri3 pm3.26 sylbi ) ABEABFGZBAFGZHLABCDILMJK $.
        $([23-May-99]$)

      $( L.t. implies not equal. $)
      ltneq $p |- ( A < B -> -. A = B ) $=
        ( clt wbr wceq wn wo lttri2 biimpr orci ) ABEFZBAEFZABGHZOMNIABCDJKL $.
        $([28-Jul-99]$)

      $( L.t. implies not equal. $)
      ltne $p |- ( A < B -> A =/= B ) $=
        ( clt wbr wceq wn wne ltneq df-ne sylibr ) ABEFABGHABIABCDJABKL $.
        $([14-May-99]$)

