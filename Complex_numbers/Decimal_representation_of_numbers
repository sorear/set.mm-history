$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Decimal representation of numbers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c 2 $. $( The decimal digit 2 $)
  $c 3 $. $( The decimal digit 3 $)
  $c 4 $. $( The decimal digit 4 $)
  $c 5 $. $( The decimal digit 5 $)
  $c 6 $. $( The decimal digit 6 $)
  $c 7 $. $( The decimal digit 7 $)
  $c 8 $. $( The decimal digit 8 $)
  $c 9 $. $( The decimal digit 9 $)

  $( Extend class notation to include the number 2. $)
  cl2 $a class 2 _10 $.
  $( Extend class notation to include the number 3. $)
  cl3 $a class 3 _10 $.
  $( Extend class notation to include the number 4. $)
  cl4 $a class 4 _10 $.
  $( Extend class notation to include the number 5. $)
  cl5 $a class 5 _10 $.
  $( Extend class notation to include the number 6. $)
  cl6 $a class 6 _10 $.
  $( Extend class notation to include the number 7. $)
  cl7 $a class 7 _10 $.
  $( Extend class notation to include the number 8. $)
  cl8 $a class 8 _10 $.
  $( Extend class notation to include the number 9. $)
  cl9 $a class 9 _10 $.

  $( Note that the numbers 0 and 1 are primitive constants in the axiom
     system. $)
  $( Define the number 2. $)
  df-2 $a |- 2 _10 = ( 1 _10 + 1 _10 ) $.
  $( Define the number 3. $)
  df-3 $a |- 3 _10 = ( 2 _10 + 1 _10 ) $.
  $( Define the number 4. $)
  df-4 $a |- 4 _10 = ( 3 _10 + 1 _10 ) $.
  $( Define the number 5. $)
  df-5 $a |- 5 _10 = ( 4 _10 + 1 _10 ) $.
  $( Define the number 6. $)
  df-6 $a |- 6 _10 = ( 5 _10 + 1 _10 ) $.
  $( Define the number 7. $)
  df-7 $a |- 7 _10 = ( 6 _10 + 1 _10 ) $.
  $( Define the number 8. $)
  df-8 $a |- 8 _10 = ( 7 _10 + 1 _10 ) $.
  $( Define the number 9. $)
  df-9 $a |- 9 _10 = ( 8 _10 + 1 _10 ) $.

  $( The number 2 is real. $)
  2re $p |- 2 _10 e. RR $=
    ( cl2 cr wcel cn1 caddc co df-2 eleq1i ax1re addrcl mpbir ) ABCDDEFZBCALBGH
    DDIIJK $.
    $([27-May-99]$)

  $( The number 3 is real. $)
  3re $p |- 3 _10 e. RR $=
    ( cl3 cr wcel cl2 cn1 caddc co df-3 eleq1i 2re ax1re addrcl mpbir ) ABCDEFG
    ZBCANBHIDEJKLM $.
    $([27-May-99]$)

  $( The number 4 is real. $)
  4re $p |- 4 _10 e. RR $=
    ( cl4 cr wcel cl3 cn1 caddc co df-4 eleq1i 3re ax1re addrcl mpbir ) ABCDEFG
    ZBCANBHIDEJKLM $.
    $([27-May-99]$)

  $( The number 5 is real. $)
  5re $p |- 5 _10 e. RR $=
    ( cl5 cr wcel cl4 cn1 caddc co df-5 eleq1i 4re ax1re addrcl mpbir ) ABCDEFG
    ZBCANBHIDEJKLM $.
    $([27-May-99]$)

  $( The number 6 is real. $)
  6re $p |- 6 _10 e. RR $=
    ( cl6 cr wcel cl5 cn1 caddc co df-6 eleq1i 5re ax1re addrcl mpbir ) ABCDEFG
    ZBCANBHIDEJKLM $.
    $([27-May-99]$)

  $( The number 7 is real. $)
  7re $p |- 7 _10 e. RR $=
    ( cl7 cr wcel cl6 cn1 caddc co df-7 eleq1i 6re ax1re addrcl mpbir ) ABCDEFG
    ZBCANBHIDEJKLM $.
    $([27-May-99]$)

  $( The number 8 is real. $)
  8re $p |- 8 _10 e. RR $=
    ( cl8 cr wcel cl7 cn1 caddc co df-8 eleq1i 7re ax1re addrcl mpbir ) ABCDEFG
    ZBCANBHIDEJKLM $.
    $([27-May-99]$)

  $( The number 9 is real. $)
  9re $p |- 9 _10 e. RR $=
    ( cl9 cr wcel cl8 cn1 caddc co df-9 eleq1i 8re ax1re addrcl mpbir ) ABCDEFG
    ZBCANBHIDEJKLM $.
    $([27-May-99]$)

  $( The number 2 is positive. $)
  2pos $p |- 0 _10 < 2 _10 $=
    ( cn0 cn1 caddc co cl2 clt ax1re lt01 addgt0i df-2 breqtrr ) ABBCDEFBBGGHHI
    JK $.
    $([27-May-99]$)

  $( The number 3 is positive. $)
  3pos $p |- 0 _10 < 3 _10 $=
    ( cn0 cl2 cn1 caddc co cl3 clt 2re ax1re 2pos lt01 addgt0i df-3 breqtrr )
    ABCDEFGBCHIJKLMN $.
    $([27-May-99]$)

  $( The number 4 is positive. $)
  4pos $p |- 0 _10 < 4 _10 $=
    ( cn0 cl3 cn1 caddc co cl4 clt 3re ax1re 3pos lt01 addgt0i df-4 breqtrr )
    ABCDEFGBCHIJKLMN $.
    $([27-May-99]$)

  $( The number 5 is positive. $)
  5pos $p |- 0 _10 < 5 _10 $=
    ( cn0 cl4 cn1 caddc co cl5 clt 4re ax1re 4pos lt01 addgt0i df-5 breqtrr )
    ABCDEFGBCHIJKLMN $.
    $([27-May-99]$)

  $( The number 6 is positive. $)
  6pos $p |- 0 _10 < 6 _10 $=
    ( cn0 cl5 cn1 caddc co cl6 clt 5re ax1re 5pos lt01 addgt0i df-6 breqtrr )
    ABCDEFGBCHIJKLMN $.
    $([27-May-99]$)

  $( The number 7 is positive. $)
  7pos $p |- 0 _10 < 7 _10 $=
    ( cn0 cl6 cn1 caddc co cl7 clt 6re ax1re 6pos lt01 addgt0i df-7 breqtrr )
    ABCDEFGBCHIJKLMN $.
    $([27-May-99]$)

  $( The number 8 is positive. $)
  8pos $p |- 0 _10 < 8 _10 $=
    ( cn0 cl7 cn1 caddc co cl8 clt 7re ax1re 7pos lt01 addgt0i df-8 breqtrr )
    ABCDEFGBCHIJKLMN $.
    $([27-May-99]$)

  $( The number 9 is positive. $)
  9pos $p |- 0 _10 < 9 _10 $=
    ( cn0 cl8 cn1 caddc co cl9 clt 8re ax1re 8pos lt01 addgt0i df-9 breqtrr )
    ABCDEFGBCHIJKLMN $.
    $([27-May-99]$)

