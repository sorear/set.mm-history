          $([6-Aug-99]$)
      $}

      ${
        discrlem2.5 $e |- ( D e. RR -> 0_10 <_
                     ( ( ( A x. ( D ^ 2_10 ) ) + ( B x. D ) ) + C ) ) $.
        $( Lemma for discriminant theorem. $)
        discrlem2 $p |- ( 0_10 < A ->
                      ( ( B ^ 2_10 ) - ( 4_10 x. ( A x. C ) ) ) <_ 0_10 ) $=
          ( cn0 clt wbr cl2 cexp co cmulc caddc cle cl4 cmin cr wcel cdiv cneg
          wne 2pos 2re mulgt0 mpan mulrcl gt0ne0 syl divrclz negrclt 3syl
          eleq1i sylibr wb cn1 cded wceq id opreq2 opreq2d negeqd syl5eq
          opreq1d opreq12d breq2d opreq1 breq1d bi2bid ax1re keepel cleqid
          elimgt0 discrlem1 dedth mpbid ) JAKLZJADMNOZPOZBDPOZQOZCQOZRLZBMNOZSA
