$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Some properties of specific numbers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Two plus two equals 4. $)
  2p2e4 $p |- ( 2 _10 + 2 _10 ) = 4 _10  $=
    ( cl2 caddc co cn1 cl4 df-2 opreq2i 2re recn 1cn 4re cl3 df-4 df-3 opreq1i
    eqtr addass2r eqtr4 ) AABCADDBCZBCEASABFGADDEAHIJJEKIELDBCADBCZDBCMLTDBNOPQ
    R $.
    $([27-May-99]$)

  ${
    2times.1 $e |- A e. CC $.
    $( Two times a number. $)
    2times $p |- ( 2 _10 x. A ) = ( A + A ) $=
      ( cl2 cmulc co cn1 caddc df-2 opreq1i 1p1times eqtr ) CADEFFGEZADEAAGECLA
      DHIABJK $.
      $([1-Aug-99]$)
  $}

  $( Two times two equals 4. $)
  2t2e4 $p |- ( 2 _10 x. 2 _10 ) = 4 _10  $=
    ( cl2 cmulc co caddc cl4 2re recn 2times 2p2e4 eqtr ) AABCAADCEAAFGHIJ $.
    $([1-Aug-99]$)

  $( The square of 1 is 1. $)
  sq1 $p |- ( 1 _10 ^ 2 _10 ) = 1 _10 $=
    ( cn1 cl2 cexp co cmulc 1cn sqval mulid1 eqtr ) ABCDAAEDAAFGAFHI $.
    $([22-Aug-99]$)

  $( The square of 2 is 4. $)
  sq2 $p |- ( 2 _10 ^ 2 _10 ) = 4 _10 $=
    ( cl2 cexp co cmulc cl4 2re recn sqval 2t2e4 eqtr ) AABCAADCEAAFGHIJ $.
    $([22-Aug-99]$)

  $( The square root of 1 is 1. $)
  sqr1 $p |- ( sqr ` 1 _10 ) = 1 _10 $=
    ( cn1 cmulc co csqr cfv 1cn mulid1 fveq2i cn0 cle wbr wceq ax0re ax1re
    lt01 ltlei sqrsqid ax-mp eqtr3 ) AABCZDEZADEATADAFGHIAJKUAALIAMNOPANQRS $.
    $([31-Jul-99]$)

  $( The square root of 4 is 2. $)
  sqr4 $p |- ( sqr ` 4 _10 ) = 2 _10 $=
    ( cl4 csqr cfv cl2 wceq cmulc co cn0 clt wbr wb cle 4re 2re sqrsq ax0re
    ltle syl2an 4pos 2pos mp2an 2t2e4 cleqcomi mpbir ) ABCDEZADDFGZEZHAIJZHDIJZ
    UEUGKZHALJHDLJUJUHUIADMNOHAPMQHDPNQRSTUAUFAUBUCUD $.
    $([3-Aug-99]$)

  ${
    binom.1 $e |- A e. CC $.
    binom.2 $e |- B e. CC $.
    $( Binomial expansion. $)
    binom $p |- ( ( A + B ) ^ 2 _10 ) = ( ( ( A ^ 2 _10 ) + ( 2 _10 x.
                ( A x. B ) ) ) + ( B ^ 2 _10 ) ) $=
      ( caddc co cmulc cl2 cexp addcl distr1 distr2 mulcom opreq2i eqtr
      opreq12i mulcl addass opreq1i eqtr3 3eqtr sqval 2times 3eqtr4 ) ABEFZUEGF
      ZAAGFZABGFZUHEFZEFZBBGFZEFZUEHIFAHIFZHUHGFZEFZBHIFZEFUFUEAGFZUEBGFZEFUGUH
      EFZUHUKEFZEFZULUEABABCDJZCDKUQUSURUTEUQUGBAGFZEFUSABACDCLVCUHUGEBADCMNOAB
      BCDDLPUSUHEFZUKEFVAULUSUHUKUGUHAACCQZABCDQZJVFBBDDQRVDUJUKEUGUHUHVEVFVFRS
      TUAUEVBUBUOUJUPUKEUMUGUNUIEACUBUHVFUCPBDUBPUD $.
      $([11-Aug-99]$)
  $}



  ${
    discrlem.1 $e |- A e. RR $.
    discrlem.2 $e |- B e. RR $.
    discrlem.3 $e |- C e. RR $.
    ${
      discrlem1.4 $e |- D = -u ( B / ( 2 _10 x. A ) ) $.
      ${
        discrlem1.5 $e |- 0 _10 < A $.
        $( Lemma for discriminant theorem. $)
        discrlem1 $p |- ( 0 _10 <_
                     ( ( ( A x. ( D ^ 2 _10 ) ) + ( B x. D ) ) + C ) <->
                     ( ( B ^ 2 _10 ) - ( 4 _10 x. ( A x. C ) ) ) <_ 0 _10 ) $=
          ( cn0 cl2 cexp co cmulc caddc cle wbr cneg cl4 cmin 4re recn mulcl
          mulzer1 cr wcel cdiv eleq1i 2re mulrcl 2pos ltne0 muln0 divrcl
          negrcl mpbir sqcl cc divcl negcl addcl distr1 breq12i clt wb 4pos
          mulgt0i ax0re sqrecl addrcl lemul2 ax-mp neg0 negdist2 cn1 divcan3
          opreq1i sqval mul2neg eqtr eqtr2 opreq12i mulass divmul13 divid
          opreq2i 1cn mulcom 3eqtr eqtr3 divcan2 negneg mulneg1 df-2 negeqi
          negdist negid addass addid2 3eqtr3 distr2 mulm1 2t2e4 divmuldiv
          eqtr4 mulneg2 3bitr4 subrcl leneg bitr4 ) JADKLMZNMZBDNMZOMZCOMZPQZJR
          ZBKLMZSACNMZNMZTMZRZPQZYAJPQSANMZJNMZYDXONMZPQZJYDXNNMZYDCNMZOMZPQXPY
          CYEJYFYJPYDSASUAUBZAEUBZUCZUDYDXNCYMXLXMAXKYLDDDUEUFBKANMZUGMZRZUEUFD
          YPUEHUHYOBYNFKAUIEUJZKAKUIUBZYLKUIUKULZAEIULZUMZUNUOZUPZUBUQUCBDBFUBZ
          DURUFYPURUFDYPURHUHYOBYNUVDYNYQUBZUVAUSZUTUPUCVACGUBZVBVCJYDVDQXPYGVE
          SAUAEVFIVGJXOYDVHXNCXLXMAXKEDUVCVIUJBDFUVCUJVJGVJSAUAEUJZVKVLXQJYBYJP
          VMYBXRRZXTOMYJXRXTXRBFVIZUBZSXSYKACYLUVGUCUCVNYHUVIYIXTOYHYDXRYDUGMZR
          ZNMYDUVLNMZRUVIXNUVMYDNXNVOBKUGMZNMZYONMZKRZUVONMZYONMZOMZUVMXLUVQXMU
          VTOYNKUGMZYOYONMZNMZXLUVQUWBAUWCXKNKAYRYLYSVPXKYPKLMZUWCDYPKLHVQUWEYP
          YPNMUWCYPYPUVBUBVRYOYOUVFUVFVSVTWAWBUWBYONMZYONMUWDUVQUWBYOYOYNKUVEYR
          YSUSUVFUVFWCUWFUVPYONUWFUVOYNYNUGMZNMUVOVONMUVPYNKBYNUVEYRUVDUVEYSUVA
          WDUWGVOUVONYNUVEUVAWEWFUVOVOBKUVDYRYSUSZWGWHWIVQWJWJXMUVSRZYPNMUVTBUW
          IDYPNKUVONMZBUWIKBYRUVDYSWKUVRRZUVONMUWJUWIUWKKUVONKYRWLVQUVRUVOKYRUT
          ZUWHWMWJWJHWBUVSYOUVRUVOUWLUWHUCZUVFVSVTWBUVPUVSOMZYONMUVORZYONMZUWAU
          VMUWNUWOYONVOUVROMZUVONMVORZUVONMUWNUWOUWQUWRUVONUWQVOUWRUWROMZOMZUWR
          UVRUWSVOOUVRVOVOOMZRUWSKUXAWNWOVOVOWGWGWPVTWFVOUWROMZUWROMJUWROMUWTUW
          RUXBJUWROVOWGWQVQVOUWRUWRWGVOWGUTZUXCWRUWRUXCWSWTVTVQVOUVRUVOWGUWLUWH
          XAUVOUWHXBWTVQUVPUVSYOVOUVOWGUWHUCUWMUVFXAUWPUVOYONMZRUVMUVOYOUWHUVFW
