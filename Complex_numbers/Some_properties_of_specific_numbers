$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Some properties of specific numbers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Two plus two equals 4. $)
  2p2e4 $p |- ( 2_10 + 2_10 ) = 4_10  $=
    ( cl2 caddc co cn1 cl4 df-2 opreq2i 2re recn 1cn 4re cl3 df-4 df-3 opreq1i
    eqtr addass2r eqtr4 ) AABCADDBCZBCEASABFGADDEAHIJJEKIELDBCADBCZDBCMLTDBNOPQ
    R $.
    $([27-May-99]$)

  ${
    2times.1 $e |- A e. CC $.
    $( Two times a number. $)
    2times $p |- ( 2_10 x. A ) = ( A + A ) $=
      ( cl2 cmulc co cn1 caddc df-2 opreq1i 1p1times eqtr ) CADEFFGEZADEAAGECLA
      DHIABJK $.
      $([1-Aug-99]$)
  $}

  $( Two times two equals 4. $)
  2t2e4 $p |- ( 2_10 x. 2_10 ) = 4_10  $=
    ( cl2 cmulc co caddc cl4 2re recn 2times 2p2e4 eqtr ) AABCAADCEAAFGHIJ $.
    $([1-Aug-99]$)

  $( One half of four is two. $)
  4d2e2 $p |- ( 4_10 / 2_10 ) = 2_10 $=
    ( cl4 cl2 cdiv co wceq cmulc 2t2e4 4re recn 2re 2pos gt0ne0i divmul mpbir )
    ABCDBEBBFDAEGABBAHIBJIZOBJKLMN $.
    $([3-Sep-99]$)

  $( The square of 1 is 1. $)
  sq1 $p |- ( 1_10 ^ 2_10 ) = 1_10 $=
    ( cn1 cl2 cexp co cmulc 1cn sqval mulid1 eqtr ) ABCDAAEDAAFGAFHI $.
    $([22-Aug-99]$)

  $( The square of 2 is 4. $)
  sq2 $p |- ( 2_10 ^ 2_10 ) = 4_10 $=
    ( cl2 cexp co cmulc cl4 2re recn sqval 2t2e4 eqtr ) AABCAADCEAAFGHIJ $.
    $([22-Aug-99]$)

  $( The square root of 1 is 1. $)
  sqr1 $p |- ( sqr ` 1_10 ) = 1_10 $=
    ( cn1 cmulc co csqr cfv 1cn mulid1 fveq2i cn0 cle wbr wceq ax0re ax1re
    lt01 ltlei sqrsqid ax-mp eqtr3 ) AABCZDEZADEATADAFGHIAJKUAALIAMNOPANQRS $.
    $([31-Jul-99]$)

  $( The square root of 4 is 2. $)
  sqr4 $p |- ( sqr ` 4_10 ) = 2_10 $=
    ( cl4 csqr cfv cl2 wceq cmulc co 2t2e4 cleqcomi cn0 clt wbr wb 4pos 2pos
    cle 4re 2re sqrsq ax0re ltle syl2an mp2an mpbir ) ABCDEZADDFGZEZUFAHIJAKLZJ
    DKLZUEUGMZNOJAPLJDPLUJUHUIADQRSJATQUAJDTRUAUBUCUD $.
    $([3-Aug-99]$)

  ${
    binom.1 $e |- A e. CC $.
    binom.2 $e |- B e. CC $.
    $( Binomial expansion. $)
    binom $p |- ( ( A + B ) ^ 2_10 ) = ( ( ( A ^ 2_10 ) + ( 2_10 x.
                ( A x. B ) ) ) + ( B ^ 2_10 ) ) $=
      ( caddc co cmulc cl2 cexp addcl distr1 distr2 mulcom opreq2i eqtr
      opreq12i mulcl addass opreq1i eqtr3 3eqtr sqval 2times 3eqtr4 ) ABEFZUEGF
      ZAAGFZABGFZUHEFZEFZBBGFZEFZUEHIFAHIFZHUHGFZEFZBHIFZEFUFUEAGFZUEBGFZEFUGUH
      EFZUHUKEFZEFZULUEABABCDJZCDKUQUSURUTEUQUGBAGFZEFUSABACDCLVCUHUGEBADCMNOAB
      BCDDLPUSUHEFZUKEFVAULUSUHUKUGUHAACCQZABCDQZJVFBBDDQRVDUJUKEUGUHUHVEVFVFRS
      TUAUEVBUBUOUJUPUKEUMUGUNUIEACUBUHVFUCPBDUBPUD $.
      $([11-Aug-99]$)
  $}

  ${
    discrlem.1 $e |- A e. RR $.
    discrlem.2 $e |- B e. RR $.
    discrlem.3 $e |- C e. RR $.
    ${
      discrlem1.4 $e |- D = -u ( B / ( 2_10 x. A ) ) $.
      ${
        discrlem1.5 $e |- 0_10 < A $.
