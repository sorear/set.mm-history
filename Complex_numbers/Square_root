$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Square root
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c sqr $. $( Positive square root of a positive real number. $)

  $( Extend class notation to include positive square root of a positive real
     number. $)
  csqr $a class sqr $.

  ${
    $d x y z $.
    $( Define a function whose value is the positive square root of a positive
       real number.  The square root of ` x ` is the supremum of all reals
       whose square is less than ` x ` . $)
    df-sqr $a |- sqr = { <. x , y >. | ( ( x e. RR /\ 0 _10 <_ x ) /\
     y = sup ( { z e. RR | ( 0 _10 <_ z /\ ( z x. z ) <_ x ) } , RR , < ) ) } $.
  $}

  ${
    $d x y z w A $.  $( ` y z w ` are dummy. $)
    $( Value of square root function. $)
    sqrval $p |- ( ( A e. RR /\ 0 _10 <_ A ) -> ( sqr ` A ) =
          sup ( { x e. RR | ( 0 _10 <_ x /\ ( x x. x ) <_ A ) } , RR , < ) ) $=
      ( vw vy vz cr wcel cn0 cle wbr wa cv crab csqr cfv cmulc co clt csup
      wceq breq2 rabcl ltsor supex biland rbiabdv supeq1 syl copab df-sqr
      biran biopab eqtr4 opabval2 sylbir ) BFGHBIJZKBHCLZIJZCFMZGBNOHALZIJZUTUT
      PQZBIJZKZAFMZFRSZTURUPCBFUQBHIUAUBDEBVAVBDLZIJZKZAFMZFRSZVFUSNFVERUCUDVGB
      TZVJVETVKVFTVLVIVDAFVLVHVCVAVGBVBIUAUEUFFVJVERUGUHNVGFGHVGIJZKZELVKTZKZDE
      UIVGUSGZVOKZDEUIDEAUJVRVPDEVQVNVOURVMCVGFUQVGHIUAUBUKULUMUNUO $.
      $([23-May-99]$)
  $}

