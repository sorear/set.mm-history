  ${
    $d x y z w f A $.  $d x y z w f B $.
    $( Define orthogonal complement of a subspace. $)
    ax-hdforth $a |- ( A e. SH -> ( _|_ ` A ) =
        { x e. Hil | A. y e. A ( x .i y ) = 0 _10 } ) $.

    $( Define subspace sum. $)
    ax-hdfshsum $a |- ( ( A e. SH /\ B e. SH ) -> ( A +H B ) =
        { x e. Hil | E. y e. A E. z e. B x = ( y +v z ) } ) $.

    $( Define meet in ` CH ` . $)
    ax-hdfchm $a |- ( ( A e. CH /\ B e. CH ) -> ( A ^H B ) =
        ( A i^i B ) $.

    $( Define join in ` CH ` . $)
    ax-hdfchj $a |- ( ( A e. CH /\ B e. CH ) -> ( A vH B ) =
        |^| { x e. CH | ( A u. B ) (_ x } ) $.

    $( Define unit in ` CH ` . $)
    df-hdfch1 $a |- 1H = Hil $.

    $( Define the zero of ` CH ` . $)
    df-hdfch0 $a |- 0H = { 0v } $.

