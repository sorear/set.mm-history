htmldef "norm" as ' norm ';
htmldef "Cauchy" as ' Cauchy ';
htmldef "~~>v" as ' <FONT FACE="Symbol"> &#126;&#126;></FONT><SUB>v</SUB> ';
htmldef "CmH" as ' <I>Cm</I><B>H</B> ';
htmldef "SH" as ' <I>S</I><B>H</B> ';
htmldef "CH" as ' <I>C</I><B>H</B> ';
/*htmldef "_|_" as '<FONT FACE="Symbol">&#94;</FONT>';*/
htmldef "-H" as '<FONT FACE="Symbol">&#216;</FONT><B><SUB>H</SUB></B >';
htmldef "+H" as ' +<B><SUB>H</SUB></B > ';
htmldef "^H" as ' <FONT FACE="Symbol">&#199;</FONT><B><SUB>H</SUB></B > ';
htmldef "vH" as ' <FONT FACE="Symbol">&#200;</FONT><B><SUB>H</SUB></B > ';
htmldef "=<H" as ' <FONT FACE="Symbol">&#163;</FONT><B><SUB>H</SUB></B > ';
htmldef "0H" as '0<B><SUB>H</SUB></B >';
htmldef "1H" as '1<B><SUB>H</SUB></B >';
htmldef "Proj" as ' Proj ';
htmldef "+P" as ' +<SUB>P</SUB> ';
htmldef "-P" as ' <FONT FACE="Symbol">&#216;</FONT><SUB>P</SUB> ';
htmldef "States" as ' States ';
htmldef "Atoms" as ' Atoms ';

/* End of typesetting definition section */
$)


