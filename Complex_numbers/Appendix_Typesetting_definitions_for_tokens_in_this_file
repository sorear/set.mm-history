  ${
    $d f g h x y $.
    $( Define the sum of two Hilbert space operators.  Beran, p. 111. $)
    df-hosum $a |- +P = { <. <. f , g >. , h >. | ( ( f : Hil --> Hil /\
          g : Hil --> Hil ) /\
          h = { <. x , y >. | ( x e. Hil /\
           y = ( ( f ` x ) +v ( g ` x ) ) ) } ) } $.

    $( Define the difference of two Hilbert space operators.  Beran, p. 111. $)
    df-pjdif $a |- -P = { <. <. f , g >. , h >. | ( ( f : Hil --> Hil /\
          g : Hil --> Hil ) /\
          h = { <. x , y >. | ( x e. Hil /\
           y = ( ( f ` x ) -v ( g ` x ) ) ) } ) } $.
  $}

