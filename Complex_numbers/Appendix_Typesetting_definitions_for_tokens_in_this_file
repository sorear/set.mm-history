  ${
    $d x y $.
    $( Define partial order in ` CH ` .  See ~ chlet for its relational
       form. $)
    df-chle $a |- =<H = { <. x , y >. | ( ( x e. CH /\ y e. CH ) /\
                x (_ y ) } $.

    $( Define meet in ` CH ` .  See ~ chmvalt for its value. $)
    df-chm $a |- ^H = { <. <. x , y >. , z >. | ( ( x e. CH /\ y e. CH ) /\
               z = ( x i^i y ) ) } $.

  $}

  $( The unit subspace belongs to ` CH ` .  Part of Proposition 1 of
     [Kalmbach] p. 65. $)
  ch1cl $p |- 1H e. CH $=
    ( c1h chil cch df-ch1 helch eqeltr ) ABCDEF $.
    $([30-May-99]$)

  ${
    $d x y z A $.  $d x y z B $.
    $( Partial order relation in ` CH ` .  Definition of [Kalmbach] p. 65. $)
    chlet $p |- ( ( A e. CH /\ B e. CH ) -> ( A =<H B <-> A (_ B ) ) $=
      ( vx vy cv cleh wbr wss wb cch wceq breq1 sseq1 bi2bid breq2 sseq2 wcel
      wa cop copab df-br df-chle eleq2i opabid 3bitr baib vtocl2ga ) CEZDEZFGZU
      HUIHZIAUIFGZAUIHZIABFGZABHZICDABJJUHAKUJULUKUMUHAUIFLUHAUIMNUIBKULUNUMUOU
