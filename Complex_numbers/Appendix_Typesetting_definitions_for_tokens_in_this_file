latexdef "+" as "+";
latexdef "-" as "-";
/*latexdef "-u" as "-_u";*/
latexdef "-u" as "-"; /* use standard minus sign */
latexdef "x." as "\cdot";
latexdef "<" as "<";
latexdef ">" as ">";
latexdef "<_" as "\le";
latexdef ">_" as "\ge";
latexdef "NN" as "\mathbb{N}";
