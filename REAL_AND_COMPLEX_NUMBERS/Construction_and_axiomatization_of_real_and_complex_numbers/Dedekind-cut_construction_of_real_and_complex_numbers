    $( [10-May-1996] $)

  ${
    $( (We don't bother to eliminate the redundant hypothesis.) $)
    mulresr.1 $e |- B e. _V $.
    $( Multiplication of real numbers in terms of intermediate signed reals. $)
    mulresr $p |- ( ( A e. R. /\ B e. R. ) ->
           ( <. A , 0R >. x. <. B , 0R >. ) = <. ( A .R B ) , 0R >. ) $=
      ( cnr wcel wa c0r cop cmul co cmr cm1r cplr wceq 00sr ax-mp oveq2i syl5eq
