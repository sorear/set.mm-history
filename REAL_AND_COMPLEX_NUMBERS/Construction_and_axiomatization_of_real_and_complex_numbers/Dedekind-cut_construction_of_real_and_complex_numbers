  $(
  Digit strings will usually be unambigous without whitespace (for better
  readability) although we will separate digits, at least initially in our
  development, to emphasize that they are separate Metamath tokens.
  $)

  $( Class of complex numbers. $)
  cc $a class CC $.
  $( Class of real numbers. $)
  cr $a class RR $.
  $( Extend class notation to include the complex number 0. $)
  cc0 $a class 0 $.
  $( Extend class notation to include the complex number 1. $)
  c1 $a class 1 $.
  $( Extend class notation to include the complex number i. $)
  ci $a class _i $.
  $( Addition on complex numbers. $)
  caddc $a class + $.
  $( 'Less than' predicate (defined over real subset of complex numbers). $)
  cltrr $a class <RR $.
  $( Multiplication on complex numbers.  The token ` x. ` is a center dot. $)
  cmul $a class x. $.

  $( Define the set of complex numbers.  The 23 axioms for complex numbers
     start at ~ cnex . $)
  df-c $a |- CC = ( R. X. R. ) $.

  $( Define the complex number 0 (base 10). $)
  df-0 $a |- 0 = <. 0R , 0R >. $.

  $( Define the complex number 1 (base 10). $)
  df-1 $a |- 1 = <. 1R , 0R >. $.

  $( Define the complex number ` _i ` (the imaginary unit). $)
  df-i $a |- _i = <. 0R , 1R >. $.

  ${
    $d x y $.
    $( Define the set of real numbers. $)
    df-r $a |- RR = ( R. X. { 0R } ) $.
  $}

  ${
    $d x y z w v u f $.
    $( Define addition over complex numbers. $)
    df-plus $a |- + = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. CC ) /\
                  E. w E. v E. u E. f ( ( x = <. w , v >. /\ y = <. u , f >. )
                  /\ z = <. ( w +R u ) , ( v +R f ) >. ) ) } $.

    $( Define multiplication over complex numbers. $)
    df-mul $a |- x. = { <. <. x , y >. , z >. | ( ( x e. CC /\ y e. CC ) /\
               E. w E. v E. u E. f ( ( x = <. w , v >. /\ y = <. u , f >. )
                  /\ z = <. ( ( w .R u ) +R ( -1R .R ( v .R f ) ) ) ,
                  ( ( v .R u ) +R ( w .R f ) ) >. ) ) } $.

    $( Define 'less than' on the real subset of complex numbers. $)
    df-lt $a |- <RR = { <. x , y >. | ( ( x e. RR /\ y e. RR ) /\
     E. z E. w ( ( x = <. z , 0R >. /\ y = <. w , 0R >. ) /\ z <R w ) ) } $.
  $}

  ${
    opelcn.1 $e |- B e. _V $.
    $( Ordered pair membership in the class of complex numbers. $)
    opelcn $p |- ( <. A , B >. e. CC <-> ( A e. R. /\ B e. R. ) ) $=
      ( cop cc wcel cnr cxp wa df-c eleq2i opelxp bitri ) ABDZEFNGGHZFAGFBGFIEO
      NJKABGGCLM $.
      $( [14-May-1996] $)
  $}

  $( Ordered pair membership in class of real subset of complex numbers. $)
  opelreal $p |- ( <. A , 0R >. e. RR <-> A e. R. ) $=
    ( c0r cop cr wcel cnr wceq csn cxp wa df-r eleq2i elexi opelxp elsnc anbi2i
    0r 3bitri eqid mpbiran2 ) ABCZDEZAFEZBBGZUBUAFBHZIZEUCBUEEZJUCUDJDUFUAKLABF
    UEBFQMZNUGUDUCBBUHOPRBST $.
    $( [22-Feb-1996] $)

  ${
    $d x y A $.
    $( Membership in class of real numbers. $)
