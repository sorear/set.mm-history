  ${
    $d k x y A $.  $d k x y F $.  $d k x y G $.  $d k x y ph $.  $d k y Z $.
    $d y M $.
    climeq.1 $e |- Z = ( ZZ>= ` M ) $.
    climeq.2 $e |- ( ph -> F e. V ) $.
    climeq.3 $e |- ( ph -> G e. W ) $.
    climeq.4 $e |- ( ph -> A e. X ) $.
    climeq.5 $e |- ( ph -> M e. ZZ ) $.
    climeq.6 $e |- ( ( ph /\ k e. Z ) -> ( F ` k ) = ( G ` k ) ) $.
    $( Two functions that are eventually equal to one another have the same
       limit. $)
    climeq $p |- ( ph -> ( F ~~> A <-> G ~~> A ) ) $=
      ( vx vy wbr wcel cli cc cv cfv cmin co cabs clt cuz wral wrex clim2 eqidd
      wa crp bitr4d ) ADBUASBUBTCUCZEUDZUBTURBUEUFUGUDQUCUHSUNCRUCUIUDUJRJUKQUO
      UJUNEBUASAQBURRCDFGIJKOLPNULAQBURRCEFHIJKOMAUQJTUNURUMNULUP $.
      $( [31-Jan-2014] $) $( [5-Nov-2013] $)
