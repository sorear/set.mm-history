    $( Closure of infimum of a non-empty bounded set of reals. $)
    infmrcl $p |- ( ( A C_ RR /\ A =/= (/) /\ E. x e. RR A. y e. A x <_ y ) ->
                  sup ( A , RR , `' < ) e. RR ) $=
      ( vv vw vz cr wss c0 wne cv cle wbr wral cneg wcel wa renegcl syl6 wceq
      wrex w3a clt ccnv csup crab infmsup wex n0 ssel cc ssel2 recn negneg 3syl
      simpr eqeltrd jcad negeq eleq1d elrab ne0i sylbir exlimdv sylan2b 3adant3
      ex wi breq2 rcla4va adantll wb lenegcon2 adantrr adantr mpbid exp31 com23
      imp syl5bi ralrimdv jctild ralbidv rexlimiv 3ad2ant3 ssrab2 suprcl mp3an1
      rcla4ev syl2anc syl ) CGHZCIJZAKZBKZLMZBCNZAGUAZUBZCGUCUDUEDKZOZCPZDGUFZG
      UCUEZOZGABDCUGWSXDGPZXEGPWSXCIJZEKZFKZLMZEXCNZFGUAZXFWLWMXGWRWMWLXICPZFUH
