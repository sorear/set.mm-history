$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Division
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  ${
    $d x y z $.
    $( Define division.  Theorem ~ divmuli relates it to multiplication, and
       ~ divcli and ~ redivcli prove its closure laws. $)
    df-div $a |- / = ( x e. CC , y e. ( CC \ { 0 } ) |->
                   ( iota_ z e. CC ( y x. z ) = x ) ) $.
    $(
    Future simpler alternative?
    df-div $a |- / = { <. <. x , y >. , z >. |
                ( ( y e. ( CC \ { 0 } ) /\ z e. CC ) /\  x = ( y x. z ) ) } $.
    $)
  $}

  ${
    $d x y z $.
    $( You can't divide by zero, because division explicitly excludes zero from
       the domain of the function.  Thus, by the definition of function value,
       it evaluates to the empty set.  (This theorem is for information only
       and normally is not referenced by other proofs.  To be meaningful, it
       assumes that ` (/) ` is not a complex number, which depends on the
       particular complex number construction that is used.)  (Contributed by
       Mario Carneiro, 1-Apr-2014.) $)
    1div0 $p |- ( 1 / 0 ) = (/) $=
      ( vx vy vz cdiv cdm cc cc0 csn cdif wceq wcel c1 wa wn co c0 cv cmul crio
      cxp df-div riotaex dmmpt2 0cn eqid eldifsn simprbi adantl necon2bi ndmovg
      wne ax-mp mp3an ) DEFFGHIZTJGFKZLFKZGUNKZMZNZLGDOPJABFUNBQCQROAQJZCFSDABC
      UAUTCFUBUCUDGGJUSGUEURGGUQGGUKZUPUQUOVAGFGUFUGUHUIULLGFFUNDUJUM $.
      $( [2-Apr-2014] $)
  $}

  ${
    $d x y z A $.  $d x y B $.  $d x y C $.
    $( Value of division: the (unique) element ` x ` such that
       ` ( B x. x ) = A ` .  This is meaningful only when ` B ` is nonzero. $)
    divval $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                   ( A / B ) = ( iota_ x e. CC ( B x. x ) = A ) ) $=
      ( vz vy cc wcel cc0 wne cdiv co cmul wceq crio csn cdif eldifsn riotabidv
      cv wa eqeq2 oveq1 eqeq1d df-div riotaex ovmpt2 sylan2br 3impb ) BFGZCFGZC
      HIZBCJKCASZLKZBMZAFNZMZUJUKTUICFHOPZGUPCFHQDEBCFUQESZULLKZDSZMZAFNUOJUSBM
      ZAFNUTBMVAVBAFUTBUSUARURCMZVBUNAFVCUSUMBURCULLUBUCRDEAUDUNAFUEUFUGUH $.
      $( [17-Feb-2014] $) $( [8-May-1999] $)

    $( Relationship between division and multiplication.  (Revised by Mario
       Carneiro, 17-Feb-2014.) $)
    divmul $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
                 -> ( ( A / C ) = B <-> ( C x. B ) = A ) ) $=
      ( vx vy cc wcel cc0 wne wa cdiv co wceq cv cmul crio 3expb 3adant2 eqeq1d
      w3a divval wreu wb simp2 receu ax-17 a17d oveq2 riota2f syl2anc bitr4d )
      AFGZBFGZCFGZCHIZJZTZACKLZBMCDNZOLZAMZDFPZBMZCBOLZAMZUQURVBBULUPURVBMZUMUL
      UNUOVFDACUAQRSUQUMVADFUBZVEVCUCULUMUPUDULUPVGUMULUNUOVGDACUEQRVAVEDEFBENB
      GDUFUMVEDUGUSBMUTVDAUSBCOUHSUIUJUK $.
      $( [17-Feb-2014] $) $( [2-Aug-2004] $)
  $}

  ${
    divmulz.1 $e |- A e. CC $.
    divmulz.2 $e |- B e. CC $.
    divmulz.3 $e |- C e. CC $.
    $( Relationship between division and multiplication. $)
    divmulzi $p |- ( B =/= 0 -> ( ( A / B ) = C <-> ( B x. C ) = A ) ) $=
      ( cc wcel cc0 wne cdiv co wceq cmul wb wa divmul mp3an12 mpan ) BGHZBIJZA
      BKLCMBCNLAMOZEAGHCGHTUAPUBDFACBQRS $.
      $( [17-Feb-2014] $) $( [8-May-1999] $)
  $}

  ${
    divmul.1 $e |- A e. CC $.
    divmul.2 $e |- B e. CC $.
    divmul.3 $e |- C e. CC $.
    divmul.4 $e |- B =/= 0 $.
    $( Relationship between division and multiplication. $)
    divmuli $p |- ( ( A / B ) = C <-> ( B x. C ) = A ) $=
      ( cc0 wne cdiv co wceq cmul wb divmulzi ax-mp ) BHIABJKCLBCMKALNGABCDEFOP
      $.
      $( [17-Feb-2014] $) $( [2-Feb-1995] $)
  $}

  $( Relationship between division and multiplication. $)
  divmul2 $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
              -> ( ( A / C ) = B <-> A = ( C x. B ) ) ) $=
    ( cc wcel cc0 wne wa w3a cdiv co wceq cmul divmul eqcom syl6bb ) ADEBDECDEC
    FGHIACJKBLCBMKZALAQLABCNQAOP $.
    $( [16-Nov-2008] $) $( [7-Feb-2006] $)

  $( Relationship between division and multiplication. $)
  divmul3 $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
              -> ( ( A / C ) = B <-> A = ( B x. C ) ) ) $=
    ( cc wcel cc0 wne wa w3a cdiv co wceq divmul2 mulcom adantrr 3adant1 eqeq2d
    cmul bitr4d ) ADEZBDEZCDEZCFGZHZIZACJKBLACBRKZLABCRKZLABCMUEUGUFAUAUDUGUFLZ
    TUAUBUHUCBCNOPQS $.
    $( [27-Nov-2008] $) $( [13-Feb-2006] $)

  ${
    $d x A $.  $d x B $.
    $( Closure law for division.  (The proof was shortened by Mario Carneiro,
       17-Feb-2014.) $)
    divcl $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) -> ( A / B ) e. CC ) $=
      ( vx cc wcel cc0 wne w3a cdiv co cmul wceq crio divval wreu receu riotacl
      cv syl eqeltrd ) ADEBDEBFGHZABIJBCRKJALZCDMZDCABNUAUBCDOUCDECABPUBCDQST
      $.
      $( [17-Feb-2014] $) $( [21-Jul-2001] $)

    divclz.1 $e |- A e. CC $.
    divclz.2 $e |- B e. CC $.
    $( Closure law for division. $)
    divclzi $p |- ( B =/= 0 -> ( A / B ) e. CC ) $=
      ( cc wcel cc0 wne cdiv co divcl mp3an12 ) AEFBEFBGHABIJEFCDABKL $.
      $( [17-Feb-2014] $) $( [7-May-1999] $)
  $}

  ${
    divcl.1 $e |- A e. CC $.
    divcl.2 $e |- B e. CC $.
    divcl.3 $e |- B =/= 0 $.
    $( Closure law for division. $)
    divcli $p |- ( A / B ) e. CC $=
      ( cc0 wne cdiv co cc wcel divclzi ax-mp ) BFGABHIJKEABCDLM $.
      $( [17-Feb-2014] $) $( [2-Feb-1995] $)
  $}

  ${
    reccl.1 $e |- A e. CC $.
    reccl.2 $e |- A =/= 0 $.
    $( Closure law for reciprocal. $)
    reccli $p |- ( 1 / A ) e. CC $=
      ( c1 ax-1cn divcli ) DAEBCF $.
      $( [30-Apr-2005] $)
  $}

  ${
    recclz.1 $e |- A e. CC $.
    $( Closure law for reciprocal. $)
    recclzi $p |- ( A =/= 0 -> ( 1 / A ) e. CC ) $=
      ( c1 ax-1cn divclzi ) CADBE $.
      $( [30-Apr-2005] $)
  $}

  $( Closure law for reciprocal. $)
  reccl $p |- ( ( A e. CC /\ A =/= 0 ) -> ( 1 / A ) e. CC ) $=
    ( c1 cc wcel cc0 wne cdiv co ax-1cn divcl mp3an1 ) BCDACDAEFBAGHCDIBAJK $.
    $( [30-Apr-2005] $)

  ${
    divcan.1 $e |- A e. CC $.
    divcan.2 $e |- B e. CC $.
    ${
      divcan.3 $e |- B =/= 0 $.
      $( A cancellation law for division. $)
      divcan2i $p |- ( B x. ( A / B ) ) = A $=
        ( cdiv co wceq cmul eqid divcli divmuli mpbi ) ABFGZNHBNIGAHNJABNCDABCD
        EKELM $.
        $( [18-Aug-2008] $) $( [9-Feb-1995] $)

      $( A cancellation law for division. $)
      divcan1i $p |- ( ( A / B ) x. B ) = A $=
        ( cdiv co cmul divcli mulcomi divcan2i eqtri ) ABFGZBHGBMHGAMBABCDEIDJA
        BCDEKL $.
        $( [18-Aug-2008] $) $( [18-May-1999] $)
    $}

    $( A cancellation law for division. $)
    divcan1zi $p |- ( B =/= 0 -> ( ( A / B ) x. B ) = A ) $=
      ( cc0 wne cdiv co cmul wceq c1 cif oveq2 id oveq12d eqeq1d ax-1cn elimne0
      cc keepel divcan1i dedth ) BEFZABGHZBIHZAJAUCBKLZGHZUFIHZAJBKBUFJZUEUHAUI
      UDUGBUFIBUFAGMUINOPAUFCUCBKSDQTBRUAUB $.
      $( [18-Aug-2008] $) $( [2-Oct-1999] $)

    $( A cancellation law for division.  We eliminate the third hypothesis of
       ~ divcan2i using the weak deduction theorem ~ dedth and keep the other
       two.  Because the first hypothesis shares the class variable ` B ` with
       the hypothesis we're eliminating, we need to use ~ keepel in order to
       keep the first hypothesis. $)
    divcan2zi $p |- ( B =/= 0 -> ( B x. ( A / B ) ) = A ) $=
      ( cc0 wne cdiv co cmul wceq c1 cif id oveq2 oveq12d eqeq1d ax-1cn elimne0
      cc keepel divcan2i dedth ) BEFZBABGHZIHZAJUCBKLZAUFGHZIHZAJBKBUFJZUEUHAUI
      BUFUDUGIUIMBUFAGNOPAUFCUCBKSDQTBRUAUB $.
      $( [21-Aug-2008] $) $( [10-Aug-1999] $)
  $}

  $( A cancellation law for division. $)
  divcan1 $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 )
        -> ( ( A / B ) x. B ) = A ) $=
    ( cc wcel cc0 wne cdiv co cmul wceq wi oveq1 oveq1d id eqeq12d imbi2d neeq1
    cif 0cn elimel oveq2 oveq12d eqeq1d imbi12d divcan1zi dedth2h 3impia ) ACDZ
    BCDZBEFZABGHZBIHZAJZUHUIUJUMKUJUHAERZBGHZBIHZUNJZKUIBERZEFZUNURGHZURIHZUNJZ
    KABEEAUNJZUMUQUJVCULUPAUNVCUKUOBIAUNBGLMVCNOPBURJZUJUSUQVBBUREQVDUPVAUNVDUO
    UTBURIBURUNGUAVDNUBUCUDUNURAECSTBECSTUEUFUG $.
    $( [18-Aug-2008] $) $( [5-Jun-2004] $)

  $( A cancellation law for division. $)
  divcan2 $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                 ( B x. ( A / B ) ) = A ) $=
    ( cc wcel cc0 wne w3a cdiv co cmul simp2 divcl mulcom syl2anc divcan1 eqtrd
    wceq ) ACDZBCDZBEFZGZBABHIZJIZUBBJIZAUASUBCDUCUDQRSTKABLBUBMNABOP $.
    $( [21-Aug-2008] $) $( [3-Feb-2004] $)

  $( The ratio of nonzero numbers is nonzero. $)
  divne0b $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                  ( A =/= 0 <-> ( A / B ) =/= 0 ) ) $=
    ( cc wcel cc0 wne cdiv co wceq cmul wb mul01 eqeq1d eqcom syl6rbbr 3ad2ant2
    w3a wa 0cn divmul mp3an2 3impb bitr4d necon3bid ) ACDZBCDZBEFZQZAEABGHZEUHA
    EIZBEJHZAIZUIEIZUFUEUJULKUGUFULEAIUJUFUKEABLMAENOPUEUFUGUMULKZUEECDUFUGRUNS
    AEBTUAUBUCUD $.
    $( [1-Jun-2005] $) $( [2-Aug-2004] $)

  $( The ratio of nonzero numbers is nonzero. $)
  divne0 $p |- ( ( ( A e. CC /\ A =/= 0 ) /\ ( B e. CC /\ B =/= 0 ) ) ->
                  ( A / B ) =/= 0 ) $=
    ( cc wcel cc0 wne cdiv co wi w3a divne0b biimpd 3exp com23 com24 imp43 ) AC
    DZAEFZBCDZBEFZABGHEFZQTSRUAQSTRUAIZQSTUBQSTJRUAABKLMNOP $.
    $( [28-Dec-2007] $)

  ${
    divneq0.1 $e |- A e. CC $.
    divneq0.2 $e |- B e. CC $.
    divneq0.3 $e |- A =/= 0 $.
    divneq0.4 $e |- B =/= 0 $.
    $( The ratio of nonzero numbers is nonzero. $)
    divne0i $p |- ( A / B ) =/= 0 $=
      ( cc wcel cc0 wne cdiv co w3a divne0b mpbii mp3an ) AGHZBGHZBIJZABKLIJZCD
      FQRSMAIJTEABNOP $.
      $( [9-Feb-1995] $)
  $}

  ${
    recneq0z.1 $e |- A e. CC $.
    $( The reciprocal of a nonzero number is nonzero. $)
    recne0zi $p |- ( A =/= 0 -> ( 1 / A ) =/= 0 ) $=
      ( cc0 wne c1 cdiv co cif wceq oveq2 neeq1d ax-1cn ax-1ne0 elimne0 divne0i
      cc keepel dedth ) ACDZEAFGZCDESAEHZFGZCDAEAUAITUBCAUAEFJKEUALSAEPBLQMANOR
      $.
      $( [14-May-1999] $)
  $}

  $( The reciprocal of a nonzero number is nonzero. $)
  recne0 $p |- ( ( A e. CC /\ A =/= 0 ) -> ( 1 / A ) =/= 0 ) $=
    ( cc wcel cc0 wne c1 cdiv co cif wceq neeq1 oveq2 neeq1d imbi12d 0cn elimel
    wi recne0zi dedth imp ) ABCZADEZFAGHZDEZUAUBUDQUAADIZDEZFUEGHZDEZQADAUEJZUB
    UFUDUHAUEDKUIUCUGDAUEFGLMNUEADBOPRST $.
    $( [9-Feb-2006] $)

  ${
    recid.1 $e |- A e. CC $.
    recid.2 $e |- A =/= 0 $.
    $( Multiplication of a number and its reciprocal. $)
    recidi $p |- ( A x. ( 1 / A ) ) = 1 $=
      ( c1 ax-1cn divcan2i ) DAEBCF $.
      $( [9-Feb-1995] $)
  $}

  ${
    recidz.1 $e |- A e. CC $.
    $( Multiplication of a number and its reciprocal. $)
    recidzi $p |- ( A =/= 0 -> ( A x. ( 1 / A ) ) = 1 ) $=
      ( c1 ax-1cn divcan2zi ) CADBE $.
      $( [14-May-1999] $)
  $}

  $( Multiplication of a number and its reciprocal. $)
  recid $p |- ( ( A e. CC /\ A =/= 0 ) -> ( A x. ( 1 / A ) ) = 1 ) $=
    ( cc wcel cc0 wne c1 cdiv co cmul wceq wi cif neeq1 id oveq2 oveq12d eqeq1d
    imbi12d 0cn elimel recidzi dedth imp ) ABCZADEZAFAGHZIHZFJZUDUEUHKUDADLZDEZ
    UIFUIGHZIHZFJZKADAUIJZUEUJUHUMAUIDMUNUGULFUNAUIUFUKIUNNAUIFGOPQRUIADBSTUAUB
    UC $.
    $( [25-Oct-1999] $)

  $( Multiplication of a number and its reciprocal. $)
  recid2 $p |- ( ( A e. CC /\ A =/= 0 ) -> ( ( 1 / A ) x. A ) = 1 ) $=
    ( cc wcel cc0 wne wa c1 cdiv co cmul reccl simpl mulcom syl2anc recid eqtrd
    wceq ) ABCZADEZFZGAHIZAJIZAUAJIZGTUABCRUBUCQAKRSLUAAMNAOP $.
    $( [22-Jun-2006] $)

  ${
    divrec.1 $e |- A e. CC $.
    divrec.2 $e |- B e. CC $.
    ${
      divrec.3 $e |- B =/= 0 $.
      $( Relationship between division and reciprocal.  Theorem I.9 of
         [Apostol] p. 18. $)
      divreci $p |- ( A / B ) = ( A x. ( 1 / B ) ) $=
        ( cdiv co c1 cmul reccli mulcli mulcomi mulassi ax-1cn divcan1i mulid1i
        wceq oveq2i eqtri 3eqtri divmuli mpbir ) ABFGAHBFGZIGZQBUDIGZAQUEUDBIGA
        UCBIGZIGZABUDDAUCCBDEJZKZLAUCBCUHDMUGAHIGAUFHAIHBNDEORACPSTABUDCDUIEUAU
        B $.
        $( [9-Feb-1995] $)
    $}

    $( Relationship between division and reciprocal.  Theorem I.9 of [Apostol]
       p. 18. $)
    divreczi $p |- ( B =/= 0 -> ( A / B ) = ( A x. ( 1 / B ) ) ) $=
      ( cc0 wne cdiv co c1 cmul wceq cif oveq2 oveq2d eqeq12d cc ax-1cn elimne0
      keepel divreci dedth ) BEFZABGHZAIBGHZJHZKAUBBILZGHZAIUFGHZJHZKBIBUFKZUCU
      GUEUIBUFAGMUJUDUHAJBUFIGMNOAUFCUBBIPDQSBRTUA $.
      $( [11-Oct-1999] $)
  $}

  $( Relationship between division and reciprocal.  Theorem I.9 of [Apostol]
     p. 18. $)
  divrec $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                ( A / B ) = ( A x. ( 1 / B ) ) ) $=
    ( cc wcel cc0 wne cdiv co c1 cmul wceq cif oveq1 eqeq12d imbi2d neeq1 oveq2
    wi 0cn elimel oveq2d imbi12d divreczi dedth2h 3impia ) ACDZBCDZBEFZABGHZAIB
    GHZJHZKZUFUGUHULRUHUFAELZBGHZUMUJJHZKZRUGBELZEFZUMUQGHZUMIUQGHZJHZKZRABEEAU
