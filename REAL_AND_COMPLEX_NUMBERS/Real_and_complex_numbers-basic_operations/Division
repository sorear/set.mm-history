  $( Relationship between division and reciprocal. $)
  divrec2 $p |- ( ( A e. CC /\ B e. CC /\ B =/= 0 ) ->
                ( A / B ) = ( ( 1 / B ) x. A ) ) $=
    ( cc wcel cc0 wne w3a cdiv co cmul divrec wceq simp1 3adant1 mulcom syl2anc
    c1 reccl eqtrd ) ACDZBCDZBEFZGZABHIAQBHIZJIZUDAJIZABKUCTUDCDZUEUFLTUAUBMUAU
    BUGTBRNAUDOPS $.
    $( [7-Feb-2006] $)

  $( An associative law for division. $)
  divass $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) ) ->
                ( ( A x. B ) / C ) = ( A x. ( B / C ) ) ) $=
    ( cc wcel cc0 wne wa cmul co c1 cdiv wceq reccl mulass syl3an3 mulcl divrec
    w3a syl3anc 3adant3 simp3l simp3r simp2 oveq2d 3eqtr4d ) ADEZBDEZCDEZCFGZHZ
    SZABIJZKCLJZIJZABUNIJZIJZUMCLJZABCLJZIJUKUGUHUNDEUOUQMCNABUNOPULUMDEZUIUJUR
    UOMUGUHUTUKABQUAUGUHUIUJUBZUGUHUIUJUCZUMCRTULUSUPAIULUHUIUJUSUPMUGUHUKUDVAV
    BBCRTUEUF $.
    $( [12-Oct-2008] $) $( [2-Aug-2004] $)

  $( A commutative/associative law for division. $)
  div23 $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
            -> ( ( A x. B ) / C ) = ( ( A / C ) x. B ) ) $=
    ( cc wcel cc0 wne w3a cmul cdiv wceq ax-mulcom oveq1d 3adant3 divass 3com12
    wa co simp2 divcl 3expb 3adant2 mulcom syl2anc 3eqtrd ) ADEZBDEZCDEZCFGZQZH
    ZABIRZCJRZBAIRZCJRZBACJRZIRZUPBIRZUFUGUMUOKUJUFUGQULUNCJABLMNUGUFUJUOUQKBAC
    OPUKUGUPDEZUQURKUFUGUJSUFUJUSUGUFUHUIUSACTUAUBBUPUCUDUE $.
    $( [27-Dec-2008] $) $( [2-Aug-2004] $)

  $( A commutative/associative law for division. $)
  div13 $p |- ( ( A e. CC /\ ( B e. CC /\ B =/= 0 ) /\ C e. CC )
            -> ( ( A / B ) x. C ) = ( ( C / B ) x. A ) ) $=
    ( cc wcel cc0 wne wa w3a cmul co cdiv ax-mulcom oveq1d 3adant2 div23 3com23
    wceq 3coml 3eqtr3d ) ADEZBDEBFGHZCDEZIACJKZBLKZCAJKZBLKZABLKCJKZCBLKAJKZUAU
    CUEUGRUBUAUCHUDUFBLACMNOUAUCUBUEUHRACBPQUCUAUBUGUIRCABPST $.
    $( [22-Apr-2005] $)

  $( A commutative/associative law for division. $)
  div12 $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
            -> ( A x. ( B / C ) ) = ( B x. ( A / C ) ) ) $=
    ( cc wcel cc0 wne wa w3a cdiv co cmul divcl 3expb mulcom sylan2 3impb div13
    wceq 3comr sylan 3impa 3com23 3eqtrd ) ADEZBDEZCDEZCFGZHZIABCJKZLKZUJALKZAC
    JKZBLKZBUMLKZUEUFUIUKULSZUFUIHUEUJDEZUPUFUGUHUQBCMNAUJOPQUFUIUEULUNSBCARTUE
    UIUFUNUOSZUEUIUFURUEUIHUMDEZUFURUEUGUHUSACMNUMBOUAUBUCUD $.
    $( [11-Feb-2009] $) $( [30-Apr-2005] $)

  ${
    divass.1 $e |- A e. CC $.
    divass.2 $e |- B e. CC $.
    divass.3 $e |- C e. CC $.
    $( An associative law for division. $)
    divasszi $p |- ( C =/= 0 -> ( ( A x. B ) / C ) = ( A x. ( B / C ) ) ) $=
      ( cc wcel cc0 wne cmul co cdiv wceq wa divass mp3an12 mpan ) CGHZCIJZABKL
      CMLABCMLKLNZFAGHBGHSTOUADEABCPQR $.
      $( [12-Aug-1999] $)

    ${
      divass.4 $e |- C =/= 0 $.
      $( An associative law for division. $)
      divassi $p |- ( ( A x. B ) / C ) = ( A x. ( B / C ) ) $=
        ( cc0 wne cmul co cdiv wceq divasszi ax-mp ) CHIABJKCLKABCLKJKMGABCDEFN
        O $.
        $( [15-Feb-1995] $)

      $( Distribution of division over addition. $)
      divdiri $p |- ( ( A + B ) / C ) = ( ( A / C ) + ( B / C ) ) $=
        ( caddc co c1 cdiv cmul reccli adddiri addcli divreci oveq12i 3eqtr4i )
        ABHIZJCKIZLIATLIZBTLIZHISCKIACKIZBCKIZHIABTDECFGMNSCABDEOFGPUCUAUDUBHAC
        DFGPBCEFGPQR $.
        $( [16-Feb-1995] $)

      $( A commutative/associative law for division. $)
      div23i $p |- ( ( A x. B ) / C ) = ( ( A / C ) x. B ) $=
        ( cmul co cdiv mulcomi oveq1i divassi divcli 3eqtri ) ABHIZCJIBAHIZCJIB
        ACJIZHIRBHIPQCJABDEKLBACEDFGMBREACDFGNKO $.
        $( [3-Sep-1999] $)
    $}

    $( Distribution of division over addition. $)
    divdirzi $p |- ( C =/= 0 ->
                    ( ( A + B ) / C ) = ( ( A / C ) + ( B / C ) ) ) $=
      ( cc0 wne caddc co cdiv wceq c1 cif oveq2 oveq12d eqeq12d ax-1cn keepel
      cc elimne0 divdiri dedth ) CGHZABIJZCKJZACKJZBCKJZIJZLUEUDCMNZKJZAUJKJZBU
      JKJZIJZLCMCUJLZUFUKUIUNCUJUEKOUOUGULUHUMICUJAKOCUJBKOPQABUJDEUDCMTFRSCUAU
      BUC $.
      $( [31-Jul-2004] $)
  $}

  $( Distribution of division over addition. $)
  divdir $p |- ( ( A e. CC /\ B e. CC /\ ( C e. CC /\ C =/= 0 ) )
               -> ( ( A + B ) / C ) = ( ( A / C ) + ( B / C ) ) ) $=
    ( cc wcel cc0 wne caddc co cdiv wceq wi cif oveq1 oveq1d eqeq12d imbi2d 0cn
