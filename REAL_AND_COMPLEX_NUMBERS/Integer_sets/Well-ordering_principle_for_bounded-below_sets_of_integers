    $( There is a unique smallest integer greater than or equal to a given real
       number. $)
    zmin $p |- ( A e. RR ->
                 E! x e. ZZ ( A <_ x /\ A. y e. ZZ ( A <_ y -> x <_ y ) ) ) $=
      ( vz vn cr wcel cv cle wbr cz crab wral wreu wi wa wrex cn wss breq2 arch
      c0 wne clt nnssz ssrexv mpsyl zre ltle sylan2 reximdva mpd sylibr cbvrabv
      rabn0 eqimssi uzwo3 mpanr1 mpdan elrab ralrab anbi12i anass bitri 3bitr4i
      weu eubii df-reu sylib ) CFGZAHZBHZIJZBCDHZIJZDKLZMZAVPNZCVKIJZCVLIJZVMOB
