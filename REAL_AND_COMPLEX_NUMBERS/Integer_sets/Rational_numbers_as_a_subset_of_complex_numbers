    UMVGUSUTACETUNVBCETUO $.
    $( [7-Nov-2008] $)

  $( The product of an irrational with a nonzero rational is irrational. $)
  irrmul $p |- ( ( A e. ( RR \ QQ ) /\ B e. QQ /\ B =/= 0 )
                -> ( A x. B ) e. ( RR \ QQ ) ) $=
    ( cr wcel cq wn wa cc0 wne w3a cmul co qre remulcl sylan2 wi 3expb cc eldif
    cdif ad2ant2r cdiv qdivcl expcom adantl wceq divcan4 syl3an1 syl3an2 eleq1d
