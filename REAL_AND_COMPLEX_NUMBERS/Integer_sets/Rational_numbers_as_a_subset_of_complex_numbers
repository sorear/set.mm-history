    $( [2-Aug-2004] $)

  $( The reciprocal of a natural number is rational. $)
  nnrecq $p |- ( A e. NN -> ( 1 / A ) e. QQ ) $=
    ( c1 cz wcel cn cdiv co cq 1z znq mpan ) BCDAEDBAFGHDIBAJK $.
    $( [17-Nov-2004] $)

  $( The sum of an irrational number and a rational number is irrational. $)
  irradd $p |- ( ( A e. ( RR \ QQ ) /\ B e. QQ )
                -> ( A + B ) e. ( RR \ QQ ) ) $=
    ( cr wcel cq wn wa caddc cdif qre readdcl sylan2 adantlr cmin qsubcl expcom
    co wi cc eldif adantl wceq qcn pncan syl2an eleq1d sylibd con3d com23 imp31
    recn ex jca anbi1i 3imtr4i ) ACDZAEDZFZGZBEDZGZABHQZCDZVBEDZFZGACEIZDZUTGVB
    VFDVAVCVEUPUTVCURUTUPBCDVCBJABKLMUPURUTVEUPUTURVEUPUTURVERUPUTGZVDUQVHVDVBB
