    $( Lemma for ~ sqrth . $)
    sqrthlem $p |- ( A e. CC -> ( ( ( sqr ` A ) ^ 2 ) = A /\
        0 <_ ( Re ` ( sqr ` A ) ) /\ ( _i x. ( sqr ` A ) ) e/ RR+ ) ) $=
      ( vx vy cc wcel cfv c2 cexp co wceq cc0 cre cle wbr ci cmul crp wnel w3a
      cv csqr crio sqrval eqcomd wreu sqrcl sqreu ax-17 a17d oveq1 eqeq1d fveq2
      wb breq2d oveq2 neleq1 syl 3anbi123d riota2f syl2anc mpbird ) ADEZAUAFZGH
