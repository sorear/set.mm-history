  $( An integer divides another iff its absolute value does.  (Contributed by
     Paul Chapman, 21-Mar-2011.) $)
  absdvdsb $p |- ( ( M e. ZZ /\ N e. ZZ ) ->
                   ( M || N <-> ( abs ` M ) || N ) ) $=
    ( cz wcel wa cabs cfv wceq wo cdivides wbr wb cr zre absor syl adantr breq1
    cneg bicomd wi a1i negdvdsb anim12i bitr ex jaod mpd ) ACDZBCDZEZAFGZAHZULA
    SZHZIZABJKZULBJKZLZUIUPUJUIAMDUPANAOPQUKUMUSUOUMUSUAUKUMURUQULABJRTUBUKUOUS
    UKUOEUQUNBJKZLZUTURLZEUSUKVAUOVBABUCUOURUTULUNBJRTUDUQUTURUEPUFUGUH $.
    $( [21-Mar-2011] $)

  $( An integer divides another iff it divides its absolute value.
     (Contributed by Paul Chapman, 21-Mar-2011.) $)
  dvdsabsb $p |- ( ( M e. ZZ /\ N e. ZZ ) ->
                   ( M || N <-> M || ( abs ` N ) ) ) $=
    ( cz wcel wa cabs cfv wceq wo cdivides wbr wb cr zre absor syl adantl breq2
    cneg bicomd wi a1i dvdsnegb anim12i bitr ex jaod mpd ) ACDZBCDZEZBFGZBHZULB
    SZHZIZABJKZAULJKZLZUJUPUIUJBMDUPBNBOPQUKUMUSUOUMUSUAUKUMURUQULBAJRTUBUKUOUS
