$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                                Prime numbers
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $c Prime $.

  $( Extend the definition of a class to include the set of prime numbers. $)
  cprime $a class Prime $.

  ${
    $d n p $.
    $( Define the set of prime numbers. $)
    df-prime $a |- Prime = { p e. NN | { n e. NN | n || p } ~~ 2o } $.
  $}

  ${
    $d P n p z $.
    $( The predicate "is a prime number".  A prime number is a positive integer
       with exactly two positive divisors.  (Contributed by Paul Chapman,
       22-Jun-2011.) $)
    isprm $p |- ( P e. Prime <->
                  ( P e. NN /\ { n e. NN | n || P } ~~ 2o ) ) $=
      ( vp cv cdivides wbr cn crab c2o cen cprime wceq wb breq2 adantr rabbidva
      wcel breq1d df-prime elrab2 ) BDZCDZEFZBGHZIJFUAAEFZBGHZIJFCAGKUBALZUDUFI
      JUGUCUEBGUGUCUEMUAGQUBAUAENOPRBCST $.
      $( [22-Jun-2011] $)


    $( A prime number is an integer.  (Contributed by Paul Chapman,
       22-Jun-2011.) $)
    prmz $p |- ( P e. Prime -> P e. ZZ ) $=
      ( vn cprime wcel cn cz cv cdivides wbr crab c2o cen isprm simplbi nnz syl
      ) ACDZAEDZAFDQRBGAHIBEJKLIABMNAOP $.
      $( [23-Nov-2012] $) $( [22-Jun-2011] $)

    $( A prime number is a positive integer.  (Contributed by Paul Chapman,
       22-Jun-2011.) $)
    prmnn $p |- ( P e. Prime -> P e. NN ) $=
      ( vz cprime wcel cn cv cdivides wbr crab c2o cen wa isprm biimpi simpld )
      ACDZAEDZBFAGHBEIJKHZPQRLABMNO $.
      $( [22-Jun-2011] $) $( [26-Jun-2011] $)
  $}

