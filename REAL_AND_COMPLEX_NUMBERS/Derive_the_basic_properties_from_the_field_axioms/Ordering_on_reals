      $( [15-Sep-2013] $)
  $}

  $( A nonzero nonnegative number is positive. $)
  ne0gt0 $p |- ( ( A e. RR /\ 0 <_ A ) -> ( A =/= 0 <-> 0 < A ) ) $=
