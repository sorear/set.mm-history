    $( If an outer volume is bounded above, then it is real.  (Contributed by
       Mario Carneiro, 18-Mar-2014.) $)
    ovollecl $p |- ( ( A C_ RR /\ B e. RR /\ ( vol* ` A ) <_ B ) ->
                     ( vol* ` A ) e. RR ) $=
      ( cr wss wcel covol cfv cle wbr w3a cxr cmnf clt simp1 ovolcl syl ovolge0
      simp2 cc0 0re mnflt ax-mp wa mnfxr xrltletr mp3an12 mpani sylc simp3 xrre
      wi 0xr syl22anc ) ACDZBCEZAFGZBHIZJZUPKEZUOLUPMIZUQUPCEURUNUSUNUOUQNZAOPZ
