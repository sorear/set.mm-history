  $( A closed real interval is measurable.  (Contributed by Mario Carneiro,
     16-Jun-2014.) $)
  iccmbl $p |- ( ( A e. RR /\ B e. RR ) -> ( A [,] B ) e. dom vol ) $=
    ( cr wcel wa cicc cdif cvol cdm wss wceq iccssre dfss4 sylib cmnf cioo cpnf
    co cun ioombl difreicc unmbl mp2an syl6eqel cmmbl syl eqeltrrd ) ACDBCDEZCC
    ABFRZGZGZUIHIZUHUICJUKUIKABLUICMNUHUJULDUKULDUHUJOAPRZBQPRZSZULABUAUMULDUNU
    LDUOULDOATBQTUMUNUBUCUDUJUEUFUG $.
    $( [16-Jun-2014] $)

  $( A closed real interval has finite volume.  (Contributed by Mario Carneiro,
     25-Aug-2014.) $)
  iccvolcl $p |- ( ( A e. RR /\ B e. RR ) -> ( vol ` ( A [,] B ) ) e. RR ) $=
    ( cr wcel wa cicc co cvol cfv covol cdm wceq iccmbl syl wbr c0 cxr rexr cc0
    eqeltrd mblvol clt icc0 syl2an biimpar fveq2 ovol0 syl6eq 0re syl6eqel cmin
