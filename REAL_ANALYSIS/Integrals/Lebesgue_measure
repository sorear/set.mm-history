  ${
    $d f g j k m x y z A $.  $d f g j k m n x y z B $.  $d f i j k m n x z F $.
    $d i j k m n w x y z J $.  $d i j m n w x y z K $.  $d i j k n w x y z L $.
    $d f j m n z H $.  $d z M $.  $d g i j k m n x y z ph $.  $d f k x y z S $.
    $d k m x G $.  $d g j k m x y z T $.  $d f j x y z U $.
    ovoliun.t $e |- T = seq 1 ( + , G ) $.
    ovoliun.g $e |- G = ( n e. NN |-> ( vol* ` A ) ) $.
    ovoliun.a $e |- ( ( ph /\ n e. NN ) -> A C_ RR ) $.
    ovoliun.v $e |- ( ( ph /\ n e. NN ) -> ( vol* ` A ) e. RR ) $.
    ${
      $d n G $.  $d n T $.
      ovoliun.r $e |- ( ph -> sup ( ran T , RR* , < ) e. RR ) $.
      ovoliun.b $e |- ( ph -> B e. RR+ ) $.
      ${
        ovoliun.s $e |- S = seq 1 ( + , ( ( abs o. - ) o. ( F ` n ) ) ) $.
        ovoliun.u $e |- U = seq 1 ( + , ( ( abs o. - ) o. H ) ) $.
        ovoliun.h $e |- H = ( k e. NN |-> ( ( F ` ( 1st ` ( J ` k ) ) ) `
                                                   ( 2nd ` ( J ` k ) ) ) ) $.
        ovoliun.j $e |- ( ph -> J : NN -1-1-onto-> ( NN X. NN ) ) $.
        ovoliun.f $e |- ( ph
               -> F : NN --> ( ( < i^i ( RR X. RR ) ) ^m NN ) ) $.
        ovoliun.x1 $e |- ( ( ph /\ n e. NN ) ->
                          A C_ U. ran ( (,) o. ( F ` n ) ) ) $.
        ovoliun.x2 $e |- ( ( ph /\ n e. NN ) -> sup ( ran S , RR* , < ) <_
                           ( ( vol* ` A ) + ( B / ( 2 ^ n ) ) ) ) $.
        ${
          ovoliun.k $e |- ( ph -> K e. NN ) $.
          ovoliun.l1 $e |- ( ph -> L e. ZZ ) $.
          ovoliun.l2 $e |- ( ph ->
                            A. w e. ( 1 ... K ) ( 1st ` ( J ` w ) ) <_ L ) $.
          $( Lemma for ~ ovoliun . $)
          ovoliunlem1 $p |- ( ph ->
                ( U ` K ) <_ ( sup ( ran T , RR* , < ) + B ) ) $=
