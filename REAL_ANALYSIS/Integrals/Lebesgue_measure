        ovolicc2.13 $e |- ( ph -> A e. C ) $.
        ovolicc2.14 $e |- ( ph -> C e. T ) $.
        ovolicc2.15 $e |- K = seq 1 ( ( H o. 1st ) , ( NN X. { C } ) ) $.
        ovolicc2.16 $e |- W = { n e. NN | B e. ( K ` n ) } $.
        $( Lemma for ~ ovolicc2 . $)
        ovolicc2lem2 $p |- ( ( ph /\ ( N e. NN /\ -. N e. W ) ) ->
