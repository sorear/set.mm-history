    ${
      ovolshft.4 $e |- M = { y e. RR* |
        E. f e. ( ( < i^i ( RR X. RR ) ) ^m NN ) ( B C_ U. ran ( (,) o. f ) /\
           y = sup ( ran seq 1 ( + , ( ( abs o. - ) o. f ) ) , RR* , < ) ) } $.
      ${
        ovolshft.5 $e |- S = seq 1 ( + , ( ( abs o. - ) o. F ) ) $.
        ovolshft.6 $e |- G = ( n e. NN |->
          <. ( ( 1st ` ( F ` n ) ) + C ) , ( ( 2nd ` ( F ` n ) ) + C ) >. ) $.
        ovolshft.7 $e |- ( ph -> F : NN --> ( < i^i ( RR X. RR ) ) ) $.
        ovolshft.8 $e |- ( ph -> A C_ U. ran ( (,) o. F ) ) $.
        $( Lemma for ~ ovolshft . $)
        ovolshftlem1 $p |- ( ph -> sup ( ran S , RR* , < ) e. M ) $=
          ( caddc cabs cmin ccom c1 cseq crn cxr clt csup cn cr cxp cin crp wfn
