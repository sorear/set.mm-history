    ${
      ovolval.2 $e |- S = seq 1 ( + , ( ( abs o. - ) o. F ) ) $.
      $( Sufficient condition for elementhood in the set ` M ` . $)
      elovolmr $p |- ( ( F : NN --> ( < i^i ( RR X. RR ) ) /\
