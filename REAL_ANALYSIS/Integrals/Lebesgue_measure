    $( Lemma for ~ ovolun . $)
    ovolunlem2 $p |- ( ph ->
        ( vol* ` ( A u. B ) ) <_ ( ( ( vol* ` A ) + ( vol* ` B ) ) + C ) ) $=
      ( vg vh ccom crn wss caddc c1 cfv co wa cr wrex wcel vn cioo cv cuni cabs
      cmin cseq cxr clt csup covol c2 cdiv cle wbr cxp cin cn cun simpld simprd
      cmap crp rphalfcl syl eqid ovolgelb syl3anc w3a cif cmpt 3ad2ant1 simp3ll
      reeanv simp2l simp3lr simp2r simp3rl simp3rr ovolunlem1 rexlimdvv syl5bir
      3exp mp2and ) ABUBHUCZJKUDLZMUEUFJZWEJNUGZKUHUIUJBUKOZDULUMPZMPUNUOZQZHUI
      RRUPUQURVBPZSZCUBIUCZJKUDLZMWGWOJNUGZKUHUIUJCUKOZWJMPUNUOZQZIWMSZBCUSUKOW
      IWRMPDMPUNUOZABRLZWIRTZWJVCTZWNAXCXDEUTAXCXDEVAADVCTZXEGDVDVEZBWJWHHWHVFZ
      VGVHACRLZWRRTZXEXAAXIXJFUTAXIXJFVAXGCWJWQIWQVFZVGVHWNXAQWLWTQZIWMSHWMSAXB
      WLWTHIWMWMVNAXLXBHIWMWMAWEWMTZWOWMTZQZXLXBAXOXLVIBCDWHWQMWGUAURUAUCZULUMP
      ZURTXQWOOXPNMPULUMPWEOVJVKZJNUGZUAWEWOXRAXOXCXDQXLEVLAXOXIXJQXLFVLAXOXFXL
      GVLXHXKXSVFAXMXNXLVOWFWKWTAXOVMWFWKWTAXOVPAXMXNXLVQWPWSWLAXOVRWPWSWLAXOVS
      XRVFVTWCWAWBWD $.
      $( [12-Jun-2014] $)
  $}

  ${
    $d f g h x y A $.  $d f g h x y B $.
    $( The Lebesgue outer measure function is finitely sub-additive.  (Unlike
       the stronger ~ ovoliun , this does not require any choice principles.)
       (Contributed by Mario Carneiro, 12-Jun-2014.) $)
    ovolun $p |- ( ( ( A C_ RR /\ ( vol* ` A ) e. RR ) /\
                     ( B C_ RR /\ ( vol* ` B ) e. RR ) ) ->
                  ( vol* ` ( A u. B ) ) <_ ( ( vol* ` A ) + ( vol* ` B ) ) ) $=
      ( vx cr wss covol cfv wcel wa cun caddc co cle wbr crp wral simpll simplr
      cv simpr ovolunlem2 ralrimiva cxr unss biimpi ad2ant2r ovolcl syl readdcl
      wb ad2ant2l xralrple syl2anc mpbird ) ADEZAFGZDHZIZBDEZBFGZDHZIZIZABJZFGZ
      UPUTKLZMNZVEVFCSZKLMNZCOPZVCVICOVCVHOHZIABVHURVBVKQURVBVKRVCVKTUAUBVCVEUC
      HZVFDHZVGVJUJVCVDDEZVLUOUSVNUQVAUOUSIVNABDUDUEUFVDUGUHUQVAVMUOUSUPUTUIUKC
      VEVFULUMUN $.
      $( [12-Jun-2014] $)
  $}

  ${
    $d k m x y z A $.  $d m x y z B $.
    $( The Lebesgue outer measure function is finitely sub-additive.  Finite
       sum version.  (Contributed by Mario Carneiro, 19-Jun-2014.) $)
    ovolfiniun $p |- ( ( A e. Fin /\ A. k e. A
                         ( B C_ RR /\ ( vol* ` B ) e. RR ) ) ->
                  ( vol* ` U_ k e. A B ) <_ sum_ k e. A ( vol* ` B ) ) $=
      ( vx vm wcel cr wss covol cfv wa wral ciun csu cle wbr wi c0 wceq fveq2d
      vy vz cfn cv csn cun raleq iuneq1 sumeq1 breq12d imbi12d cc0 leidi fveq2i
      0re 0iun ovol0 eqtri sum0 3brtr4i a1i ssun1 ssralv ax-mp imim1i csb caddc
      wn co simprl vex hbcsb1 hbss hbfv hbel hban csbeq1a sseq1d eleq1d anbi12d
      ax-17 rcla4 impcom sylan simpld sylibr iunss1 syl5ss simpll simprd sylan2
      iunss elun1 fsumrecl simprr cbviun cbvsumi 3brtr3g ovollecl syl3anc ssun2
      ralrimiva sselii syl readdcl syl2anc iunxun csbeq1 iunxsn uneq2i syl21anc
      snid ovolun syl5eqbr snfi unfi mpan2 ad2antrr wb leadd1 cin simplr disjsn
      mpbid eqidd recnd fsumsplit cvv sumsn sylancr oveq2d eqtrd breqtrrd letrd
      cc 3brtr4g expr ex a2d syl5 findcard2s imp ) AUCFBGHZBIJZGFZKZCALZCABMZIJ
      ZAUUDCNZOPZUUFCDUDZLZCUULBMZIJZUULUUDCNZOPZQUUFCRLZCRBMZIJZRUUDCNZOPZQUUF
      CUAUDZLZCUVCBMZIJZUVCUUDCNZOPZQZUUFCUVCUBUDZUEZUFZLZCUVLBMZIJZUVLUUDCNZOP
      ZQZUUGUUKQDUAUBAUULRSZUUMUURUUQUVBUUFCUULRUGUVSUUOUUTUUPUVAOUVSUUNUUSICUU
      LRBUHTUULRUUDCUIUJUKUULUVCSZUUMUVDUUQUVHUUFCUULUVCUGUVTUUOUVFUUPUVGOUVTUU
      NUVEICUULUVCBUHTUULUVCUUDCUIUJUKUULUVLSZUUMUVMUUQUVQUUFCUULUVLUGUWAUUOUVO
