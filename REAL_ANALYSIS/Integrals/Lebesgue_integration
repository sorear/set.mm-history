    $( Reverse a directed integral. $)
    ditgswap $p |- ( ph -> S_ [ B -> A ] C _d x = -u S_ [ A -> B ] C _d x ) $=
      ( cr wcel cle wbr co cxr cdit cneg wceq cicc w3a wb elicc2 syl2anc simp1d
      mpbid wa cioo simpr adantr ditgneg ditgpos negeqd eqtr4d cc cv wss ressxr
