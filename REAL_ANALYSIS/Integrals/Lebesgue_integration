    $(
    @( Refinement of ~ itg2seq : the sequence of functions in ~ itg2seq can be
       chosen to be weakly increasing. @)
    itg2seq2 @p |- ( F : RR --> ( 0 [,] +oo ) -> E. g ( g : NN --> dom S.1 /\
        A. n e. NN ( ( g ` ( n + 1 ) ) oR <_ ( g ` n ) /\
        ( g ` n ) oR <_ F ) /\ ( S.2 ` F ) =
        sup ( ran ( n e. NN |-> ( S.1 ` ( g ` n ) ) ) , RR* , < ) ) ) @=
      ? @.
    $)
  $}

  ${
    $d x y A $.  $d x F $.  $d x y G $.  $d x y ph $.
    itg2uba.1 $e |- ( ph -> F : RR --> ( 0 [,] +oo ) ) $.
    itg2uba.2 $e |- ( ph -> G e. dom S.1 ) $.
    itg2uba.3 $e |- ( ph -> A C_ RR ) $.
    itg2uba.4 $e |- ( ph -> ( vol* ` A ) = 0 ) $.
    itg2uba.5 $e |- ( ( ph /\ x e. ( RR \ A ) ) -> ( G ` x ) <_ ( F ` x ) ) $.
    $( Approximate version of ~ itg2ub .  If ` F ` approximately dominates
       ` G ` , then ` S.1 G <_ S.2 F ` .  (Contributed by Mario Carneiro,
       11-Aug-2014.) $)
    itg2uba $p |- ( ph -> ( S.1 ` G ) <_ ( S.2 ` F ) ) $=
      ( cfv cr wcel cc0 cle wbr wceq syl wa cxr vy citg1 cv cif cmpt citg2 cdif
      cdm cvol covol nulmbl syl2anc cmmbl wn ifnot eldif baibr syl5eqr mpteq2ia
      ifbid i1fres wf i1ff eldifi ffvelrn syl2an leid eleq1 fveq2 ifbieq2d eqid
      wss 0re elexi fvex ifex fvmpt iffalse sylan9eq sylbi adantl breqtrrd cpnf
      itg1lea cicc cofr wral iftrue sylan elxrge0 sylib simprd eqbrtrd sylan2br
      co adantr anassrs pm2.61dan ralrimiva cvv reex a1i eqidd wfn ffn ofrfval2
      dffn5v mpbird itg2ub syl3anc wi ressxr itg1cl sseldi itg2cl xrletr mp2and
      ) AEUBKZBLBUCZCMZNXSEKZUDZUEZUBKZOPZYDDUFKZOPZXRYFOPZAUACEYCGHIAEUBUHZMZL
