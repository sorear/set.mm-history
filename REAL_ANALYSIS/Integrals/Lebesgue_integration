    ditgsplit.b $e |- ( ph -> B e. ( X [,] Y ) ) $.
    ditgsplit.c $e |- ( ph -> C e. ( X [,] Y ) ) $.
    ditgsplit.d $e |- ( ( ph /\ x e. ( X [,] Y ) ) -> D e. V ) $.
    ditgsplit.i $e |- ( ph -> ( x e. ( X [,] Y ) |-> D ) e. Itgbl ) $.
    ${
      ditgsplit.1 $e |- ( ( ps /\ th ) <-> ( A <_ B /\ B <_ C ) ) $.
      $( Lemma for ~ ditgsplit . $)
      ditgsplitlem $p |- ( ( ( ph /\ ps ) /\ th ) -> S_ [ A -> C ] D _d x =
        ( S_ [ A -> B ] D _d x + S_ [ B -> C ] D _d x ) ) $=
        ( wcel cdit caddc co wceq wa cicc citg cc cr cle wbr w3a elicc2 syl2anc
        wb mpbid simp1d adantr simpr sylib simpld simprd mpbir3and cv wss iccss
        simp2d simp3d syl222anc sselda cmpt cibl cmbf iblmbf syl syldan adantlr
        mbfmptcl cvol cdm iccmbl iblss itgspliticc wi idi letrd ditgpos oveq12d
        3eqtr4d anassrs ) ABCDEGHUAZDEFHUAZDFGHUAZUBUCZUDABCUEZUEZDEGUFUCZHUGDE
        FUFUCZHUGZDFGUFUCZHUGZUBUCWKWNWPDEFGHUHAEUITZWOAXBJEUJUKZEKUJUKZAEJKUFU
