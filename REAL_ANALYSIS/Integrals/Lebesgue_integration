    $( A function is measurable iff its positive and negative parts are
       measurable.  (Contributed by Mario Carneiro, 11-Aug-2014.) $)
    mbfposb $p |- ( ph -> ( ( x e. A |-> B ) e. MblFn <->
      ( ( x e. A |-> if ( 0 <_ B , B , 0 ) ) e. MblFn /\
        ( x e. A |-> if ( 0 <_ -u B , -u B , 0 ) ) e. MblFn ) ) ) $=
