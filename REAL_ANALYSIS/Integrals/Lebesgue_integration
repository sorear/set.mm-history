  ${
    $d f g j k m n x y z F $.  $d f g j k m n x y P $.  $d f g j k m n x y Q $.
    $d f g j k m n x y z G $.  $d f g j k m y z ph $.
    itg2add.f1 $e |- ( ph -> F e. MblFn ) $.
    itg2add.f2 $e |- ( ph -> F : RR --> ( 0 [,) +oo ) ) $.
    itg2add.f3 $e |- ( ph -> ( S.2 ` F ) e. RR ) $.
    itg2add.g1 $e |- ( ph -> G e. MblFn ) $.
    itg2add.g2 $e |- ( ph -> G : RR --> ( 0 [,) +oo ) ) $.
    itg2add.g3 $e |- ( ph -> ( S.2 ` G ) e. RR ) $.
    ${
      itg2add.p1 $e |- ( ph -> P : NN --> dom S.1 ) $.
      itg2add.p2 $e |- ( ph -> A. n e. NN
        ( 0p oR <_ ( P ` n ) /\ ( P ` n ) oR <_ ( P ` ( n + 1 ) ) ) ) $.
      itg2add.p3 $e |- ( ph -> A. x e. RR
        ( n e. NN |-> ( ( P ` n ) ` x ) ) ~~> ( F ` x ) ) $.
      itg2add.q1 $e |- ( ph -> Q : NN --> dom S.1 ) $.
      itg2add.q2 $e |- ( ph -> A. n e. NN
        ( 0p oR <_ ( Q ` n ) /\ ( Q ` n ) oR <_ ( Q ` ( n + 1 ) ) ) ) $.
      itg2add.q3 $e |- ( ph -> A. x e. RR
        ( n e. NN |-> ( ( Q ` n ) ` x ) ) ~~> ( G ` x ) ) $.
