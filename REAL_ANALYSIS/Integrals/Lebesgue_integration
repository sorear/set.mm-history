  $(
  @{
    @d k A @.  @d k B @.  @d k C @.  @d k x ph @.
    itgmulc2lem.1 @e |- ( ph -> C e. CC ) @.
    itgmulc2lem.2 @e |- ( ( ph /\ x e. A ) -> B e. RR ) @.
    itgmulc2lem.3 @e |- ( ( ph /\ x e. A ) -> 0 <_ B ) @.
    itgmulc2lem.4 @e |- ( ph -> ( x e. A |-> B ) e. Itgbl ) @.
    @( Lemma for ~ itgmulc2 . @)
    itgmulc2lem @p |- ( ph -> ( ( x e. A |-> ( C x. B ) ) e. Itgbl /\
                        ( C x. S. A B _d x ) = S. A ( C x. B ) _d x ) ) @=
      ? @.
  @}

  @{
    @d k A @.  @d k B @.  @d k C @.  @d k x ph @.
    itgmulc2.1 @e |- ( ph -> C e. CC ) @.
    itgmulc2.2 @e |- ( ( ph /\ x e. A ) -> B e. V ) @.
    itgmulc2.3 @e |- ( ph -> ( x e. A |-> B ) e. Itgbl ) @.
    @( Multiply an integral by a constant. @)
    iblmulc2 @p |- ( ph -> ( x e. A |-> ( C x. B ) ) e. Itgbl ) @=
      ? @.

    @( Multiply an integral by a constant. @)
    itgmulc2 @p |- ( ph -> ( C x. S. A B _d x ) = S. A ( C x. B ) _d x ) @=
      ? @.
  @}
