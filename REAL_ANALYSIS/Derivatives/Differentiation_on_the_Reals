      $( The product rule for derivatives at a point. $)
      dvmulbr $p |- ( ph -> C ( _D ` ( F oF x. G ) )
                      ( ( ( ( _D ` F ) ` C ) x. ( G ` C ) ) +
                        ( ( ( _D ` G ) ` C ) x. ( F ` C ) ) ) ) $=
        ( vy vx cdv cfv cmul co caddc cin cnt wcel cv wceq cmin cdiv cif csubsp
        cmpt cop ccnp cdm cc wss dvbssntr syl2anc sseldd elin sylanbrc ctop a1i
        wf cr cuni syl3anc eleqtrrd ccom wa sseli wi dvbss dvf ffvelrn syl22anc
        dvlem imp sylan2 mulcl opelxpi eqidd feqmptd fveq2 df-ov syl6eqr fmptco
        syl5eqel cc0 ad2antrr simplr subcl sylanl2 wb syl112anc oveq12d cvv wfn
        eqtrd ffn syl reex ssexg sylancl eqid ofval mpdan 3eqtr4d oveq1d adantl
        iffalse syl5eq div23 eqtr4d iftrue sylancr stoig3 ax-mp syl6sseq stoig2
        eleqtrd mpteq1 cres resmpt fmptd feq2d mpbid cnpsubsp syl23anc eqeltrrd
        cncnpi cnpco syl33anc cof wbr cioo crn ctg retop uniretop unieqi eqtr4i
        eqeltri ntrin cxp inss1 syl2an adantr ax-addopr wn wne ax-resscn syl6ss
        inss2 simpr subeq0 necon3abid mpbird divdir npncan subdir mulcom eqtr3d
        subdi ex pm2.61d2 mpteq2dva ctx cabs cnmet opntop syl5ss txtopi cnmetba
        cme uniopn2 ax-mulopr mpteq2i dvcnp syl5sseq cdif cun difss unssd ssun1
        ntrss subntr syl5eqelr subsubtop fveq1d reseq1d syl6eq dvcnp2 txcnp ccn
        eqeltrd mulcn txunii oveq1 cnmptid cnmptc cnmpt12f addcn off addcl eldv
