    $( The sum rule for derivatives at a point.  (Contributed by Mario
       Carneiro, 9-Aug-2014.) $)
    dvadd $p |- ( ph -> ( ( _D ` ( F oF + G ) ) ` C ) =
                  ( ( ( _D ` F ) ` C ) + ( ( _D ` G ) ` C ) ) ) $=
      ( caddc co cfv cc cr cvv wcel eqid vx vy vz cof cdv wfun wbr wceq cdm cin
      wf wss cv wa addcl adantl reex ssexg sylancl off inss1 syl5ss dvf syl2anc
      ffun syl cmin cdiv cif cioo crn cabs ccom copn dvaddbr ovex funbrfv sylc
      ctg ) ACDMUDNZUEOZUFZBBCUEOOZBDUEOOZMNZWAUGBWAOWEUHAWAUIZPWAUKZWBAEFUJZPV
      TUKWHQULWGAUAUBEFWHMPPPCDRRUAUMZPSUBUMZPSUNWIWJMNPSAWIWJUOUPGIAEQULQRSZER
      SHUQEQRURUSAFQULWKFRSJUQFQRURUSWHTUTAWHEQEFVAHVBWHVTVCVDWFPWAVEVFAUCUCUMZ
      BUHZWCWLCOBCOVGNWLBVGNZVHNVIZWMWDWLDOBDOVGNWNVHNVIZBCDVJVKVSOZVLVGVMVNOZE
      FGHIJKLWQTWRTWOTWPTVOBWEWAWCWDMVPVQVR $.
      $( [9-Aug-2014] $)

    $( The product rule for derivatives at a point.  (Contributed by Mario
       Carneiro, 9-Aug-2014.) $)
    dvmul $p |- ( ph -> ( ( _D ` ( F oF x. G ) ) ` C ) =
                  ( ( ( ( _D ` F ) ` C ) x. ( G ` C ) ) +
                    ( ( ( _D ` G ) ` C ) x. ( F ` C ) ) ) ) $=
      ( cmul co cfv cc cr cvv wcel eqid vx vy vz cof cdv wfun caddc wbr wceq wf
      cdm cin wss cv wa mulcl adantl ssexg sylancl off inss1 syl5ss dvf syl2anc
      reex ffun syl cmin cdiv cif cioo crn ctg cabs ccom copn dvmulbr ovex sylc
      funbrfv ) ACDMUDNZUEOZUFZBBCUEOOZBDOZMNZBDUEOOZBCOZMNZUGNZWBUHBWBOWJUIAWB
      UKZPWBUJZWCAEFULZPWAUJWMQUMWLAUAUBEFWMMPPPCDRRUAUNZPSUBUNZPSUOWNWOMNPSAWN
      WOUPUQGIAEQUMQRSZERSHVEEQRURUSAFQUMWPFRSJVEFQRURUSWMTUTAWMEQEFVAHVBWMWAVC
      VDWKPWBVFVGAUCUCUNZBUIZWDWQCOWHVHNWQBVHNZVINVJZWRWGWQDOWEVHNWSVINVJZBCDVK
      VLVMOZVNVHVOVPOZEFGHIJKLXBTXCTWTTXATVQBWJWBWFWIUGVRVTVS $.
      $( [9-Aug-2014] $)
  $}

  ${
    $d x y z F $.  $d x y z G $.  $d x y z ph $.  $d x z X $.
    dvaddf.f $e |- ( ph -> F : X --> CC ) $.
    dvaddf.g $e |- ( ph -> G : X --> CC ) $.
    dvaddf.x $e |- ( ph -> X C_ RR ) $.
    dvaddf.df $e |- ( ph -> dom ( _D ` F ) = X ) $.
    dvaddf.dg $e |- ( ph -> dom ( _D ` G ) = X ) $.
    $( The sum rule for everywhere-differentiable functions.  (Contributed by
       Mario Carneiro, 9-Aug-2014.) $)
    dvaddf $p |- ( ph -> ( _D ` ( F oF + G ) ) =
                   ( ( _D ` F ) oF + ( _D ` G ) ) ) $=
      ( vx cfv caddc co cvv wcel cc wf syl2anc syl cmin vy vz cdv cof cv cr wss
      ssexg sylancl wfn cdm dvf feq2d mpbid ffn wa addcl adantl inidm off dvbss
      reex wbr wceq cdiv cif cioo crn cabs ccom copn adantr eleq2d biimpar eqid
      ctg dvaddbr vex breldm ex ssrdv eqssd eqidd dvadd eqcomd offveq ) ABUCKZC
      UCKZLUDZMBCWIMZUCKZAJDJUEZWGKZWLWHKZLWGWHWKNADUFUGZUFNODNOGVBDUFNUHUIZADP
