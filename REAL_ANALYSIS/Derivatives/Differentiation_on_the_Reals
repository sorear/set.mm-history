    dvmptco.y $e |- ( ph -> Y C_ RR ) $.
    dvmptco.a $e |- ( ( ph /\ x e. X ) -> A e. Y ) $.
    dvmptco.b $e |- ( ( ph /\ x e. X ) -> B e. V ) $.
    dvmptco.c $e |- ( ( ph /\ y e. Y ) -> C e. CC ) $.
    dvmptco.d $e |- ( ( ph /\ y e. Y ) -> D e. W ) $.
    dvmptco.da $e |- ( ph -> ( _D ` ( x e. X |-> A ) ) = ( x e. X |-> B ) ) $.
    dvmptco.dc $e |- ( ph -> ( _D ` ( y e. Y |-> C ) ) = ( y e. Y |-> D ) ) $.
    dvmptco.e $e |- ( y = A -> C = E ) $.
    dvmptco.f $e |- ( y = A -> D = F ) $.
