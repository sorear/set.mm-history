  ${
    $d m n y z B $.  $d m n u y z E $.  $d m n u x z K $.  $d t u v x y z M $.
    $d m n t u v x y z ph $.  $d m n t u v x y z X $.  $d m n t u v x y z Y $.
    $d m n t u v y z A $.  $d m n t v x y z C $.  $d m n u v x y z W $.
    $d m n u x z L $.  $d t u v x y z N $.  $d m n u v x y z Z $.
    $d m n u x z L $.  $d m n u x y z W $.  $d m n u x y z Z $.
    itgsubst.x $e |- ( ph -> X e. RR ) $.
    itgsubst.y $e |- ( ph -> Y e. RR ) $.
    itgsubst.le $e |- ( ph -> X <_ Y ) $.
    itgsubst.z $e |- ( ph -> Z e. RR* ) $.
    itgsubst.w $e |- ( ph -> W e. RR* ) $.
    itgsubst.a $e |- ( ph ->
      ( x e. ( X [,] Y ) |-> A ) e. ( ( X [,] Y ) -cn-> ( Z (,) W ) ) ) $.
    itgsubst.b $e |- ( ph ->
      ( x e. ( X (,) Y ) |-> B ) e. ( ( ( X (,) Y ) -cn-> CC ) i^i L^1 ) ) $.
    itgsubst.c $e |- ( ph ->
      ( u e. ( Z (,) W ) |-> C ) e. ( ( Z (,) W ) -cn-> CC ) ) $.
    itgsubst.da $e |- ( ph ->
      ( RR _D ( x e. ( X [,] Y ) |-> A ) ) = ( x e. ( X (,) Y ) |-> B ) ) $.
    itgsubst.e $e |- ( u = A -> C = E ) $.
    itgsubst.k $e |- ( x = X -> A = K ) $.
    itgsubst.l $e |- ( x = Y -> A = L ) $.
    ${
      itgsubst.m $e |- ( ph -> M e. ( Z (,) W ) ) $.
      itgsubst.n $e |- ( ph -> N e. ( Z (,) W ) ) $.
      itgsubst.cl2 $e |- ( ( ph /\ x e. ( X [,] Y ) ) -> A e. ( M (,) N ) ) $.
