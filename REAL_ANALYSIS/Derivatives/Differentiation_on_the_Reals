    dvbss.j $e |- J = ( topGen ` ran (,) ) $.
    $( The set of differentiable points is a subset of the interior of the
       domain of the function.  (Contributed by Mario Carneiro, 7-Aug-2014.) $)
    dvbssntr $p |- ( ( F : A --> CC /\ A C_ RR ) ->
      dom ( _D ` F ) C_ ( ( int ` J ) ` A ) ) $=
      ( vx vz vy cc wf cr wss wa cfv cdm cv wcel wceq cmin co eqid cdv cnt cdiv
      cxp cif cmpt cop csubsp cabs ccom copn ccnp dvfval simprd dmss syl dmxpss
      copab syl6ss ) AHBIAJKLZBUAMZNZACUBMMZHUDZNZVCUTVAVDKZVBVEKUTVAEOZVCPFAFO
      ZVGQGOVHBMVGBMRSVHVGRSUCSUEUFVGACUGUHMZUIRUJUKMZULSMPLEGURQVFEGFACBVIVJDV
      ITVJTUMUNVAVDUOUPVCHUQUS $.
      $( [6-Aug-2014] $)
  $}

  $( The set of differentiable points is a subset of the domain of the
     function. $)
  dvbss $p |- ( ( F : A --> CC /\ A C_ RR ) -> dom ( _D ` F ) C_ A ) $=
    ( cc wf cr wss cdv cfv cdm cioo crn ctg eqid dvbssntr ctop wcel retop simpr
    wa cnt uniretop ntrss2 sylancr sstrd ) ACBDZAEFZSZBGHIAJKLHZTHHZAABUHUHMNUG
    UHOPUFUIAFQUEUFRAUHEUAUBUCUD $.
    $( [6-Aug-2014] $)

  ${
    $d j k z A $.  $d j k z B $.  $d j z C $.  $d z F $.  $d j G $.  $d j ph $.
    $d j k H $.  $d j M $.  $d j k N $.  $d k T $.  $d j k Z $.
    dvlm.z $e |- Z = ( ZZ>= ` M ) $.
    dvlm.m $e |- ( ph -> M e. ZZ ) $.
    dvlm.f $e |- ( ph -> F : A --> CC ) $.
    dvlm.a $e |- ( ph -> A C_ RR ) $.
    dvlm.hf $e |- ( ph -> H : Z --> RR ) $.
    dvlm.h $e |- ( ph -> H ~~> B ) $.
    dvlm.c $e |- ( ph -> C e. V ) $.
    dvlm.d $e |- ( ph -> B ( _D ` F ) C ) $.
    dvlm.g $e |- G = ( z e. A |->
        if ( z = B , C , ( ( ( F ` z ) - ( F ` B ) ) / ( z - B ) ) ) ) $.
    ${
      dvlm.t $e |- T = ( topGen ` ran (,) ) $.
      $( Lemma for ~ dvlm . $)
      dvlmlem1 $p |- ( ph -> H ( ~~>t ` T ) B ) $=
        ( cabs cmin ccom copn cfv clm wbr cli cvv wcel cz cc wf wb cdv brrelexi
        reldv syl cr wss ax-resscn fss sylancl eqid lmclimf syl3anc mpbird cioo
        crn ctg cop csubsp tgioo2 eqtri reex a1i ctop cme cnmet ax-mp cdm dvbss
        opntop syl2anc wrel releldm sylancr sseldd lmss mpbid ) AIDUCUDUEZUFUGZ
