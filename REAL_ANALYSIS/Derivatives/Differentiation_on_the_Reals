      $( The product rule for derivatives at a point. $)
      dvmulbr $p |- ( ph -> C ( _D ` ( F oF x. G ) )
                      ( ( ( ( _D ` F ) ` C ) x. ( G ` C ) ) +
                        ( ( ( _D ` G ) ` C ) x. ( F ` C ) ) ) ) $=
        ( vy vx cdv cfv cmul co caddc cin cnt wcel cv wceq cmin cdiv cif csubsp
        cmpt cop ccnp cdm cc wss dvbssntr syl2anc sseldd elin sylanbrc ctop a1i
        wf cr cuni syl3anc eleqtrrd ccom wa sseli wi dvbss dvf ffvelrn syl22anc
        dvlem imp syl5eqel sylan2 mulcl opelxpi eqidd wfn ffn ax-mp dffn5v mpbi
        fveq2 df-ov syl6eqr fmptco cc0 ad2antrr simplr subcl wb syl112anc eqtrd
        sylanl2 oveq12d cvv reex ssexg sylancl eqid ofval mpdan 3eqtr4d iffalse
        oveq1d adantl syl5eq eqtr4d iftrue sylancr stoig3 stoig2 eleqtrd mpteq1
        syl syl6sseq resmpt fmptd feq2d mpbid cnpsubsp syl23anc eqeltrrd cncnpi
        cres cnpco syl33anc cof wbr cioo crn ctg eqeltri uniretop unieqi eqtr4i
        retop ntrin cxp inss1 inss2 syl2an adantr ax-addopr wn ax-resscn syl6ss
        simpr subeq0 necon3abid mpbird divdir npncan subdir mulcom subdi eqtr3d
        wne div23 ex pm2.61d2 mpteq2dva ctx cabs cme cnmet opntop syl5ss txtopi
        cnmetba uniopn2 ax-mulopr mpteq2i dvcnp syl5sseq cdif difss unssd ssun1
        cun ntrss subntr syl5eqelr subsubtop fveq1d sylib reseq1d syl6eq dvcnp2
        txcnp eqeltrd ccn mulcn txunii oveq1 cnmptid cnmptc cnmpt12f addcn eldv
        off addcl mpbir2and ) AEEFUDUEZUEZEGUEZUFUGZEGUDUEZUEZEFUEZUFUGZUHUGZFG
