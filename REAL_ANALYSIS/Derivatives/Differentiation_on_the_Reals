        rolle.l $e |- ( ph -> A. z e. ( A [,] B ) ( ( abs ` ( U - z ) ) < T ->
          ( abs ` ( ( ( _D ` F ) ` U ) - if ( z = U , ( ( _D ` F ) ` U ) ,
            ( ( ( F ` z ) - ( F ` U ) ) / ( z - U ) ) ) ) ) <
          ( abs ` ( ( _D ` F ) ` U ) ) ) ) $.
        rolle.s $e |- ( ph -> S e. RR+ ) $.
        rolle.c $e |- ( ph -> ( ( U - S ) (,) ( U + S ) ) C_ ( A [,] B ) ) $.
