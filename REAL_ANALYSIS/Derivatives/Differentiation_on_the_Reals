  ${
    $d t u v w x y z A $.  $d t u v w x y ph $.  $d z T $.  $d y z X $.
    $d t u v w x y z B $.  $d t u v w x y z F $.  $d u v w x y z U $.
    rolle.a $e |- ( ph -> A e. RR ) $.
    rolle.b $e |- ( ph -> B e. RR ) $.
    rolle.lt $e |- ( ph -> A < B ) $.
    rolle.f $e |- ( ph -> F e. ( ( A [,] B ) -cn-> RR ) ) $.
    rolle.d $e |- ( ph -> dom ( _D ` F ) = ( A (,) B ) ) $.
    rolle.e $e |- ( ph -> ( F ` A ) = ( F ` B ) ) $.
