  ${
    $d t u v w x y z A $.  $d t u v w x y ph $.  $d z T $.  $d y z X $.
    $d t u v w x y z B $.  $d t u v w x y z F $.  $d u v w x y z U $.
    rolle.a $e |- ( ph -> A e. RR ) $.
    rolle.b $e |- ( ph -> B e. RR ) $.
    rolle.lt $e |- ( ph -> A < B ) $.
    rolle.f $e |- ( ph -> F e. ( ( A [,] B ) -cn-> RR ) ) $.
    rolle.d $e |- ( ph -> dom ( RR _D F ) = ( A (,) B ) ) $.
    rolle.e $e |- ( ph -> ( F ` A ) = ( F ` B ) ) $.
    ${
      rolle.r $e |- ( ph -> A. y e. ( A [,] B ) ( F ` y ) <_ ( F ` U ) ) $.
      ${
        rolle.uo $e |- ( ph -> U e. ( A (,) B ) ) $.
        rolle.z $e |- ( ph -> ( ( RR _D F ) ` U ) =/= 0 ) $.
        rolle.t $e |- ( ph -> T e. RR+ ) $.
        rolle.l $e |- ( ph -> A. z e. ( A [,] B ) ( ( abs ` ( U - z ) ) < T ->
          ( abs ` ( ( ( RR _D F ) ` U ) - if ( z = U , ( ( RR _D F ) ` U ) ,
            ( ( ( F ` z ) - ( F ` U ) ) / ( z - U ) ) ) ) ) <
          ( abs ` ( ( RR _D F ) ` U ) ) ) ) $.
        rolle.s $e |- ( ph -> S e. RR+ ) $.
        rolle.c $e |- ( ph -> ( ( U - S ) (,) ( U + S ) ) C_ ( A [,] B ) ) $.
        rolle.x $e |- X = ( U + ( ( if ( S <_ T , S , T ) / 2 ) x.
          if ( 0 < ( ( RR _D F ) ` U ) , 1 , -u 1 ) ) ) $.
        $( Lemma for ~ rolle . $)
        rollelem1 $p |- -. ph $=
          ( cfv clt wbr cc0 cmin co cr cdv wo wne wcel wb cdm wf cicc wss ccncf
          cc ax-resscn cncff sylancr iccssre syl2anc dvfre eleqtrrd ffvelrn 0re
          cioo lttri2 sylancl mpbid wa cneg cdiv caddc cabs wceq adantr wi ltle
          cle imp absnid oveq2d cif c2 c1 cmul ioossre sseldi crp ifcl rphalfcl
          syl rpre 1re renegcli readdcl recnd fveq2d absid ax-mp lt01 mp2an a1i
          ax-1cn oveq12d eqtrd ltletrd eqbrtrd syl3anc simpld simprd cxr ressxr
          absdiflt resubcl ax-1ne0 neeq1 syl22anc wn iffalse wral breq1d rcla4v
          cv fveq2 eqbrtrrd syldan renegcl biimpar breqtrd negeqd gt0div mpbird
          breqtrrd keepel syl5eqel oveq1i pncan2 syl5eq recni absmul rpge0 fvif
          absneg ifeq2 ifid ltleii eqtri 3eqtri mulid1 3eqtrd rphalflt min1 w3a
          remulcl elioo2 mpbir3and sseldd ioossicc eqeltrd rpne0 negne0i ifboth
          mulne0 eqnetrd subneg subeq0 necon3abid abssub min2 oveq2 eqeq1 oveq1
          redivcl oveq1d ifbieq2d imbi12d syl3c abscl ltaddsub2 biimpa ltaddpos
          negsub divneg2 rpgt0 eqtr4d ltnsym syl6eq iftrue adantl mulgt0 jaodan
          mulneg2 negnegi ltaddpos2 mpdan posdif sylc lenlt pm2.65i ) AHIUEZJIU
