      $( The product rule for derivatives at a point. $)
      dvmulbr $p |- ( ph -> C ( _D ` ( F oF x. G ) )
                      ( ( ( ( _D ` F ) ` C ) x. ( G ` C ) ) +
                        ( ( ( _D ` G ) ` C ) x. ( F ` C ) ) ) ) $=
        ( vx vy cdv cfv cmul co caddc cin cnt wcel cv wceq cmin cdiv cif csubsp
        cmpt cop ccnp cc wf cr wss wa wb cvv mulcl adantl reex sylancl eqid cdm
        ssexg dvf syl2anc ffvelrn dvbss syl3anc dvbssntr elin sylanbrc ctop a1i
        sseldd cuni eleqtrrd ccom sseli wi syl22anc imp syl5eqel sylan2 opelxpi
        dvlem eqidd wfn ffn ax-mp dffn5v mpbi fveq2 syl6eqr fmptco cc0 ad2antrr
        df-ov simplr subcl sylanl2 syl112anc eqtrd oveq12d ofval 3eqtr4d oveq1d
        syl iffalse syl5eq eqtr4d iftrue sylancr stoig3 syl6sseq stoig2 eleqtrd
        mpdan mpteq1 resmpt fmptd feq2d mpbid cnpsubsp syl23anc eqeltrrd cncnpi
        cres cnpco syl33anc cof wbr off inss1 syl5ss addcl eldv crn ctg eqeltri
        retop uniretop unieqi eqtr4i ntrin cxp inss2 syl2an adantr ax-addopr wn
        cioo wne ax-resscn syl6ss subeq0 necon3abid mpbird divdir npncan subdir
        simpr mulcom subdi eqtr3d div23 ex pm2.61d2 mpteq2dva cabs cnmet opntop
        ctx cme cnmetba uniopn2 ax-mulopr mpteq2i dvcnp syl5sseq cdif cun difss
        txtopi unssd ssun1 ntrss subntr syl5eqelr subsubtop fveq1d sylib syl6eq
        reseq1d dvcnp2 txcnp eqeltrd mulcn txunii oveq1 cnmptid cnmptc cnmpt12f
        ccn addcn mpbir2and ) AEEFUDUEZUEZEGUEZUFUGZEGUDUEZUEZEFUEZUFUGZUHUGZFG
