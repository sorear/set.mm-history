  ${
    $d j k z A $.  $d j k z B $.  $d j z C $.  $d z F $.  $d j G $.  $d j ph $.
    $d j k H $.  $d z K $.  $d j M $.  $d j k N $.  $d j k z S $.  $d j k Z $.
    $d k T $.
    dvlm.z $e |- Z = ( ZZ>= ` M ) $.
    dvlm.m $e |- ( ph -> M e. ZZ ) $.
    dvlm.s $e |- ( ph -> S C_ CC ) $.
    dvlm.f $e |- ( ph -> F : A --> CC ) $.
    dvlm.a $e |- ( ph -> A C_ S ) $.
    dvlm.hf $e |- ( ph -> H : Z --> S ) $.
    dvlm.h $e |- ( ph -> H ~~> B ) $.
    dvlm.c $e |- ( ph -> C e. V ) $.
    dvlm.d $e |- ( ph -> B ( S _D F ) C ) $.
    dvlm.g $e |- G = ( z e. A |->
        if ( z = B , C , ( ( ( F ` z ) - ( F ` B ) ) / ( z - B ) ) ) ) $.
