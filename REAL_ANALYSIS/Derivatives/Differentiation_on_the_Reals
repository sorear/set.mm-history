$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                       Differentiation on the Reals
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Introduce new constant symbols. $)
  $c _D $. $( Derivative operator $)
  $c Dn $. $( n-th derivative operator $)
  $c C^n $. $( Continuously differentiable function $)

  $( The derivative operator. $)
  cdv $a class _D $.

  $( The ` n ` -th derivative operator. $)
  cdvn $a class Dn $.

  $( The set of ` n ` -times continuously differentiable functions. $)
  ccpn $a class C^n $.

  ${
    $d f j s x y z $.
    $( Define the derivative operator on functions on the reals.  This acts on
       functions to produce a function that is defined where the original
       function is differentiable, with value the derivative of the function at
       these points.  The set ` s ` here is the ambient topological space under
       which we are evaluating the continuity of the difference quotient.
       Although the definition is valid for any subset of ` CC ` and is well
       behaved when ` s ` contains no isolated points, we will restrict our
       attention to the cases ` s = RR ` or ` s = CC ` for the majority of the
       development, these corresponding respectively to real and complex
       differentiation. $)
    df-dv $a |- _D = ( s e. ~P CC , f e. ( CC ^pm s ) |-> { <. x , y >. |
      [ ( MetOpen ` ( abs o. - ) ) / j ]
      ( x e. ( ( int ` ( s subSp j ) ) ` dom f ) /\ ( z e. dom f |->
      if ( z = x , y , ( ( ( f ` z ) - ( f ` x ) ) / ( z - x ) ) ) ) e.
        ( ( ( dom f subSp j ) CnP j ) ` x ) ) } ) $.

    $( Define the ` n ` -th derivative operator on functions on the reals.
       This just iterates the derivative operation according to the first
       argument. $)
    df-dvn $a |- Dn = ( s e. ~P CC , f e. ( CC ^pm s ) |->
      seq 0 ( ( ( x e. _V |-> ( s _D x ) ) o. 1st ) , ( NN0 X. { f } ) ) ) $.

    $( Define the set of ` n ` -times continuously differentiable functions. $)
    df-cpn $a |- C^n = ( s e. ~P CC |-> ( x e. NN0 |-> { f e. ( CC ^pm s ) |
                         ( ( s Dn f ) ` x ) e. ( dom f -cn-> CC ) } ) ) $.

    $( The derivative function is a relation.  (Contributed by Mario Carneiro,
       7-Aug-2014.) $)
    reldv $p |- Rel ( S _D F ) $=
      ( vx vf vs vj vz vy cv cdm csubsp co cnt cfv wcel wceq cmin cdiv cif cc
      cmpt ccnp wa cabs ccom copn wsbc cpw cpm cdv df-dv relmpt2opab ) CIZDIZJZ
      EIZFIZKLMNNOGUOGIZUMPHIURUNNUMUNNQLURUMQLRLSUAUMUOUQKLUQUBLNOUCFUDQUEUFNU
      GEDCHTUHTUPUILABUJCHGDFEUKUL $.
      $( [9-Feb-2015] $) $( [6-Aug-2014] $)
  $}

  ${
    $d f j s x y z A $.  $d x y z B $.  $d f j s x y z F $.  $d f j s x y J $.
    $d x y z C $.  $d f j s x y z K $.  $d f j s x y z S $.  $d f j s x y T $.
    $d x y G $.
    dvval.t $e |- T = ( subSp ` <. S , K >. ) $.
    dvval.j $e |- J = ( subSp ` <. A , K >. ) $.
    dvval.k $e |- K = ( MetOpen ` ( abs o. - ) ) $.
    $( Value and set bounds on the derivative operator. $)
    dvfval $p |- ( ( S C_ CC /\ F : A --> CC /\ A C_ S ) -> ( ( S _D F ) =
      { <. x , y >. | ( x e. ( ( int ` T ) ` A ) /\ ( z e. A |->
        if ( z = x , y , ( ( ( F ` z ) - ( F ` x ) ) / ( z - x ) ) ) ) e.
          ( ( J CnP K ) ` x ) ) } /\
          ( S _D F ) C_ ( ( ( int ` T ) ` A ) X. CC ) ) ) $=
      ( cc co cfv wcel wceq wa csubsp cvv vs vf vj wss wf w3a cdv cnt cmin cdiv
      cv cif cmpt ccnp copab cxp cpw cpm cdm cabs ccom copn wsbc cmpt2 a1i fvex
      df-dv eqcomi simplrl simpr oveq12d cop df-ov eqtr4i syl6eqr simplrr dmeqd
      fveq2d fdm 3ad2ant2 ad2antrr eqtrd fveq12d eleq2d fveq1d oveq1d mpteq12dv
      ifeq2d eleq12d anbi12d sbcied2 oveq2d simp1 cnex elpw2 sylibr simp2 simp3
      opabbidv elpm2r syl22anc wi ctop cuni cme eqid cnmet opntop adantr stoig3
      ax-mp syl5eqel sylancr simpl3 simpl1 cnmetba uniopn2 stoig2 unieqi ntrss2
      syl6sseq sseqtrd syl2anc simprl sseldd iftrue vex fvmpt syl ssexg eleqtrd
      idi sstrd simprr cnpf syl31anc ffvelrn eqeltrrd expr imdistanda ssopab2dv
      df-xp syl6sseqr xpex sylancl ovmpt2dx eqsstrd jca ) EMUDZDMGUEZDEUDZUFZEG
      UGNZAUKZDFUHOZOZPZCDCUKZUUNQZBUKZUURGOZUUNGOZUINZUURUUNUINZUJNZULZUMZUUNH
