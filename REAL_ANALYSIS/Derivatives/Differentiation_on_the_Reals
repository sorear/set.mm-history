$(
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                       Differentiation on the Reals
=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
$)

  $( Introduce new constant symbols. $)
  $c _D $. $( Derivative operator $)
  $c Dn $. $( n-th derivative operator $)
  $c C^n $. $( Continuously differentiable function $)

  $( The derivative operator. $)
  cdv $a class _D $.

  $( The ` n ` -th derivative operator. $)
  cdvn $a class Dn $.

  $( The set of ` n ` -times continuously differentiable functions. $)
  ccpn $a class C^n $.

  ${
    $d f x y z $.
    $( Define the derivative operator on functions on the reals.  This acts on
       functions to produce a function that is defined where the original
       function is differentiable, with value the derivative of the function at
       these points. $)
    df-dv $a |- _D = ( f e. ( CC ^pm RR ) |-> { <. x , y >. |
      ( x e. ( ( int ` ( topGen ` ran (,) ) ) ` dom f ) /\ ( z e. dom f |->
      if ( z = x , y , ( ( ( f ` z ) - ( f ` x ) ) / ( z - x ) ) ) ) e.
        ( ( ( dom f subSp ( topGen ` ran (,) ) ) CnP
          ( MetOpen ` ( abs o. - ) ) ) ` x ) ) } ) $.

    $( Define the ` n ` -th derivative operator on functions on the reals.
       This just iterates the derivative operation according to the first
       argument. $)
    df-dvn $a |- Dn = ( x e. NN0 , f e. ( CC ^pm RR ) |->
      ( seq 0 ( ( _D o. 1st ) , ( NN0 X. { f } ) ) ` x ) ) $.

    $( Define the set of ` n ` -times continuously differentiable functions. $)
    df-cpn $a |- C^n = ( x e. NN0 |-> { f e. ( CC ^pm RR ) |
                         ( x Dn f ) e. ( dom f -cn-> CC ) } ) $.
  $}

  ${
    $d x y z A $.  $d x y z B $.  $d x y z C $.  $d f x y z F $.  $d f x y K $.
    $d x y G $.  $d x y J $.  $d f x y T $.
    $( The derivative function is a relation.  (Contributed by Mario Carneiro,
       7-Aug-2014.) $)
    reldv $p |- Rel ( _D ` F ) $=
      ( vx vf vz vy cv cdm cioo crn ctg cfv cnt wcel wceq cmin co cdiv cif cmpt
      csubsp cabs ccom copn ccnp wa cc cr cpm cdv df-dv relmptopab ) BFZCFZGZHI
      JKZLKKMDUNDFZULNEFUPUMKULUMKOPUPULOPQPRSULUNUOTPUAOUBUCKUDPKMUECBEUFUGUHP
      AUIBEDCUJUK $.
      $( [6-Aug-2014] $)

    dvval.t $e |- T = ( topGen ` ran (,) ) $.
    dvval.j $e |- J = ( subSp ` <. A , T >. ) $.
    dvval.k $e |- K = ( MetOpen ` ( abs o. - ) ) $.
    $( Value and set bounds on the derivative operator. $)
    dvfval $p |- ( ( F : A --> CC /\ A C_ RR ) -> ( ( _D ` F ) =
      { <. x , y >. | ( x e. ( ( int ` T ) ` A ) /\ ( z e. A |->
        if ( z = x , y , ( ( ( F ` z ) - ( F ` x ) ) / ( z - x ) ) ) ) e.
          ( ( J CnP K ) ` x ) ) } /\
          ( _D ` F ) C_ ( ( ( int ` T ) ` A ) X. CC ) ) ) $=
      ( cc cr wa cfv wcel wceq co csubsp cvv vf wf wss cdv cv cnt cmin cdiv cif
      cmpt ccnp copab cxp cdm cpm wb feq2d sseq1d anbi12d adantr ibir cnex reex
      fdm elpm2 sylibr fveq2d eleq2d mpteq1 syl oveq1d cop df-ov eqtr4i syl6eqr
      fveq1d eleq12d opabbidv ctop crn ctg retop eqeltri simplr uniretop unieqi
      cioo cuni ntrss2 sylancr simprl sseldd iftrue eqid vex fvmpt ssexg stoig3
      sylancl syl5eqel cabs ccom cme cnmet opntop ax-mp syl6sseq stoig2 eleqtrd
      a1i simprr cnmetba uniopn2 cnpf syl31anc ffvelrn eqeltrrd expr imdistanda
      syl2anc ssopab2dv df-xp syl6sseqr eqsstrd fvex xpex fveq2i fveq1i syl5eqr
      copn dmeq fveq1 oveq12d ifeq2d mpteq12dv oveq2i df-dv fvmptg eqtrd jca )
      DLFUBZDMUCZNZFUDOZAUEZDEUFOZOZPZCDCUEZUUEQZBUEZUUIFOZUUEFOZUGRZUUIUUEUGRZ
