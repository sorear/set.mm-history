    $( The set of differentiable points is a subset of the domain of the
       function. $)
    dvbss $p |- ( ph -> dom ( S _D F ) C_ A ) $=
      ( cdv co cfv eqid ctop wcel cuni wss cvv ax-mp cc sylancr wceq cdm csubsp
      cabs cmin ccom copn cop cnt dvbssntr cme cnmet opntop cnex sylancl stoig3
      ssexg cnmetba uniopn2 syl6sseq stoig2 sseqtrd ntrss2 syl2anc sstrd ) ACDH
