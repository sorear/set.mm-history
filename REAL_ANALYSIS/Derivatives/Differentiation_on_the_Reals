    $( The product rule for derivatives at a point.  (Contributed by Mario
       Carneiro, 9-Aug-2014.) $)
    dvmul $p |- ( ph -> ( ( S _D ( F oF x. G ) ) ` C ) =
                  ( ( ( ( S _D F ) ` C ) x. ( G ` C ) ) +
                    ( ( ( S _D G ) ` C ) x. ( F ` C ) ) ) ) $=
      ( co cfv cc wcel syl cmin vz cmul cof cdv wfun caddc wbr wceq cdm wf dvfg
      cr cpr ffun cv cdiv cif cabs ccom copn cvv wss recnprss fvex a1i funfvbrb
      wb 3syl mpbid eqid dvmulbr ovex funbrfv sylc ) ACDEUBUCOZUDOZUEZBBCDUDOZP
