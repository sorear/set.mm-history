      TUAUBEABUCUDUE $.
      $( [26-Jun-2011] $) $( [28-Mar-2007] $)

    $( Members of disjoint sets are not equal. $)
    disjneOLD $p |- ( ( ( A i^i B ) = (/) /\ C e. A /\ D e. B ) ->
                   C =/= D ) $=
      ( vx cin c0 wceq wcel wne cv wn wral wi wa nelneq df-ne sylibr necomd ex
      eleq1 notbid rcla4cva syl5com disj sylanb 3impia ) ABFGHZCAIZDBIZCDJZEKZB
      IZLZEAMZUIUJUKNUHUJCBIZLZUKUOUIOUJUQUKUJUQOZDCURDCHLDCJDCBPDCQRSTUNUQECAU
      LCHUMUPULCBUAUBUCUDEABUEUFUG $.
      $( [28-Mar-2007] $)
  $}

  $( Two ways of saying that two classes are disjoint. $)
  disj2 $p |- ( ( A i^i B ) = (/) <-> A C_ ( _V \ B ) ) $=
    ( cvv wss cin c0 wceq cdif wb ssv reldisj ax-mp ) ACDABEFGACBHDIAJABCKL $.
    $( [17-May-1998] $)

  $( Two ways of saying that two classes are disjoint. $)
  disj4 $p |- ( ( A i^i B ) = (/) <-> -. ( A \ B ) C. A ) $=
