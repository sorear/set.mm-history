      $( [28-Oct-2004] $) $( [27-Sep-2004] $)
  $}

  ${
    $d x A $.  $d x B $.
    $( The subclass relationship is antisymmetric.  Compare Theorem 4 of
       [Suppes] p. 22. $)
    eqss $p |- ( A = B <-> ( A (_ B /\ B (_ A ) ) $=
      ( vx cv wcel wb wal wi wa wceq wss albi dfcleq dfss2 anbi12i 3bitr4i ) CD
      ZAEZQBEZFCGRSHCGZSRHCGZIABJABKZBAKZIRSCLCABMUBTUCUACABNCBANOP $.
      $( [5-Aug-1993] $)
  $}

  ${
    eqssi.1 $e |- A (_ B $.
    eqssi.2 $e |- B (_ A $.
    $( Infer equality from two subclass relationships.  Compare Theorem 4 of
       [Suppes] p. 22. $)
    eqssi $p |- A = B $=
      ( wceq wss eqss mpbir2an ) ABEABFBAFABGCDH $.
      $( [9-Sep-1993] $)
  $}

  ${
    eqssd.1 $e |- ( ph -> A (_ B ) $.
    eqssd.2 $e |- ( ph -> B (_ A ) $.
    $( Equality deduction from two subclass relationships.  Compare Theorem 4
       of [Suppes] p. 22. $)
    eqssd $p |- ( ph -> A = B ) $=
      ( wss wa wceq jca eqss sylibr ) ABCFZCBFZGBCHALMDEIBCJK $.
      $( [28-Jun-2004] $) $( [27-Jun-2004] $)
  $}

  $( Any class is a subclass of itself.  Exercise 10 of [TakeutiZaring]
     p. 18. $)
  ssid $p |- A (_ A $=
    ( wss wceq wa eqid eqss mpbi pm3.26i ) AABZIAACIIDAEAAFGH $.
    $( [5-Aug-1993] $)

  ${
    $d A x $.
    $( Any class is a subclass of the universal class. $)
    ssv $p |- A (_ V $=
      ( vx cvv cv elisset ssriv ) BACBDAEF $.
      $( [31-Oct-1995] $)
  $}

  $( Equality theorem for subclasses. $)
  sseq1 $p |- ( A = B -> ( A (_ C <-> B (_ C ) ) $=
    ( wss wb wa wi wceq sstr2 anim12i eqss dfbi2 3imtr4i eqcoms ) ACDZBCDZEZBAB
    ADZABDZFOPGZPOGZFBAHQRTSUABACIABCIJBAKOPLMN $.
    $( [5-Aug-1993] $)

  $( Equality theorem for the subclass relationship. $)
  sseq2 $p |- ( A = B -> ( C (_ A <-> C (_ B ) ) $=
    ( wss wa wi wceq wb sstr2 com12 anim12i eqss dfbi2 3imtr4i ) ABDZBADZECADZC
    BDZFZRQFZEABGQRHOSPTQORCABIJRPQCBAIJKABLQRMN $.
    $( [25-Jun-1998] $)

  $( Equality theorem for the subclass relationship. $)
  sseq12 $p |- ( ( A = B /\ C = D ) -> ( A (_ C <-> B (_ D ) ) $=
    ( wceq wss sseq1 sseq2 sylan9bb ) ABEACFBCFCDEBDFABCGCDBHI $.
    $( [31-May-1999] $)

  ${
    sseq1i.1 $e |- A = B $.
